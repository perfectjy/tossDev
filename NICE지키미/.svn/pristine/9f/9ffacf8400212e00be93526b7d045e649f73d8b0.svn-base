/* 월 datepicker */
.mthW .listWrap, .mthW .keyList{margin-top:15px;}
.mthW .btnBox{margin-top:20px;}
.mthW .h2{margin-top:40px;}
.mthW .h2:first-of-type{margin-top:0;}
.mthW.t1{margin-top:-35px;}
.mthInfo{display:flex; align-items:center; min-height:50px; margin-top:40px;}
.mthInfo .dateItem{margin-left:-5px;}
.mthInfo .opt{margin-left:auto;}
.mthInfo .opt .ct{position:relative; padding-left:17px; color:#767676;}
.mthInfo .opt .ct:before{content:''; position:absolute; top:5px; left:0; display:inline-block; width:12px; height:12px; border-radius:2px; background-color:#c6cfdb;}
.dateItem{display:flex; align-items:center;}
.dateItem .intDate{position:static; display:flex; align-items:center; margin:0 10px; color:#444; font-size:15px; font-weight:700;}
.dateItem .intDate input{width:20px; height:30px; padding:0; color:#066bfd; font-size:16px; font-weight:700; text-align:center; border:0;}
.dateItem .btnDate{position:relative; right:0; top:0; display:inline-block; width:24px; height:24px;}
