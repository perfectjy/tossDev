.matchList.peer .mine span:not(.tit):after{content:''; position:absolute; left:50%; bottom:-2px; display:block; width:5px; height:5px; background:#2e3653; transform:translateX(-50%) rotate(45deg);}
.matchList.peer .sortBar .mine .barArea .bar, .matchList.peer .sortBar .age .barArea .bar{background:#9fc6ff;}
.matchList.peer .sortBar .age .barArea .bar:before{content:''; position:absolute; top:0; right:0; display:block; width:80px; height:1px; border-top:1px dashed #000;}
.matchList.peer .age .tit, .matchList.peer .age .bar span{color:#222;}
.matchList.peer .sortBar{min-width:280px; width:auto; margin:0 auto;}
.matchList.peer .sortBar>li{display:block; flex:1; width:100%;}
.matchList.peer .sortBar>li .tit{width:100%; position:absolute; left:50%; transform:translateX(-50%);}
.matchList.peer .sortBar .barArea{justify-content:center;}
.matchList.peer .sortBar .barArea .bar{position:relative; width:25px; background:#eee; border-top-right-radius:2px; border-top-left-radius:2px;}
.matchList.peer .sortBar .barArea .bar.plus{position:absolute; top:0; right:0; left:auto; width:25px; display:inline-block; min-width:auto; background:#066bfd; border-bottom-left-radius:0; border-bottom-right-radius:0; transform:none;}
.matchList.peer .sortBar .barArea .bar.plus:after{display:none;}
.matchList.peer .barArea span{color:#767676;}
.matchCont{margin-top:70px;}
.matchCont .opt{color:#767676; text-align:right;}

/* 내 또래 비교 e */
.matchBar .bar span:first-child{border-right:2px solid #fff;}
.matchBar .bar .cardA, .matchBar .bar .cardB{display:inline-block;}
.matchBar .bar .cardA{background:rgb(74,175,255); background:linear-gradient(90deg, rgba(74,175,255,1) 0%, rgba(53,118,255,1) 100%); border-top-left-radius:4px; border-bottom-left-radius:4px;}
.matchBar .bar .cardB{background:rgb(255,120,113); background:linear-gradient(90deg, rgba(255,120,113,1) 0%, rgba(255,188,183,1) 100%); border-top-right-radius:4px; border-bottom-right-radius:4px;}
.matchBar .cardContA, .matchBar .cardContB{position:absolute;}
.matchBar .cardContA span, .matchBar .cardContB span{display:block; margin-top:9px; color:#767676; font-size:14px; line-height:1;}
.matchBar .cardContA, .matchBar .cardContA .tip{left:0;}
.matchBar .cardContA span{text-align:left;}
.matchBar .cardContB, .matchBar .cardContB .tip{right:0;}
.matchBar .cardContB span{text-align:right;}

.sortInfo{display:flex; justify-content:left; align-items:center;}
.sortInfo>li{color:#767676;}
.sortInfo>li:not(:first-child){padding-left:12px;}
.sortInfo>li:before{content:''; display:inline-block; width:13px; height:13px; margin-right:5px; border-radius:3px;}
.sortInfo .c1:before{background:#ee2e63 url(../../img/bzw/common/bg/bg_info_pattern01.png) center no-repeat; background-size:13px;}
.sortInfo .c2:before{background:#975bc5 url(../../img/bzw/common/bg/bg_info_pattern02.png) center no-repeat; background-size:7px 13px;}
.sortInfo .c3:before{background:#4b9ef7 url(../../img/bzw/common/bg/bg_info_pattern03.png) center repeat-x; background-size:7px;}
.sortInfo .c4:before{background:#18987b url(../../img/bzw/common/bg/bg_info_pattern04.png) center no-repeat; background-size:9px;}

.sortCont{position:relative; display:flex; align-items:flex-end; height:177px; background:url(../../img/bzw/common/bg/bg_graph01.png) repeat-x; background-size:1px 177px;}
.sortCont .perText{display:flex; justify-content:space-between; flex-direction:column; height:100%; position:absolute; right:-45px; bottom:0;}
.sortCont .perText>li{margin:-5px 0 0; color:#767676; font-size:13px; line-height:1;}
.sortBar{width:100%; height:100%; display:flex; justify-content:space-between;}
.sortBar>li{display:flex; align-items:flex-end; position:relative; margin:0 auto; text-align:center;}
.sortBar>li .tit{position:relative; left:50%; bottom:-30px; display:inline-block; color:#767676;}
.sortBar .barArea{height:100%; display:flex; justify-content:left; align-items:flex-end; padding-bottom:1px;}
.sortBar .barArea .bar{position:relative; width:9px; height:auto; margin-right:7px; bottom:0; border-top-left-radius:3px; border-top-right-radius:3px;}
.sortBar .barArea .bar:last-child{margin-right:0;}
.sortBar .barArea span{position:absolute; left:-20px; top:-20px; min-width:60px; font-size:13px;}
.sortBar .barArea span i{padding-right:1px; letter-spacing:-1px;}
.sortBar .barArea .c1{background:#ee2e63 url(../../img/bzw/common/bg/bg_pattern01.png) repeat; background-size:4px;}
.sortBar .barArea .c2{background:#975bc5 url(../../img/bzw/common/bg/bg_pattern02.png) repeat; background-size:6px 9px;}
.sortBar .barArea .c3{background:#4b9ef7 url(../../img/bzw/common/bg/bg_pattern03.png) repeat; background-size:9px;}
.sortBar .barArea .c4{background:#18987b url(../../img/bzw/common/bg/bg_pattern04.png) repeat; background-size:9px 4px;}

/* 소비현황 */
.flagList{display:flex;}
.flagList>li{display:flex; flex-direction:column; margin-right:5px;}
.flagList>li:last-child{margin-right:0;}
.flagList>li .tag{height:22px;}
.flagList>li strong{display:inline-block; margin-top:12px; font-size:21px; font-weight:700;}
.flagList>li .bl{color:#066bfd;}
.flagList>li .re{color:#ce4a4a;}
.flagList>li .gr{color:#18987b;}
.resultTxt{margin-top:22px; font-size:15px;}
.resultTxt strong{font-weight:700;}
.resultTxt .tit{font-size:21px;}
.resultTxt p:not(.exp){margin-top:5px; color:#222;}

.infoTitBox .h2{margin-top:40px;}
.infoTitBox .payItem{display:inline-flex; min-width:255px; margin:15px 2px 0 0;}

/* 신용카드로 얼마나 빌릴 수 있을까? */
/* 카드 사용금액  */
.barList .fRow{display:flex; align-items:center;}
.barList>li{margin-top:25px;}
.barList .h4{margin:0 0 10px 0; font-weight:700;}
.barList .infoTxt{display:flex; justify-content:space-between; margin-top:12px; line-height:1;}
.barList .infoTxt dt{color:#767676; font-size:14px;}
.barList .infoTxt dd{color:#222; font-size:17px;}
.barList .infoTxt dd strong{font-weight:700;}
.barList .bar{position:relative; display:block; width:100%; height:10px; border-radius:4px; background:#eef3f7;}
.barList .bar i{display:block; height:10px; border-radius:4px; background:rgb(54,118,254); background:linear-gradient(90deg, rgba(54,118,254,1) 0%, rgba(255,120,113,1) 100%);}
.barList.rank>li{position:relative; margin-top:30px;}
.barList.rank>li:first-child{margin-top:15px;}
.barList.rank .num{position:absolute; top:0; left:0; font-size:27px; font-family:'roboto'; font-weight:700;}
.barList.rank .groupItem{padding-left:25px;}

/* 이모지 타입 */
.emjTxt{position:relative; margin-top:10px; font-size:15px;}
.emjTxt strong{font-weight:700;}
.emjTxt:after{content:''; display:inline-block; width:30px; height:30px; margin-left:7px; vertical-align:middle; background-image:url(../../img/bzw/sub/ico_emo11.png); background-position:center; background-repeat:no-repeat; background-size:30px;}
.emjTxt.good:after{background-image:url(../../img/bzw/sub/ico_emo11.png);}
.emjTxt.bad:after{background-image:url(../../img/bzw/sub/ico_emo11.png);}

.ineBox .resultTxt{margin-top:22px;}
.ineBox .inList>li{margin-top:15px;}
.ineBox .inList>li .link{position:relative; display:inline-flex; justify-content:space-between; align-items:center; min-width:130px; line-height:1; font-size:16px; font-weight:700;}
.ineBox .inList>li .link:after{content:''; position:absolute; top:50%; right:0; display:inline-block; width:8px; height:8px; border-top:2px solid #767676; border-right:2px solid #767676; transform:translateY(-50%) rotate(45deg);}
.ineBox .inList>li .link .cnt{padding-right:18px; font-size:21px;}
.ineBox .inList>li:first-child{margin-top:0;}
.ineBox .cardSum{display:flex; align-items:center; margin-top:28px; font-size:21px;}
.ineBox .cardSum span{font-size:30px; font-weight:700; letter-spacing:-1.2px;}

/* NICE평가정보 & 신용정보원 (카드사 기준) */
/* 결재방법 선택 */
.listWrap{margin-top:25px;}
.listWrap.pay{margin-top:0;}
.listWrap.pay>li:not(:first-child){margin-top:20px;}
.listWrap.pay>li{display:flex; align-items:center; justify-content:space-between;}
.listWrap.pay .orgItem{display:flex; align-items:center;}
.listWrap.pay .orgItem i{margin-right:10px;}
.listWrap.pay strong{flex:1; font-size:17px; text-align:right;}
