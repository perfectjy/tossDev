.useMain{padding:0 20px 30px 20px; margin:0 -20px 50px -20px; background:rgb(239,242,248); background:linear-gradient(360deg, rgba(239,242,248,1) 0%, rgba(255,255,255,1) 100%);}
.useMain .governing{margin-bottom:15px;}

.governingTitle .btn.ics2{margin-top:-4px;}

a.governingTitle{display:inline-block; text-align:left;}
a.governingTitle:after{content:''; display:inline-block; width:8px; height:8px; margin:-3px 0 0 7px; vertical-align:middle; border-right:2px solid #767676; border-top:2px solid #767676; transform:rotate(45deg);}

.governingAmount .tagList{margin-top:35px;}
.governingAmount .tit{margin-top:15px; line-height:1;}
.governingAmount .tit .btn{margin-top:-5px;}
.governingAmount .tit strong{padding-right:3px; vertical-align:middle; letter-spacing:-1.2px;}
.governingAmount .exp{margin-top:5px;}
.dRight{font-weight:700;}

.userAmount{margin-top:40px;}
.userAmount .ineBox{overflow:visible;}
.userAmount .tooltipWrap{margin-left:5px;}
.userAmount .governing{display:flex; justify-content:space-between; align-items:flex-start; margin-bottom:0; text-align:left;}
.userAmount .governing.fCol{flex-direction:column;}
.userAmount .governing .btnArrow{margin-top:3px;}
.userAmount .btnConts{margin-top:20px;}

.consume .ineBox{overflow:visible; padding:22px 25px; border:0; box-shadow:1px 4px 7px 0px rgb(204 204 204 / 40%);}
.consume .ineBox .emo{padding-top:12px;}
.consume .fx{flex:none;}
.consume .fx .exp{margin-top:0;}
.consume .fx .tit{display:block; margin-top:18px; color:#767676; font-weight:700;}
