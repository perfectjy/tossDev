.idxInfo strong span{padding-right:5px; vertical-align:middle; letter-spacing:-1.2px; font-size:30px; font-weight:700;}
.idxInfo .tooltipWrap{margin:-3px 0 0 7px;}

/* 어디에 많이 썼을까요 */
