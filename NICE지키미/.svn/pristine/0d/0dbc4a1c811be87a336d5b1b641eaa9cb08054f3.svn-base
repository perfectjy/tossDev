/* 서브메인 */
.governingTitle .btn.ics2{margin-top:-4px;}

a.governingTitle{display:inline-block; text-align:left;}
a.governingTitle:after{content:''; display:inline-block; width:8px; height:8px; margin:-3px 0 0 7px; vertical-align:middle; border-right:2px solid #767676; border-top:2px solid #767676; transform:rotate(45deg);}

.useMain .governing{position:relative;}
.useMain .optRight{position:absolute; right:0; bottom:0; color:#767676; font-size:12px;}
.useMain .consume{padding-top:30px; margin-top:40px; border-top:1px solid #eee;}

.consume .governing{margin-bottom:20px;}
.consume .ineBox{overflow:visible; padding:30px 25px;}
.consume .ineBox .emo{padding-top:12px;}
.consume .ineBox .sum strong{font-size:27px;}
.consume .ineBox .fRow:first-child{align-items:center;}
.consume .tooltipWrap{margin-left:5px;}

/* .userAmount{margin-top:40px;} */
.userAmount .ineBox{overflow:visible; margin-top:25px;}
.userAmount .tooltipWrap{margin-left:5px;}
.userAmount .governing{display:flex; justify-content:space-between; align-items:flex-start; margin-bottom:0; text-align:left;}
.userAmount .governing.fCol{flex-direction:column;}
.userAmount .governing .btnArrow{margin-top:3px;}
.userAmount .btnConts{margin-top:20px;}
.userAmount .infoToggle.pay .payItem{margin-top:25px;}

.infoPay{position:relative; margin-top:25px; padding:24px 9px 9px; border:1px solid #d4d4d4; border-radius:15px;}
.infoPay .tagList{position:absolute; top:24px; left:24px; margin-top:0;}
.infoPay .bgBoxList{margin-top:10px;}
.infoPay .infoToggle.pay{margin-top:0;}
.infoPay .infoTogConts{margin-left:15px;}
.infoPay .infoTogConts .payItem:first-child{margin-top:0;}
.infoPay .payBox{padding:30px 10px 0 15px;}
.infoPay .payItem{display:flex; align-items:center; justify-content:left; padding:0; margin-top:20px; border:0; border-radius:0;}
.infoPay .payItem:not(:first-of-type){margin-top:20px;}
.infoPay .payItem .payCont{padding-left:15px;}
.infoPay .payItem .payCont strong{padding-top:2px; line-height:1; font-size:17px; font-weight:400;}
.infoPay .payItem .payCont strong em{font-size:18px; font-weight:500;}
.infoPay .inTop{margin:0 15px; align-items:flex-end;}
.infoPay .inTop strong{letter-spacing:-1px;}
.infoPay .inTop dl dd{margin-top:12px; font-size:17px; line-height:1;}
.infoPay .inTop dl dd strong{font-size:21px; vertical-align:top;}
.infoPay .inTop .cnt{font-size:17px;}
.infoPay .fRow{display:flex; justify-content:space-between;}
.infoPay .fCol{display:flex; flex-direction:column;}

.infoCard{margin-top:25px;}
.infoCard .lineList{display:flex; flex-wrap:wrap; justify-content:space-between; margin-top:4px;}
.infoCard .lineList>li{box-sizing:border-box; display:inline-flex; align-items:center; width:calc(50% - 5px); padding:12px 14px; margin:10px 10px 0 0; border:1px solid #d4d4d4; border-radius:10px;}
.infoCard .lineList>li i{display:inline-block; width:38px; height:38px; margin-right:10px;}
.infoCard .lineList>li i img{max-width:100%; max-height:100%;}
.infoCard .lineList>li:nth-child(2n){margin-right:0;}
.infoCard .noList{margin-bottom:45px;}

.payItem{display:flex; align-items:center; justify-content:left; padding:20px 24px; border:1px solid #d4d4d4; border-radius:15px;}
.payItem.active{border:2px solid #066bfd;}
.payItem i{display:inline-block; width:38px; height:38px;}
.payItem i img{max-width:100%; max-height:100%;}
.payItem .payCont{padding-left:10px;}
.payItem .payCont p{line-height:22px; font-size:14px; font-weight:400;}
.payItem .payCont .date{color:#767676; font-size:13px;}
.payItem .payCont .date span{padding-left:3px;}
.payItem .payCont b, .payItem .payCont strong{display:block; font-weight:700;}
.payItem .inItem{display:flex; width:100%;}
.payItem .ex{margin-top:15px; color:#767676; font-size:12px; line-height:20px;}
.payItem dl{width:100%; display:flex; justify-content:space-between; align-items:center; padding-top:12px;}
.payItem dl dt{color:#767676; font-size:13px;}
.payItem dl dd{font-size:17px; font-weight:400; text-align:right;}
.payItem dl dd strong{padding-right:2px; font-size:23px; font-weight:700; vertical-align:top; letter-spacing:-0.5px;}
.totalBox{margin-top:25px;}
.totalBox dd{padding-right:10px; font-size:17px; font-weight:400; text-align:right;}
.totalBox dd .btn{margin-top:-3px;}
.totalBox dd strong{padding-right:2px; font-size:23px; font-weight:700; vertical-align:top; letter-spacing:-0.5px;}


/* toggle */
.infoToggle.pay{margin-top:30px;}
.infoToggle.pay .infoTogConts{margin-top:20px;}
.infoToggle.pay .tit{width:100%; text-align:right;}
.infoToggle.pay .tit .infoTogBtn{position:relative; margin-right:38px; padding-right:3px; color:#444;}
.infoToggle.pay .tit .infoTogBtn:after, .infoToggle.pay .governingTitle .infoTogBtn:after{content: ''; position:absolute; right:-24px; top:-1px; width: 24px; height: 24px; background:url(../../../img/zkm/bzw/common/ico/ico_acco_arr.png) no-repeat; background-size: 100%; transform: rotate(0);}
.infoToggle.pay .governingTitle .infoTogBtn{position:relative;}
.infoToggle.pay .governingTitle .infoTogBtn:after{top:auto; right:-30px; bottom:3px;}
.infoToggle.pay .infoTogBtn.active:after{transform:rotate(180deg);}
.infoToggle.pay .listWrap, .infoToggle.pay .payItem:first-child{margin-top:0;}
.infoToggle.date .dateList{margin-bottom:30px;}
.infoToggle.date .tit{position:absolute; top:10px; right:0;}
.infoToggle.date .infoTogBtn{display:inline-block; height:30px; padding:0 14px; line-height:28px; text-align:center; font-size:13px; border:1px solid #000; border-radius:7px;}

.cprItem{margin-top:15px; text-align:right; color:#767676; font-size:12px;}
.cprItem span{padding-right:2px; letter-spacing:-1px; font-size:13px;}

.bgBoxList{display:flex; justify-content:space-between; flex-wrap:wrap;}
.bgBoxList>li{box-sizing:border-box; width:calc(50% - 5px); margin:10px 10px 0 0; padding:19px 15px; background:#f8f9fb; border-radius:10px;}
.bgBoxList>li.active{background:#fff; border:2px solid #066bfd;}
.bgBoxList>li:nth-child(2n){margin-right:0;}
.bgBoxList>li dl dt{position:relative; color:#767676; line-height:22px;}
.bgBoxList>li dl dt .age{position:absolute; right:0; top:0; color:#066bfd; font-weight:700;}
.bgBoxList>li dl dd{margin-top:15px; text-align:right; font-size:16px; line-height:1;}
.bgBoxList>li dl dd strong{padding-right:3px; vertical-align:text-bottom; letter-spacing:-0.5px; font-size:21px; font-weight:700;}
.bgBoxList>li .flag{display:inline-block; height:20px; padding:0 10px; margin-top:10px; line-height:18px; text-align:center; color:#fff; font-size:13px; background:#2e3653; border-radius:5px;}
.bgBoxList>li .flag i{font-size:14px;}
.bgBoxList>li .link dt{color:#222; font-size:15px;}
.bgBoxList>li .link dd{font-size:13px;}
.bgBoxList>li .link dd strong{font-size:17px;}

.linkArea .bgBoxLink, .bnrBox{margin-top:20px;}
/* .linkArea .bgBoxLink{padding:20px;} */
.linkArea .bgBoxLink:first-child{margin-bottom:0;}
.linkArea .bgBoxLink .link{position:absolute; top:0; left:0; right:0; bottom:0; margin:0;}
.linkArea .bgBoxLink .link:after{content: ''; position: absolute; right: 20px; top: 50%; width: 8px; height: 8px; border-top: 2px solid #767676; border-right: 2px solid #767676; transform: rotate(45deg) translateY(-50%);}
.linkArea .bgBoxLink p{width:auto;}
.linkArea .bnrBox.card{padding-left:75px;}
.linkArea .bnrBox.card:before{background-color:transparent; left:5px;}
.linkArea .bnrBox.card p{margin-top:5px; color:#222; font-size:13px;}
.linkArea .bnrBox.card span{font-weight:500;}
.linkArea .bnrBox.card strong{display:block; font-size:16px; font-weight:700;}
.linkArea .bnrBox.card strong i{font-size:17px;}

/* 카드발급 가능지수 */
.userCredit{margin-top:40px;}
.userCredit .governing{margin-bottom:35px;}
.userCredit .idxInfo:after{content:''; position:absolute; top:0; right:-10px; display:block; width:95px; height:95px; background:url(../../../img/zkm/bzw/common/bg/bg_Ucard.png) 50% no-repeat; background-size:95px;}
.idxInfo{position:relative;}
.idxInfo h2, .idxInfo strong{color:#066bfd;}
.idxInfo .fRow{display:flex; align-items:center; margin-top:10px; color:#767676; line-height:22px;}
.idxInfo .cnt{display:block; width:20px; height:20px; margin-left:5px; text-align:center; line-height:20px; color:#222; font-size:12px; background:#eff2f8; border-radius:50%;}
.idxInfo strong{display:block; margin-top:8px; line-height:1; font-size:21px;}
.idxInfo strong span{padding-right:5px; vertical-align:middle; font-size:30px; font-weight:700;}
.idxInfo .tooltipWrap{margin-left:3px;}

/* 어디에 많이 썼을까요 */
.userCons .h2:first-child{margin-top:0;}
.userCons [class*="group"] .fRow{display:flex; align-items:center; justify-content:space-between;}
.userCons .bnrItem{margin-top:20px;}
.gCont{position:relative; display:flex; align-items:center; justify-content:space-between;}
.gCont:before{content:''; display:inline-block; width:40px; height:40px; margin-right:10px; background-size:40px; background-position:50%; background-repeat:no-repeat;}
.groupBox{position:relative; padding:26px 23px; margin-top:20px; border:1px solid #d4d4d4; border-radius:15px;}
.groupBox .fRow{width:calc(100% - 55px);}
.groupBox a{position:absolute; left:0; top:0; bottom:0; right:0;}
.groupBox .h3{position:relative; display:block; margin-bottom:18px; font-size:17px; font-weight:700; line-height:25px;}
.groupBox .h3:after{content:''; display:inline-block; width:8px; height:8px; margin:-3px 0 0 7px; vertical-align:middle; border-right:2px solid #767676; border-top:2px solid #767676; transform:rotate(45deg);}
.groupBox .gCont dt{width:40px; height:40px; margin-right:15px;}
.groupBox .gCont dd{line-height:1; font-size:21px;}
.groupBox .gCont dd p{flex:1; color:#222; font-size:14px; text-align:right;}
.groupBox .gCont strong{padding-right:5px; font-size:30px; vertical-align:middle; letter-spacing:-1.2px;}
.groupBox .gCont em{padding-right:1px; font-size:17px; vertical-align:top;}
.groupItem .gCont dt{flex:1; font-size:17px; font-weight:700;}
.groupItem .gCont dd strong{color:#222;}
.groupItem .gCont strong{padding-right:2px; letter-spacing:-1px; font-size:17px; font-weight:700;}
.groupItem .barList>li{display:flex; align-items:flex-end; justify-content:space-between; margin-top:5px;}
.groupItem .barList>li:first-child strong{line-height:1; font-size:19px;}
.groupItem .barList>li:first-child strong span{padding-right:3px; font-size:27px; letter-spacing:-1px; vertical-align:text-bottom;}
.groupItem .barList>li:last-child{flex-direction:column; width:100%; margin-top:3px; color:#767676;}
.groupItem .barList>li:last-child .fRow span{padding-right:3px; font-size:17px;}
.groupItem .barList .fRow{width:100%;}
.groupItem .barList .bar{height:10px; margin-right:10px; background:#c6cfdb; border-radius:4px;}
.groupItem.ct3 .barList>li:first-child .bar{background:#ff4c6a;}
.groupItem.ct3 .barList>li:first-child strong{color:#ff4c6a;}
.groupItem.ct5 .barList>li:first-child .bar{background:#ffa200;}
.groupItem.ct5 .barList>li:first-child strong{color:#ffa200;}
.groupItem.ct4 .barList>li:first-child .bar{background:#00aa88;}
.groupItem.ct4 .barList>li:first-child strong{color:#00aa88;}

.groupItem dl{width:100%;}
.groupItem dl dd{color:#222; font-size:12px;}
.groupItem dl dd span{padding-right:1px; font-size:13px; letter-spacing:-0.5px;}

.ct1 .gCont:before{background-image:url(../../../img/zkm/bzw/common/bg/bg_consume01.png);}  /* 교육 */
.ct2 .gCont:before{background-image:url(../../../img/zkm/bzw/common/bg/bg_consume02.png);}  /* 교통 */
.ct3 .gCont:before{background-image:url(../../../img/zkm/bzw/common/bg/bg_consume03.png);}  /* 쇼핑 */
.ct4 .gCont:before{background-image:url(../../../img/zkm/bzw/common/bg/bg_consume04.png);}  /* 마트/편의점 */
.ct5 .gCont:before{background-image:url(../../../img/zkm/bzw/common/bg/bg_consume05.png);}  /* 외식 */
.ct5 .ctTit, .ct5 .gCont strong, .ct5 dd{color:#ffa200;}
.ct6 .gCont:before{background-image:url(../../../img/zkm/bzw/common/bg/bg_consume06.png);}  /* 취미/여가 */
.ct7 .gCont:before{background-image:url(../../../img/zkm/bzw/common/bg/bg_consume07.png);}  /* 생활 */
.ct8 .gCont:before{background-image:url(../../../img/zkm/bzw/common/bg/bg_consume08.png);}  /* 유흥 */
.ct9 .gCont:before{background-image:url(../../../img/zkm/bzw/common/bg/bg_consume09.png);}  /* 미용 */
.ct10 .gCont:before{background-image:url(../../../img/zkm/bzw/common/bg/bg_consume10.png);}  /* 금융/세금 */
.ct11 .gCont:before{background-image:url(../../../img/zkm/bzw/common/bg/bg_consume11.png);}  /* 기부 */
.ct12 .gCont:before{background-image:url(../../../img/zkm/bzw/common/bg/bg_consume12.png);}  /* 병원 */
.ct13 .gCont:before{background-image:url(../../../img/zkm/bzw/common/bg/bg_consume13.png);}  /* 기타 */

/* 내 소비습관 카드추천 */
.bnrItem{position:relative; min-height:306px; display:flex; flex-direction:column; align-items:center; padding-top:55px; margin-top:50px; text-align:center; background:#edf0ff; border-radius:15px;}
.bnrItem:after{content:''; display:block; width:130px; height:130px; background-repeat:no-repeat; background-position:center; background-size:130px;}
.bnrItem.card:after{background-image:url(../../../img/zkm/bzw/common/bg/bg_Ucard02.png);}
.bnrItem p{margin-top:20px; line-height:23px;}
.bnrItem a{position:absolute; top:0; right:0; bottom:0; left:0;}

/* 지난 3개월간 카드생활 */
.myLife{margin-top:45px;}
.myLife .optRight{margin-top:2px; color:#767676; font-size:14px;}
.myLife .link{display:inline-block; margin-top:20px; color:#222; font-size:15px; line-height:23px;}
.myLife .link strong{font-weight:700;}
.myLife .link:after{content:''; display:inline-block; width:6px; height:6px; margin:-3px 0 0 9px; vertical-align:middle; border-right:2px solid #767676; border-top:2px solid #767676; transform:rotate(45deg);}
.myLife .infoToggle.detail:before{content:''; position:absolute; left:0; top:20px; display:block; width:100%; height:1px; background-color:#eee;}
.infoToggle.detail{position:relative; margin-top:65px;}
.infoToggle.detail .tit{position:relative; min-width:30px; display:flex; justify-content:center; align-items:center; color:#222;}
.infoToggle.detail .infoTogBtn{width:45px; height:45px; border:1px solid #dcdff1; border-radius:23px; background-color:#fff;}
.infoToggle.detail .infoTogBtn:before{position:absolute; top:50%; left:50%; display:block; content:''; width:10px; height:10px; margin-top:-2px; border-top:2px solid #222; border-right:2px solid #222; transform:translate(-50%, -50%) rotate(135deg);}
.infoToggle.detail .infoTogBtn.active:before{margin-top:2px; transform:translate(-50%, -50%) rotate(315deg);}
.infoToggle.detail .matchList{margin-top:40px;}
.matchWrap{position:relative; margin-top:45px;}
.matchWrap .tit{position:absolute; top:30px; left:50%; display:flex; align-items:center; justify-content:center; min-width:65px; height:30px; color:#066bfd; border:1px solid #066bfd; border-radius:15px; transform:translateX(-50%);}
.matchBox{display:flex; align-items:center; justify-content:space-between;}
.matchBox dl:last-child dt{text-align:right;}
.matchBox dl dd{margin-top:15px; line-height:1; font-size:21px;}
.matchBox dl strong{vertical-align:middle; font-size:30px;}
.matchBox dl strong:not(.robotoB){display:block; margin-top:3px; color:#222; font-size:14px;}
.matchBox dl strong span{padding-right:2px; letter-spacing:-0.5px; vertical-align:top; font-size:17px;}
.matchBox .cardA dd{color:#066bfd;}
.matchBox .cardB dd{color:#ff1a4e;}
.matchList{margin-top:50px;}
.matchList>li{position:relative;}
.matchList>li:not(:first-child){margin-top:70px;}
.matchList .mTit{box-sizing:border-box; position:absolute; left:50%; top:-10px; z-index:1; display:inline-block; height:30px; padding:0 15px; line-height:28px; color:#767676; font-size:14px; background:#fff; border:1px solid #d4d4d4; border-radius:15px; transform:translateX(-50%);}
.matchList .matchBar .bar{display:flex; height:10px;}
.matchList .matchBar .tip{position:absolute; top:-38px; min-width:46px; height:20px; line-height:1; padding-top:4px; text-align:center; color:#fff; font-size:14px; background:#a7aab7; border-radius:10px;}
.matchList .matchBar .tip:after{content:''; position:absolute; left:50%; bottom:-2px; display:block; width:5px; height:5px; background:#a7aab7; transform:translateX(-50%) rotate(45deg);}
/* 소비생활 카테고리 비교 */
.matchList.cate .mTit{top:-20px; padding:0; color:#222; font-weight:700; text-align:center; border:0; background:none;}
.matchList.cate .mTit:before{content:''; display:block; width:40px; height:40px; margin:0 auto; border:2px solid #fff; border-radius:18px; background-position:center; background-repeat:no-repeat; background-size:40px;}
.matchList.cate .matchBar{display:flex; justify-content:space-between; margin:0 30px;}
.matchList.cate .cont{position:relative; flex:1; display:flex; flex-direction:column;}
.matchList.cate .cont:first-child{margin-right:40px;}
.matchList.cate .cont:first-child .num{left:-30px; right:auto;}
.matchList.cate .cont:first-child .bar{justify-content:flex-end; border-top-left-radius:5px; border-bottom-left-radius:5px;}
.matchList.cate .cont:first-child .bar .rate{border-right:0; border-radius:0; border-top-left-radius:5px; border-bottom-left-radius:5px;}
.matchList.cate .cont:first-child .item{left:0; text-align:left;}
.matchList.cate .cont .bar{border-top-right-radius:5px; border-bottom-right-radius:5px;}
.matchList.cate .cont .num{position:absolute; right:-30px; top:-15px; color:#767676; font-size:27px;}
.matchList.cate .cont.active .num{color:#222; font-weight:700;}
.matchList.cate .cont .item{min-height:60px; position:absolute; top:-25px; right:0; display:flex; flex-direction:column; justify-content:space-between; text-align:right; font-size:12px;}
.matchList.cate .cont .item p{line-height:1;}
.matchList.cate .cont .item span{color:#767676;}
.matchList.cate .cont .item i{font-size:15px;}
.matchList.cate .bar{height:10px; background:#eef3f7;}
.matchList.cate .bar span:first-child{border-right:0;}

.matchList.cate .ct3 .mTit:before{background-image:url(../../../img/zkm/bzw/common/bg/bg_consume03.png);}
.matchList.cate .ct3 .rate{background:#ff4c6a;}
.matchList.cate .ct4 .mTit:before{background-image:url(../../../img/zkm/bzw/common/bg/bg_consume04.png);}
.matchList.cate .ct4 .rate{background:#00aa88;}
.matchList.cate .ct5 .mTit:before{background-image:url(../../../img/zkm/bzw/common/bg/bg_consume05.png);}
.matchList.cate .ct5 .rate{background:#ffa200;}
/* 소비생활 카테고리 비교 e */
/* 내 또래 비교 */
.pweb .matchList.peer .sortBar{width:100%;}
.pweb .matchList.peer .sortBar>li{width:auto;}
.pweb .matchList.peer .sortBar .age .barArea .bar:before{width:185px;}
.matchList.peer, .matchList.peer .sortCont{margin-top:30px;}
.matchList.peer .sortCont{display:flex; align-items:flex-end; background:url(../../../img/zkm/bzw/common/bg/bg_graph01.png) repeat-x; background-size:1px 177px;}
.matchList.peer .sortBar{height:100%; min-width:280px; width:auto; padding-bottom:33px; display:flex; flex-direction:row; justify-content:space-between; margin:0 auto;}
.matchList.peer .sortBar>li{position:relative; display:block; flex:1;}
.matchList.peer .sortBar>li .tit{width:100%; position:absolute; left:50%; bottom:-30px; color:#767676; transform:translateX(-50%);}
.matchList.peer .sortBar .barArea{height:100%; display:flex; flex-direction:row; justify-content:center; align-items:flex-end; padding-bottom:1px;}
.matchList.peer .sortBar .barArea .bar{position:relative; width:25px; height:auto; bottom:0; background:#eee; border-top-right-radius:2px; border-top-left-radius:2px;}
