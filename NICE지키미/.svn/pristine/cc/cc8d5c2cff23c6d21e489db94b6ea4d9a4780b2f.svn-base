/* 내 또래 비교 */
.pweb .matchList.peer .sortBar{width:100%;}
.pweb .matchList.peer .sortBar li{width:auto;}
.pweb .matchList.peer .sortBar .age .barArea .bar:before{width:185px;}
.matchList{margin-top:50px;}
.matchList .sortCont{display:flex; align-items:flex-end; background:url(../../../img/zkm/rzm/common/bg/bg_graph01.png) repeat-x; background-size:1px 177px;}
.matchList .sortBar{height:100%; min-width:280px; width:auto; padding-bottom:33px; display:flex; flex-direction:row; justify-content:space-between; margin:0 auto;}
.matchList .sortBar li{position:relative; display:block; flex:1;}
.matchList .sortBar .tit{width:100%; position:absolute; left:50%; bottom:-26px; color:#222; font-size:13px; transform:translateX(-50%);}
.matchList .sortBar .barArea{height:100%; display:flex; flex-direction:row; justify-content:center; align-items:flex-end; padding-bottom:1px;}
.matchList .sortBar .barArea .bar{position:relative; width:25px; height:auto; bottom:0; background:#dde6ed; border-top-right-radius:2px; border-top-left-radius:2px;}
.matchList .sortBar .barArea .bar.plus{position:absolute; top:-6px; right:0; left:auto; width:25px; display:inline-block; min-width:auto; background:#066bfd; border-bottom-left-radius:0; border-bottom-right-radius:0; transform:none;}
.matchList .sortBar .barArea .bar.plus:before, .matchList .sortBar .barArea .bar.plus:after{display:none;}
.matchList .sortBar .barArea span{position:absolute; left:-25px; top:-20px; min-width:75px; font-size:13px;}
.matchList .sortBar .barArea span i{padding-right:1px; font-weight:bold; letter-spacing:-1px;}
.matchList .standard .barArea .bar{background:#066bfd;}
.matchList .sortBar .standard .tit{color:#066bfd; font-weight:bold;}
