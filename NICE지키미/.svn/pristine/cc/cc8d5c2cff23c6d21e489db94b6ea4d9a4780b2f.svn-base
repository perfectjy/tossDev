/* 서브메인 */
.governingTitle .btn.ics2{margin-top:-4px;}

a.governingTitle{display:inline-block; text-align:left;}
a.governingTitle:after{content:''; display:inline-block; width:8px; height:8px; margin:-3px 0 0 7px; vertical-align:middle; border-right:2px solid #767676; border-top:2px solid #767676; transform:rotate(45deg);}

.amountItem {position:relative; padding-bottom:80px; text-align:left; border-bottom:1px solid #eee;}
.amountItem .tit{font-size:21px;}
.amountItem .tit h2{font-size:19px;}
.amountItem .tit p{margin-top:10px; line-height:1; font-size:21px;}
.amountItem .tit strong{padding-right:2px; font-size:40px; font-weight:bold; vertical-align:sub;}
.amountItem .exp{margin-top:15px; font-size:15px;}
.amountItem .exp span{font-weight:bold;}
.amountItem .exp span i{padding-right:1px;}
.amountItem .dRight{position:absolute; right:0; bottom:30px; color:#767676;}

.payItem{position:relative; display:flex; justify-content:left; margin-top:25px;}
.payItem .btn.reload{position:absolute; right:-3px; top:1px;}
.payItem.first{margin-top:0;}
.payItem.none i{background:url(../../../img/zkm/rzm/common/bg/bg_noCard.png) 50% no-repeat; background-size:38px;}
.payItem.none i:before{content:''; position:absolute; z-index:-1; display:block; width:38px; height:38px; border-radius:50%; background:#f9f9f9;}
.payItem i:not(.num){display:inline-block; width:38px; height:38px; margin-right:10px;}
.payItem i img{max-height:100%;}
.payItem strong{display:block; padding-right:25px; font-weight:400;}
.payItem strong span{padding-right:1px; font-weight:bold; font-size:17px; line-height:25px;}
.payItem a{position:relative; width:100%; display:flex; justify-content:space-between; align-items:center;}
.payItem .cont.plex{display:flex; flex-direction:column; flex:1; align-self:center;}
.payItem .cont p{font-size:14px; font-weight:500;}
.payItem .cont span{color:#767676; font-size:12px; letter-spacing:0;}
.payItem .flag {display:inline-block; width:auto; height:22px; line-height:20px; padding:0 10px; margin:8px 0 0 10px; color:#444; font-size:12px; background:#eee; border-radius:11px;}
.payItem .link:before{content:''; display:block; position:absolute; right:0; transform:rotate(225deg) translateY(-50%); width:8px; height:8px; border:2px solid #767676; border-right:0; border-top:0;}

.ppItem{position:relative; display:flex; align-items:center; justify-content:space-between; margin-top:10px;}
.ppItem .btn.reload{margin:0 -8px -1px 2px;}
.ppItem p{flex:1;}
.ppItem strong{font-size:14px;}
.ppItem strong span{font-size:17px; font-weight:bold;}
.ppItem .btnArrow{width:20px; height:20px; padding:0;}
.ppItem .btnArrow:before{border-color:#767676;}

/* 월평균 카드값 */
.linkTit .btnArrow:before{right:-32px; width:8px; height:8px; margin-top:-2px; border-color:#767676;}
.outItem{margin-top:25px;}
.outItem .bar{height:25px; line-height:23px; padding-right:10px; margin-top:15px; font-size:14px; text-align:right; background:#dde6ed; border-radius:2px;}
.outItem.mine .bar{color:#fff; background:#066bfd;}

.infoCard{margin-top:25px;}
.infoCard .lineList{display:flex; flex-wrap:wrap; justify-content:space-between; margin-top:4px;}
.infoCard .lineList>li{box-sizing:border-box; display:inline-flex; align-items:center; width:calc(50% - 5px); padding:12px 14px; margin:10px 10px 0 0; border:1px solid #d4d4d4; border-radius:10px;}
.infoCard .lineList>li i{display:inline-block; width:38px; height:38px; margin-right:10px;}
.infoCard .lineList>li i img{max-width:100%; max-height:100%;}
.infoCard .lineList>li:nth-child(2n){margin-right:0;}
.infoCard .noList{margin-bottom:45px;}


/* .userAmount{margin-top:40px;} */
.userAmount .ineBox{overflow:visible; margin-top:25px;}
.userAmount .governing{display:flex; justify-content:space-between; align-items:flex-start; margin-bottom:0; text-align:left;}
.userAmount .governing.fCol{flex-direction:column;}
.userAmount .governing .btnArrow{margin-top:3px;}
.userAmount .btnConts{margin-top:20px;}

/* toggle */
.infoToggle.date{margin:30px 0 20px; text-align:right;}
.infoToggle.date .dateList{margin-bottom:30px;}
.infoToggle.date .infoTogBtn{display:inline-block; height:30px; padding:0 14px; line-height:28px; text-align:center; font-size:13px; border:1px solid #000; border-radius:7px;}

.bgBoxList{display:flex; justify-content:space-between; flex-wrap:wrap;}
.bgBoxList li{box-sizing:border-box; width:calc(50% - 5px); margin-top:10px; padding:19px 15px; background:#f8f9fb; border-radius:10px;}
.bgBoxList .active{background:#fff; border:2px solid #066bfd;}
.bgBoxList dl dt{position:relative; color:#767676; line-height:22px;}
.bgBoxList dl dt .age{position:absolute; right:0; top:0; color:#066bfd; font-weight:700;}
.bgBoxList dl dd{margin-top:15px; text-align:right; font-size:16px; line-height:1;}
.bgBoxList dl dd strong{padding-right:3px; letter-spacing:-0.5px; font-size:21px; font-weight:700;}
.bgBoxList .flag{display:inline-block; height:20px; padding:0 10px; margin-top:10px; line-height:18px; text-align:center; color:#fff; font-size:13px; background:#2e3653; border-radius:5px;}
.bgBoxList .flag i{font-size:14px;}
.bgBoxList .link dt{color:#222; font-size:15px;}
.bgBoxList .link dd{font-size:13px;}
.bgBoxList .link dd strong{font-size:17px;}

