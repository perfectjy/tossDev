<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="euc-kr">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>고객센터</title>

    <script src="../../../../js/bzw/jquery-1.11.3.js"></script>
  <script src="../../../../js/bzw/jquery-ui.min.js"></script>    
  <script src="../../../../js/bzw/ui.common.js"></script>

  <link rel="stylesheet" href="../../../../css/bzw/common.css">
  <script type="text/javascript">
    mbHeader = { title: '고객센터', home: true, prev: false, menu: true };
    $(function() {
      var $obj = $('.keywordList ul');
      var keywordList = [];
      var totalWidth = 0;

      $obj.children('li').each(function() {
        var keywordWidth = $(this).outerWidth();
        var idx = $(this).index();

        keywordList.push(keywordWidth);
        totalWidth += keywordList[idx];
      });

      $obj.css({'width': totalWidth});
    });
</script>
</head>
<body>
  <div id="skipNavi"> 
		<a href="#content">본문내용 바로가기</a> 
	</div>
