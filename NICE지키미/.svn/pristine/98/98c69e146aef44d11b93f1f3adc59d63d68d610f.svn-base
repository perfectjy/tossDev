@charset 'euc-kr';

/* button */
.btn { display: inline-block; min-width:250px; font-size: 17px; text-align: center; border-radius: 10px; }
.mobile .btn {width:100%; min-width:auto;}
.btn.primary { color: #fff; font-weight: 700; line-height: 54px; background-color: #066bfd; border:1px solid #066bfd; }
.btn.primary:hover {background-color:#0155c3; border-color:#0155c3}
.btn.primary:disabled {background-color:#c6cfdb; border-color:#c6cfdb;}
.btn.second { color: #222; font-weight: 700; line-height: 54px; background-color: #fff; border: 1px solid #c6cfdb;} 
.btn.second:hover {background-color:#eff2f8; border-color:#89909a;}
.btn.base {font-size: 15px; font-weight: 700; color:#222; line-height: 48px; border-radius: 10px; border:1px solid #444;}
.btn.base:hover {background-color: #eff2f8;}
.btn.base:focus {border-color: #3587ff; color:#066bfd;}
.btn.base:disabled {border-color:#d4d4d4; color:#d4d4d4;}
.btn.sub {display:inline-block; width:auto; min-width:85px; height:30px; border:1px solid #444; border-radius: 8px; font-size:13px; font-weight: 500; line-height: 28px; color:#222;}
.btn.ics {display:inline-block; width:auto; height:38px; padding:0 20px; border:1px solid #444; font-size:13px; line-height: 19px; border-radius: 8px;}
.btn.ics span {display: block; position:relative; padding-left:29px;}
.btn.ics span:before {position:absolute; content:''; top:0; left:0; width:19px; height:19px; background-color:#ddd;}
.btn.round{width:45px; height:45px; margin-right:10px; border:1px solid #dcdff1; border-radius:23px;}
.btn.round>i{display:block; height:100%; background-size: 100%;}
.btn.round:last-child{margin-right:0;}
.btn.round .capture{background-image:url(../../img/bzw/common/btn/btn_capture.png);}
.btn.round .share{background-image:url(../../img/bzw/common/btn/btn_share.png);}

.btn.more {width:auto; height:45px; font-size:15px; padding:0 30px; border:1px solid #d4d4d4; border-radius:23px; vertical-align:top;}
.btn.more span {display:inline-block; position:relative; padding-left:18px;}
.btn.more span:before {position:absolute; top:50%; left:0; display:block; content:''; width:12px; height:2px; background-color:#222; margin-top:-1px;}
.btn.more span:after {position:absolute; content: ''; top:50%; left:5px; width:2px; height:12px; background-color:#222; margin-top:-6px;}
/* .btn.round .more{background-image:url(../../img/bzw/common/btn/btn_more.png);} */


.btn.fullback {height:96px; background-color:#f5f5f5; text-align:center; font-size:16px; border-radius: 0;}
.btn.fullback span {display: inline-block; line-height: 24px; padding-top:36px;}
.btn.fullback span:after {content: ''; display: inline-block; width: 24px; height: 24px; background: url(../../img/bzw/common/ico/ico_acco_arr.png) no-repeat; background-size: 100%; transform: rotate(180deg); vertical-align: top; margin-left: 5px;}

.btnLine {text-decoration: underline;}

/* button box */
.btnBox{text-align:center;}
.mobile .btnBox{margin-top:20px;}

.mobile .btnConts {display:flex; margin-top:40px;}
.mobile .btnConts .btn {flex:1;}
.mobile .btnConts .btn:nth-child(2) {width:calc(50% - 10px); margin-left:10px;}
.mobile .btnConts.over .btn:nth-child(2) {flex:none; width:calc(60% - 10px); margin-left:10px;}
.mobile .btnConts.plex {justify-content: space-between;}
.mobile .btnConts.plex .btn {flex:none; width:calc(33.333% - 6.66px); margin:0;}

.mobile .bottomActiveBtns {height:76px;}
.mobile .bottomActiveBtns .btnConts {position:fixed; lefT:0; right:0; bottom:0; margin:0; padding:0 15px 20px 15px; z-index:10;}
.mobile .btnFullBox {position:fixed; left:0; right:0; bottom:0; z-index:5;}

.btnMoreBox {margin-top:20px; text-align:center}


/* form ---------------------------------------------- */
::-webkit-input-placeholder{font-family:'Noto Sans';color:#767676;opacity:1}
:-moz-placeholder{font-family:'Noto Sans';color:#767676;opacity:1}
::-moz-placeholder{font-family:'Noto Sans';color:#767676;opacity:1}
:-ms-input-placeholder{font-family:'Noto Sans'!important;color:#767676!important;opacity:1}
input[type=text]:focus::-webkit-input-placeholder{visibility:hidden}
input[type=text]:focus:-moz-placeholder{visibility:hidden}
input[type=text]:focus::-moz-placeholder{visibility:hidden}
input[type=text]:focus:-ms-input-placeholder{visibility:hidden}


/* input text */
input[type=text],
input[type=password],
input[type=email],
input[type=tel] {width:100%; height:50px; border:1px solid #dcdff1; border-radius: 0; padding:0 15px; font-size: 15px;}
input[type=text]:focus,
input[type=password]:focus,
input[type=email]:focus,
input[type=tel]:focus {border-color:#222; outline:0}
/* select */
select {width:100%; height:50px; border:1px solid #dcdff1; font-size:15px; padding:0 35px 0 10px; background:url(../../img/bzw/common/ico/ico_acco_arr.png) no-repeat calc(100% - 10px) 13px; background-size:24px;}
select:focus {border-color:#222; outline:0}
select option:first-child {color:#ff0000}

/* radio & checkbox ------------------------------------*/
.uiItem {display:inline-block;}
.uiItem:not(:first-child) { margin-left:40px;}
input[type=radio], input[type=checkbox] {position:absolute; width:1px; height:1px; margin:-1px; border:0; overflow:hidden; clip:rect(0,0,0,0); -webkit-appearance: none; opacity:0;}
.uiItem input[type=radio] + label, .uiItem input[type=checkbox] + label {display:inline-block; position:relative; font-size:15px; line-height: 23px; color:#222; padding:1px 0 0 34px; cursor: pointer; word-break: keep-all;}
.uiItem input[type=radio] + label:before, 
.uiItem input[type=checkbox] + label:before {position:absolute; content:''; top:0; left:0; width:24px; height:24px; border:1px solid #d4d4d4; border-radius: 50%; box-sizing: border-box;}
.uiItem input[type=radio]:checked + label:before {border-width:2px; border-color:#066bfd}
.uiItem input[type=radio]:checked + label:after {position:absolute; content:''; top:50%; left:6px; width:12px; height:12px; margin:-6px 0 0 0; border-radius: 50%; background-color:#066bfd;}
.uiItem input[type=radio]:disabled + label:before,
.uiItem input[type=checkbox]:disabled + label:before {background-color:#f9f9f9;}
.uiItem input[type=radio]:checked:disabled + label:before { border-width:2px; border-color:#d4d4d4;}
.uiItem input[type=radio]:checked:disabled + label:after {background-color:#bababa;}
.uiItem input[type=checkbox]:checked + label:before {border-color:#066bfd; background-color:#066bfd}
.uiItem input[type=checkbox]:checked + label:after {position:absolute; content:''; display:block; border-color:#fff; border-style:solid; border-width:0 0 2px 2px; width:10px; height:5px; left:6px; top:7px; transform: rotate(-45deg);}
.uiItem input[type=checkbox]:checked:disabled + label:before {background-color:#d4d4d4; border-color:#d4d4d4;}

/* box type */
.rdoGroup input[type=radio] + label {display:block; height:56px; padding:15px 0 0; font-size:17px; font-weight: 700; text-align:center; border:1px solid #d9d9d9; border-radius: 10px;}
.rdoGroup input[type=radio]:checked + label {color:#066bfd; border-color:#066bfd;}
.mobile .rdoGroup {display:flex; gap:10px 10px; margin-top:10px;}
.mobile .rdoGroup .item {flex:1}
.mobile .rdoGroup.col3 {flex-wrap: wrap;}
.mobile .rdoGroup.col3 .item {flex:none; width:calc(33.3333% - 7px);}

.rdoGroup input[type=checkbox] + label {display:block; height:38px; padding:8px 0 0; font-size:13px; text-align:center; border:1px solid #d9d9d9; border-radius: 10px;}
.rdoGroup input[type=checkbox]:checked + label {color:#fff; border-color:#066bfd; background-color:#066bfd;}

/* toogle switch */
.btnSwitch{position:relative;display:inline-block; width:33px;height:20px;border-radius:10px; vertical-align: top;}
.btnSwitch input{position:relative;width:33px;height:20px;border-radius:10px;border:1px solid #e8e8e8;margin:0;background:#e8e8e8;opacity:1;}
.btnSwitch input:checked{background-color:#066bfd;}
.btnSwitch input+label:before,
.btnSwitch input+label:after{z-index:2;position:absolute;top:50%;color:#fff;width:auto; height:auto;background:none;transform:translateY(-50%);}
.btnSwitch input+label{z-index:1;position:absolute;top:1px;border-radius:50%;background-color:#fff;padding:0;width:18px;height:18px;border:1px solid #e8e8e8;}
.btnSwitch input:checked+label:before{color:#252525;left:1px;width:auto; height:auto;background:none;-webkit-transition:color 0.5s 0.2s, left 0.5s 0.2s;transition:color 0.5s 0.2s, left 0.5s 0.2s;}
.btnSwitch input:checked+label{left:14px;border:1px solid #066bfd;padding:0;-webkit-transition:left 0.5s, right 0.4s 0.2s;transition:left 0.5s, right 0.4s 0.2s;}
.btnSwitch input:not(:checked)+label{left:1px;-webkit-transition:left 0.5s, right 0.4s 0.2s;transition:left 0.5s, right 0.4s 0.2s;}

/* accordion -----------------------------------------------*/
.uiAccoWrap {border-bottom:1px solid #eee;}
.uiAccoWrap .uiAccoHeader { position: relative;}
.uiAccoWrap .uiAccoHeader .uiBtn {width:100%; position:relative;}
.uiAccoWrap .uiAccoHeader .uiBtn:after { content: ''; position: absolute; right: 0; top:22px; width: 24px; height: 24px; background:url(../../img/bzw/common/ico/ico_acco_arr.png) no-repeat; background-size: 100%; transform: rotate(0); transition: all 0.3s ease-out;}
.uiAccoWrap .uiAccoHeader .accoTitle {display:block; text-align:left; padding: 20px 36px 20px 5px; font-size: 15px; line-height: 23px; color:#222; border-top:1px solid #eee; word-break: keep-all;}
.uiAccoWrap .uiAccoContent {position:relative; padding:25px 15px; font-size:14px; line-height: 22px; color:#444; background-color:#f9f9f9; border-bottom:1px solid #bababa; margin-bottom:-1px; z-index:1;}
.uiAccoWrap .uiAccoHeader .uiBtn.active {font-weight: 700; border-color:#bababa;}
.uiAccoWrap .uiAccoHeader .uiBtn.active:before {position:absolute; content:''; top:0; left:0; right:0; border-top:1px solid #bababa;}
.uiAccoWrap .uiAccoHeader .uiBtn.active:after { transform: rotate(180deg); }

/* 꼭 알아두세요 */
.infoToggle {margin-top:50px;}
.infoToggle .tit {position:relative;}
.infoToggle .tit strong {display:block; font-size:15px; font-weight:700; color:#222; padding:0 36px 0 28px;}
.infoToggle .tit strong:before {content: ''; position: absolute; width: 20px; height: 20px; left: 0; top: 50%; transform: translateY(-50%); background: url('../../img/bzw/common/ico/ico_notice.svg') center no-repeat; background-size: 100%;}
.infoToggle .tit .infoTogBtn {position:absolute; top:0; left:0; right:0; bottom:0;}
.infoToggle .tit .infoTogBtn:after { content: ''; position: absolute; right: 0; top:0; width: 24px; height: 24px; background:url(../../img/bzw/common/ico/ico_acco_arr.png) no-repeat; background-size: 100%; transform: rotate(0); transition: all 0.3s ease-out;}
.infoToggle .tit .infoTogBtn.active:after { transform: rotate(180deg); }
.infoToggle .infoTogConts{display:none; font-size:14px; line-height: 22px; padding-top:15px;}
.infoToggle.open .infoTogConts {display:block}

/* Tab */
.tabWrap .tabList { display: flex; flex-wrap: nowrap; width: 100%; background-color: #f9f9f9; border: 1px solid #eee; border-radius: 10px; }
.tabWrap .tabList li {position:relative; flex:1; box-sizing: border-box;}
.tabWrap .tabList li:not(:first-child):before {position:absolute; content:''; left:-1px; top:50%; margin-top:-7px; height:14px; border-left:1px solid #d4d4d4; z-index:1;}
.tabWrap .tabList li a {display:block; height: 100%; padding: 14px 0; color: #727272; font-size: 15px; line-height: 18px; text-align: center;}
.tabWrap .tabList li.active {background-color:#fff; }
.tabWrap .tabList li.active:after {position:absolute; content:''; top:-1px; left:-1px; right:-1px; bottom:-1px; border:2px solid #066bfd; border-radius: 10px; z-index:2;}
.tabWrap .tabList li.active a { color: #066bfd; font-weight: 700; position:relative;}
.tabWrap .tabConts {margin-top:40px;}
.tabWrap .tabConts .tabPanel {display:none;}
.tabWrap .tabConts .tabPanel.active {display:block;}

