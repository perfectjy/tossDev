@charset "euc-kr";

/* 공통 */
.listStar > li{position:relative; padding-left:12px; margin-top:10px; line-height:22px;}
.listStar > li:before {position:absolute; content:'*'; top:3px; left:0; width:4px; height:4px; color:#444; font-size:14px;}
.listDot strong {font-weight:700; color:#444;}
.listDot strong > .roboto {font-weight:700;}
.listDot.mg .dash {margin-top:12px;}
.listNum.mg {margin-top:12px;}
.listNum.mg li{margin-top:0;} 

