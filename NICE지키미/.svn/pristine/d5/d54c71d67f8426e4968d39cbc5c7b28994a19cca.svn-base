@charset "euc-kr";

/* 공통 */
.listStar > li{position:relative; padding-left:12px; margin-top:10px; line-height:22px;}
.listStar > li:before {position:absolute; content:'*'; top:3px; left:0; width:4px; height:4px; color:#444; font-size:14px;}
.listDot em {margin-top:14px; display:block; font-weight:700; color:#444;}
.listDot strong > .roboto {font-weight:700;}
.listDot.mg .dash {margin-top:12px;}
.listNum.mg {margin-top:12px;}
.listNum.mg li{margin-top:0;} 

.pweb .inquireList .h4 + .contArea {min-height:313px; margin-top:-20px;}
.pweb .inquireList .standBy {min-height:313px;}
.inquireList .h4 +  .contArea {margin-top:-16px;}

.guideTxt.listDot:before {top:50%}
.pweb .guideTxt.listDot:before {top:18px;}
.pweb .detailTit {margin-bottom:46px;}
.pweb .faqWrap .keywordList {overflow: initial;}

.pweb .controlpage .swiper-pagination {margin-top:0; vertical-align:sub;}
.pweb .eventSwiper [class^=btn] {top:1px;}

.subTxt {display:block; font-size:15px; font-weight:700;}
/* 고객선터 > 서비스문의 */

.infoWrap {margin-top:25px;}
/* .infoWrap .tblList + .listDash {margin-top:20px;} */
.infoWrap .btnConts {margin-top:15px;}

.selectBox {margin-bottom:36px}
.listStar strong {display:inline-block; font-weight:700; color:#444; font-family:'roboto';}
.btnConts.mg {margin:15px auto 30px;}



/* 맞춤대출 > 대출진단결과 (동의후)*/
.resultBox {position:relative; padding:35px 145px 35px 27px; min-height:130px; background-color:#eff2f8; border-radius:18px;}
.resultBox .tit {font-size:21px; display:inline-block; font-weight:700;}
.resultBox:before {content:''; position:absolute; top:53%; right:30px; width:89px; height:89px; background:url(../../../img/zkm/bzw/common/bg/bg_result.png) no-repeat right center/ 100%; transform:translateY(-50%);}

.pweb .resultBox {padding:62px 375px 62px 40px;}
.pweb .resultBox .tit {font-size:32px;}
.pweb .resultBox:before {width:123px; height:123px; right:58px;}

.rollingImgbox {height:178px; background-color:#ffa3a3;}
.lineBlueBox {display:flex; padding:24px 25px; justify-content:space-between; align-items:center; border:1px solid #066bfd; border-radius:20px;}
.lineBlueBox .h3 {margin:0;}
.lineBlueBox .num {font-weight:700; font-size:21px;}
.lineBlueBox .num > em {font-size:32px;}

/* 신용점수 올리기 */
.iconLinkWrap {position:relative;}
[class*=bgGrade]:before {content:''; display:block; margin:0 auto 45px; width:150px; height:150px; background-color:#ffa3a3; background-size:100%; background-position:50%;}
.bgGradeUp:before {background-image:url(../../../img/zkm/bzw/common/bg/bg_up.png); background-color:transparent;}
.iconLinkWrap h3 {margin:8px 0 0;}

.modalContent .txtLink {text-align:center;}
.modalContent .txtLink .btnLine {display:inline-block;  font-size:15px;}

.lineTopList li {position:relative; padding:24px 0 24px 80px;}
.lineTopList li:not(:first-child) {border-top:1px solid #eee}
.lineTopList li strong.resultTxt {display:block; margin-top:8px; padding:0; font-weight:500; font-size:17px;}
.lineTopList li.fail .resultTxt {color: #767676;}
.lineTopList li:before {position:absolute; content:''; top:50%; left:0; width:65px; height:65px; border-radius:50%; transform:translateY(-50%); background-size:auto 65px; background-image:url(../../../img/zkm/bzw/common/bg/bg_complete.png);} 
.lineTopList li.success:before {background-color:#ddf9fb; background-position:0 50%;}
.lineTopList li.fail:before {background-color:#ffeded; background-position:68px 50%;}
span.topInfoTxt {display:block; margin:16px 0 0; font-size:15px;}
.certifWrap {margin-top:12px;}
.certifWrap .tit {display:block; margin-bottom:12px;}

.comVisualBg {position:absolute; left:50%; top:50%; width:100%; text-align:center; transform:translate(-50%, -50%);}
.comVisualBg:before {content:''; display:block; margin: 0 auto 25px; width:170px; height:170px; background-size:100%; background-color:#ffb2b2;}
.comVisualBg .governing .subTxt {margin-top:19px; color: #767676;}

.methodWrap ~ .methodWrap {margin-top:40px;}
.methodWrap > .h3:before {display:inline-block; content:''; width:24px; height:24px; margin-right:8px; background-position:50%; background-repeat:no-repeat; background-size:auto 24px; background-image:url(../../../img/zkm/bzw/common/ico/ico_sort.png); vertical-align:middle;}
.methodWrap > .h3.ico01:before {background-position:0 50%}
.methodWrap > .h3.ico02:before {background-position:-24px 50%;}
.methodWrap > .h3.ico03:before {background-position:-48px 50%;}
.infoGoList li {position:relative;}
.infoGoList li .btnWrap {position:absolute; right:0; top:-6%;}
.infoGoList li .tel > span {display:inline-block; border-bottom:1px solid #222; line-height:18px;}
.infoGoList li .txt strong {font-weight:700; color:#444;}
.bgBox2 {margin-top:17px; padding:26px 23px; text-align:center; border-radius:10px; background-color:#f9f9f9;}
.bgBox2 span {line-height:22px;}

.infoGoList li:first-child  > .h4{margin-top:0;}
.infoGoList .impTxt {color:#767676;}
.loanLink.bg li a:after {right:10px;}

.labelBtn {position:absolute; right:10px; top:50%; transform:translateY(-50%); height:30px; padding:0 14px; line-height:28px; border:1px solid #444; font-size:13px; border-radius:8px;}




/* 지키미케어십 */
.careImgWrap {margin-top:25px; text-align:center;}
.careImgWrap .tit {display:block; margin:20px 0 8px; font-size:16px; font-weight:700;}
.careImgWrap:before {content:''; display:block; width:300px; height:231px; margin:0 auto; background-color:#eff2f8;}
.careServiceList {margin-bottom:40px;}
.careServiceList li {position:relative; padding:13px 0;}
.careServiceList li ~ li {margin-top:20px;}
.careServiceList li:before {position:absolute; right:0; top:50%; content:''; width:65px; height:65px; background-image:url(../../../img/zkm/bzw/common/bg/bg_loan04.png); background-position:100%; background-size:100%; border-radius:50%; background-color:#f6f3ff; transform:translateY(-50%);}
.careServiceList li .tit {display:block; font-weight:700; font-size:16px;}
.careServiceList li .txt {display:block; margin-top:8px;}

.lineBorTop {border-top:1px solid #eee;}
.lineBorTop .h4 {margin-bottom:8px;}
.lineBorTop .subTit {display:block; font-size:17px; font-weight:700;}
.lineBorTop .subTit > span {display:block;}
.protectImg {width:300px; height:191px; margin:36px auto; text-align:center; background: #444;}
.protectImg2 {width:300px; height:212px; margin:36px auto 30px;}
[class^=protectImg] img {max-width:100%; max-height:100%;}
.lineBorTop .txt {margin-bottom:38px; text-align:center;}
.circleData {position:relative; width:280px; height:227px; margin:32px auto 30px; box-sizing:content-box; text-align:center; padding:0 20px;}
.circleData li {display:flex; position:absolute;  flex-direction:column; justify-content:center; border-radius:50%; color:#fff; text-align:center; }
.circleData li .tit {display:block; font-weight:300; line-height:20px;}                           
.circleData li .num {display:block; font-weight:700;}
.circleData li:nth-child(1) {width:157px; height:157px; background-color:#0669fa; box-shadow:2px 2px 10px 4px rgb(53 135 255 / 30%);}
.circleData li:nth-child(2) {width:110px; height:110px; background-color:#ff4956; /*left:calc(100% - 140px);*/ right:9%; top:37px; box-shadow:2px 2px 10px 4px rgba(255 73 86 / 30%);}
.circleData li:nth-child(3) {width:90px; height:90px; background-color:#2fcc71; top:131px; left:50%; transform:translateX(-31%); box-shadow:2px 2px 10px 4px rgb(47 204 113 / 30%);}

.protectBgWrap {text-align:center;}
.protectBgWrap p {display:block; margin:35px auto 38px; font-size:13px;}
.protectBgWrap p:after {display:block; content:''; width:176px; height:125px; margin:13px auto; background:#f9fafb url(../../../img/zkm/bzw/@tmp/@care03.jpg) no-repeat center / 176px auto}

.viewSliderWrap {overflow:hidden; height:330px; margin:30px auto 0; padding:30px 23px; background-color:#f9f9f9;}
.viewSliderWrap li ~ li {margin-left:10px;}
.viewSliderWrap li {width:320px;}
.viewSliderWrap .h4 {margin:0;}

.imgBtnWrap {width:300px; height:231px; display:block; margin:20px auto 30px; background-color:#eff2f8; cursor: pointer; text-align:center;}
.imgBtnWrap img {max-width:100%;}

.lineBox {padding:28px 25px; border:1px solid #d9d9d9; border-radius:15px;}
.lineBox .tit {font-size:19px; font-weight:700;}
.lineBox .number {margin:8px 0 22px;}
.lineBox .number em {position:relative; margin-right:10px; font-size:17px;font-weight:700; color:#0669fa;}
.lineBox .number em:after {content:''; display:inline-block; width:1px; height:15px; margin-left:9px; background-color:#d4d4d4;}
.lineBox .listDot em {display:inline-block; margin-top:0; font-weight:700;}

.couponWrap {position:relative}
.couponWrap .btnWrap {position:absolute; right:0; top:-4px;}
.couponWrap .bgBox2 p {font-size:17px;}
.couponWrap .bgBox2 p * {font-weight:700;}

.paySortList {display:flex; justify-content:space-between; margin:30px auto 12px;}
.paySortList li {position:relative; padding:14px 6px; width:calc(25% - 8px); min-height:72px; border:1px solid #c6cfdb; border-radius:10px; box-sizing:border-box;}
.paySortList li:before {content:''; display:block; margin:0 auto; width:52px; height:30px; background-color:#e6e9ff; background-size:auto 30px; background-repeat:no-repeat; background-image:url(../../../img/zkm/bzw/common/ico/ico_payment.png)}
.paySortList li.ico05:before {background-position:0 50%; background-color:transparent;}
.paySortList li.ico06:before {background-position:-52px 50%; background-color:transparent;}
.paySortList li.ico07:before {background-position:-104px 50%; background-color:transparent;}
.paySortList li.ico08:before {background-position:-157px 50%; background-color:transparent;}
.paySortList li span {display:block; margin-top:3px; color:#444; font-size:12px; text-align:center;}
.paySortList .paybtn {position:absolute; top:0; left:0; right:0; bottom:0;}
.paySortList + .formItem .intHalf .inp {width: calc(50% - 7.5px);}

.payInfoBox {position:relative; margin:65px 0 25px; padding:31px 25px 28px; border:1px solid #d4d4d4; border-radius:15px;}
.payInfoBox:before {content:''; position:absolute; top:-32.5px; left:50%; width:65px; height:65px; border-radius:50%; transform:translateX(-50%); background:url(../../../img/zkm/bzw/sub/ico_man2.png) no-repeat center / 100%;}
.payInfoBox div {display:flex; justify-content:space-between; padding:5px 0;}
.payInfoBox dt {color:#767676;}
.payInfoBox dd,
.payInfoBox dd span {color:#444; font-weight:500;}

/* 팝업 */
.hSubTxt em {font-weight:700;}
.unit {display:block; margin-bottom:7px; text-align:right; font-size:13px; color:#767676;} 
.valueList {padding:17px 0; border-top:1px solid #eee; border-bottom:1px solid #eee;}
.valueList > div {display:flex; justify-content:space-between;}
.valueList > div:not(:first-child){margin-top:10px;}
.valueList dt {color:#767676;}
.txtBtm {display:block; margin-top:20px; font-size: 19px; font-weight:700; line-height:27px; letter-spacing:-1px;}
.moSubTxt {margin-top:13px; color:#444;}

.paymentList li a {position:relative; width:100%; padding:15px 48px 15px 55px; display:inline-block; border:1px solid #dadce0; border-radius:4px;}
.paymentList li a:before {content:''; position:absolute; left:15px; top:15px; width:24px; height:24px; background-size:24px 24px; background-position:top left;}
.paymentList li:nth-child(1) a:before {background-image:url(../../../img/zkm/bzw/common/ico/ico_nice.png);}
.paymentList li:nth-child(2) a:before {background-image:url(../../../img/zkm/bzw/common/ico/ico_Android.png);}
.paymentList li ~ li {margin-top:14px}
.paymentList li .h4 {margin:0; display:inline-block;}
.paymentList li .moSubTxt {display:inline-block; margin-top:5px; color:#5f6368;}
.cardPaylist li {display:inline-block; width:32px; height:20px; border:1px solid #edf0ff; border-radius:4px; background-repeat:no-repeat; background-position:center;}
.cardPaylist li:before {content:''; display:inline-block; background-size:100%;}
.cardPaylist li ~ li {margin-left:8px;}
.cardPaylist li:nth-child(1) {background-image:url(../../../img/zkm/bzw/common/ico/ico_pay_bc.png); background-size:14px auto;}
.cardPaylist li:nth-child(2) {background-image:url(../../../img/zkm/bzw/common/ico/ico_pay_kb.png);  background-size:100%;}
.cardPaylist li:nth-child(3) {background-image:url(../../../img/zkm/bzw/common/ico/ico_pay_kakao.png); background-size:22px auto; background-color:#ffea00; border-color:#ffce1b;}
.cardPaylist li:nth-child(4) {background-image:url(../../../img/zkm/bzw/common/ico/ico_pay_sk.png); background-size:18px auto;}
.cardPaylist li:nth-child(5) {background-image:url(../../../img/zkm/bzw/common/ico/ico_pay_android.png); background-size:12px auto;}

.labCopon * {display:block;}
.labCopon + .labCopon {margin-top:12px;}
.labCopon .txtS {padding-left:34px; color:#767676; font-size:11px;}
.couponWrap {display:flex; justify-content:space-between; align-items:center; padding-top:38px; border-top:1px solid #eee;}
.couponWrap .sColor {margin-top:4px; font-size:12px;}
/* .formItem.numInp {margin-top:25px;} */
.couponWrap .tit {margin:0; font-size:15px; font-weight:700;}
.formItem.numInp .titItem{font-size:15px; font-weight:700;}
.couponWrap > div.numInp:not(:first-child) {margin-top:25px}

.moConbg {position:relative;}
.moConbg:after {content:''; display:block; margin:20px auto 0; width:124px; height:96px; background:#eff2f8;}






/* content이동 */
.loanLink.bg li a {border-radius:14px;}
.loanLink.bg li a:before {background-color:transparent;}
.loanLink.bg li a.ico01 {background-color:#ddf9fb;}
.loanLink.bg li a.ico04 {background-color:#f0ebff;}
.loanLink.bg li a.ico05 {background-color:#edf0ff;}
