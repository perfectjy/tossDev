/* 관리 설정 - 결제/서비스 이용내역 */
.borderBoxList li{border:1px solid #d4d4d4; border-radius: 15px; padding: 28px 24px 20px 25px; margin-top:15px;}
.borderBoxList li .topTit {position:relative; display:block;}
.borderBoxList li .topTit strong { font-size:16px; line-height: 24px; font-weight: 700;}
.borderBoxList li .topTit a {position:absolute; top:0; right:0; bottom:0; left:0;}
.borderBoxList li .topTit .tagList {display:inline-block; margin-left:4px;}
.borderBoxList li dl {font-size:13px; line-height: 24px; font-weight: 400; margin-top:15px;}
.borderBoxList li dl div {display:flex; justify-content:space-between; margin-top:5px;}
.borderBoxList li dl div.sale {color:#767676; }
.borderBoxList li dl div.sale dd {font-weight: 400;}
.borderBoxList li dl div dd {font-weight: 700;}
.borderBoxList li.wrapUp {background: #f9f9f9;}
.borderBoxList li .lie {color:#222; font-weight: 400; text-decoration: line-through; margin-right:14px;}




/* 관리 설정 - 	내 정보관리(서브메인) */
.manageBoxList li {position:relative; border:1px solid #d4d4d4; border-radius: 15px; padding: 26px 20px; margin-top:15px;}
.manageBoxList li strong {font-size:16px; line-height: 26px; font-weight: 700;}
.manageBoxList li a {position:absolute; top:0; right:0; bottom:0; left:0;}
.manageBoxList li a:after {content: ''; position: absolute; right: 20px; top: 50%; width: 8px; height: 8px; border-top: 2px solid #bababa; border-right: 2px solid #bababa; transform: rotate(45deg) translateY(-50%);}
.userId {position:relative; font-size:13px; line-height: 26px; font-weight: 400; padding-left:10px; }
.userId::before {content:''; position:absolute; left: 0; top: 4px; width:2px; height: 12px; background: #767676; margin-right:10px;}





/* 관리 설정 - 지키미케어십관리 */
.careTit {position:relative; display:block; font-size:16px; line-height: 24px; padding-left:80px; margin-bottom:38px;}
.careTit::before {position:absolute; content:''; top:50%; left:7px; width:65px; height:65px; background:url(../../../img/zkm/rzm/common/bg/bg_shield.png); background-size:100%;  transform:translateY(-50%); border-radius:50%;}
.extSrvLink {background: #eff2f8; border-radius: 15px; margin-top:20px;}
.extSrvLink a {position:relative; display:flex; justify-content: space-between; align-items: flex-end; padding:26px 33px 26px 20px ;}
.extSrvLink a span {font-size:15px; line-height: 23px;}
.extSrvLink a::before {content:''; position: absolute; right:20px; bottom:32px; width: 6px; height: 6px; border-top: 1px solid #222; border-right: 1px solid #222; transform: rotate(45deg) translateY(-50%); margin-right:6px;}

.careBoxLink {display: flex; justify-content: space-between; margin-top:25px;}
.careBoxLink li {width:50%; border: 1px solid #d4d4d4; border-radius: 15px; text-align: center; margin-left:10px; }
.careBoxLink li:first-child {margin-left:0;}
.careBoxLink a {position:relative; display:block; padding: 24px 0px; margin:0 auto;}
.careBoxLink a::before {content:''; position: absolute; top:24px; left:50%; transform: translateX(-50%); width:60px; height:60px; border-radius: 50%;}
.careBoxLink a.ico01::before {background: url(../../../img/zkm/rzm/common/bg/bg_home.png); background-size:100%;}
.careBoxLink a.ico02::before {background: url(../../../img/zkm/rzm/common/bg/bg_car.png); background-size:100%;}
.careBoxLink a::after {content:''; position: absolute; right:20px; top:50%; transform: translateY(-50%); width: 6px; height: 6px; border-top: 1px solid #222; border-right: 1px solid #222; transform: rotate(45deg) translateY(-50%);}
.careBoxLink a strong {display:block; font-size:15px; line-height: 1; padding-top:72px;}
.careBoxLink a span {font-size:21px; line-height: 30px; font-weight: 700;}
.careBoxLink a span.letter {font-size:19px; font-weight: 400;}

.srvLink {margin-top:25px;}
.srvLink li {font-size:15px; line-height: 26px; font-weight: 700;}
.srvLink li:not(:first-child) {margin-top:20px;}
.srvLink li a {display:block; position:relative; padding:20px 20px 20px 80px;}
.srvLink li a:before {position:absolute; content:''; top:50%; left:0px; width:65px; height:65px; transform:translateY(-50%); border-radius:25px; }
.srvLink li a.ico01:before {background: #edf0ff url(../../../img/zkm/rzm/common/ico/ico_srv01.png)no-repeat center; background-size:50%;}
.srvLink li a.ico02:before {background: #ffeded url(../../../img/zkm/rzm/common/ico/ico_srv02.png)no-repeat center; background-size:50%;}
.srvLink li a.ico03:before {background: #e0fdf0 url(../../../img/zkm/rzm/common/ico/ico_srv03.png)no-repeat center; background-size:50%;}
.srvLink li a.ico04:before {background: #f0ebff url(../../../img/zkm/rzm/common/ico/ico_srv04.png)no-repeat center; background-size:50%;}
.srvLink li a.ico05:before {background: #fff7df url(../../../img/zkm/rzm/common/ico/ico_srv05.png)no-repeat center; background-size:50%;}
.srvLink li a.ico06:before {background: #edf0ff url(../../../img/zkm/rzm/common/ico/ico_srv01.png)no-repeat center; background-size:50%;}
.srvLink li a.ico07:before {background: #ffeded url(../../../img/zkm/rzm/common/ico/ico_srv02.png)no-repeat center; background-size:50%;}
.srvLink li a:after {content:''; position:absolute; right:10px; top:50%; width:8px; height:8px; margin-top:-7px; border-top:2px solid #767676; border-right:2px solid #767676; transform:rotate(45deg);}






/* 관리 설정 - 가족구성원 관리 */
.fmlyLineBox {border:1px solid #d4d4d4; border-radius: 15px; padding: 27px 24px 20px 24px; margin-top:-15px;}
.fmlyLineBox.bg {background:#f9f9f9;}
.fmlyLineBox:not(:first-child) {margin-top:20px;}
.fmlyTit {position:relative; display: block; font-size:16px; line-height: 24px; font-weight: 700; border-bottom: 1px solid #d4d4d4; padding-bottom:20px;  margin-bottom:20px;}
.fmlyTit button.btn.sub {position:absolute; right:0; top:-4px; min-width: 62px;}

.fmlyListBox {position:relative;}
.fmlyList {position:relative;  display:flex; overflow-x: scroll; margin-right:20px;}
.fmlyList li {display:flex; flex-direction: column; align-items: center;}
.fmlyList li:not(:first-child) {margin-left:20px;}
.fmlyList li .fmlyBtn {width:53px; height:53px; border-radius: 50%; border: 1px solid #066bfd; background:#d6d6d6; overflow:hidden;}
.fmlyList li .fmlyBtn.none {border: 1px solid #000;}
.fmlyList li button img {width:100%;}
.fmlyList li strong {font-size:14px; line-height: 22px; font-weight: 700;}
.fmlyList li span {font-size:13px; line-height: 22px; color:#767676;}
.fmlyListBox button.addBtn {position:absolute; top:0; right:0; width:18px; height:18px; background:url(../../../img/zkm/rzm/common/ico/ico_add.png)no-repeat center; background-size: 100%; margin-top:19px;}
.fmlyInfo div {display:flex; justify-content: space-between; font-size:15px; line-height: 23px; margin-top:7px;}
.fmlyInfo div dt {color:#767676;}
.fmlyInfo div dd {font-weight: 700;}
button.fmlyMenu {position:absolute; right:0; top:4px;  width:4px; height:18px; background:url(../../../img/zkm/rzm/common/btn/btn_menu.png)no-repeat center; background-size:100%;}




/* 관리 설정 - 대표이미지 */
.fmlyImgList {padding:0 32px;} 
.fmlyImgList li {display:inline-block; width:53px; height:53px; border-radius: 50%; margin: 15px 15px 0 15px;; overflow:hidden;}
.fmlyImgList li img {width:100%;}

.sinfo {display: block; font-size:12px; line-height: 1; margin-top:12px; color:#d50f3c;}
.underLink {display:inline-block; font-size:15px; line-height: 24px; margin-top:35px; border-bottom:1px solid #222;}
 

/* 관리 설정 - 공동인증서 목록 */
.jcInput div {border:1px solid #ddd; border-radius: 10px; padding:21px 0  ; margin-top:15px;}
.jcInput div input + label {font-size:17px; line-height: 24px; }
.jcInput div input + label span {display:block; font-size:12px; line-height: 23px; color:#767676;}




/* 관리 설정 - 내정보관리 [팝업]구성원 동의 요청  */
.listDot2 {display:flex; flex-wrap: wrap;}
.listDot2 li {display:inline-block; position:relative; padding-left: 12px; line-height:22px ; margin-top:10px; width:calc(50% - 14px) ;}
.listDot2 li::before {content: ''; position: absolute; top: 9px; left: 0; width: 4px; height: 4px; background-color: #444; border-radius: 100%;}
.btnLineBox {text-align: center; margin-top:14px;}




/* 내 정보 관리 - 로그인 설정 */
.snsLoginList {display:flex; justify-content: center;}
.snsLoginList li {margin:0 25px;}
.snsLoginList li a {display: inline-block; width:50px; height:50px; border-radius: 50%;}
.snsLoginList li a.snsNaver {background:url(../../../img/zkm/rzm/common/ico/ico_naver.png)no-repeat; background-size:100%;}
.snsLoginList li a.snsKakao {background:url(../../../img/zkm/rzm/common/ico/ico_kakao.png)no-repeat; background-size:100%;}
.snsLoginList li a.snsIos {background:url(../../../img/zkm/rzm/common/ico/ico_ios2.png)no-repeat; background-size:100%;}
.snsLoginList li.done a {border: 2px solid #066bfd;}
.snsLoginList li strong {display:block; font-size:13px; line-height: 21px; color:#767676; margin-top:5px;}
.snsLoginList li.done strong {font-weight: 700; color:#066bfd;}





/* 내 정보 관리 - 앱 설정 */
.verInfo {display:block; font-size: 17px; line-height: 25px; font-weight: 700; text-align: center;}
.verBtn {margin-top:40px;}

.alrListWrap.group {border: 1px solid #d4d4d4; border-radius: 15px;}
.alrListWrap.group .receiveChkBox {position:relative; border:0; background:none; margin:0;}
.alrListWrap.group .receiveChkBox::before {content:''; position:absolute; left:0; bottom:0; width:calc(100% - 48px); height:1px; background:#eee; margin-left:24px}




/* PC */
.lineFormSearch.t2 {text-align: center; margin-bottom:29px;}
.lineFormSearch .custForm.noline:after {border-bottom:0;}
.pweb .lineFormSearch.t2 .custForm select {font-size:32px; line-height: 44px;}
.pweb .lineFormSearch.t2 {margin-bottom:60px;}