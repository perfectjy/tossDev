/* 	사장님 신용관리 - 사장님 신용 상품소개 */
.catgryBox {margin:-30px 0 0px 0;}
.catgryBox p {line-height: 22px; margin-bottom:15px;}
.catgryList {position:relative; left:0; right:0; top:0; margin:-9px -4px 0 -4px; overflow:hidden;}
.catgryList >li {display:inline-block; height:22px; line-height:22px; padding:0 10px; font-size:12px; border-radius:11px; background:#eaf1ff;}
.catgryItem {margin-top:40px;}

.pweb .catgryBox {text-align: center;}
.pweb .catgryBox p {font-size:18px; line-height: 28px; margin-bottom:19px;}
.pweb .catgryList li { height:28px; line-height:28px; padding:0 15px; font-size:14px; border-radius:14px;}
.pweb .catgryItem {margin-top:70px;}

.relySwiper {position:relative; padding:30px 0px 25px 0;  background:#f9f9f9; border-radius: 15px; overflow:hidden; margin-top:37px;}
.scoreList {position:relative; height: 252px; padding:0 25px;}
.scoreList.sl01::before {content:''; position:absolute; right:39px; bottom:38px; width:177px; height:158px ; background:url(../../../img/zkm/rzm/common/bg/bg_credit01.png) no-repeat; background-size: 100%;}
.scoreList p {font-size:13px; line-height: 22px;}
.scoreList .myScore {position:absolute; right:25px; bottom:11px; display:flex; flex-direction: row;}
.scoreList .myScore >div {display:inline-block; width:40px; height:50px; font-size:27px; line-height: 50px; text-align: center; font-weight: 700; border-radius: 5px; background:#fff; box-shadow: 0 0 5px #3b3b3b; margin-left:7px;}

.pweb .relySwiper {padding:45px 0 40px 0;}
.pweb .scoreList {height:278px;  margin:0 170px;}
.pweb .scoreList p {font-size:15px; line-height: 25px;}
.pweb .scoreList.sl01::before {width:254px; height:228px;}
.pweb .scoreList .myScore >div {width: 58px; height:72px; line-height: 72px; border-radius: 7px;}
.pweb .scoreList.sl01::before {right:20px; bottom:0px;}
.pweb .scoreList .myScore {right:0px; bottom:0;}

.cimList {display:flex; justify-content: center; padding-bottom:18px; margin-top: 25px;}
.cimList >div {width:75px; height:75px; line-height: 75px; text-align: center; background-color: #eaf1ff; margin:0 -8px; border-radius: 50%;}
.cimList >div.mix { mix-blend-mode: multiply;}
.ScaleAct {width:271px; height:190px; margin:9px auto 0 auto;}
.ScaleAct img {width:100%;}

.pweb .cimList {margin-top: 50px;}
.pweb .cimList >div {width:150px; height:150px; line-height: 150px;}
.pweb .ScaleAct {width:390px; height:278px; margin: 45px auto 0 auto;}





/* 사장님 신용관리 - 사업장 관리 */
.lineSetBox li {position:relative; border:1px solid #d4d4d4; border-radius: 15px; padding:20px; overflow:hidden;}
.lineSetBox li:not(:first-child) {margin-top:15px;}
.lineSetBox li strong {font-size:16px; line-height: 24px; }
.lineSetBox li span {display:block; font-size:13px; line-height: 19px; color:#767676; margin-top:5px;}
.lineSetBox li a {position:absolute; top:0; left:0; right:0; bottom:0;}
.lineSetBox li .link:after {content:''; position: absolute; right: 28px; top: 50%; width: 8px; height: 8px; border-top: 2px solid #bababa; border-right: 2px solid #bababa; transform: rotate(45deg) translateY(-50%);}

.pweb .lineSetBox li {padding: 35px 39px;}
.pweb .lineSetBox li:not(:first-child) {margin-top:40px;}
.pweb .lineSetBox li strong {font-size: 20px; line-height: 30px;}
.pweb .lineSetBox li span {font-size:15px; line-height: 25px; margin-top:10px;}
.pweb .lineSetBox li .link:after {right:40px;}

.lineSetBox.move li {padding-left:52px;}
.lineSetBox.move li:after {position:absolute; content:''; right:20px; top:50%; margin-top:-3px; width:20px; height:6px; border:1px solid #767676; border-width: 1px 0;}
.lineSetBox.move li .btnDel{position:absolute; top:50%; transform:translateY(-50%); left:20px; width:24px; height:24px; background:#066bfd; border-radius: 50%;}
.lineSetBox.move li .btnDel::before {content:''; position:absolute; left:6px; top:11px; width:12px; height:2px; background:#fff; border-radius: 1px; }

.pweb .lineSetBox.move li {padding-left:87px;}
.pweb .lineSetBox.move li:after {right:40px; width:28px; height:5px; border:2px solid #767676; border-width: 2px 0;}
.pweb .lineSetBox.move li .btnDel {left:40px; width:28px; height:28px;}
.pweb .lineSetBox.move li .btnDel::before {left:8px; top:13px;}





/* 사장님 신용관리 - 사업장 등록 */
.bizNumInfo {padding:30px 0; margin:-75px 0 40px 0 ;}
.pweb .bizNumInfo {text-align: center; padding: 65px 0 70px 0; margin-bottom:70px;}






/* 사장님 신용관리 - 	사업장 신용관리 */
.scoreInfo {margin-top:10px;font-size: 13px; }
.scoreInfo .date {font-size:13px; line-height: 22px; color:#066bfd; margin-bottom:13px;}
.isScore {position:relative; border:1px solid #d4d4d4; border-radius: 15px; padding:28px 25px;}
.isScore a {position:absolute; top:0; left:0; right:0; bottom:0;}
.isScore a:before {right:24px;}
.isScore .score {display:inline-block; height:40px; line-height: 40px;}
.isScore .score strong {font-size:32px; font-weight: 200; font-family: 'Roboto';}
.isScore .vary {position:relative; display:inline-block; vertical-align:top; margin:10px 0 0 8px; height:22px; line-height: 22px; color:#fff; padding:0 10px 0 23px; background-color:#0062ff; border-radius: 11px;}
.isScore .up:before {position:absolute; content:''; left:10px; top:2px; border:8px solid #fff; border-width:5px 5px 8px 5px; border-color:transparent transparent #fff transparent}
.isScore .grade {position:relative; display:inline-block; margin-left:35px; padding-left:35px;}
.isScore .grade:before {position:absolute; content:''; left:0; top:-9px; bottom:-9px;; border-left:1px solid #eee;}
.isScore .grade .pColor {font-size:32px; font-weight: 200; font-family: 'Roboto';}

.pweb .isScore a:before {right:44px;}
.pweb .scoreInfo {margin-top:20px;font-size: 13px;}
.pweb .scoreInfo .date {font-size:18px; line-height: 28px; color:#066bfd; margin-bottom:65px; text-align: center;}
.pweb .isScore { display:flex; justify-content: center;}
.pweb .isScore .score {width:70%; text-align: center;}
.pweb .isScore .grade {width:30%; text-align: center;}

