<!DOCTYPE html>
<html lang="ko">

<head>
  <meta charset="euc-kr">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>NICE지키미</title>

  <script src="../../../js/bzw/jquery-1.11.3.js"></script>
  <script src="../../../js/bzw/jquery-ui.min.js"></script>
  <script src="../../../js/bzw/ui.common.js"></script>

  <link rel="stylesheet" href="../../../css/zkm/bzw/common.css">
  <script type="text/javascript">
    mbHeader = { title: '소비현황', prev: true, menu: true };

    $(function() {        
      modal.openModal('#schMonth');
    });
  </script>
</head>

<body>
  <div id="skipNavi">
    <a href="#content">본문내용 바로가기</a>
  </div>
  <div class="wrap" id="wrap">
    <!-- header -->
    <script type="text/javascript" src="../../../bzw/inc/header.js"></script>
    <script type="text/javascript" src="../../../bzw/inc/breadcrumbs.js"></script>
    <!--// header -->

    <!-- container -->
    <div class="container" id="container">
      <div class="content">
        <div class="section">
          <a href="#payOpt" class="btn base modalOpen">[팝업] 달력(월)</a>
        </div> 
      </div>
    </div>
    <!--// container -->

    <!-- popup -->
    <div class="uiModal bottom" id="schMonth">
      <div class="modalWrap">
        <button type="button" class="uiBtn typeModalClose"><span class="hidden">닫기</span></button>
        <div class="modalHeader subHead">
          <h1>조회할 월을 선택해주세요</h1>
        </div>
        <div class="modalContent">
          <div class="inner">
            <!-- content -->
            <div class="dateItem">
              <div class="control">
                <div class="intDate t1">
                  <span>2022</span>
                </div>
                <a href="#none" class="btnArrow prev"><span class="hidden">이전달 선택</span></a>
                <a href="#none" class="btnArrow next"><span class="hidden">다음달 선택</span></a>
              </div>
              <ul class="mthList">
                <li><div class="chkItem"><input type="checkbox" name="month" id="m1" checked="checked"><label for="m1"><span>1월</span></label></div></li>
                <li><div class="chkItem"><input type="checkbox" name="month" id="m2" checked="checked"><label for="m2"><span>2월</span></label></div></li>
                <li><div class="chkItem"><input type="checkbox" name="month" id="m3"><label for="m3"><span>3월</span></label></div></li>
                <li><div class="chkItem"><input type="checkbox" name="month" id="m4"><label for="m4"><span>4월</span></label></div></li>
                <li><div class="chkItem"><input type="checkbox" name="month" id="m5"><label for="m5"><span>5월</span></label></div></li>
                <li><div class="chkItem"><input type="checkbox" name="month" id="m6"><label for="m6"><span>6월</span></label></div></li>
                <li><div class="chkItem"><input type="checkbox" name="month" id="m7"><label for="m7"><span>7월</span></label></div></li>
                <li><div class="chkItem"><input type="checkbox" name="month" id="m8"><label for="m8"><span>8월</span></label></div></li>
                <li><div class="chkItem"><input type="checkbox" name="month" id="m9"><label for="m9"><span>9월</span></label></div></li>
                <li><div class="chkItem"><input type="checkbox" disabled="disabled" name="month" id="m10"><label for="m10"><span>10월</span></label></div></li>
                <li><div class="chkItem"><input type="checkbox" disabled="disabled" name="month" id="m11"><label for="m11"><span>11월</span></label></div></li>
                <li><div class="chkItem"><input type="checkbox" disabled="disabled" name="month" id="m12"><label for="m12"><span>12월</span></label></div></li>
              </div>
            </ul>
            <!--// content -->
          </div>
          <div class="modalBottom">
            <div class="btns">
              <button type="button" class="btn primary"><span>확인</span></button>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!--// popup -->

    <!-- footer -->
    <div class="footer"></div>
    <!--// footer -->
  </div>
</body>

</html>