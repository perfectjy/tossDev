.dataWrap .insuranceList dt.d1::before {background-color: #3587ff;}
.dataWrap .insuranceList dt.d2::before {background-color: #fa9759;}
.dataWrap .insuranceList dt.d3::before {background-color: #314cff;}
.dataWrap .insuranceList dt.d4::before {background-color: #ff7070;}
.dataWrap .insuranceList dd {font-size: 13px; font-weight: 700; color: #444;}

.btnCircle { display: flex; align-items: center; justify-content: center; display: block; width: 50px; height: 50px; border-radius: 50%; background-color: #eff2f8; margin-left: 5px;}
.btnCircle span { color: #767676; }
.btnCircle.active { background-color: #fff; border: 0; box-shadow:0px 4px 4px 1px rgba(0,97,255,0.3)}
.btnCircle.active span { color: #066bfd; font-weight: 700;}

/* .nList {padding-top: 40px;} */
.nList > ul {border-top: 1px solid #222; border-bottom: 1px solid #bababa;}
.nList > ul li {padding: 17px 10px; font-size: 13px; display: flex; justify-content: space-between;}
.nList > ul li:not(:first-child) {border-top: 1px solid #eee;}
.nList > ul strong {font-weight: 700;}
.nList > ul span {color: #444;}

.twoTabListArea {padding-top: 40px;}
.twoTabListArea .twoTab {display: flex; flex-wrap: wrap; }
.twoTabListArea .twoTab div dt {color: #767676; font-size: 13px;}
.twoTabListArea .twoTab div {border: 1px solid #d4d4d4; border-radius: 10px; padding: 15px 10px 10px; width: calc(50% - 5px);}
.twoTabListArea .twoTab div dd {background-color: #eff2f8; line-height: 22px; border-radius: 5px; text-align: center; font-size: 18px; font-weight: 700; padding: 15px 0; margin-top: 12px;}
.twoTabListArea .twoTab div:nth-child(even){margin-left: 10px;}
.twoTabListArea .twoTab div:nth-child(even) dt {text-align: right;}
.twoTabListArea .twoTab div:nth-child(n + 3){margin-top: 15px;}

.listCircle .innerBox {padding: 15px; border: 1px solid #d4d4d4; border-radius: 5px; margin-top: 15px;}
.listCircle .innerBox .listDot>li:not(:first-child) {margin-top:2px;}
.listCircle .innerBox ol>li:not(:first-child) {margin-top:2px;}

.lineboxList.sideIcon li {padding-left: 88px; position: relative;}
.lineboxList.sideIcon li::before {content: ''; position: absolute; width: 66px; height: 66px; left: 10px; top: 50%; transform: translateY(-50%);  background-repeat: no-repeat; background-position: center; background-size: 100%;}
.lineboxList.sideIcon li.iLoan::before  {background-image: url(../../../img/zkm/rzm/common/bg/bg_loan01.png);}
.lineboxList.sideIcon li.iTax::before  {background-image: url(../../../img/zkm/rzm/common/bg/bg_tax.png);}

.myAccIndex {padding-top: 30px;}
.keywordList2.acco.active ul {flex-wrap: wrap; height: auto;}

.btnPlus {border: 1px solid #d4d4d4; border-radius: 50%; width: 30px; height: 30px; background: url(../../../img/zkm/rzm/common/ico/ico_add.png) no-repeat; background-size: 12px; background-position: center;}
.tabList.careIdx {display: flex;}

.atUp::before, .atDown::before {content: ''; position: absolute; top: 50%; left: 0; transform: translateY(-50%); display: block; height: 21px; width: 21px; border-radius: 50%; background: no-repeat center; background-size: contain;}
.atUp {color: #ff003a; position: relative; padding-left: 25px;}
.atUp::before {background-image: url(../../../img/zkm/rzm/common/ico/ico_cricle_up.png);}
.atDown {color: #066bfb; position: relative; padding-left: 25px;}
.atDown::before {background-image: url(../../../img/zkm/rzm/common/ico/ico_cricle_down.png);}

.bUp::before, .bDown::before {content: ''; margin-right: 6px; display: inline-block; width: 8px; height: 11px; background: no-repeat center; background-size: contain;}
.bUp::before {background-image: url(../../../img/zkm/rzm/common/ico/ico_blk_up.png);}
.bDown::before {background-image: url(../../../img/zkm/rzm/common/ico/ico_blk_down.png);}

.arUp, .arDown {position: relative; padding-left: 15px;}
.arUp::before, .arDown::before {content: ''; position: absolute; left: 0; top: 50%; transform: translateY(-50%); background: no-repeat center; background-size: contain; width: 10px; height: 10px;}
.arUp {color: #ff003a !important;}
.arDown {color: #066bfb !important;}
.arUp::before {background-image: url(../../../img/zkm/rzm/common/ico/ico_ar_up.png);}
.arDown::before {background-image: url(../../../img/zkm/rzm/common/ico/ico_ar_down.png);}


.tag.secondary {border: 1px solid #066bfb; color: #066bfb; background-color: #fff;}
.inq::after {content: "*"; display: inline-block; padding-left: 2px; color: #066bfb; font-size: 15px; font-weight: 700;}

.contMainTit {font-size: 17px; font-weight: 700; padding-bottom: 20px; line-height: 22px;}
.contMainTit .subInfo {display: inline-block; padding-top: 7px; font-size: 13px; color: #767676; font-weight: 400;}
.contSubTit {font-size: 16px; font-weight: 700; padding-bottom: 20px; line-height: 26px;}
.contSubTit2 {font-size: 15px; font-weight: 700; padding-bottom: 20px; line-height: 20px;}

.tagLinkWrap .contMainTit {padding-bottom: 10px;}
.tagLinkWrap:not(:first-child) {margin-top: 40px;}
.tagLinkList li {border-bottom: 1px solid #eee;}
.tagLinkList li a {display: flex; align-items: flex-start; font-size: 15px; padding: 30px 30px 30px 0;}
.tagLinkList li a .tag {font-size:inherit; font-weight: 700; margin-right: 10px; padding: 4px 12px; border-radius: 15px;}
.tagLinkList li a strong {display: block;  font-weight: 700;  margin-top: 3px;}
.tagLinkList li a span {font-size: 13px; color: #767676; display: block; font-weight: 400;}

.rndLink ul {white-space: nowrap; font-size: 0; overflow-x: auto; overflow-y: hidden;}
.rndLink li {display: inline-block; vertical-align: top; padding: 0 4px;}
.rndLink ul li a {background-color: #f5f5f5; display: block; height: 42px; line-height: 42px; padding: 0 14px; font-size: 16px; border-radius: 21px; }
.rndLink.tR ul li a {background-color: #fff; color: #222; border: 1px solid;}
