.rIcon.driven::before {background-image: url(../../../img/zkm/rzm/common/ico/ico_car_tag2.png);}
.rIcon.start::before {background-image: url(../../../img/zkm/rzm/common/ico/ico_car_tag1.png);}

.btnArrow.cw {text-align: center; color: #fff;}
