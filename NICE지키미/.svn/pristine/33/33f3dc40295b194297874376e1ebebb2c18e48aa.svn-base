.h4 strong {font-weight: 700;}

.btnAddR {font-size: 15px; position: relative; padding-right: 20px; display: block;}
.btnAddR::after {content: ''; position: absolute; right: 0; top: 50%; transform: translateY(-50%); width: 12px; height: 12px; background: url(../../../img/zkm/rzm/common/ico/ico_add_bl.png) no-repeat center; background-size: 100%;}
.btnAddR.gov {margin-top: 18px; line-height: 15px;}

