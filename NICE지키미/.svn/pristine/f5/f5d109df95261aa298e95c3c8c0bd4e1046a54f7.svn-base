    <script type="text/javascript">
        mbHeader = { title: '모으다', home: false, prev: true, menu: true };

        $(function () {
            // swiper
            if (!isMobile) {
                swiper1.destroy();
                swiper1 = undefined;
            } else {
                var swiper1 = new Swiper('.bnrSlider', {
                    spaceBetween: 20,
                    pagination: {
