<!DOCTYPE html>
<html lang="ko">

<head>
    <meta charset="euc-kr">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>NICE지키미</title>

    <script src="../../../js/zkm/rzm/jquery-1.11.3.js"></script>
    <script src="../../../js/zkm/rzm/jquery-ui.min.js"></script>
    <script src="../../../js/zkm/rzm/swiper.min.js"></script>
    <script src="../../../js/zkm/rzm/ui.common.js"></script>

    <link rel="stylesheet" href="../../../css/zkm/rzm/swiper.min.css">
    <link rel="stylesheet" href="../../../css/zkm/rzm/common.css">
    <script>
        $(function () {
            modal.openModal('#modal');

            var swiper = new Swiper(".notiSwiper", {
                loop: true,
                pagination: {
                    el: ".swiper-pagination",
                    clickable: true,
                },
            });
        });
    </script>
</head>

<body>
    <div id="wrap">
        <div id="skipNavi">
            <a href="#content">본문내용 바로가기</a>
        </div>
        <!-- header -->
        <script type="text/javascript" src="../../../bzw/inc/header.js"></script>
        <script type="text/javascript" src="../../../bzw/inc/breadcrumbs.js"></script>
        <!--// header -->

        <!-- container -->
        <div id="container">
            <div id="content">
                <div class="section">
                    <a href="#modal" class="btn base modalOpen">소득정보 모아보기</a>
                </div>
            </div>
        </div>
        <!--// container -->

        <!-- popup -->
        <div class="uiModal modal" id="modal">
            <div class="modalWrap notiPop">
                <button type="button" class="ui-btn typeModalClose"><span class="hidden">닫기</span></button>
                <div class="modalHeader hidden">
                    <h1>공지</h1>
                </div>
                <div class="modalContent">
                    <!-- content -->
                    <div class="notiSwiper">
                        <ul class="swiper-wrapper">
                            <li class="swiper-slide">
                                <div class="notiConts">
                                    <div class="contView">
                                        첫번째 공지
                                    </div>
                                    <div class="btnBox">
                                        <button type="button" class="btn primary"><span>바로가기</span></button>
                                    </div>
                                </div>
                            </li>
                            <li class="swiper-slide">
                                <div class="notiConts">
                                    <div class="contView">
                                        두번째 공지
                                    </div>
                                    <div class="btnBox">
                                        <button type="button" class="btn primary"><span>바로가기</span></button>
                                    </div>
                                </div>
                            </li>
                            <li class="swiper-slide">
                                <div class="notiConts">
                                    <div class="contView">
                                        세번째 공지
                                    </div>
                                    <div class="btnBox">
                                        <button type="button" class="btn primary"><span>바로가기</span></button>
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </div>
                    <!--// content -->
                </div>
                <div class="cookieBox">
                    <input type="checkbox" name="period" id="today">
                    <label for="today"><span>오늘 그만 보기</span></label>
                </div>
            </div>
        </div>
        <!--// popup -->

        <!-- footer -->
        <div class="footer"><script type="text/javascript" src="../../../bzw/inc/footer.js"></script></div>
        <!--// footer -->
    </div>
</body>

</html>