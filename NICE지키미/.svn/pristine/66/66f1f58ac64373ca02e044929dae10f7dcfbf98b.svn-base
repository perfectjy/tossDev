.locateCertifyWrap .swipeGMenu {overflow:hidden; height:52px; padding:0 25px; border-bottom:1px solid #e8e8e8; background:#f8f8f8;}
.locateCertifyWrap .swipeGMenu > div {position:relative; padding:8px 0;}
.locateCertifyWrap .swipeGMenu > div > button {position:absolute; top:0; bottom:0; width:25px; background:url('../../img/rzm/btn_swipe_prev_next.png') no-repeat; background-size:auto 52px;}
.locateCertifyWrap .swipeGMenu > div .swipePrev {left:-25px; background-position:left top; z-index:1;}
.locateCertifyWrap .swipeGMenu > div .swipeNext {right:-25px; background-position:right top; z-index:1;}
.locateCertifyWrap .swipeGMenu > div .swiper-wrapper {overflow:hidden; width:1000em;}
.locateCertifyWrap .swipeGMenu > div .swiper-slide {float:left; }
.locateCertifyWrap .swipeGMenu > div .swiper-slide button {position:relative; box-sizing:border-box;-webkit-box-sizing:border-box; width:100%; height:36px; padding:1px 15px 0; line-height:34px; }
.locateCertifyWrap .swipeGMenu > div .swiper-slide button:after {display:block; content:""; position:absolute; top:50%; left:-1px; width:1px; height:16px; background:#dedede; transform:translate(0, -50%);}
.locateCertifyWrap .swipeGMenu > div .swiper-slide.active button {color:#fff; font-weight:700; border-radius:3px; background:#525d6f; z-index:1;}
.locateCertifyWrap .swipeGMenu > div .swiper-slide.active button:after {display:none;}

