/* 관리 설정 - 상품권 등록 */
.niceCoupon {overflow: hidden;}
.niceCoupon li {position:relative; border: 1px solid #d4d4d4; padding:9px 24px; border-radius: 15px; }
.niceCoupon li:not(:first-child) {margin-top: 20px;}
.niceCoupon li::before {content:''; position:absolute; left:-11px; top:50%; transform: translateY(-50%); width:22px; height:22px; border: 1px solid #d4d4d4; background:#fff; border-radius:50%; }
.niceCoupon li::after {content:''; position:absolute; right:-11px; top:50%; transform: translateY(-50%); width:22px; height:22px; border: 1px solid #d4d4d4; background:#fff; border-radius:50%; }
.niceCoupon li > div {width:100%; height:100%; border-right: 2px dashed #d4d4d4; border-left: 2px dashed #d4d4d4; padding:5px 14px; text-align: center;}
.niceCoupon li div strong {display:block; font-size:17px; font-weight: 700; line-height: 1; margin-top:10px;}
.niceCoupon li div span {display: block; font-size:12px; line-height: 1; color:#767676; margin-top:10px;}
.niceCoupon li div div.couponNum {background:#f9f9f9; font-size:15px; font-weight: 700; line-height: 1; padding:8px 0; text-align: center; color:#0669fa; border-radius: 5px; margin-top:15px;}







/* 관리 설정 - 공동인증서 목록 */
.jcInput ul li {position:relative; border:1px solid #ddd; border-radius: 10px; padding:17px 23px 17px 14px; margin-top:15px;}
.jcInput ul li.act {border: 2px solid #066bfd;}
.jcInput ul li.dis {background:#f9f9f9;}
.jcInput ul li.dis .xBtn {background:#8e8e8e;}
.jcInput ul li.dis .xBtn::before,
.jcInput ul li.dis .xBtn::after {background:#fff;}

.jcInput input[type=radio] + label {position:relative; padding-left:24px; font-size:17px; line-height: 24px;}
.jcInput input[type=radio] + label strong {font-weight: 700;}
.jcInput input[type=radio] + label span {display:block; padding-left:24px; font-size:12px; line-height: 23px; color:#767676;}
.jcInput input[type=radio]:focus + label:after {box-shadow:0px 0px 4px 2px rgba(0,97,255,0.3)}
.jcInput input[type=radio] + label:after {position:absolute; content:''; display:block;/* border-color:#bababa; */border-style:none; border-width:0 0 2px 2px; width:10px; height:5px; left:0; top:7px; transform: rotate(-45deg);}
.jcInput input[type=radio]:checked + label:after {border-color:#066bfd; border-style:solid;}
.jcInput .xBtn {position:absolute; right:23px; top:50%; transform: translateY(-50%); width:20px; height:20px; background:#ececec; border-radius: 50%;}
.jcInput .xBtn::before {content:''; position: absolute; left:9px; top:5px; width:2px; height: 10px; background:#444; transform:rotate(45deg);}
.jcInput .xBtn::after {content:''; position: absolute; left:9px; top:5px; width:2px; height: 10px; background:#444; transform:rotate(-45deg);}





