.familyWrap .incomeBox {position: relative; min-height: 230px; width: calc(50% - 5px); padding: 25px 20px; background-color: #fff; border: 1px solid #066bfd; border-radius: 15px;}
.incomeBox .incomeExp {word-break: keep-all;}
