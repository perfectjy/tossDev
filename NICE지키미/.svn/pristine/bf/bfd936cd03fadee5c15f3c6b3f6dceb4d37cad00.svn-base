.familyWrap {display: flex; margin-bottom: 30px;}
.familyWrap .familyList {width: calc(50% - 5px); margin-right: 10px;}
.familyList button.addBtn {position: relative; height: 55px; }
.familyList button.addBtn::before {position: absolute; content: ''; left: 50%; top: 50%; transform: translate(-50%, -50%); width: 16px; height: 16px; background: url(../../../img/zkm/bzw/common/ico/ico_add.png) no-repeat center; background-size: contain;}
.familyList button {position: relative; padding: 20px 0 20px 63px; color: #767676; width: 100%; text-align: left;}
.familyList button.active {font-size: 15px; font-weight: 700; color: #222;}
.familyList button.active::before,
.familyList button.active::after {content: ''; position:absolute; left: 0; width: 100%; height: 1px; background-color: #c3d9ed; background: linear-gradient(to left, #c3d9ed00, #c3d9ed, #c3d9ed00);}
.familyList button.active::before {top: 0;}
.familyList button.active::after {bottom: 0;}
.familyList button i {position: absolute; left: 0; top: 50%; transform: translateY(-50%) scale(0.8); width: 53px; height: 53px; border-radius: 50%; background-color: #f0f1f3; background-size: cover; background-repeat: no-repeat;}
.familyList button.cF1 i {background-image: url(../../../img/zkm/bzw/sub/ico_family01.png);}
.familyList button.cF2 i {background-image: url(../../../img/zkm/bzw/sub/ico_family03.png);}
.familyList button.cF3 i {background-image: url(../../../img/zkm/bzw/sub/ico_family02.png);}
.familyList button.active i {transform: translateY(-50%) scale(1); background-image: none; background-color: #5c86e5; text-align: center; line-height: 53px; color: #fff; font-size: 17px; font-weight: 500;}

