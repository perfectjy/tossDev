.colsBox dt {margin-top:35px; font-size:12px; }
.colsBox dd {font-size:14px; font-weight: 700;}

strong.bld {font-weight: 700;}

.uiModal.bottom .modalHeader.addInfo {padding-bottom: 30px;}
.uiModal.bottom .modalHeader.addInfo h1 {padding-bottom: 5px;}

.btnUpdate {font-size: 12px; display: inline-block; color: #767676; position: relative; padding-right: 14px; margin-top: 10px;}
.btnUpdate::after {content: ''; position: absolute; top: 50%; right: 0; transform: translateY(-50%); display: block; width: 11px; height: 10px; margin-left: 4px; background: url(../../../img/zkm/rzm/common/ico/ico_pencil.png) no-repeat center; background-size: contain;}

.srcLineBox2 {border-top: 1px solid #eee; margin-top: 50px; padding-top: 50px;}

.tabmenu.tb1 ul {white-space: inherit; overflow: unset;}
.tabmenu.tb1 ul li {margin-top: 10px;}

.tagSq {display: inline-block; height: 22px; line-height: 20px; justify-content: center; font-weight: 300; color: #fff; font-size: 13px; padding: 1px 10px 2px; border-radius: 4px; margin-right: 3px;}
.tagSq.re {background-color: #d50f3c;}
.tagSq.bl {background-color: #066bfd;}
.tagSq.pr {background-color: #066bfd;}
.tagSq.bk {background-color: #222;}

.listDash strong.bld {font-weight: 500;}


.pweb .governingTitle.pcLeft {text-align: left;}
.pweb .toggleBox input[type=radio] + label {font-size: 15px;}

.pweb .uiModal.bottom .modalHeader .hSubTxt {margin: 20px 0 0 0; font-size: 16px;}

.pweb .formSingle .fRow > div:not(:first-of-type) {margin-left: 10px;}

.pweb .formWrap .formItem.spb {margin-bottom: 50px;}

.pweb .cntNum {height: 30px; font-size: 18px; line-height: 28px; padding: 0 10px; border-radius: 15px;}

