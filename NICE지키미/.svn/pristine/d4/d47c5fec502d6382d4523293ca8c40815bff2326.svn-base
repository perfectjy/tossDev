.familyWrap .incomeBox {position: relative; min-height: 230px; width: calc(50% - 5px); padding: 25px 20px; background-color: #066bfd; border: 1px solid #066bfd; border-radius: 15px;}
.familyWrap .incomeBox::after {content: ''; display: block; width: 0; height: 0; border-top: 10px solid #066bfd; border-left: 8px solid transparent; border-right: 8px solid transparent; position: absolute; bottom: -8px; left: 50%; transform: translateX(-50%);}
.familyWrap .incomeBox * {color: #fff !important;}

.familyWrap {display: flex; margin-bottom: 30px; align-items: center;}
.familyWrap .familyList {width: calc(50% - 5px); margin-right: 10px;}
.familyList button {position: relative; padding: 20px 0 20px 63px; color: #767676; font-weight: 300; width: 100%; text-align: left;}
.familyList button.active {font-size: 15px; font-weight: 700; color: #222;}
.familyList button.active::before,
.familyList button.active::after {content: ''; position:absolute; left: 0; width: 100%; height: 1px; background-color: #c3d9ed; background: linear-gradient(to left, #c3d9ed00, #c3d9ed, #c3d9ed00);}
.familyList button.active::before {top: 0;}
.familyList button.active::after {bottom: 0;}
.familyList button i {display: inline-block; position: absolute; left: 0; top: 50%; transform: translateY(-50%) scale(0.9); width: 53px; height: 53px; border-radius: 50%; background-color: #f0f1f3; background-size: cover; background-repeat: no-repeat;}
.familyList button i img {max-width: 100%;}
.familyList button.cF1 i {background-image: url(../../../img/zkm/rzm/sub/ico_family01.png);}
.familyList button.cF2 i {background-image: url(../../../img/zkm/rzm/sub/ico_family02.png);}
.familyList button.cF3 i {background-image: url(../../../img/zkm/rzm/sub/ico_family03.png);}
.familyList button.cF4 i {background-image: url(../../../img/zkm/rzm/sub/ico_family04.png);}
.familyList button.active i {transform: translateY(-50%) scale(1); background-color: #5c86e5;}
.familyList button.cF1.active i {background-image: url(../../../img/zkm/rzm/sub/ico_family01Active.png);}
.familyList button.cF2.active i {background-image: url(../../../img/zkm/rzm/sub/ico_family02Active.png);}
.familyList button.cF3.active i {background-image: url(../../../img/zkm/rzm/sub/ico_family03Active.png);}
.familyList button.cF4.active i {background-image: url(../../../img/zkm/rzm/sub/ico_family04Active.png);}
.familyList button.btnAdd {position: relative; height: 55px; }
.familyList button.btnAdd::before {position: absolute; content: ''; left: 50%; top: 50%; transform: translate(-50%, -50%); width: 16px; height: 16px; background: url(../../../img/zkm/rzm/common/ico/ico_add.png) no-repeat center; background-size: contain;}

