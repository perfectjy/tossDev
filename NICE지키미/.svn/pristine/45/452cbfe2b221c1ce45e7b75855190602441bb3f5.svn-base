		$(function () {
			// swiper
			if (!isMobile) {
				swiper.destroy();
				swiper = undefined;
			} else {
				var swiper = new Swiper('.requestSwiper', {
					pagination: {
						el: '.swiper-pagination',
						clickable: true,
