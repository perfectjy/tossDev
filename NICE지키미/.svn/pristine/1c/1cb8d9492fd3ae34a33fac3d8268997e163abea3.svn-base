.chartBox.gender{display:flex; flex-direction:column; justify-content:center; text-align:center;}
.chartBox.gender dl{display:flex; justify-content:center;}
.chartBox.gender dl dd{font-size:18px;}
.chartBox.gender dl dd strong{font-size:21px; font-weight:700;}
.chartBox.gender dl>div{text-align:center;}
.chartBox.gender dl>div::before{content:''; display:inline-block; width:140px; height:113px;}
.chartBox.gender .man::before{background:url(../../img/bzw/common/bg/bg_puzzle_man.png) no-repeat; background-size:140px 113px;}
.chartBox.gender .woman::before{background:url(../../img/bzw/common/bg/bg_puzzle_woman.png) no-repeat; background-size:140px 113px;}
.chartBox.gender p{margin-top:13px; color:#767676; font-size:15px;}
.chartBox.gender p>strong{color:#444; font-weight:700;}
.chartBox.nobg{background:none;}


/* fixedList */
.fixedList{margin-top:78px;}
.listSwiper li{display:inline-block; margin-right:16px;}
.listSwiper .btn{width:65px; height:65px; padding-top:23px; line-height:1; color:#767676; font-size:14px; border-radius:12px; background:#f2f3f5;}