/* 고객센터 */

/* 서비스문의, 신용상담신청 */
.questionList >li {border: 1px solid #eee; border-radius: 15px;}
.questionList >li:not(:first-child) {margin-top:12px;}
.questionList >li a{position:relative; display:block; padding: 25px 15px;}
.questionList >li a::before{content:"";position:absolute; right:15px; top:50%; transform: translateY(-50%) rotate(45deg); width:11px; height:11px; border-top:1px solid #222; border-right:1px solid #222;}
.questionList >li a div.questionTit {padding-right:62px; margin-top:15px; font-weight: 700; font-size:16px; line-height: 26px;}
.questionList >li a div.date{margin-top:7px;}
.flagWrap span{display:inline-block; padding:5px 10px; font-size:13px; border-radius:5px; box-sizing: border-box; margin-right:8px;}
.flagWrap .answer {background:#767676; color:#fff;}
.flagWrap .answer.done {background:#066bfd;}
.flagWrap .mark {border:1px solid #066bfd; color:#066bfd;}


/* 공통 고려사항 */
.ui-allim.scroll { height:216px; padding:20px 15px; background: #fafafa; overflow-y: auto;}

