@charset "euc-kr";
 

/* 미소짓는 데이터생활 */

/* 반려동물 */
.myPet {position:relative; padding-top:65px;}
.myPet .petImg {position:absolute; top:0; left:0; right:0; text-align: center;}
[class^="pet"] .photo {display:block; overflow:hidden; width:130px; height:130px; background:#fff url(../../img/bzw/etc/etc_mypet_default.png) no-repeat; background-size:100%; border:1px solid #8e9abd; border-radius: 50%;}
[class^="pet"] .photo img {max-width:100%;}
.myPet .petImg .photo {margin:0 auto;}
.myPet .petImg p {font-size:16px; line-height:30px; color:#fff;}
.myPet .petList {width: auto; border-radius:30px; padding:122px 0 30px 0; text-align: center; background:linear-gradient(#29b0ff, #127ef4, #0969ef); box-shadow: 10px 10px 20px rgba(53, 135, 255, 0.3);}
.myPet .petList li a {color:#fff;}
.myPet .petList li .photo {border:0;margin:0 auto;}
.myPet .petList li .petRegister {position:relative; display:block; width:60px; height:60px; border-radius: 24px; border-color: #d4d4d4; background:#fff ;}
.myPet .petList li .register {display:block; font-size:14px; line-height: 20px; margin-top:6px; text-align: center;}
.myPet .petList li .petRegister::before{content:""; position:absolute; left:0; top:0; bottom:0; right:0; width:100%; height:100%; background:url(../../img/bzw/common/ico/ico_add.png) no-repeat center; background-size:15px;}


/* 반려동물 정보 */
.myPetInfo {position:relative; margin-top:32px; padding: 0 25px;}
.myPetInfo .infoTit {font-size:15px; line-height: 22px; font-weight: 500;}
.myPetInfo .infoList {font-size:14px; line-height: 30px; color:#444; margin-top:12px;}
.myPetInfo .infoList >dt {float:left; margin-right:24px;}
.myPetInfo .myPetInfoBtn {position:absolute; right:25px; top:-6px; width:55px; height:26px; line-height: 26px; border-radius: 13px; background: #1d6fdb; text-align: center;}
.myPetInfo .myPetInfoBtn::before {content:""; position:absolute; right:10px; top:50%; transform:translateY(-50%); width:10px; height:17px; background:url(../../img/bzw/common/ico/ico_arr_fix.png) no-repeat center; background-size:50%;}
.myPetInfo .myPetInfoBtn >span {font-size: 14px; letter-spacing: -.8px; color:#fff; margin-right:12px;}


/* 카드정보 */
.cardInfoWrap {position:relative; background: #f0f1f3; padding: 30px 20px 20px; margin:35px -20px 0 -20px; }
.cardInfoWrap h3 {font-size:20px; line-height: 28px; font-weight: 700;}
.cardInfoWrap strong {font-weight: 700;}
.cardInfoWrap .cardInfo {position:relative; margin-top:16px; padding:25px; background: #fff; border-radius: 19px;}
.cardInfoWrap .cardInfo > strong {font-size:17px; line-height: 25px;}
.cardInfoWrap .cardInfo p {font-size:14px; line-height:24px; color:#767676; margin-top:14px;}
.cardInfoWrap .cardInfo p strong:not(.pColor) {color:#000;}
.cardInfoWrap .cardInfo .cardImg {width: 100%; margin-top:17px;}
.cardInfoWrap .cardInfo .cardImg img {width: 100%;}
.cardInfoWrap .cardInfo a {position:absolute; top:0; left:0; right:0; bottom:0;}


/* 반려동물 사진등록 */
.petImgInt {position:relative; width:130px; margin:0 auto;}
.petImgInt a {position:absolute; top:0; left:0; display:block; width:100%; height:100%;}
.petImgInt a:after {position: absolute; content:''; width:32px; height:32px; top:86px; right:-4px; background:url(../../img/bzw/common/ico/ico_add_round.png) no-repeat; background-size: contain;}


/* 품종검색 */
.resultWrap {margin-top:30px; overflow-y:auto; height:calc(100vh - 380px)}
.resultWrap .petList li {padding:0; display:block;}
.resultWrap .petList li:not(:first-child) { margin-top:20px;}
.resultWrap .petList label {display:flex; width:100%;}
.resultWrap .petList .photo {border-radius: 50%;}
.resultWrap .petList .name {padding-left:15px;font-size:16px; align-self: center; margin:0;}


/* 반려동물 나이 계산기 */
.lineFormSearch .btn {position:absolute; right:0; bottom:5px;}
.petListWrap {margin:20px -20px 0 -20px; text-align:center;}
.petList, .petList ul {font-size:0;}
.petList li {display:inline-block; padding:0 5px;}
.petList li a {display:block; color:#222;}
.petList li .photo {width:60px; height:60px; border-radius: 24px; border-color: #d4d4d4;} 
.petList li .name {display:block; font-size:14px; line-height: 20px; margin-top:6px; text-align: center;}
.petList li .btnPet {width:60px; font-size:14px; line-height: 20px;}
.petList li input:checked + label {font-weight: bold; color:#066bfd}
.petList li input:checked + label .photo {border-color:#066bfd;}
.petDesc {margin-top: 90px;}
.petDesc .listDot {color:#767676;}
.petDesc .listDot li::before {background: #767676;}


/* 반려동물 나이 계산결과 */
.ageResult {position:relative; padding-top:65px; color: #fff;}
.ageResult .ageResultTit {position:absolute; left:0; top:0px; right: 0; display:flex; justify-content: center; align-items: center; text-align: center;}
.ageResult .ageResultTit >span {margin: 0 -5px; }
.ageResult .ageResultTit .thumb {display:block; overflow:hidden; width:100px; height:100px; border-radius: 50%;}
.ageResult .ageResultTit .thumb img {max-width:100%;}
.ageResult .ageResultTit .agePet .thumb {background:#fff url(../../img/bzw/etc/etc_mypet_default.png) no-repeat; background-size:100%; border:1px solid #8e9abd;}
.ageResultContent {border-radius:30px; padding:80px 20px 30px 20px; text-align: center; background:linear-gradient(#29b0ff, #127ef4, #0969ef); box-shadow: 10px 10px 20px rgba(53, 135, 255, 0.3);}
.ageResultContent strong {font-size: 18px;}
.ageResultContent .ageResultTxt {margin-top: 10px; text-align: left;}


/* 이름추천 */
.nameRecomInfo {margin-top:20px;}


/* 색상선택 */
.colorSelect .selList {display:flex; flex-wrap: wrap; margin-top:-18px;}
.colorSelect .selList >li {width: 25%; text-align:center; font-size: 12px; line-height: 21px; margin-top:18px;}
.colorSelect .selList >li label {position:relative; display:inline-block;}
.colorSelect .selList >li label:before {display:block; content:''; width:60px; height:60px;border:1px solid #eee; background-color:#fff; border-radius: 50%; margin:0 auto 5px; box-sizing: border-box;}
.colorSelect .selList >li .beige:before {background-color: #faebd7;}
.colorSelect .selList >li .blackBullet:before {background-color: url();}
.colorSelect .selList >li .brown:before {background-color: #835136;}
.colorSelect .selList >li .black:before {background-color: #000;}
.colorSelect .selList >li .gray:before {background-color: #adadad;}
.colorSelect .selList >li .yellow:before {background-color: #adadad;}
.colorSelect .selList >li .darkBrown:before {background-color: #594944;}
.colorSelect .selList >li .lightBrown:before {background-color: #cd9369}
.colorSelect .selList >li .orange:before {background-color: #de8529;}
.colorSelect .selList >li .reddishBrown:before {background-color: #743217;}
.colorSelect .selList >li .blue:before {background-color: #a3d7ec;}
.colorSelect .selList >li .grayBrown:before {background-color: #a79c88;}
.colorSelect .selList >li .combin:before {background: linear-gradient(45deg, #e2a810, #fe1201)}
.colorSelect .selList >li .blackTan:before {background:radial-gradient(black, white);}
.colorSelect .selList >li input[type="radio"]:checked + label {color:#066bfd}
.colorSelect .selList >li input[type="radio"]:checked + label:before {border:5px solid #fff;}
.colorSelect .selList >li input[type="radio"]:checked + label:after {position:absolute; content:''; top:0; left:0; width:60px; height:60px; border:2px solid #066bfd; border-radius: 50%; box-sizing: border-box;}


/* 이름상세, 색상별상세 */
.grayWrap {background: #f0f1f3; margin:0 -20px 0 -20px; padding:0 20px; }
.grayWrap .governing {background:#fff; width:calc(100% + 40px); margin:0 -20px; padding:0 20px 35px 20px;}
.cardInfoWrap.gray {margin-top:0;}
.rankDesc {padding-top:25px;}
.rankDesc .nameList {font-size:16px; color:#222;}
.rankDesc .nameList > li {display:inline-block;}
.rankDescTxt {font-size:14px; line-height: 24px; color:#444;}
.rankDescTxt strong {display:block; font-size:17px; font-weight: 700; color:#066bfd; margin:23px 0 15px 0;}
.nameDescContent {position:relative; background: #fff; border-radius: 16px; padding:20px; margin-top:20px;}
.nameDescContent .nameDescTit {display:block; font-size:15px; color:#222; margin-bottom: 20px;}
.nameDescContent .nameDescTxt {font-size:14px; color:#444;}
.nameDescContent .nameDescTxt.img {display: flex; justify-content: flex-start; align-items: center;}
.nameDescContent .nameDescTxt.img img {width:61px; height:61px; background: #fff; border: 1px solid #dcdff1; border-radius: 50%; margin-right:10px; overflow: hidden; object-fit: contain;}
.nameDescContent .rankView {margin-top:0px;}
.nameDescContent a.link {position: absolute; left:0; top:0; right:0; bottom:0; border-radius: 16px;}
.genderRatio {position:relative; display:flex; justify-content: center; align-items: flex-start; margin-top:17px;}
.genderRatio >dl{text-align: center; font-size:14px; color:#222;}
.genderRatio >dl::before {content:""; display:inline-block; width:85px; height:85px; margin-bottom:10px;}
.genderRatio >dl.female::before {background: url(../../img/bzw/@tmp/@tmp_pet_female.png) no-repeat; background-size: contain;}
.genderRatio >dl.male::before {background: url(../../img/bzw/@tmp/@tmp_pet_male.png) no-repeat; background-size: contain;}
.genderRatio >dl dd {font-weight: 700;}
.genderRatio .than {display:flex; justify-content: center; align-items: center; width:78px; height:85px;}
.genderRatio .than::before {content:"";display:inline-block; width:58px; height:100px; background:url(../../img/bzw/@tmp/@tmp_than.png)no-repeat center; background-size:50%;}


/* 이름랭킹 */
.custForm.widAll {width:100%;}
.rankView .rankList .first {color:#066bfd; font-weight: 500;}


/* 금융소비자보호 -(탭)소비자보호 */
.consumerprotectItem {padding-bottom:46px;}
.consumerprotectItem:not(:first-child) {padding-top:46px;}
.consumerprotectItem .h3 {margin-bottom:12px;}
.consumerprotectItem .txt {margin-bottom:8px;}
.consumerprotectItem .txt .pColor {font-weight: 700;}
.hr {width:calc(100% + 40px); height:10px; background: #f9f9f9; border:0; margin:0 -20px; }
.consumerprotectItem .organizationChart {position:relative; background: #f5f8ff; border-radius: 15px; padding:20px 15px; margin-bottom:20px;}
.consumerprotectItem .organizationChart:not(:first-child) {margin-top: 20px;}
.consumerprotectItem .organizationChart::before {content:""; position: absolute; left:20px; bottom:-5px; width: 0; height: 0; border-left: 10px solid transparent; border-right: 10px solid transparent; border-top: 15px solid #cad5ee; }
.consumerprotectItem .organizationChart:last-child::before {display:none;}
.consumerprotectItem .organizationChart .ko {display:block; font-size:14px; font-weight: 700; color:#066bfd;}
.consumerprotectItem .organizationChart .en {display:block; font-size:12px; font-weight: 400; color:#767676; margin-top:6px;}


/* 금융소비자보호 -(탭)체계, (탭)금융소비자권리  */
.ruleContent:not(:nth-child(2)) {margin-top:36px;}
.ruleContent h3 {position:relative; padding-left:12px;}
.ruleContent h3::before {position:absolute; content:''; top:9px; left:0; width:4px; height:4px; background-color:#444; border-radius: 50%;}
.ruleContent dl.ruleList {position:relative; font-size:14px; line-height: 22px; color:#444; }
.ruleContent dl.ruleList .line {text-decoration:underline; font-weight: 500;}
.ruleContent dl.ruleList >div {margin-top:22px; padding-left:21px;}
.ruleContent dl.ruleList >div dt {position:relative;}
.ruleContent dl.ruleList >div dt::before {position:absolute; content:''; top:10px; left:-12px; width:4px; height:1px; background-color:#444;}
/* .ruleContent dl.ruleList >div dd .btnWrap button {width:100%; margin-top:15px;} */
.ruleContent dl.ruleList >div dd ul.ruleListDepth2 li {display:flex; flex-wrap: wrap; margin-top:6px; margin-bottom:2px;}
.ruleContent dl.ruleList >div dd ul.ruleListDepth2 li .front {width:17px;}
.ruleContent dl.ruleList >div dd ul.ruleListDepth2 li .back {width:calc(100% - 17px)}
.ruleContent dl.ruleList >div dd ul.ruleListDepth2 li ul.ruleListDepth3 {padding-left:15px; margin-top:6px; flex:1;}
.ruleContent dl.ruleList >div dd ul.ruleListDepth2 li ul.ruleListDepth3 li {margin-top:0;}


/* 금융소비자보호 - (탭)금융상품판매대리.중개업자 */
.listNumDot >li {display:flex; flex-wrap: wrap;}
.listNumDot >li span:first-child {width:17px;}
.listNumDot >li span:nth-child(2){width:calc(100% - 17px);}
.listNumDot >li .btnWrap {flex:1;}


/* 금융소비자보호 - (탭)민원접수 */
dl.aaa {}
dl.aaa div {}
dl.aaa div dt{}
dl.aaa div dd{}

dl.bbb div {display:flex; flex-wrap: nowrap;}
dl.bbb div dt {}
dl.bbb div dd {}


/* 금융소비자보호 -(탭)관련사이트 */
.siteList >div {position:relative; left:0; top:0; display:block; border:1px solid #d4d4d4; border-radius: 15px; padding:21px 75px 21px 24px;}
.siteList >div:not(:first-child) {margin-top:15px;}
.siteList >div::before {content:""; position:absolute; right:20px; top:50%; transform:translateY(-50%) rotate(45deg);; width:11px; height:11px;  border-top:2px solid #d4d4d4; border-right:2px solid #d4d4d4;}
.siteList >div .h3 {font-size:17px; font-weight: 700; line-height: 25px; margin-bottom: 0;}
.siteList >div dd {font-size:13px; color:#767676; margin-top:6px;}
.siteList >div a{position:absolute; left:0; top:0; bottom:0; right:0;}


