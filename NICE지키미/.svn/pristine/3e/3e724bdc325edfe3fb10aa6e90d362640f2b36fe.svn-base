.graphWrap {margin-top:15px;}
.graphWrap .des {position:relative; font-size:14px;}
.graphWrap div.des {height:60px;} 
.graphWrap .des em {position:absolute; left:110px; top:10px; line-height:22px;}
.graphWrap .des em:before {content:':'; position:absolute; left:-10px; top:8px; width:3px; height:7px;}
.graphWrap .des > span {display:block; padding-left:17px;}
.graphWrap .des [class^=element] {position:absolute; left:63px} 
.graphWrap .des .elementTop {top:8px; left:56px;}
.graphWrap .des .elementBottom {bottom:8px; border-top:1px solid #2e3653;}
.graphWrap .des + .listDot {margin-top:20px;}
.graphWrap .des > strong {display:inline-block;  font-weight:700;}
.graphWrap div.des > strong {line-height:60px;}
.desWrap {margin-top:13px; margin-left:-20px; text-align:center;}
.desEl, .desEl .sign {position:relative;}
.desEl [class^=element] {position:absolute; margin-left:6px;}
.desEl .elementTop {top:8px;}
.desEl .elementBottom {bottom:8px; border-top:1px solid #2e3653;}
.desEl strong {display:inline-block; line-height:60px; font-weight:700;}
.desEl .txt{position:absolute; bottom:-18px; left:-16px}
.desEl + .pColor {margin-top:6px;}
.graphbox {width:100%; background-color:#fff}
.graphbox img {max-width:100%; max-height:100%;}


/* 맞춤금융상품 > 맞춤대출 */
.directWrap {display:flex; justify-content:space-between; align-items:center; margin-top:20px;}
.comSearchList {overflow-y:auto; height:600px; margin-top:18px;}
.comSearchList .comSearchBtn {padding:20px 0;}
.comSearchList li {border-bottom:1px solid #eee;}
.comSearchList li * {display:block; text-align:left;}
.comSearchList li .name {font-size:16px; color:#222; font-weight:500;}
.comSearchList li .name.pColor,
.comSearchList li .number.pColor {color:#066bfd;}
.comSearchList li .number {margin-top:10px; font-size:13px; color:#767676; font-family:'roboto'; font-weight:100;}
.inInfoTxt {margin-top:36px; margin-bottom:12px; color:#767676; font-size:12px;}
.inInfoTxt > span {display:block;}

.flexEnd {display:flex; justify-content:flex-end;}
.filterBtn {color:#767676; font-size:13px;}
.filterBtn span {display:inline-block;}
.filterBtn span:before {content:''; display:inline-block; width:20px; height:14px; background:url(../../img/bzw/common/btn/btn_filter.png) no-repeat 50% / 100%; vertical-align:middle;}

.total + .loadGoods {margin-top:18px;}
.total > span.pColor {font-weight:700;}
.loadGoods li .tit {margin-top:4px; font-size:14px; font-weight:500;}
.loadGoods .bank + .tag {margin-left:15px;}
.loadGoods.noLink > li {position:relative; padding:30px 55px 30px 25px; border:1px solid #d4d4d4; border-radius:15px; font-size:17px; line-height: 25px;}
.loadGoods.noLink > li img {height:20px;}
.loadGoods.noLink > li strong {display:block; margin-top:9px;}
.loadGoods + .linkList {margin-top:50px;}
.loadGoods .linkTxtWrap {margin-top:45px;}
.loadGoods a.linkTxt {display:inline-block; padding:0; border:none; font-size:15px; color:#222; text-decoration:underline;}
.loadGoods a.linkTxt:after {display:none;}
.loadGoods .guideTxt {padding-top:0; color:#767676; letter-spacing:-1.3px;}
.loadGoods .subTxt {display:block; font-size:13px; color:#767676;}

.goodsInfo {margin:10px 0;}
.goodsInfo div {display:inline-block;}
.goodsInfo .rate {min-width:94px;}
.goodsInfo dt {font-size:13px; color:#222;}
.goodsInfo dd {font-size:17px; color:#066bfd; font-weight:700; line-height:22px;}
.goodsInfo dd .roboto {font-weight:700;}
.goodsOption > li {position:relative; display:inline-block; margin-right:10px; font-size: 13px; line-height: 20px; color: #767676; margin-top:0;}
.goodsOption > li:before {position:absolute; content:''; left:-7px; top:9px; width:2px; height:2px; background-color:#d4d4d4; border-radius:50%;}
.goodsOption > li:first-child:before,
.goodsOption > li:nth-child(4n):before {display:none;}

.goodsDetail .bank img{height:19px;}
.goodsDetail .governing{text-align:left;}
.goodsDetail .governingTitle {margin-top:14px;}
.goodsDetail .goodsInfo dt {margin-bottom:6px; font-size:15px; line-height:24px;}
.goodsDetail .goodsInfo {margin-bottom:23px;}
.goodsDetail .goodsInfo dd {font-size:23px;}


.searchDate {display:block; color:#767676; font-size:12px;}
.flexEnd + .tabConts {margin-top:15px;}
.linkList {margin-top:5px;}
.linkList li {margin-top:20px;}
.linkList a {position: relative; display:block; padding:10px 35px 10px 81px; font-size:16px;}
.linkList a:before {content:''; position:absolute; left:0; top:50%; width:65px; height:72px; background-repeat: no-repeat; background-size:65px auto; background-position:0 0; transform:translateY(-50%);}
.linkList a.ico01:before {background-image:url(../../img/bzw/common/bg/bg_loan01.png);}
.linkList a.ico02:before {background-image:url(../../img/bzw/common/bg/bg_loan02.png);}
.linkList a.ico03:before {background-image:url(../../img/bzw/common/bg/bg_loan03.png);}
.linkList a:after {content:''; position:absolute; right:3px; top:50%; width:8px; height:8px; margin-top:-7px; border-top:2px solid #767676; border-right:2px solid #767676; transform:rotate(45deg);}
.linkList a strong {display:inline-block; font-weight:700; color:#222; font-size:16px;}
.linkList a span {display:block; font-size:13px;}

.detailList > div:not(:first-child) {margin-top:30px;}
.detailList dt {margin-bottom:10px; font-weight:700; color:#222;}
.detailList dd {color:#444;}

.bgBoxLink.flex {display:flex; justify-content:space-between; align-items:flex-end}

/* 맞춤금융상품 > 맞춤대출 > 대출진단 */
.loanTopWrap .governing {margin-bottom:24px; text-align:left;}
.gradientBox {position:relative; padding:25px 20px 20px; background:linear-gradient(to bottom, #066bfd, #003abb); border-radius:18px;}
.gradientBox .tit {font-size:17px; color:#fff; font-weight:700;}
.gradientBox .btnConts {margin-top:19px;}
.gradientBox:before {content:''; position:absolute; bottom:100%; right:0; width:100px; height:119px; background-repeat:no-repeat; background-image:url(../../img/bzw/common/bg/bg_man.png); background-size:100%; background-position:100%;}

/* 대출로딩 */
.loadingSection .guideSubTxt {margin-top:20px; text-align:center;}
.loadingSection .guideSubTxt .linkTxt {display:block; text-decoration:underline; font-size:15px;}
.loadingSection .guideTxt {color:#767676;}

/* eventPagSwiper */
.eventPagSwiper {overflow:hidden; position:relative; margin:20px auto;}
.eventPageBox {position:relative; display:block; padding:10px 20px; background-color:#fff; background-image:url(../../img/bzw/common/bg/bg_event01.png); background-repeat:no-repeat; background-size:78px auto; background-position:94% 100%;}
.eventPageBox span {display:block; font-size:14px; color:#222;}
.eventPageBox strong {display:block; margin-bottom:20px; font-size:17px; font-weight:700;}
.eventPagSwiper .swiper-pagination {text-align:left;}
.eventPagSwiper .swiper-pagination-bullets {position:relative; display:inline-block; width:auto;}
.eventPagSwiper .swiper-pagination-bullets .swiper-pagination-bullet {margin:0 4px;}
.eventPagSwiper .swiper-pagination-bullets:after {content:''; position:absolute; right:0; width:1px; height:5px; background-color:#eee;}
.eventPagSwiper .swiper-pagination-bullets .swiper-pagination-bullet:last-child {margin-right:8px;}
.controlBtnWrap {position:absolute; left:20px; bottom:-10px; z-index:1;}
.eventPagSwiper [class^=btn] {position:relative; display:inline-block; top:50%; bottom:10px; width:12px; height:11px; cursor: pointer; transform:translateY(-50%);}
.eventPagSwiper .btnPlay {display:none;}
.eventPagSwiper .btnPlay:before,
.eventPagSwiper .btnStop:before {content:''; position:absolute; left:4px; bottom:0px;}
.eventPagSwiper .btnPlay:before {width:7px; height:9px; background:url(../../img/bzw/common/btn/btn_play.png) no-repeat center / 7px auto;}
.eventPagSwiper .btnStop:before {margin-bottom:1px; width:2px; height:7px; border-left:2.5px solid #767676; border-right:2.5px solid #767676;}
    

