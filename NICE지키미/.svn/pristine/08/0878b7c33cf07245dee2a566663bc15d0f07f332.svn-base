/*****************************************************************
  약관동의
*****************************************************************/
.agreeTitle label {position:relative; display:block; font-size:17px; line-height: 23px; font-weight: 700; padding:16px 20px 19px 52px; border:1px solid #c6cfdb; border-radius: 10px; box-sizing: border-box;}
.agreeTitle :checked + label {border-color:#066bfd;}
.agreeTitle :checked + label span:after {content:' 완료'}
.agreeTitle label:before {position:absolute; content:''; top:17px; left:19px; width:24px; height:24px; border:1px solid #d4d4d4; border-radius: 50%; box-sizing: border-box; -webkit-transition: 0.3s; transition:background-color, border-color 0.3s;}
.agreeTitle :checked + label:before {border-color:#066bfd; background-color:#066bfd}
.agreeTitle :checked + label:after {position:absolute; content:''; display:block; border-color:#fff; border-style:solid; border-width:0 0 2px 2px; width:10px; height:5px; left:25px; top:24px; transform: rotate(-45deg);}

.pweb .agreeTitle label {font-size:18px; line-height: 28px; padding:20px 24px 20px 62px;}
.pweb .agreeTitle input[type=checkbox]:focus + label {box-shadow:0px 0px 4px 2px rgba(0,97,255,0.3)}
.pweb .agreeTitle label:before {top:20px; left:24px; width:28px; height:28px;}
.pweb .agreeTitle :checked + label:after {width:11px; height:6px; top:29px; left:32px;}

.agreement .agreeSection {border-top:1px solid #eee; margin-top:20px;}
.agreement .uiAcco .uiAccoHeader {padding:0 35px 0 5px; margin-top:25px;}
.agreement .uiAcco .uiAccoHeader .uiBtn {position:absolute; top:-3px; right:0; width:30px; height:30px;}
.agreement .uiAcco .uiAccoHeader .uiBtn:after {top:4px;}
.agreement .uiAcco .uiAccoHeader .uiBtn.active:before {display:none;}
.agreement .uiAcco .uiAccoContent {padding:23px 2px 22px 23px; border-bottom:1px solid #eee;}

.pweb .agreement .agreeSection {margin-top:25px;}
.pweb .agreement .uiAcco .uiAccoHeader {margin-top:30px;}

.agreeItem li {position:relative; padding:7px 0;}
.agreeItem label {position:relative; display:inline-block;font-size:15px; line-height: 23px; padding:0 40px 0 28px;}
.agreeItem input[type=checkbox]:focus + label:after {box-shadow:0px 0px 4px 2px rgba(0,97,255,0.3)}
.agreeItem input[type=checkbox] + label:after {position:absolute; content:''; display:block; border-color:#bababa; border-style:solid; border-width:0 0 2px 2px; width:10px; height:5px; left:1px; top:7px; transform: rotate(-45deg);}
.agreeItem input[type=checkbox]:checked + label:after {border-color:#066bfd;}
.agreeItem li .uiBtn {position:absolute; width:24px; height:24px; top:7px; right:0;overflow:hidden;} 
.agreeItem li .uiBtn:after {position: absolute;left:7px; top:6px; content: '';width: 8px;height: 8px; border-top: 2px solid #bababa; border-right: 2px solid #bababa; transform: rotate(45deg);}

.pweb .agreeItem label {font-size:18px;}
.pweb .agreement .uiAcco .uiAccoContent {padding:30px 5px 28px 26px;}
.pweb .agreeItem li {padding:13px 0;}

.agreeList li {position:relative; margin-top:25px; padding:0 35px 0 5px;}
.agreeList li .uiBtn {position:absolute; width:24px; height:24px; top:0; right:0;overflow:hidden;}
.agreeList li .uiBtn:after {position: absolute;left:7px; top:6px; content: '';width: 8px;height: 8px; border-top: 2px solid #bababa; border-right: 2px solid #bababa; transform: rotate(45deg);}
.agreeList li .agr {font-size:15px; line-height: 24px;}

.pweb .agreeList li {margin-top:35px;}
.pweb .agreeList li .agr {font-size:18px; line-height: 28px;}

.agreement .agreeList .agreeSub {padding:17px 0 0 34px;}
.agreement .agrLink {display:inline-block; font-size:13px; line-height: 17px; color:#767676; border-bottom:1px solid #767676;}
.agreement .subSelBox p {line-height: 22px; margin-bottom:13px;}
.agreement .subSelBox .agreeSub {margin-bottom:24px;}
.agreement .subSelBox .agreeItem li {display:inline-block;}

.pweb .agreement .agrLink {font-size:15px;}
.pweb .agreement .subSelBox p {font-size:16px;}


/*****************************************************************
  List type
*****************************************************************/
.lineboxList li {position: relative; overflow:hidden; padding:17px 55px 17px 25px; font-size:17px; line-height: 24px; display: flex; flex-direction: column; justify-content: center; border: #8e8e8e solid 1px; border-radius: 10px;}
.lineboxList li:not(:first-child) {margin-top:15px;}
.lineboxList li > span,
.lineboxList li button span {display:block; font-size:13px; line-height: 19px; color:#767676; padding:3px 0;}
.lineboxList li strong {display:block; font-weight: 700; padding:3px 0;}
.lineboxList li a {position:absolute; top:0; left:0; right:0; bottom:0; }
.lineboxList li a.btnArrow.tp:before {right:24px; top:34px; margin:0;}
.lineboxList li a:focus {border:1px solid #000;}
.lineboxList li .del,
.lineboxList li .import {font-size:15px; color:#222; position:absolute; right:20px; line-height:20px; top:50%; margin-top:-11px;}
.lineboxList li a .del::before {content:'+'; display: inline-block; transform: rotate(45deg) translateY(15%); font-size: 1.5em;}
.lineboxList li .import::before {content:'+'; display: inline-block; margin-right: 5px;}
.lineboxList li button {margin:-17px -55px -17px -25px; padding:20px 55px 20px 25px; text-align:left; min-height:88px;}
.lineboxList li button:disabled {background:#eff2f8; color:#87909b;}
.lineboxList li button:disabled span {color:#87909b;}
.lineboxList li button.link {position:relative;}
.lineboxList li .link:after {content: ''; position: absolute; right: 20px; top: 50%; width: 8px; height: 8px; border-top: 2px solid #bababa; border-right: 2px solid #bababa; transform: rotate(45deg) translateY(-50%);}
.lineboxList li button.link:disabled:after {display:none;}
.lineboxList li .tagList {margin-bottom:7px;}

.pweb .lineboxList li {font-size:20px; padding: 32px 28px; line-height: 32px; border-radius: 20px;}
.pweb .lineboxList li:not(:first-child) {margin-top:20px;}
.pweb .lineboxList li > strong {padding:8px 0;}
.pweb .lineboxList li button {margin:-32px -28px; padding:28px 40px;}
.pweb .lineboxList li > span {font-size:15px; padding:8px 0;}
.pweb .lineboxList li button span {font-size:15px; }
.pweb .lineboxList li .date {margin-top:2px;}
.pweb .lineboxList li .link:after {right:28px; width:10px; height:10px;}
.pweb .lineboxList li .tagList {margin-bottom: 16px;}

.pweb .lineboxList li .listTit strong {float: left; margin-right:30px;}
.pweb .lineboxList li .listTit .tagList{ margin-top:12px;}

.rdoList li {position:relative; border-top:1px solid #eee; padding-top:18px; margin-top:18px;}
.rdoList .uiItem {display:block;}
.rdoList .uiItem label {display:block; font-weight: 500;font-size: 17px; line-height:24px;}
.rdoList .uiItem span {font-size: 12px; color:#767676; display: block; margin-top:4px; font-weight: 400;}
.rdoList .item {padding-left:18px;}
.rdoList .item strong {display:block; font-weight: 500;font-size: 17px; line-height:24px;}
.rdoList .item span {font-size: 12px; color:#767676; display: block; margin-top:4px; font-weight: 400;}
.rdoList .status {display:inline-block; font-size:11px; font-weight:400; margin-left:10px; color:#066bfd; vertical-align:top;}

.pweb .rdoList {padding-top:15px;}
.pweb .rdoList .uiItem label {display:block;}
.pweb .rdoList .uiItem strong {display:block;}
.pweb .rdoList .uiItem span {font-size:14px; line-height: 24px; margin-top:7px;}
.pweb .rdoList li {padding:25px 30px 0 30px; margin-top:25px;}
.pweb .rdoList .item strong {font-size:18px; line-height: 34px;}
.pweb .rdoList .item span {font-size:14px; line-height: 24px; margin-top:7px;}
.pweb .rdoList .status {font-size:13px;}

.chkList>li{margin-top:14px;}
.chkList .first{margin-top:0;}
.chkList .item{position:relative; display:block; padding:26px 20px; border:1px solid #d4d4d4; border-radius:15px; background:#fff;}
.chkList .item{color:#767676;}
.chkList .item:after{position:absolute; content:''; display:block; border-color:#d4d4d4; border-style:solid; border-width:0 0 2px 2px; width:10px; height:5px; right:20px; top:25px; transform: rotate(-45deg);}
.chkList .item .tit, .chkList .item strong{color:#222; font-size:14px; font-weight:bold;}
.chkList .item strong{display:block; margin-bottom:3px;}
.chkList .item dl{margin-top:12px;}
.chkList .item dl dd{color:#444;}
.chkList .item dl div{display:flex; justify-content:space-between; align-items:center; margin-top:8px;}

.chkList :checked + label{border-width:2px; border-color:#066bfd;}
.chkList :checked + label:after{border-color:#066bfd;}

.pweb .chkList>li{margin-top:30px;}
.pweb .chkList .item{padding:35px; border:1px solid #8e8e8e; border-radius:20px;}
.pweb .chkList .item:after{width:12px; height:7px; right:35px; top:35px;}
.pweb .chkList .item .tit{font-size:24px;}
.pweb .chkList .item strong{margin-bottom:10px; font-size:20px; font-weight:500;}
.pweb .chkList .item dl{margin-top:32px;}
.pweb .chkList .item dl dd{font-weight:500;}
.pweb .chkList .item dl div{margin-top:15px;}
.pweb .chkList .item span{font-size:15px;}
.pweb .chkList :checked + label{border-width:1px; border-color:#066bfd;}


