/* swiper pagination */
.swiper-pagination {position:static; margin-top:18px; height:5px;}
.swiper-pagination-bullet {vertical-align:top; width:5px; height:5px; margin:0 4px;}
.swiper-pagination-bullet[role='button']:focus{opacity:1; background:transparent;}
.swiper-pagination-bullet-active {width:15px; border-radius:2.5px}

.pweb .swiper-pagination {height:7px; margin-top:30px;} 
.pweb .swiper-pagination-bullet {width:7px; height:7px; border-radius: 4px;}
.pweb .swiper-pagination-bullet-active {width:21px;}

.topListInfo {margin-bottom:16px; display:flex; align-items:flex-end}
.topListInfo .h2 {margin-top:40px;}
.topListInfo .h3, .topListInfo .h4 {margin:0;}
.topListInfo > .cnt {font-size:13px; line-height: 16px; margin-left:auto;}
.topListInfo > .cnt i {font-weight: 700;}
.topListInfo .opt {margin-left:auto; color:#767676;}

.pweb .topListInfo {margin-bottom:24px;}
.pweb .topListInfo .cnt {font-size:16px; line-height: 26px;}

