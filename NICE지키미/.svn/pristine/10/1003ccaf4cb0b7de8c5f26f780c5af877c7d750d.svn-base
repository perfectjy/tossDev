.infoBox + .listDot {margin-top:16px;}
.conInfoWrap .listDot {margin-bottom:24px;}
.arrow {margin-bottom:20px;}
.arrow span {display:block; padding-left:16px;}
.arrow + .infoWrap {margin-top:0}
.infoWrap {margin-top:25px;}
.infoWrap > .listDot {margin-bottom:20px; font-weight:700;}
.infoWrap .tblList + .listDash {margin-top:20px;}
.infoWrap .btnConts {margin-top:15px;}

.selectBox {margin-bottom:36px}
.listInfo .date {display:block; margin-top:8px;}
.listInfo.t1 > .subTxt {display:block; margin-top:6px; color:#767676;}
.listStar strong {display:inline-block; font-weight:700; color:#444; font-family:'roboto';}
.btnConts.mg {margin:15px auto 30px;}

.graphWrap {margin-top:15px;}
.graphWrap .des {position:relative; font-size:14px;}
.graphWrap div.des {height:60px;} 
.graphWrap .des em {position:absolute; left:110px; top:10px; line-height:22px;}
.graphWrap .des em:before {content:':'; position:absolute; left:-10px; top:8px; width:3px; height:7px;}
.graphWrap .des > span {display:block; padding-left:17px;}
.graphWrap .des [class^=element] {position:absolute; left:63px} 
.graphWrap .des .elementTop {top:8px; left:56px;}
.graphWrap .des .elementBottom {bottom:8px; border-top:1px solid #2e3653;}
.graphWrap .des + .listDot {margin-top:20px;}
.graphWrap .des > strong {display:inline-block;  font-weight:700;}
.graphWrap div.des > strong {line-height:60px;}
.desWrap {margin-top:13px; margin-left:-20px; text-align:center;}
.desEl, .desEl .sign {position:relative;}
.desEl [class^=element] {position:absolute; margin-left:6px;}
.desEl .elementTop {top:8px;}
.desEl .elementBottom {bottom:8px; border-top:1px solid #2e3653;}
.desEl strong {display:inline-block; line-height:60px; font-weight:700;}
.desEl .txt{position:absolute; bottom:-18px; left:-16px}
.desEl + .pColor {margin-top:6px;}
.graphbox {width:100%; background-color:#fff}
.graphbox img {max-width:100%; max-height:100%;}

/* 맞춤대출 > 대출진단결과 (동의후)*/
.resultBox {position:relative; padding:35px 145px 35px 27px; min-height:130px; background-color:#eff2f8; border-radius:18px;}
.resultBox .tit {font-size:21px; display:inline-block; font-weight:700;}
.resultBox:before {content:''; position:absolute; top:53%; right:30px; width:89px; height:89px; background:url(../../img/bzw/common/bg/bg_result.png) no-repeat right center/ 100%; transform:translateY(-50%);}

.pweb .resultBox {padding:62px 375px 62px 40px;}
.pweb .resultBox .tit {font-size:32px;}
.pweb .resultBox:before {width:123px; height:123px; right:58px;}
.pweb .noList p:before {content:''; width:220px; height:161px; background-image:url(../../img/bzw/common/bg/bg_nolist.png); border:none; border-radius:0;}
.pweb .noList p {font-size:32px; color:#767676; line-height:42px; font-weight:500;}

.rollingImgbox {height:178px;}
.lineBlueBox {display:flex; padding:24px 25px; justify-content:space-between; align-items:center; border:1px solid #066bfd; border-radius:20px;}
.lineBlueBox .h3 {margin:0;}
.lineBlueBox .num {font-weight:700; font-size:21px;}
.lineBlueBox .num > em {font-size:32px; font-weight:700;}

.btnLink {margin-top:10px;}
.iconLinkWrap h3 {margin:8px 0 0;}
.modalContent .txtLink {text-align:center;}
.modalContent .txtLink a {display:block; text-decoration:underline; font-size:15px;}
.modalSubTxt {margin-bottom:9px; font-size:13px; color:#767676;}
.modalSubTxt span {color:#222; font-weight:700;}
