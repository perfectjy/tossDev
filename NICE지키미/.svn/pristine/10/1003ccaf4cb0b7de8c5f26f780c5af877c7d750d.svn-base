.listStar > li{position:relative; padding-left:12px; margin-top:10px; line-height:22px;}
.listStar > li:before {position:absolute; content:'*'; top:3px; left:0; width:4px; height:4px; color:#444; font-size:14px;}
.listDot em {margin-top:14px; display:block; font-weight:700; color:#444;}
.listDot strong > .roboto {font-weight:700;}
.listDot.mg .dash {margin-top:12px;}
.listNum.mg {margin-top:12px;}
.listNum.mg li{margin-top:0;} 

.pweb .inquireList .h4 + .contArea {min-height:313px; margin-top:-20px;}
.pweb .inquireList .standBy {min-height:313px;}
.inquireList .h4 +  .contArea {margin-top:-16px;}

.guideTxt.listDot:before {top:50%}
.pweb .guideTxt.listDot:before {top:18px;}
.pweb .detailTit {margin-bottom:46px;}
.pweb .faqWrap .keywordList {overflow: initial;}

.pweb .controlpage .swiper-pagination {margin-top:0; vertical-align:sub;}
.pweb .eventSwiper [class^=btn] {top:1px;}

