.sideLineArea .sideLineList {}
.sideLineArea .sideLineList > li {padding-left: 27px; position: relative;}
.sideLineArea .sideLineList > li+li {margin-top: 20px;}

.sideLineArea .sideLineList > li::before, .sideLineArea .sideLineList > li::after {content: ''; position: absolute; top: 8px; }
.sideLineArea .sideLineList > li::before {left: 5px; width: 1px; background-color: #c3d9ed; height: calc(100% + 20px); }
.sideLineArea .sideLineList > li:last-child::before { height: calc(100% - 8px); }
.sideLineArea .sideLineList > li::after {left:2px; width: 8px; height: 8px; background-color: #c3d9ed; border-radius: 50%;}
.sideLineArea .sideLineList > li.active::after {background-color: #fff; border: 3.5px solid #066bfb; width: 5px; height: 5px; left: 0;}


