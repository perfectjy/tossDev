/* nice����� start */
.labRtxt {margin: -30px 0 -10px 0; text-align: right; color: #767676;}
.labBox {display: flex; flex-direction: column; justify-content: space-between; margin-top: 20px; padding:30px; min-height: 380px; border-radius: 30px;}
.labBox.lab01 {background: url(../../img/bzw/sub/bg_labTest.png) no-repeat 70% 50% / auto 153px; background-color: #dbeeff;}
.labBox.lab02 {background: url(../../img/bzw/sub/bg_labComing.png) no-repeat 70% 50% / auto 153px;}
.labBox.disabled {background-color: #f2f3f5;}
.labBox .tit {font-size: 20px; font-weight: 700;}
.labBox .exp {margin-top: 10px; color: #767676; font-size: 14px;}

.myResult {margin-top:-20px;}
.myResult dt {color:#066bfd; font-size: 18px; font-weight: 700;}
.myResult dd {margin-top: 20px; color: #767676; font-size: 14px;}
.myResult .btnArrow strong {color:#066bfd;}

.grayArea {margin: 40px -20px -70px -20px; padding: 20px 20px 40px 20px;  background-color: #f0f1f3;}
.labResultBox {display: flex; flex-direction: column; justify-content: space-between; margin-top: 20px; padding:25px; min-height: 250px; background-color: #fff; border-radius: 30px;}
.labResultBox .labResultTop {display: flex; flex-direction: row; justify-content: space-between;}
.labResultBox h3 {font-size: 17px;}
.labResultBox .score {text-align: right;}
.labResultBox .score span {color: #767676; font-size: 13px;}
.labResultBox .score strong {color: #066bfd; font-size: 21px; font-weight: 700;}
.labResultBox .score em {color: #066bfd; font-size: 21px;}
.labResultBox .resultType {margin-top: 10px;}
.labResultBox .resultType .labResultImg {width: 100px; height: 100px; margin: 0 auto;}
.labResultBox .resultType .labResultImg img {max-width: 100%;}
.labResultBox .resultType p {margin-top: 15px; text-align:center; color: #066bfd; font-size: 21px; font-weight: 700;}
.labResultBox .labResultGraph ul {display: flex; flex-direction: row;}
.labResultBox .labResultGraph ul>li {flex: 1; text-align: center;}
.labResultBox .labResultGraph i {color: #b9c1cb;}
.labResultBox .labResultGraph em {color: #b9c1cb;}
.labResultBox .labResultGraph .bar {border-bottom: #e8e8e8 solid 1px; height: 140px; display: flex; flex-direction: column; justify-content: flex-end; align-items: center; margin-bottom: 10px;}
.labResultBox .labResultGraph .bar span {display: block; background-color: #eaeef1; width: 45px; border-radius: 7px 7px 0 0;}
.labResultBox .labResultGraph .primary i {color: #066bfd;}
.labResultBox .labResultGraph .primary em {color: #222;}
.labResultBox .labResultGraph .primary .bar span {background: #066bfd; background: linear-gradient(0deg, #066bfd 0%, #25a7fd 100%);}

.labResultBtn {margin-top: 20px; text-align: center;}
.labResultBtn button, .labResultBtn a {background-color: #fff;}
