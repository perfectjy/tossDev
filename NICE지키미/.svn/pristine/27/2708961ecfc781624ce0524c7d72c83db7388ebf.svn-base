/* partnerSwiper */ 
.partnerSwiper{overflow:hidden; position:relative; margin-bottom:45px;}
.mobile .partnerSwiper:after{content:''; position:absolute; top:0; z-index:2; display:block; width:40px; height:100%; right:0; background:linear-gradient(90deg, rgba(255,255,255,0.19931722689075626) 0%, rgba(255,255,255,1) 100%);}
.partnerSwiper .swiper-wrapper{display:flex; justify-content:left; gap:10px; align-items:end; font-size:0; margin:10px auto 0;}
.mobile .partnerSwiper .swiper-wrapper{gap:0;}
.partnerSwiper .swiper-wrapper>.swiper-slide{display:inline-block; width:auto; text-align:center;}
.partnerSwiper .swiper-wrapper>.swiper-slide:not(:last-child){margin-right:5px;}
.partnerSwiper .swiper-wrapper>.swiper-slide input{position:relative; margin:0;}
.partnerSwiper .swiper-wrapper>.swiper-slide input + label{display:inline-block; min-width:57px; height:42px; padding:0 14px; text-align:center; line-height:40px; color:#444; font-size:16px; font-weight:500; background:#f5f5f5; border-radius:21px;}
.partnerSwiper .swiper-wrapper>.swiper-slide input:checked + label{color:#fff; font-weight:700; background:#3587ff;}
.partnerSwiper .swiper-wrapper>.swiper-slide input + label:before, 
.partnerSwiper .swiper-wrapper>.swiper-slide input + label:after{display:none;}
.partnerSwiper .swiper-wrapper>.swiper-slide input:checked + label:before, 
.partnerSwiper .swiper-wrapper>.swiper-slide input:checked + label:after{display:none;}

