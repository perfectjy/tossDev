/* common color */
.pColor{color:#066bfd;}
.sColor{color:#d50f3c;}

/* munber font */
.roboto {font-family: 'Roboto'; font-weight: 100; letter-spacing: 0;}
.robotoB {font-family: 'Roboto'; font-weight: 200; letter-spacing: 0;}

/* swiper pagination */
.swiper-pagination {position:static; margin-top:18px; height:5px;}
.swiper-pagination-bullet {vertical-align:top; width:5px; height:5px;}
.swiper-pagination-bullet-active {width:15px; border-radius:2.5px}




/* 안내 및 완료가이드 */
.completeWrap {text-align: center;}
.completeWrap.cert:before {display:block; content:''; height:174px; background:url(../../img/bzw/common/bg/bg_lock_att.png) 50% 0 no-repeat; background-size:150px;}
.completeWrap.addInfo:before {display:block; content:''; height:174px; background:url(../../img/bzw/common/bg/bg_addinfo.png) 50% 0 no-repeat; background-size:150px;}
.completeWrap.autoLog:before {display:block; content:''; height:174px; background:url(../../img/bzw/common/bg/bg_autologout.png) 50% 0 no-repeat; background-size:150px;}
.completeWrap.logOut:before {display:block; content:''; height:174px; background:url(../../img/bzw/common/bg/bg_logout.png) 50% 0 no-repeat; background-size:150px;}
.completeWrap p {font-size:14px; line-height: 24px;}
.completeWrap p:not(:first-child) {margin-top:26px;}

/* 자동로그아웃 */
.autoLogInfo {text-align: center; font-size:14px; line-height: 22px;}
.autoLogInfo .timeBox .time {display:block; font-size:22px; line-height: 40px; font-weight: 700; padding:7px 0;}
.autoLogInfo .timeBox .time strong {font-size:30px;}
.autoLogInfo > strong {display:block; font-size: 19px; line-height: 27px; font-weight: 700; margin-top:40px;}

/*****************************************************************
  약관동의
*****************************************************************/
.agreeTitle label {position:relative; display:block; font-size:17px; line-height: 23px; font-weight: 700; padding:16px 20px 19px 52px; border:1px solid #c6cfdb; border-radius: 10px; box-sizing: border-box;}
.agreeTitle :checked + label {border-color:#066bfd;}
.agreeTitle :checked + label span:after {content:' 완료'}
.agreeTitle label:before {position:absolute; content:''; top:17px; left:19px; width:24px; height:24px; border:1px solid #d4d4d4; border-radius: 50%; box-sizing: border-box; -webkit-transition: 0.3s; transition: 0.3s;}
.agreeTitle :checked + label:before {border-color:#066bfd; background-color:#066bfd}
.agreeTitle :checked + label:after {position:absolute; content:''; display:block; border-color:#fff; border-style:solid; border-width:0 0 2px 2px; width:10px; height:5px; left:25px; top:24px; transform: rotate(-45deg);}

.agreement .agreeSection {border-top:1px solid #eee; margin-top:20px;}
.agreement .uiAcco .uiAccoHeader {padding:0 35px 0 5px; margin-top:25px;}
.agreement .uiAcco .uiAccoHeader .uiBtn {position:absolute; top:-3px; right:0; width:30px; height:30px;}
.agreement .uiAcco .uiAccoHeader .uiBtn:after {top:4px;}
.agreement .uiAcco .uiAccoHeader .uiBtn.active:before {display:none;}
.agreement .uiAcco .uiAccoContent {padding:23px 2px 22px 23px; border-bottom:1px solid #eee;}

.agreeItem li {position:relative; padding:7px 0;}
.agreeItem label {position:relative; display:inline-block;font-size:15px; line-height: 23px; padding:0 40px 0 28px;}
.agreeItem input[type=checkbox] + label:after {position:absolute; content:''; display:block; border-color:#bababa; border-style:solid; border-width:0 0 2px 2px; width:10px; height:5px; left:1px; top:7px; transform: rotate(-45deg);}
.agreeItem input[type=checkbox]:checked + label:after {border-color:#066bfd;}
.agreeItem li .uiBtn {position:absolute; width:24px; height:24px; top:7px; right:0;overflow:hidden;} 
.agreeItem li .uiBtn:after {position: absolute;left:7px; top:6px; content: '';width: 8px;height: 8px; border-top: 2px solid #bababa; border-right: 2px solid #bababa; transform: rotate(45deg);}

.agreeList li {position:relative; margin-top:25px; padding:0 35px 0 5px;}
.agreeList li .uiBtn {position:absolute; width:24px; height:24px; top:0; right:0;overflow:hidden;}
.agreeList li .uiBtn:after {position: absolute;left:7px; top:6px; content: '';width: 8px;height: 8px; border-top: 2px solid #bababa; border-right: 2px solid #bababa; transform: rotate(45deg);}

.agreement .agreeList .agreeSub {padding:17px 0 0 34px;}
.agreement .agrLink {display:inline-block; font-size:13px; line-height: 17px; color:#767676; border-bottom:1px solid #767676;}
.agreement .subSelBox p {line-height: 22px; margin-bottom:13px;}
.agreement .subSelBox .agreeSub {margin-bottom:24px;}
.agreement .subSelBox .agreeItem li {display:inline-block;}

/*****************************************************************
  List type
*****************************************************************/
.lineboxList li {position: relative; overflow:hidden; padding:20px 55px 20px 25px; font-size:17px; line-height: 24px; display: flex; flex-direction: column; justify-content: center; border: #d9d9d9 solid 1px; border-radius: 10px;}
.lineboxList li:not(:first-child) {margin-top:15px;}
.lineboxList li > span {display:block; font-size:13px; line-height: 19px; color:#767676; margin-top:5px;}
.lineboxList li strong {font-weight: 700;}
.lineboxList li a {position:absolute; top:0; left:0; right:0; bottom:0; }
.lineboxList li .del,
.lineboxList li .import {font-size:15px; color:#222; position:absolute; right:20px; line-height:20px; top:50%; margin-top:-11px;}
.lineboxList li a .del::before {content:'+'; display: inline-block; transform: rotate(45deg) translateY(15%); font-size: 1.5em;}
.lineboxList li .import::before {content:'+'; display: inline-block; margin-right: 5px;}
.lineboxList li button {margin:-20px -55px -20px -25px; padding:20px 55px 20px 25px; text-align:left; min-height:88px;}
.lineboxList li button:disabled {background:#eff2f8; color:#87909b;}
.lineboxList li button:disabled span {color:#87909b;}
.lineboxList li button.link {position:relative;}
.lineboxList li .link:after {content: ''; position: absolute; right: 20px; top: 50%; width: 8px; height: 8px; border-top: 2px solid #bababa; border-right: 2px solid #bababa; transform: rotate(45deg) translateY(-50%);}
.lineboxList li button.link:disabled:after {display:none;}
.lineboxList li .tagList {margin-bottom:10px;}

.rdoList li {position:relative;}
.rdoList li:not(:first-child) {margin-top:15px;}
.rdoList label {display:block; position:relative; border: #d9d9d9 solid 1px; border-radius: 10px; background-color: #fff; text-align: left; padding: 20px 20px 19px 42px;}
.rdoList label strong {display:block; font-weight: 500;font-size: 17px; line-height:24px;}
.rdoList label span {font-size: 12px; color:#767676; display: block; margin-top:4px;}
.rdoList label:before {position:absolute; content:''; top:20px; left:9px; width:24px; height:24px; background:#fff; border-radius: 50%; box-sizing: border-box;-webkit-transition: 0.3s; transition: 0.3s;}
.rdoList label:after {position: absolute; content: ''; display: block; border-color: #b9b9b9; border-style: solid; border-width: 0 0 2px 2px; width: 10px; height: 5px; left: 15px; top: 27px; transform: rotate(-45deg);}
.rdoList :checked + label strong {font-weight: 700;}
.rdoList :checked + label, .rdoList :checked + label:after {border-color: #066bfd;}
.rdoList .mng label {background-color: #eff2f8; border-color:#dcdff1;}
.rdoList .mng .status {width: 21px; height: 16px; position: absolute; top: 10px; right: 10px; background: url(../../img/bzw/common/ico/ico_connected.png) no-repeat center; background-size: 100% auto;}



