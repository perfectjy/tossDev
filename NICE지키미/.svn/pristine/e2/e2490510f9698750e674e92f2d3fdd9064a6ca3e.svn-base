/* 공통 고려사항 */
.rdoGroup.petType1, .rdoGroup.petType2 {margin-top: 10px;}


.petInfoItem .date {display:flex; justify-content: space-between; align-items: center; gap: 11px;}
.petInfoItem .date>div {width:100%;}



