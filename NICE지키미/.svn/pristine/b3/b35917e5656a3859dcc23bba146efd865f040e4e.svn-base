                        <dd>300,000원</dd>
                        <dt>납입방법</dt>
                        <dd>신용카드</dd>
                    </dl>
                </div>

                <!-- 내역이 없을때 -->
                <div class="noList">
                    <p>보험 납입내역이 없네요.</p>
                </div>
                
                <div class="btnConts">
                    <button type="button" class="btn primary"><span>확인</span></button>
                </div>
                
                <!--// content -->
                </div>
            </div>
            </div>
        </div>
        <!--// popup -->

        <!-- footer -->
        <script type="text/javascript" src="../../../bzw/inc/footer.js"></script>
