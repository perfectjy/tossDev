.pagenation li span::before {content: ''; position: absolute; left: 0; top: 50%; transform: translateY(-50%); width: 24px; height: 24px; background: url(../../img/bzw/common/ico/ico_acco_arr.png) no-repeat center; background-size: cover; }
.pagenation li.prev span::before {transform: translateY(-50%) rotate(180deg); }
