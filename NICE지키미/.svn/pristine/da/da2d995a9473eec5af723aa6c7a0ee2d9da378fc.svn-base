/*****************************************************************
  소비자보호
*****************************************************************/
.overdueWrap .governing{margin-bottom:25px;}

.consuInfo {position:relative; height:279px; margin-bottom:28px;}
.consuInfo img, .requestSwiper img {display:block; height:279px; margin:0 auto;}
.consuInfo .bubbleTip{min-width:125px; bottom:20px; right:calc(50% - 135px); text-align:left;}
.requestSwiper .consuInfo {margin:0;}

/* requestSwiper */
.requestSwiper{overflow:hidden; position:relative; margin-bottom:15px;}

/* diagram - 정보수집흐름도, 조직도 */
.flowlist {display:flex; margin-bottom:10px; justify-content:space-between; flex-wrap:wrap;}
.flowlist li {position:relative; width:calc(50% - 10px); margin-bottom:20px; padding:16px 15px;  background-color:#f5f8ff; border-radius:12px; box-sizing:border-box;}
.flowlist li:after{content:''; position:absolute; left:50%; bottom:-7px; display:block; width:0; border-top:12px solid #cad5ee; border-right:10px solid transparent; border-left:10px solid transparent; transform:translateX(-50%);}
.flowlist li.up:after {top:-6px; border-bottom:12px solid #cad5ee; border-top:0; bottom:inherit;}
.flowlist li .title {display:block; font-size:13px; line-height:21px; font-weight:700; color:#066bfd;}
.flowlist li .txt {margin-top:7px; font-size:12px; line-height: 16px; color:#767676;}
.flowlist li.full {width:100%; padding:0;}
.flowlist li.full::after {display:none;}
.flowlist li .datab {position:relative; height:150px; border-radius:15px; background:linear-gradient(to bottom, #066bfd, #003abb); box-shadow:2px 2px 10px 4px rgb(53 135 255 / 30%);}
.flowlist li .datab:before {content:''; position:absolute; top:15px; left:50%; width:229px; height:94px;transform: translateX(-50%); background:url(../../img/bzw/common/bg/bg_nice.png) no-repeat; background-size:100%;}
.flowlist li .datab > span {position:absolute; left:50%; bottom:13px; display:block; text-align:center; color:#fff; font-weight:700; font-size:13px; transform:translateX(-50%);}
.flowlist li.center {display:block; margin:0 auto 20px auto;}

.flowOrg li {position:relative; font-size:14px; background: #f5f8ff; border-radius: 15px; padding:16px 15px; margin-bottom:20px;}
.flowOrg li:not(:last-child):after {content:""; position: absolute; left:20px; bottom:-5px; width: 0; height: 0; border-left: 10px solid transparent; border-right: 10px solid transparent; border-top: 15px solid #cad5ee; }
.flowOrg li strong {font-weight: 700; color:#066bfd; line-height:22px;}
.flowOrg li .en {display:block; margin-top:8px; font-size:12px; line-height: 16px; color:#767676; font-family: "Rototo"; font-weight: 100;}
.txtBlock {margin:-10px 0 10px 0;}
.txtBlock strong {font-weight: 700;}

/* 수신설정 */
.receiveChkList b{display:block; padding:0 0 20px 10px; font-weight:700;}
.receiveChkList>li{position:relative; margin-bottom:40px;}
.receiveChkList>li:before{position:absolute; content:''; top:9px; left:0; width:4px; height:4px; background-color:#222; border-radius:100%;}
.receiveChkList>li .receiveChkBox{margin-bottom:15px;}
.receiveChkBox{display:flex; align-items:center; justify-content:space-between; padding:24px; margin-bottom:50px; background:#f9f9f9; border:1px solid #d4d4d4; border-radius:15px;}
.receiveChkBox p{font-size:15px; font-weight:500; line-height:23px;}

/* 말풍선 */
.bubbleTip{position:absolute; width:auto; z-index:1; padding:13px; color:#fff; font-size:13px; line-height:18px; background:rgb(0,57,186); background: linear-gradient(0deg, rgba(0,57,186,1) 0%, rgba(6,107,253,1) 100%); border-radius:15px; box-shadow:2px 2px 10px 4px rgb(53 135 255 / 30%);}
.bubbleTip:before{content:''; position:absolute; top:-7px; left:25px; display:inline-block; width:17px; height:17px; background:#0669fa; border-top-left-radius:4px; transform:rotate(45deg);}

/* 연체정보 해제이력 조회 */
.overdueWrap .rdoGroup.full{margin:0 0 45px;}
.overdueWrap .infoBox{font-size:15px;}

/* 제휴사 신용조회 */
.bankList{display:flex; margin-bottom:35px;}
.bankList>li{display:inline-flex; flex-direction:column; align-items:center; flex:1; text-align:center;}
.bankList>li i{display:inline-block; width:38px; height:38px;}
.bankList>li i img{max-width:100%; max-height:100%;}
.bankList>li span{display:inline-block; margin-top:10px; line-height:15px;}

/* 제휴사별 신용조회내역 */
.partnerAccoList .listInfo{margin:40px 0 15px 0;}
.infoAss .assTitle{display:flex; align-items:center; padding:14px 36px 14px 5px; border-top:1px solid #eee}
.infoAss .assTitle i{display:inline-block; width:38px; height:38px; margin-right:10px;}
.infoAss .assTitle i img{max-width:100%; max-height:100%;}
.infoAss .acInText li:not(:first-child){margin-top:10px;}
.infoAss .acInText li em{color:#222; font-weight:700;}

/*****************************************************************
  쓰다
