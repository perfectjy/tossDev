.sortInfo .c2:before{background:#975bc5 url(../../img/bzw/common/bg/bg_info_pattern02.png) center no-repeat; background-size:7px 13px;}
.sortInfo .c3:before{background:#4b9ef7 url(../../img/bzw/common/bg/bg_info_pattern03.png) center repeat-x; background-size:7px;}
.sortInfo .c4:before{background:#18987b url(../../img/bzw/common/bg/bg_info_pattern04.png) center no-repeat; background-size:9px;}

.sortDetail{position:relative; display:flex; align-items:flex-end; height:175px; margin:20px 45px 0 0; background:url(../../img/bzw/common/bg/bg_graph01.png) repeat-x; background-size:1px 175px;}
.sortDetail .sortBar{width:100%; height:100%; display:flex; justify-content:space-between;}
.sortDetail .sortBar>li{position:relative; margin:0 auto; text-align:center;}
.sortDetail .sortBar>li .mth{position:relative; bottom:-180px; display:inline-block; color:#767676;}
.sortDetail .sortBar .bar{height:100%; display:flex; justify-content:left; align-items:flex-end; margin-top:-21px;}
.sortDetail .sortBar .bar p{position:relative; width:9px; height:auto; margin-right:7px; bottom:0; border-top-left-radius:3px; border-top-right-radius:3px;}
.sortDetail .sortBar .bar p:last-child{margin-right:0;}
.sortDetail .sortBar .bar span{position:absolute; left:-20px; top:-20px; min-width:60px; font-size:13px;}
.sortDetail .sortBar .bar span i{padding-right:1px; letter-spacing:-1px;}
.sortDetail .sortBar .bar .c1{background:#ee2e63 url(../../img/bzw/common/bg/bg_pattern01.png) repeat; background-size:4px;}
.sortDetail .sortBar .bar .c2{background:#975bc5 url(../../img/bzw/common/bg/bg_pattern02.png) repeat; background-size:6px 9px;}
.sortDetail .sortBar .bar .c3{background:#4b9ef7 url(../../img/bzw/common/bg/bg_pattern03.png) repeat; background-size:9px;}
.sortDetail .sortBar .bar .c4{background:#18987b url(../../img/bzw/common/bg/bg_pattern04.png) repeat; background-size:9px 4px;}

.sortDetail .perText{display:flex; justify-content:space-between; flex-direction:column; height:100%; position:absolute; right:-45px; bottom:0;}
.sortDetail .perText>li{margin:-5px 0 0; color:#767676; font-size:13px; line-height:1;}

.consume .fx{flex:none;}
.consume .fx .exp{margin-top:0;}
.consume .fx .tit{display:block; margin-top:18px; color:#767676; font-weight:700;}
.consume .fx .tit span{line-height:1; color:#222;}
.consume .fx .tit i{vertical-align:bottom; line-height:1; font-size:21px; letter-spacing:-1px;}
.consume .fRow.line{margin-top:0;}

/* 소비현황 */
.flagList{display:flex;}
