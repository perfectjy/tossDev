.emjTxt.up:after{background-image:url(../../img/bzw/sub/ico_emo11.png);}
.emjTxt.down:after{background-image:url(../../img/bzw/sub/ico_emo11.png);}
.emjTxt.equal:after{background-image:url(../../img/bzw/sub/ico_emo11.png);}

.ineBox .inList>li{margin-top:15px;}
