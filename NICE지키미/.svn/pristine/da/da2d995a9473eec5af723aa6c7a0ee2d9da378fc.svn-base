/* 서브메인 */
.useMain{padding:0 20px 30px 20px; margin:0 -20px 50px -20px; background:rgb(239,242,248); background:linear-gradient(360deg, rgba(239,242,248,1) 0%, rgba(255,255,255,1) 100%);}

a.governingTitle{display:inline-block;}
a.governingTitle:after{content:''; display:inline-block; width:8px; height:8px; margin:-3px 0 0 7px; vertical-align:middle; border-right:2px solid #767676; border-top:2px solid #767676; transform:rotate(45deg);}

.governingAmount .tagList{margin-top:35px;}
.governingAmount .tit{margin-top:15px; line-height:1;}
.governingAmount .tit .btn{margin-top:-5px;}
.governingAmount .tit strong{padding-right:3px; vertical-align:middle; letter-spacing:-1.2px;}
.governingAmount .exp{margin-top:5px;}
.dRight{font-weight:700;}

.userAmount .governing{display:flex; justify-content:space-between; align-items:center; margin-bottom:0; text-align:left;}
.userAmount .governing.t1{align-items:flex-start;}
.userAmount .btnConts{margin-top:20px;}

.consume .ineBox{overflow:visible; padding: 22px 25px; border:0; box-shadow: 1px 4px 7px 0px rgb(204 204 204 / 40%);}
.consume .ineBox .emo{padding-top:12px;}

.infoPay{position:relative; margin-top:25px; padding:24px 15px 15px; border:1px solid #d4d4d4; border-radius:15px;}
.infoPay .tagList{position:absolute; top:24px; left:24px; margin-top:0;}
.infoPay .bgBoxList{margin-top:10px;}
.infoPay .infoToggle.pay{margin-top:0;}
.infoPay .infoTogConts{margin-left:10px;}
.infoPay .infoTogConts .payItem:first-child{margin-top:0;}
.infoPay .payBox{padding:30px 10px 0 10px;}
.infoPay .payItem{display:flex; align-items:center; justify-content:left; padding:0; margin-top:20px; border:0; border-radius:0;}
.infoPay .payItem:not(:first-of-type){margin-top:20px;}
.infoPay .payItem .payCont{padding-left:15px;}
.infoPay .payItem .payCont strong{font-size:17px; font-weight:400;}
.infoPay .payItem .payCont strong em{font-weight:700;}
.infoPay .payItem .payCont p em{padding-left:5px;}

.infoCard{margin-top:25px;}
.infoCard .lineList{display:flex; flex-wrap:wrap; justify-content:space-between; margin-top:4px;}
.infoCard .lineList>li{box-sizing:border-box; display:inline-flex; align-items:center; width:calc(50% - 5px); padding:12px 14px; margin:10px 10px 0 0; border:1px solid #d4d4d4; border-radius:10px;}
.infoCard .lineList>li i{display:inline-block; width:38px; height:38px; margin-right:10px;}
.infoCard .lineList>li i img{max-width:100%; max-height:100%;}
.infoCard .lineList>li:nth-child(2n){margin-right:0;}
.infoCard .noList{margin-bottom:45px;}

.payItem{display:flex; align-items:center; justify-content:left; padding:20px 24px; border:1px solid #d4d4d4; border-radius:15px;}
.payItem i{display:inline-block; width:38px; height:38px;}
.payItem i img{max-width:100%; max-height:100%;}
.payItem .payCont{padding-left:10px;}
.payItem .payCont p{line-height:22px; font-size:14px; font-weight:400;}
.payItem .payCont .date{color:#767676; font-size:13px;}
.payItem .payCont .date span{padding-left:3px;}
.payItem .payCont b, .payItem .payCont strong{display:block; font-weight:700;}
.payItem .inItem{display:flex; width:100%;}
.payItem .ex{margin-top:15px; color:#767676; font-size:12px; line-height:20px;}


/* toggle */
.infoToggle.pay{margin-top:50px;}
.infoToggle.pay .infoTogConts{margin-top:20px;}
.infoToggle.pay .tit{width:100%; text-align:right;}
.infoToggle.pay .tit .infoTogBtn{position:relative; margin-right:38px; padding-right:3px; color:#444;}
.infoToggle.pay .tit .infoTogBtn:after, .infoToggle.pay .governingTitle .infoTogBtn:after{content: ''; position:absolute; right:-24px; top:-1px; width: 24px; height: 24px; background:url(../../img/bzw/common/ico/ico_acco_arr.png) no-repeat; background-size: 100%; transform: rotate(0);}
.infoToggle.pay .governingTitle .infoTogBtn{position:relative;}
.infoToggle.pay .governingTitle .infoTogBtn:after{top:auto; right:-30px; bottom:3px;}
.infoToggle.pay .infoTogBtn.active:after{transform:rotate(180deg);}
.infoToggle.pay .listWrap, .infoToggle.pay .payItem:first-child{margin-top:0;}
.infoToggle.pay .payItem{margin-top:15px;}

.payBox{width:100%; display:flex; justify-content:space-between; align-items:center; padding-top:15px;}
.payBox .sum{font-size:17px; font-weight:400; text-align:right;}
.payBox .sum .btn{margin-top:-3px;}
.payBox .sum strong{padding-right:2px; font-size:23px; font-weight:700; vertical-align:top; letter-spacing:-1px;}

.cprItem{margin-top:15px; text-align:right; color:#767676; font-size:12px;}
.cprItem span{padding-right:2px; letter-spacing:-1px; font-size:13px;}

.bgBoxList{display:flex; justify-content:space-between; flex-wrap:wrap;}
.bgBoxList>li{box-sizing:border-box; width:calc(50% - 5px); margin:10px 10px 0 0; padding:25px 15px; background:#f8f9fb; border-radius:10px;}
.bgBoxList>li:nth-child(2n){margin-right:0;}
.bgBoxList>li .loan dt{color:#767676; line-height:22px;}
.bgBoxList>li .loan dd{margin-top:15px; text-align:right; font-size:16px; line-height:1;}
.bgBoxList>li .loan dd strong{padding-right:3px; vertical-align:text-bottom; letter-spacing:-1px; font-size:21px; font-weight:700;}

.linkArea .bgBoxLink, .bnrBox{margin-top:20px;}
.linkArea .bgBoxLink{padding:20px;}
.linkArea .bgBoxLink:first-child{margin-bottom:0;}
.linkArea .bgBoxLink .link{position:absolute; top:0; left:0; right:0; bottom:0; margin:0;}
.linkArea .bgBoxLink .link:after{content: ''; position: absolute; right: 20px; top: 50%; width: 8px; height: 8px; border-top: 2px solid #767676; border-right: 2px solid #767676; transform: rotate(45deg) translateY(-50%);}
.linkArea .bnrBox.gat5 p{margin-top:5px; color:#222; font-size:13px;}
.linkArea .bnrBox.gat5 strong{display:block; font-size:16px; font-weight:700;}

/* 카드발급 가능지수 */
.userCredit{margin-top:40px;}
.userCredit .idxInfo:after{content:''; position:absolute; top:0; right:12px; display:block; width:95px; height:95px; background:url(../../img/bzw/common/bg/bg_wallet.png) 50% no-repeat; background-size:95px;}
.idxInfo{position:relative;}
.idxInfo h3{font-size:14px;}
.idxInfo .txt{display:flex; align-items:center; color:#767676; line-height:22px;}
.idxInfo .cnt{display:block; width:20px; height:20px; margin-left:5px; text-align:center; line-height:20px; color:#222; background:#eff2f8; border-radius:50%;}
.idxInfo strong{display:block; margin-top:15px; line-height:1; font-size:21px;}
.idxInfo strong span{padding-right:5px; vertical-align:middle; letter-spacing:-1.2px; font-size:30px; font-weight:700;}
.idxInfo .tooltipWrap{margin:-3px 0 0 7px;}

/* 어디에 많이 썼을까요 */
.userCons .h2:first-child{margin-top:0;}
.userCons dl[class*='Item']{display:flex; align-items:center;}
.userCons dl[class*='Item'] strong{vertical-align:middle; letter-spacing:-1px;}
.userCons .rateItem dt{width:40px; height:40px; margin-right:15px; background-size:40px; background-position:50%; background-repeat:no-repeat;}
.userCons .rateItem dd{line-height:1; font-size:21px;}
.userCons .rateItem strong{padding-right:5px; font-size:30px;}
.userCons .moneyItem strong{padding-right:2px;}

/* 내 소비습관 카드추천 */
.recomCard{width:100%; min-height:150px; margin-top:30px; background:#eee;}

.lineBox{position:relative; padding:26px 23px; border:1px solid #d4d4d4; border-radius:15px;}
.lineBox a{position:absolute; left:0; top:0; bottom:0; right:0;}
.lineBox .tit{position:relative; margin-bottom:18px; font-size:17px; font-weight:700; line-height:25px;}
.lineBox .tit:after{content:''; display:inline-block; width:8px; height:8px; margin:-3px 0 0 7px; vertical-align:middle; border-right:2px solid #767676; border-top:2px solid #767676; transform:rotate(45deg);}
.lineBox .cont{display:flex; align-items:center; justify-content:space-between;}
.lineBox.ct1 .rateItem dt{background-image:url(../../img/bzw/common/bg/bg_eatout.png);} /* 외식 */

/* 지난 3개월간 카드생활 */
.myCardLife .link{display:inline-block; margin-top:15px; font-size:15px; line-height:23px;}
.myCardLife .link strong{font-weight:700;}
.myCardLife .link:after{content:''; display:inline-block; width:6px; height:6px; margin:-3px 0 0 9px; vertical-align:middle; border-right:2px solid #767676; border-top:2px solid #767676; transform:rotate(45deg);}
.myCardLife .infoToggle.detail:before{content:''; position:absolute; left:0; top:20px; display:block; width:100%; height:1px; background-color:#eee;}
.infoToggle.detail{position:relative; margin-top:50px;}
.infoToggle.detail .tit{position:relative; display:flex; justify-content:center;}
.infoToggle.detail .infoTogBtn{width:45px; height:45px; border:1px solid #dcdff1; border-radius:23px; background-color:#fff;}
.infoToggle.detail .infoTogBtn:before{position:absolute; top:50%; left:50%; display:block; content:''; width:10px; height:10px; margin-top:-2px; border-top:2px solid #222; border-right:2px solid #222; transform:translate(-50%, -50%) rotate(135deg);}
.infoToggle.detail .infoTogBtn.active:before{margin-top:2px; transform:translate(-50%, -50%) rotate(315deg);}
.matchWrap{position:relative; margin-top:30px;}
.matchWrap .tit{position:absolute; top:30px; left:50%; display:flex; align-items:center; justify-content:center; width:65px; height:65px; color:#066bfd; background:#eef3f7; border-radius:50%; transform:translateX(-50%);}
.matchBox{display:flex; align-items:center; justify-content:space-between;}
.matchBox dl:last-child dt{text-align:right;}
.matchBox dl dd{margin-top:15px; line-height:1; font-size:21px;}
.matchBox dl strong{padding-right:5px; vertical-align:middle; letter-spacing:-1.2px; font-size:30px;}
.matchBox dl span:not(.tag){display:block; margin-top:3px; font-size:14px;}
.matchBox dl span strong{padding-right:3px; vertical-align:top; font-size:17px;}
.matchList{margin-top:45px;}
.matchList>li{position:relative;}
.matchList>li:not(:first-child){margin-top:70px;}
.matchList em{box-sizing:border-box; position:absolute; left:50%; top:-5px; z-index:1; display:inline-block; height:30px; padding:0 15px; line-height:28px; color:#767676; font-size:14px; background:#fff; border:1px solid #d4d4d4; border-radius:15px; transform:translateX(-50%);}
.matchList .matchBar .bar{display:flex; width:100%; height:18px;}
.matchList .matchBar .tip{position:absolute; top:-45px; width:46px; height:20px; padding-top:3px; line-height:1; text-align:center; color:#fff; font-size:14px; background:#2e3653; border-radius:5px;}
.matchList .matchBar .tip:after{content:''; position:absolute; left:50%; display:block; width:5px; height:5px; background:#2e3653; transform:translateX(-50%) rotate(45deg);}
.matchBar .bar span:first-child{border-right:2px solid #fff;}
.matchBar .bar .cardA, .matchBar .bar .cardB{display:inline-block;}
.matchBar .bar .cardA{background:rgb(74,175,255); background:linear-gradient(90deg, rgba(74,175,255,1) 0%, rgba(53,118,255,1) 100%); border-top-left-radius:4px; border-bottom-left-radius:4px;}
.matchBar .bar .cardB{background:rgb(255,120,113); background:linear-gradient(90deg, rgba(255,120,113,1) 0%, rgba(255,188,183,1) 100%); border-top-right-radius:4px; border-bottom-right-radius:4px;}
.matchBar .cardContA, .matchBar .cardContB{position:absolute;}
.matchBar .cardContA span, .matchBar .cardContB span{display:block; margin-top:9px; color:#767676; font-size:14px; line-height:1;}
.matchBar .cardContA, .matchBar .cardContA .tip{left:0;}
.matchBar .cardContA span{text-align:left;}
.matchBar .cardContB, .matchBar .cardContB .tip{right:0;}
.matchBar .cardContB span{text-align:right;}

.sortInfo{display:flex; justify-content:left; align-items:center;}
.sortInfo>li{color:#767676;}
.sortInfo>li:not(:first-child){padding-left:12px;}
.sortInfo>li:before{content:''; display:inline-block; width:13px; height:13px; margin-right:5px; border-radius:3px;}
