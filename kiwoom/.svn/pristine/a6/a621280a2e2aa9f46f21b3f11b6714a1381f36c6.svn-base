.tbl-info.flex {display:flex; align-items:center; justify-content: space-between;}
.tbl-info.mo.flex {margin:40px 0 0;}
.tbl-info.mo .info .total {font-size:14px; line-height:21px;}


.sub-head-wrap .find-fund .sch-top { position:relative; }
.sub-head-wrap .find-fund .inp-text { height:48px; border:1px solid #1A399E; border-radius:100px; }
.sub-head-wrap .find-fund .btn-sch { position:absolute; top:50%; right:15px; width:24px; height:24px; margin-top:-12px; background:url(/kosef/moWeb/resources/images/common/ico_comm.png) no-repeat -30px -20px; background-size:500px auto; }

.sub-conts-wrap {position: relative; padding:0 24px 0 24px;}

.sch-list .list-item + .list-item {margin-top: 16px;}
.sch-list .list-item .imgs {position:relative; background:#d7d7d7;}
.sch-list .list-item.v3 {padding:0; border-bottom:0;}
.sch-list .list-item.v3 .list-cont {display:block; margin:24px 24px 0; text-align:left;}
.sch-list .list-item.v3 .tit {overflow:hidden; display: -webkit-box; -webkit-box-orient: vertical; -webkit-line-clamp: 2; text-overflow:ellipsis; word-break: break-word; margin-bottom:6px; font-size:14px; line-height:21px; color:#222;}
.sch-list .list-item.v3 .stxt {display:block; margin-bottom:6px; font-size:14px; color:#73737A;}
.sch-list .list-item.v3 .stxt.date {font-size:12px;}
.sch-list .list-item.v3 > a {display:block; width:100%; min-height:317px; padding:0; border:1px solid #DFE0E2; border-radius:8px 8px 8px 8px;}
.sch-list .list-item.v3 > a:hover,
.sch-list .list-item.v3 > a:focus {border:1px solid #1A399E; box-shadow:0 3px 8px rgba(26, 57, 158, 0.16); border-radius:8px 8px 8px 8px;}
.sch-list .list-item.v3 > a:hover .tit{color:#1A399E;}
.sch-list .list-item.v3 .imgs{display:block; width:100%; height:180px; border-radius:8px 8px 0 0;}

.sub-conts-wrap { margin:25px auto 0 auto; padding:0 24px; }
.sub-conts-wrap .sch-box .sch-top {position:relative;}
/* .sub-conts-wrap .sch-top { position:relative; margin:0 auto; padding:0 24px; height:48px; text-align:left; border-radius:100px; border:2px solid #113585; box-shadow:0px 2px 4px rgba(57, 53, 210, 0.2); } */
.sub-conts-wrap .sch-box .inp-txt { height:48px; border:1px solid #1A399E; border-radius:100px; }
.sub-conts-wrap .sch-box .btn-sch { position:absolute; top:50%; right:15px; width:24px; height:24px; margin-top:-12px; background:url(/kosef/moWeb/resources/images/common/ico_comm.png) no-repeat -30px -20px; background-size:500px auto; }


.board-conts .board-head {position:relative}
.board-conts .btn-sns {position: absolute; bottom:0; right: 0;}
.board-head .board-tit {margin-bottom:4px; font-size:20px; line-height:30px; color:#222; font-weight:700; text-align:left;}
.board-sub-tit {display:inline-block; font-size:0;}
.board-sub-tit .board-date,
.board-sub-tit .writer {display:inline-block; font-size:12px; line-height:18px; color:#9D9DA5; text-align:left;}
.board-cont .imgs {display:block; width:100%; height:180px; background: #d7d7d7;}
.board-cont {padding:20px 0;}
.board-prev-next > dl {display:flex; align-items:center;  border-top:1px solid #d7d7d7; color: #222; }
.board-prev-next > dl dt {min-width:48px; line-height:50px;}
.board-prev-next > dl dd {overflow:hidden; display:-webkit-box; -webkit-box-orient: vertical; -webkit-line-clamp:1; text-overflow:ellipsis; word-break: break-word; }
.board-prev-next > dl dd a {display:block; font-size:14px; line-height:50px; color:#222;}
.board-prev-next > dl dd a:hover {text-decoration:underline;}
.board-prev-next > dl:last-child {border-bottom:1px solid #d7d7d7;}
