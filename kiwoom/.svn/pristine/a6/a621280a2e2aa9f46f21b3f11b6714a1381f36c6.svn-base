/* 공통 */
.sub-head-wrap { position:relative; padding:16px 24px 0 24px; text-align:center; }
.sub-head-wrap h2 { color:#fff; font-size:20px; line-height:30px; font-weight:700; text-align:center; word-break:keep-all; }
.sub-head-wrap .title { display:block; margin-bottom:24px; text-align:left; font-size:20px; line-height:30px; font-weight:700; }

.sub-wrap.fund-detail-wrap .sub-head-wrap { min-height:270px; padding:40px 12% 0; background: linear-gradient(142.43deg, #1A399E 35.19%, #4D38CE 103.99%); }
.sub-wrap.fund-detail-wrap .btn-sns { position:absolute; top:17px; right:24px; }

.sub-wrap.fund-detail-wrap .fund-code { color:#B9BBC6; font-size:12px; line-height:18px;  }
.sub-head-wrap .tag-wrap { margin-bottom:10px; text-align:center; }
.sub-head-wrap .tag-wrap em { margin:2px 2px; }

