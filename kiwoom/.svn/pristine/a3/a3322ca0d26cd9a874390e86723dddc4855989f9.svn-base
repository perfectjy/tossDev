@charset "utf-8";

/* mo Tab */
.ui-tab.scroll-tab.t1 .ui-tab-btns {padding:0 24px; font-size:0; border-bottom:1px solid #DFE0E2; }
.ui-tab.scroll-tab.t1 .ui-tab-btn {position:relative; padding:0 8px; color:#73737A;}
.ui-tab.scroll-tab.t1 .ui-tab-btn.selected {color:#222; font-weight:700;}
.ui-tab.scroll-tab.t1 .ui-tab-pnls {padding:24px 24px 0 24px;}
.ui-tab.scroll-tab.t1 .ui-tab-btn.selected {background-color:#fff;}
.ui-tab.scroll-tab.t1 .ui-tab-btn {background-color:#fff;}
.ui-tab.scroll-tab.t1 .ui-tab-btn.selected {color: #222; font-weight:700; }

.ui-tab.scroll-tab .ui-tab-btns:after { content:''; display:block; clear:both;}
.ui-tab.scroll-tab .ui-tab-btn { display:inline-block; font-size:14px; height:47px; color:#73737A; }
/* .ui-tab.scroll-tab .ui-tab-btn.selected:after { content:''; display:block; position:absolute; bottom:0; left:0; width:100%; height:2px; background:#EC008C; } */
.ui-tab.scroll-tab .ui-tab-pnls { padding:40px 24px; }
.sub-conts-wrap .ui-tab.scroll-tab .ui-tab-btn.selected {background:#fff ;}


.main-tab.search {position:relative;}
.main-tab.search .ui-tab-btns {font-size:0;}
.main-tab.search .ui-tab-btn {display:inline-block; height:32px; padding:0 16px; margin-top:8px; margin-right:8px; color:#73737A; font-size:14px; background-color:#fff; border:1px solid #DFE0E2; border-radius:40px; vertical-align:top;}
.main-tab.search .ui-tab-btn:last-child {margin-right:0;}
.main-tab.search .ui-tab-btn.selected {color:#fff; background-color:#EC008C; border-color:#EC008C;}

.ui-tab.sub-tab {}
.ui-tab.sub-tab .ui-tab-btns {display:flex; padding:0 24px; border-bottom:1px solid #DFE0E2;}
.ui-tab.sub-tab .ui-tab-btn {position: relative; flex:1 1; display:block; height:48px; font-size:14px; color:#B9BBC6;}
.ui-tab.sub-tab .ui-tab-btn.selected {font-weight:700; color:#222;}
.ui-tab.sub-tab .ui-tab-btn.selected:after { content:''; display:block; position:absolute; bottom:-1px; left:0; width:100%; height:2px; background:#EC008C; }



