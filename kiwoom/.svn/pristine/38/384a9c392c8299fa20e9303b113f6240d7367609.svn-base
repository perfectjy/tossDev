@charset "utf-8";

/* common */
* { box-sizing:border-box; padding:0; margin:0; line-height:1; }
*:after,
*:before { box-sizing:border-box; }
html, body { background:#fff; user-select:none; -webkit-user-select:none; -ms-user-select:none; -webkit-user-select:none; -webkit-tap-highlight-color:transparent; }
body { padding:0; margin:0; font-family:"Pretendard", Sans-serif; font-size:16px; font-weight:500; color:#222; word-break:break-all; letter-spacing:0; }

h1, h2, h3, h4, h5, h6 { font-weight:400; color:#000; }
a { text-decoration:none; outline-offset:1px; color:inherit; }
a:hover { text-decoration:none; }
p { line-height:1.5; }
dl, ol, ul, dt, dd, li { display:block; list-style:none; }
table { width:100%; border-collapse:collapse; border-spacing:0; }
caption { height:0; opacity:0; }
th, td { border-collapse:collapse; vertical-align:middle; }
button{overflow:visible}
button{margin:0;padding:0;border:0;background:none;width:auto;cursor:pointer;overflow:visible;vertical-align:middle;background:transparent;cursor:pointer; font-family:"Pretendard", Sans-serif; }
button span, button:active span, button:focus span{position:relative}
select, textarea, input { -webkit-appearance:none; border:0; }
select { height:40px; padding:0 10px 0 10px; }
input { border:0; }
em, i { font-style:normal; text-decoration:none;}
hr { position:relative; display:block; height:1px; border:0; background:#eee; margin:30px 0; }
img { border:0; }
.hanja { font-family:'맑은 고딕', '돋움', '굴림'; }

hr.sub-line { height:5px; }

*.disabled, *:disabled {  pointer-events: none !important; cursor: default !important; opacity: 0.2 !important; box-shadow: none !important; }

/* input box block type */
[class*="inp-"] { position:relative; display:inline-block; width:100%; height:50px; font-size:15px; color:#000; line-height:1; padding:12px 15px; border:1px #bdbdbd solid; background:#fff; font-family:"Pretendard", Sans-serif; outline:0; -webkit-appearance:none; -webkit-border-radius:0; box-sizing:border-box; }
[class*="inp-"]:focus { border-color:#0091ea; }
[class*="inp-"]::placeholder { font-family:"Pretendard", Sans-serif; color:#9e9e9e; }
[class*="inp-"]::-webkit-input-placeholder { font-family:"Pretendard", Sans-serif; color:#9e9e9e; }
[class*="inp-"]:-ms-input-placeholder { font-family:"Pretendard", Sans-serif; color:#9e9e9e; }
[class*="inp-"]:focus::placeholder { color:transparent; }
[class*="inp-"]:focus::-webkit-input-placeholder { color:transparent; }
[class*="inp-"]:focus:-ms-input-placeholder { color:transparent; }
[class*="inp-"][readonly]:focus { border-color:#e5e5e5; }
[class*="inp-"][disabled],
[class*="inp-"][readonly] { background:#f2f2f2; border-color:#e5e5e5; }
[class*="inp-"][readonly]::placeholder,
[class*="inp-"][disabled]::placeholder { color:#9e9e9e; }
[class*="inp-"][readonly]::-webkit-input-placeholder,
[class*="inp-"][disabled]::-webkit-input-placeholder { color:#9e9e9e; }
[class*="inp-"][readonly]:-ms-input-placeholder,
[class*="inp-"][disabled]:-ms-input-placeholder { color:#9e9e9e; }
[class*="inp-"][type="password"] { -webkit-text-security:disc; }
[class*="inp-"]::-ms-clear { display:none; }
textarea[class*="inp-"] { display:block; max-height:200px; padding-top:14px; line-height:22px; overflow-y:hidden; border-color:1px #e0e0e0 solid; }
textarea[class*="inp-"]::placeholder { line-height:22px; }
/* textarea[class*="inp-"]:focus { padding:12px 15px 13px; } */

.ui-input-txt { position:relative; }
.ui-input-txt .btn-clear { position:absolute; top:50%; right:10px; margin-top:-12px; }
.btn-clear { display:inline-block; width:24px; height:24px; right:20px; background:url(/kosef/pcWeb/resources/images/common/ico_comm.svg) no-repeat -330px -40px; }

label { position:relative; display:inline-block; font-size:14px; }

input[type="radio"],
input[type="checkbox"] { position:absolute; bottom:0; left:0; width:0; height:0; opacity:0; }
input[type="password"] { font-family:Arial, sans-serif; }

/* headline style */
.h2 { font-weight:700; font-size:40px; line-height:56px; }
.h3-sub { margin:95px 0 24px 0; font-size:24px; line-height:36px; font-weight:700; }
.h3 { margin:80px 0 25px 0; font-weight:700; font-size:20px; line-height:30px; }
.h4 { margin:25px 0 15px 0; font-weight:700; font-size:16px; line-height:24px;  }
h3.dot, h4.dot { position:relative; padding-left:14px; font-weight:700; font-size:24px; line-height:36px;  }
h3.dot:before, h4.dot:before { content:''; position:absolute; top:4px; left:0; width:8px; height:8px; background:#EC008C; border-radius:50%;  }


/* margin only top */
.mgt0 { margin-top:0 !important; }
.mgt10 { margin-top:10px !important; }
.mgt15 { margin-top:15px !important; }
.mgt20 { margin-top:20px !important; }
.mgt25 { margin-top:25px !important; }
.mgt30 { margin-top:30px !important; }
.mgt40 { margin-top:40px !important; }
.mgt80 { margin-top:80px !important; }
.mgt95 { margin-top:95px !important; }
.mgt100 { margin-top:100px !important; }

/* font color */
.fc-primary { color:#EC008C !important; }
.fc-secondary { color:#1A399E !important; }
.fc-sub { color:#757575 !important; }
.fc-warn { color:#f9a825 !important; }
.fc-bad { color:#d32f2f !important; }
.fc-good { color:#4caf50 !important; }
.fc-black { color:#000000 !important; }

/* keep-all */
.keep-word { word-break:keep-all !important; }

/* a */
a { cursor:pointer; }
/* a.link { color:#003cff; text-decoration:underline; } */

/* hide/hidden style */
.a11y-hidden,
.hide { position:absolute !important; width:1px !important; height:1px !important; color:transparent !important; overflow:hidden !important; clip:rect(1px, 1px, 1px, 1px); -webkit-clip-path:inset(50%); clip-path:inset(50%); margin:0 !important; padding:0 !important;}

/* float */
.fclear::after { content:''; display:block; clear:both; }
.f-left { float:left !important; }
.f-right { float:right !important; }

/* txt bold */
.txt-bold{font-weight:600 !important;}

.ellips {  overflow:hidden; display:block; text-overflow:ellipsis; white-space:nowrap; } /* 한줄 처리 */

/* button */
.btn.base { min-width:160px; line-height:46px; font-weight:600; font-size:16px; border:1px solid #222; }
.btn.base.primary { min-width:160px; background:none; border-color:#EC008C; color:#EC008C; }
.btn.base.cancel { color:#74757E; border-color:#74757E; }


.btn { display:inline-flex; justify-content:center; align-items:center; min-width:160px; min-height:48px; padding:0 40px; font-size:16px; border-radius:32px; background-color:#FFF; text-align:center;}
.btn:disabled { color:#B9BBC6; border:1px solid #DFE0E2; background-color:#DFE0E2; cursor:default; }
.btn:disabled:hover, .btn:disabled:active { color:#B9BBC6; border:1px solid #DFE0E2; background-color:#DFE0E2; }
.btn.primary { color:#EC008C; font-weight:600; border:1px solid #EC008C; background-color:#FFF;}
.btn.primary:hover, .btn.primary:active { color:#FFF; background-color:#EC008C; }
.btn.secondary { color:#222; border:1px solid #222; }
.btn.secondary:hover, .btn.secondary:active { background-color:#F7F8F8; }
.btn.tertiary { color:#9D9DA4; border:1px solid #9D9DA4;}
.btn.tertiary:hover, .btn.tertiary:active { background-color:#F7F8F8; }
.btn.fill { color:#FFF; background-color:#EC008C;}
.btn.fill:hover, .btn.fill:active { background-color:#C02882; }
.btn.lg { min-width:200px; min-height:56px; font-size:20px; }
.btn.search{ min-width:90px; min-height:48px; padding :0 20px; margin-left: 8px; color:#EC008C; font-size:14px; font-weight: 500; border:1px solid #EC008C; border-radius:8px;}
.btn.search:hover, .btn.search:active{ color:#FFF; background-color:#EC008C;}

.btn.primary.arrow span { position:relative; padding-right:18px; }
.btn.primary.arrow span:after { content:'';  display:block; position:absolute; top:0; right:0; width:16px; height:16px; transform:rotate(90deg);  background:url(/kosef/pcWeb/resources/images/common/ico_comm.svg) no-repeat -380px 0;  }

.btn-reset { position:relative; display:inline-block; padding-right:24px; line-height:24px; font-size:16px; }
.btn-reset:after { content:''; display:block; position:absolute; top:4px; right:0; width:16px; height:16px; background:url(/kosef/pcWeb/resources/images/common/ico_comm.svg) no-repeat -320px 0; }
.btn-group { margin-top:32px; text-align:center; line-height:0; font-size:0; } 
.btn-group button,
.btn-group a { margin:0 8px; }
.btn-group button:first-child,
.btn-group a:first-child { margin-left:0; }
.btn-group button:last-child,
.btn-group a:last-child { margin-right:0; }
.btn-group.btn-r { text-align:right; }
.board-prev-next + .btn-group { margin-top:16px; }
.board-prev-next + .btn-group .btn { font-size:14px; font-weight: 500;}

.btn-txt { display:inline-block; position:relative; font-size:14px; line-height:21px; padding-right:15px; color:#74757e; }
.btn-txt:after { content:''; position:absolute; background-image:url(/kosef/pcWeb/resources/images/common/ico_comm.svg); background-repeat:no-repeat ;  }
.btn-txt.link:after { top:2px; right:0; width:16px; height:16px; background-position:-40px 0; opacity:0.5;   }
.btn-txt:hover { color:#222; font-weight:500;  }
.btn-txt.link:hover:after { opacity:1; }
.btn-txt.reset { padding-right:24px; }
.btn-txt.reset:after { top:3px; right:0; width:16px; height:16px; background-position:-320px 0; opacity:0.5;  }
.btn-txt.reset:hover:after { opacity:1; }
.btn-txt.down { padding-right:24px; font-size:16px; line-height:24px; }
.btn-txt.down:after { top:4px; right:0; width:16px; height:16px; background-position:-300px 0; opacity:0.5;  }
.btn-txt.down:hover:after { opacity:1; }

.btn-icon { display:inline-block; min-height:48px; padding:0 24px; border:1px solid #222; border-radius:40px; background:#fff;  }
.btn-icon:hover, .btn-icon:active {background-color:#F7F8F8; } 
.btn-icon span { position:relative; display:inline-block; padding:13px 0; line-height:21px; color:#222; font-size:14px; font-weight:500; text-align:center; padding-right:21px; }
.btn-icon span::after { content:''; position:absolute; top:14px; right:0; display:inline-block; width:16px; height:16px; background:url(/kosef/pcWeb/resources/images/common/ico_comm.svg) no-repeat -300px 0;}
.btn-icon.box{ border-radius:8px; }
.btn-icon.no-icon span { padding-right:0; }
.btn-icon.no-icon span::after { content:none; }
.btn-icon.icon-more span::after{ background-position-x:-340px;}
.btn-icon.icon-new span:after { background-position:-140px 0;  }
.btn-icon.icon-new:hover { background:#EC008C; color:#fff; border-color:#EC008C;  }
.btn-icon.icon-new:hover span {  color:#fff; }
.btn-icon.icon-new:hover span:after { background-position:-140px -20px;  }
.btn-icon.icon-link { min-height:auto; height:32px; padding:0 32px; }
.btn-icon.icon-link span { position:relative; padding:5px 20px 0 8px; }
.btn-icon.icon-link span:before { content:''; display:block; position:absolute; top:10px; left:0;  width:4px; height:4px; border-radius:50%; background:#EC008C; }
.btn-icon.icon-link span:after { top:7px; width:16px; height:16px; background-position:-40px 0;  }
.btn-icon.icon-go span:after { top:15px; width:16px; height:16px; background-position:-40px 0;  } 

.btn-more{ width:100%; margin-top:24px; background-color:#FFF; }
.btn-more span{ display:inline-flex; color:#74757e; font-size:14px; font-weight:500; padding:16px 0;}
.btn-more span::after{ content:''; display:inline-block; width:16px; height:16px; padding-left:4px; background:url(/kosef/pcWeb/resources/images/common/ico_comm.svg) no-repeat 2px 0; opacity:0.6;}
.btn-more:hover{ background-color:#F7F8F8; }
.btn-more:hover span{ color:#222; }
.btn-more:hover span::after{ opacity:1; }

.btn-txt-arrow { display:inline-block; position:relative; padding-right:20px; color:#74757e; font-size:14px; line-height:21px; }
.btn-txt-arrow:after { content:""; position:absolute; top:2px; right:0; width:16px; height:16px; opacity:0.5; background:url(/kosef/pcWeb/resources/images/common/ico_comm.svg) no-repeat -40px 0; }

/* skip ********************************************************/
#skipNavi{position:absolute;width:100%;margin:0 auto;z-index:999}
#skipNavi a{text-indent:0;background:#000;position:absolute;z-index:1;top:-500px;width:100%;padding:5px 0;text-align:center}
#skipNavi a:active,
#skipNavi a:focus{top:0;color:#fff}

/* layout */

.ui-tab-pnl, .ui-tab-pnl[aria-hidden=true] { display:none; visibility:hidden; opacity:0; }
.ui-tab-pnl.selected,
.ui-tab-pnl[aria-hidden='false'] { display:block; visibility:visible; position:relative; opacity:1; z-index:1; transition:opacity 0.3s linear 2s; }

/* inputs */
.lb-item { position:relative; display:inline-block; margin-right:8px; box-sizing:border-box; white-space:nowrap; vertical-align:middle; }
.lb-radio,
.lb-checkbox { position:relative; display:inline-flex; vertical-align:top; align-items:center; min-height:24px; padding:0 0 0 32px; margin:0; cursor:pointer; line-height:1; }
.lb-radio::before{ content:""; display:block; flex:0; width:24px; height:24px; background-color:#fff; position:absolute; top:0; left:0; background:url(/kosef/pcWeb/resources/images/common/ico_comm.svg) no-repeat -150px -210px;}
.lb-checkbox::before { content:""; display:block; flex:0; position:absolute; top:0; left:0; width:24px; height:24px; background:url(/kosef/pcWeb/resources/images/common/ico_comm.svg) no-repeat 0 -210px;}
.lb-checkbox.no-label.checked:before,
.lb-checkbox:hover:before {background-position-x:-60px;}
.lb-radio:hover:before {background-position-x:-210px;}
.lb-checkbox:hover:disabled:before {background-position-x:-90px;}
input:checked + .lb-checkbox::before {background-position-x:-30px;}
input:checked + .lb-radio::before {background-position-x:-180px;}
input:disabled + .lb-checkbox:before {background-position-x:-90px;}
input:disabled + .lb-radio:before {background-position-x:-240px;}
input:checked:disabled + .lb-checkbox:before {background-position-x:-120px;}
input:checked:disabled + .lb-radio:before {background-position-x:-270px;}
.lb-checkbox.no-label { padding-left:20px; }
.lb-checkbox.no-label.checked { padding-left:20px; }

/* input text */
.ui-input-txt {position:relative;}
.ui-input-txt > input {display:block; width:100%; height:48px; padding:0 24px; border:1px solid #BFC0C5; border-radius:8px; font-size:14px; color:#74757e; outline:0;}
.ui-input-txt > input:focus {border-color:#EC008C;} 
.btn-del,
.btn-search {position:absolute; top:50%; right:4px; width:60px; height:40px; transform:translateY(-50%);}
.btn-del:after,
.btn-search:after {content:''; display:inline-block; position:absolute; top:50%; background:url(/kosef/pcWeb/resources/images/common/ico_comm.svg) no-repeat; transform:translateY(-50%);}
.btn-del {display:none;}
.btn-del::after {width:24px; height:24px; right:20px;  background-position:-330px -40px;}
.btn-search::after {width:16px; height:16px; right:22px; background-position:-360px 0; opacity:0.5;} 
.ui-input-txt .inp.active + .btn-del {display:block;}
.ui-input-txt > [readonly],
.ui-input-txt > [disabled],
.ui-input-txt > [readonly]:focus,
.ui-input-txt > [disabled]:focus {border-color:#B9BBC6; background:#DFE0E2; color:#B9BBC6;}
.ui-input-txt.error > input {border-color:#F2303C; color:#222;}


/* Drop + input */
.ui-drop-input { display:flex; position:relative; }
.ui-drop-input .inp { border-left:none; border-top-left-radius:0; border-bottom-left-radius:0;}
.ui-drop-input .ui-select-btn,
.ui-drop-input .ui-select.on .ui-select-btn  {border-right:none; border-top-right-radius:0; border-bottom-right-radius:0;}
.ui-drop-input .ui-input-txt::before {content:''; display:block; position:absolute; width:1px; height:16px; margin-top:16px; background-color:#B9BBC6;}
.ui-drop-input .ui-input-txt.error > input:focus {border-color:#F2303C; color:#222;}
.ui-drop-input .ui-input-txt { flex-shrink:0; width:300px; }
.ui-drop-input .ui-input-txt > input:focus  {border-color:#BFC0C5;}
.ui-drop-input .ui-input-txt > input:focus + .btn-search {opacity:1;}
.ui-drop-input .ui-select { flex-shrink:0; width:200px; }
.ui-drop-input .ui-select select {height:100%;}
.ui-drop-input .ui-select.on + .ui-input-txt .inp {border-color:#EC008C;}                                      
.ui-drop-input .ui-select.on + .ui-input-txt .btn-search {opacity:1;}
.ui-drop-input .ui-select-btn span {width:calc(100% - 15px);}

.tab-tog { position:relative; }
.tab-tog .lb-txts { position:absolute; top:4px; left:50px; color:#74757E; }
.tab-tog .tab-chkbox { display:inline-block; position:relative; width:auto; min-width:44px; height:24px; cursor:pointer; box-sizing:border-box; vertical-align:top; }
.tab-tog input + .tab-chkbox:before, 
.tab-tog input + .tab-chkbox:after { content:''; display:block; position:absolute; }
.tab-tog input + .tab-chkbox:before { left:0; top:0; width:100%; height:24px; background:#B9BBC6; border-radius:24px; transition:all 0.2s ease-in-out; box-sizing:border-box; }
.tab-tog input + .tab-chkbox:after { left:2px; top:2px; width:21px; height:21px; background:url(/kosef/pcWeb/resources/images/common/ico_comm.svg) no-repeat -550px 0; }
.tab-tog input:checked + .tab-chkbox:after { left:calc(100% - 23px); background-position-x:-520px;}
.tab-tog input:checked + .tab-chkbox:before { background:#EC008C; }
.tab-tog input:focus + .tab-chkbox { outline:1px dotted #000; }

select[class], textarea[class], input[class] { appearance:none; -moz-appearance:none; -webkit-appearance:none; -webkit-border-radius:0; }

/* range */

.ui-range { width:100%; height:6px; padding:0 0; display:flex; flex-direction:column; justify-content:flex-start; align-items:center; position:relative; -webkit-user-select:none; -moz-user-select:none; -ms-user-select:none; user-select:none; }
.ui-range:after { content:''; position:absolute; top:0; left:0;  right:0; z-index:2; height:6px; background:url(/kosef/pcWeb/resources/images/bg_fundslider.png) no-repeat -5px 0; background-size:979px auto;  }
.ui-range[data-type=single] .ui-range-track { z-index: 10; }
.ui-range[data-type=single] .ui-range-bar { right:auto; }
.ui-range[data-type=single] .ui-range-point::before { content:"code" !important; }
.ui-range[data-orient=vertical] { transform-origin:5rem 5rem; transform:rotate(-90deg); margin-left:6rem;}
.ui-range[data-orient=vertical] .ui-range-point { justify-content:center; }
.ui-range[data-orient=vertical] .ui-range-txt { display:none; top:2.4rem; left:2rem; transform:rotate(90deg); transform-origin:0 0; }
.ui-range .ui-range-inp { width:100%; top:0; left:0; position:absolute; border:none; z-index:10; -webkit-appearance:none; -moz-appearance:none; appearance:none; opacity:0; padding:0; margin:0; height:2rem; border:1px solid red; cursor:pointer; }
.ui-range .ui-range-inp:focus ~ .ui-range-track { outline:1px solid #000; }
.ui-range .ui-range-inp::-webkit-slider-thumb { pointer-events:all; -webkit-appearance:none; appearance:none; background-color:red; width:2rem; height:2rem; margin-top:0rem; }
.ui-range .ui-range-inp[data-range=from]:focus ~ .ui-range-track .left.ui-range-point { outline:2px solid #000; pointer-events:none; }
.ui-range .ui-range-inp[data-range=to]:focus ~ .ui-range-track .right.ui-range-point { outline:2px solid #000; pointer-events:none; }
.ui-range .ui-range-track { position:relative; width:100%; height:6px; pointer-events:none; z-index:12; }
.ui-range .ui-range-bar { position:absolute; pointer-events:none; top:0; bottom:0; left:0; right:0; border-radius:0.5rem; background:#fff url(/kosef/pcWeb/resources/images/bg_fundtrack01.png) no-repeat 0 0; background-size:979px auto; }
.ui-range .ui-range-point { position:absolute; top:50%; width:20px; height:20px; border-radius:50%; border:3px solid #fff; background-color:#1E66F2; pointer-events: none; z-index:13; cursor:pointer; box-shadow:0px 4px 4px rgba(0, 0, 0, 0.1); z-index:20;  }
.ui-range .ui-range-point:hover { pointer-events:none; }
.ui-range .ui-range-point.left { left:0; transform:translate(-10px, -50%); }
.ui-range .ui-range-point.left:after,
.ui-range .ui-range-point.right:after { content:''; display:block; position:absolute; top:2px; left:2px; width:10px; height:10px; background:url(/kosef/pcWeb/resources/images/common/ico_comm.svg) no-repeat -400px 0;   } 
.ui-range .ui-range-point.right:after { transform:rotate(180deg); }
.ui-range .ui-range-point.right { right:0; transform:translate(10px, -50%); }
.ui-range .ui-range-txt { position:absolute; top:-1.8rem; left:50%; text-align:center; font-size:12px; pointer-events:none; white-space: nowrap; transform: translate(-50%, 0);}
.ui-range .ui-range-marks { position:absolute; display:block; width:100%; padding:0 10px; height:20px; top:25px; }
.ui-range .ui-range-btn { position:absolute; top:0; font-size:14px; font-weight:600; padding:0; text-align:center; pointer-events: none; }

.ui-range.same .ui-range-point { pointer-events: none !important; }
.ui-range-inp { width: 100%; top: 0.5rem; left: 0; position: absolute; border: none; z-index: 10; -moz-appearance: none; appearance: none; opacity: 0; padding: 0; margin: 0; height: 2rem; cursor: pointer; background-color: transparent; pointer-events: none; -webkit-appearance: none; }
.ui-range-inp:focus ~ .ui-range-track { outline: 1px solid #000; }
.ui-range-inp.on { pointer-events: all; z-index: 9; }
.ui-range-inp::-webkit-slider-thumb { pointer-events: all; /* appearance, background-color는 지워도 됨 */ appearance: none; background-color: transparent; box-shadow: none; width: 2rem; height: 2rem; margin-top: 0rem; z-index: 13; cursor: pointer; -webkit-appearance: none; }
.ui-range-inp[data-range=from]:focus ~ .ui-range-track .left.ui-range-point, 
