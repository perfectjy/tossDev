/* sheet */
.sheet-bottom.datepicker .datepicker-wrap {
    padding: 1rem;
    background: #FAFAFA;
    
    border: 1px solid #FAFAFA;
    
    box-shadow: 0 0.3rem 0.3rem rgba(0,0,0,0.1);
    box-shadow: 0 0.3rem 0.3rem rgba(0,0,0,0.1);
    position: relative;
    z-index: 1;
}

.sheet-bottom {
    position: absolute;
    top: 0;
    left: 0;
    opacity: 0;
    z-index: 301;
    transition: opacity .2s ease-in-out;
}

.sheet-bottom.on {
    opacity: 1;
}

.size-mobile .datepicker.sheet-bottom {
    max-width: 100%;
    top: auto !important;
    left: 0 !important;
    bottom: 0;
    position: fixed;
}

.size-mobile .datepicker-body {
    max-width: 23rem;
    margin: 1rem auto;
}

.size-mobile .datepicker.sheet-bottom {
    bottom: -50vh;
}

.size-mobile .datepicker.sheet-bottom.on {
    top: auto;
    animation-name: slideUpIn;
    animation-duration: .2s;
    animation-iteration-count: 1;
    animation-timing-function: ease-in-out;
    animation-fill-mode: forwards;
}

.size-mobile .datepicker.sheet-bottom.off {
    top: auto;
    animation-name: slideUpOut;
    animation-duration: .2s;
    animation-iteration-count: 1;
    animation-timing-function: ease-in-out;
    animation-fill-mode: forwards;
}

.sheet-dim {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 0;
    background: rgba(0, 0, 0, 0.3);
    
    opacity: 0;
    transition: opacity .2s ease-in-out;
}

.sheet-dim.on {
    opacity: 1;
    transition: opacity .2s ease-in-out;
}




/* box */
.box-base {
    position: relative;
    width: 100%;
    margin: 10px 0;
    padding: 10px 20px;
    background: #FFFFFF;
    background: #fff;
    border: 1px solid rgb(180, 180, 180);
}









/* GRID guide */
.base-grid.on {
    display: flex;
}

.base-grid {
    display: none;
    position: fixed;
    z-index: 9999;
    pointer-events: none;
    top: 0;
    left: 0;
    width: 100%;
    max-width: 119rem;
    height: 100%;
    justify-content: space-between;
    padding: 0 2rem;
}

.base-grid div {
    flex: 1;
    height: 100%;
    background: rgba(247, 89, 181, 0.096);
}

.base-grid div+div {
    margin-left: 2.4rem;
}



