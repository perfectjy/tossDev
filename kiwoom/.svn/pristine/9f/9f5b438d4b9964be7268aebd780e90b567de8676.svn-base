/* LAYOUT */
.base-skip {
    position: fixed;
    z-index: 500;
}

.base-skip .skip {
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    height: 5rem;
    background-color: #616161;
    color: #FAFAFA;
    width: 100%;
    position: fixed;
    left: 0;
    top: -5rem;
}

.base-skip .skip:focus {
    top: 0;
}

.base-wrap {
    position: relative;
    z-index: 0;
}



/* PARAGRAPH */
em,
u,
cite,
i,
abbr {
    text-decoration: none;
}

a {
    color: #6296df;
}






/* HR */
hr {
    border: 0;
    border-top: 1px solid #EEEEEE;
    margin: 4rem 0;
}

.hr-sec {
    width: 100%;
    height: 1rem;
    background-color: #EEEEEE;
    border: 0;
    margin: 0.5rem 0;
}

.hr-cont {
    width: 100%;
    height: 0.1rem;
    background-color: #EEEEEE;
    border: 0;
    margin: 2rem 0;
}



/* FORM */
/* Removes the clear button from date inputs */
input[type="date"]::-webkit-clear-button,
input[type="date"]::-webkit-inner-spin-button,
input[type="date"]::-webkit-calendar-picker-indicator {
    display: none;
}

input[type="date"] {
    appearance: none;
    -webkit-appearance: none;
}

input[type="radio"],
input[type="checkbox"] {
    opacity: 0;
    filter: alpha(opacity=0);
    display: inline-block;
    position: absolute;
    z-index: -1;
    vertical-align: top;
    appearance: none;
    -moz-appearance: none;
    -webkit-appearance: none;
    margin: 0;
}

.lb-radio,
.lb-checkbox {
    position: relative;
    display: inline-flex;
    vertical-align: top;
    align-items: center;
    min-height: 4rem;
    font-size: 1.4rem;
    padding: 0;
    margin: 0;
    cursor: pointer;
    line-height: 1;
}

.lb-radio:before,
.lb-checkbox:before {
    content: "";
    display: block;
    flex: 0;
    min-width: 2rem;
    min-height: 2rem;
    margin-right: .5rem;
    border: 2px solid $gray900;
    transition: box-shadow 0.5s, border 0.3s;
}

.lb-radio:before {
    border-radius: 50%;
}

.lb-checkbox:before {
    border-radius: .3rem;
}


:checked+.lb-radio:before,
:checked+.lb-checkbox:before,
.lb-radio.checked:before,
.lb-checkbox.checked:before {
    border: .6rem solid $gray900;
}


.press .lb-checkbox:before,
.press .lb-radio:before,
.lb-radio.pressed:before,
.lb-checkbox.pressed:before{
    box-shadow: 0 0 .3rem rgba(0, 0, 0, 0.9);
}

.hover .lb-checkbox:before,
.hover .lb-radio:before,
.lb-radio:hover:before,
.lb-checkbox:hover:before{
    box-shadow: 0 0 .7rem rgba(0, 0, 0, 0.3);
}

.focus .lb-checkbox:before,
.focus .lb-radio:before,
.lb-radio:focus:before,
.lb-checkbox:focus:before {
    box-shadow: 0 0 .5rem rgba(0, 0, 0, 0.7);
}

:checked+.lb-radio,
:checked+.lb-checkbox,
.lb-radio.checked,
.lb-checkbox.checked {
    color:$gray900;
}

.focus .lb-radio,
.focus .lb-checkbox,
:focus+.lb-radio,
:focus+.lb-checkbox {
    text-decoration: underline;
}

:disabled+.lb-radio,
:disabled+.lb-checkbox,
.lb-radio.disabled,
.lb-checkbox.disabled {
    opacity: 0.3;
    filter: alpha(opacity=30);
    cursor: not-allowed;
}

.lb-switch span,
.lb-radio span,
.lb-checkbox span {
    display: inline-block;
    vertical-align: middle;
    height: 100%;
    border-bottom: 1px solid transparent;
}

.lb-checkbox.all {
    font-size: 1.6rem;
    color: #616161;
}

.lb-checkbox.all span {
    padding-top: 1px;
}

.lb-switch{
    position: relative;
    padding-left:4.4rem;
    font-size: 1.4rem;
}
.lb-switch::before {
    content:"";
    display: block;
    width:4rem;
    height:1rem;
    border-radius:1rem;
    background-color: #BDBDBD;

    position: absolute;
    top:50%;
    transform: translate(0,-50%);
    transition: all .2s ease-in;
}
.lb-switch::after {
    content:"";
    display: block;
    width:2rem;
    height:2rem;
    border-radius:2rem;
    background-color:$white;
    
    box-shadow: 0.1rem .1rem .4rem rgba(0,0,0,0.1);
    position: absolute;
    top:50%;
    transform: translate(0,-50%);
    transition: all .2s ease-in;
    opacity: 1;
}
:checked + .lb-switch::before{
    background-color: hsl(265deg 100% 47% / 100%);
    opacity: .5;
}
:checked + .lb-switch::after{
    box-shadow: -0.1rem .1rem .4rem rgba(0,0,0,0.1);
    transform: translate(2rem,-50%);
    background-color: hsl(265deg 100% 47% / 100%);
}
:disabled + .lb-switch{
    opacity: 0.3;
}

.lb-tit {
    display: inline-block;
    height: 4rem;
    margin-right: 1rem;
    padding-top: 1rem;
    vertical-align: top;
    cursor: pointer;
}

/* input text */
select,
.ui-select-btn,
.inp-base {
    position: relative;
    display: inline-block;
    vertical-align: top;
    width: 100%;
    border: 1px solid #EEEEEE;
    background-color: $white;
    border-radius: 0;
    height: 4rem;
    padding: 0 1rem;
    outline: 0;
    margin: 0;
    color: #424242;
    font-size: 1.6rem;
}

.inp-base:focus {
    border-color: #FAFAFA;
}

.inp-base::-webkit-search-cancel-button {
    display: none;
}

textarea.inp-base {
    padding: 1rem;
    height: 10rem;
}

select[class] {
    position: relative;
    cursor: pointer;
}

select.inp-base {
    padding: 0 1rem;
}




