.etf-bg-wrap .ani-box .obj-01 { position: absolute; right: 0; top: 0px; width:321px; height: 274px; background:url(/kosef/moWeb/resources/images/etf_bg.svg )no-repeat; animation: etf-main-cloud1  3s infinite cubic-bezier(0.42, 0, 0.58, 1.0); animation-fill-mode: both; }  
.etf-bg-wrap .ani-box .txt{ font-size: 12px; font-weight: 400; color: #DFE0E2; text-align: center;}
.etf-bg-wrap .ani-box .txt:nth-of-type(1){ position: absolute; top: 146px; left: 80px;}
.etf-bg-wrap .ani-box .txt:nth-of-type(2){ position: absolute; top: 64px; right: 67px;}
.etf-bg-wrap .ani-box .txt em{ display: block; font-size: 24px; font-weight: 600; color: #FFF; margin-bottom: 4px;}

.etf-bg-wrap .ani-box .obj-02 { position: absolute; left: 67px; top: 42px; width: 18px; height: 18px; background: linear-gradient(98.43deg, #263BDC 26.72%, #B629D6 135.94%); border-radius: 50%; animation: etf-main-cloud2  2.0s infinite alternate cubic-bezier(0.43, 0.66, 0.37, 0.57); animation-delay: 1s; animation-fill-mode: both;}
.etf-bg-wrap .ani-box .obj-03 { position: absolute; left: 89px; top: 29px; width: 8px; height: 8px; background: linear-gradient(98.43deg, #263BDC 26.72%, #B629D6 135.94%); border-radius: 50%; animation: etf-main-cloud2  2.0s infinite alternate cubic-bezier(0.43, 0.66, 0.37, 0.57); animation-delay: 1s; animation-fill-mode: both;}

@keyframes etf-main-cloud1 {
    0%{
        top: 0;
    }
    60%{
        top: 10px;
    }
    80%{
        top: 5px;
    }
    100%{
        top: 0;   
    }
}
@keyframes etf-main-cloud2 {
    0% { transform: translateY(0) rotate(0deg); }
    100% { transform: translateY(-20px) rotate(15deg); }
}

