@keyframes motion { 0% {bottom:15px;} 100% {bottom:10px;} }
@keyframes investScrl { 0% {top:0;} 100% {top:60px;} }
@keyframes fadeInUp {
  0% { opacity: 0; -webkit-transform: translateY(50px); -ms-transform: translateY(50px); transform: translateY(50px); }
  100% { opacity: 1; -webkit-transform: translateY(0); -ms-transform: translateY(0); transform: translateY(0);}
}
@keyframes fadeIn { 0% { opacity: 0; } 100% { opacity: 1;} }
@keyframes spin { from { transform:rotate(0deg); } to { transform:rotate(360deg); } }
@keyframes slide-top{
  0%{-webkit-transform:translateY(100px);transform:translateY(100px);opacity: 0;}
  100%{-webkit-transform:translateY(0);transform:translateY(0);opacity: 1;}
}
@keyframes ani-digonal { 0% {transform: translate(0, 0);} 100% { transform: translate(20px, -20px); } }
@keyframes slide-top2{
  0%{-webkit-transform:translateY(100px);transform:translateY(100px);opacity: 0;}
  100%{-webkit-transform:translateY(0);transform:translateY(0);opacity: 0.3;}
}