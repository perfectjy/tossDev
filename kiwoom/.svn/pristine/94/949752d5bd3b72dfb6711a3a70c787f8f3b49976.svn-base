@charset "utf-8";

/* common */
* { box-sizing:border-box; padding:0; margin:0; line-height:1; }
*:after,
*:before { box-sizing:border-box; }
html, body { background:#fff; user-select:none; -webkit-user-select:none; -ms-user-select:none; -webkit-user-select:none; -webkit-tap-highlight-color:transparent; }
body { padding:0; margin:0; font-family:"Pretendard", Sans-serif; font-size:14px; font-weight:500; color:#222; word-break:break-all; letter-spacing:0; }

h1, h2, h3, h4, h5, h6 { font-weight:400; color:#000; }
a { text-decoration:none; outline-offset:1px; color:inherit; }
a:hover { text-decoration:none; }
p { line-height:1.5; }
dl, ol, ul, dt, dd, li { display:block; list-style:none; }
table { width:100%; border-collapse:collapse; border-spacing:0; }
caption { height:0; opacity:0; }
th, td { border-collapse:collapse; vertical-align:middle; }
button{overflow:visible}
button{margin:0;padding:0;border:0;background:none;width:auto;cursor:pointer;overflow:visible;vertical-align:middle;background:transparent;cursor:pointer; font-family:"Pretendard", Sans-serif; }
button span, button:active span, button:focus span{position:relative}
select, textarea, input { -webkit-appearance:none; border:0; }
select { height:40px; padding:0 10px 0 10px; }
input { border:0; }
em, i { font-style:normal; text-decoration:none;}
hr { position:relative; display:block; height:1px; border:0; background:#eee; margin:30px 0; }
img { border:0; }
.hanja { font-family:'맑은 고딕', '돋움', '굴림'; }

.lock {height:100%; overflow:hidden;}


/* input box block type */
[class*="inp-"] { position:relative; display:inline-block; width:100%; height:50px; font-size:15px; color:#000; line-height:1; padding:12px 15px; border:1px #bdbdbd solid; background:#fff; font-family:"Pretendard", Sans-serif; outline:0; -webkit-appearance:none; -webkit-border-radius:0; box-sizing:border-box;  }
[class*="inp-"]:focus { border-color:#0091ea; }
[class*="inp-"]::placeholder { font-family:"Pretendard", Sans-serif; color:#9e9e9e; }
[class*="inp-"]::-webkit-input-placeholder { font-family:"Pretendard", Sans-serif; color:#9e9e9e; }
[class*="inp-"]:-ms-input-placeholder { font-family:"Pretendard", Sans-serif; color:#9e9e9e; }
[class*="inp-"]:focus::placeholder { color:transparent; }
[class*="inp-"]:focus::-webkit-input-placeholder { color:transparent; }
[class*="inp-"]:focus:-ms-input-placeholder { color:transparent; }
[class*="inp-"][readonly]:focus { border-color:#e5e5e5; }
[class*="inp-"][disabled],
[class*="inp-"][readonly] { background:#f2f2f2; border-color:#e5e5e5; }
[class*="inp-"][readonly]::placeholder,
[class*="inp-"][disabled]::placeholder { color:#9e9e9e; }
[class*="inp-"][readonly]::-webkit-input-placeholder,
[class*="inp-"][disabled]::-webkit-input-placeholder { color:#9e9e9e; }
[class*="inp-"][readonly]:-ms-input-placeholder,
[class*="inp-"][disabled]:-ms-input-placeholder { color:#9e9e9e; }
[class*="inp-"][type="password"] { -webkit-text-security:disc; }
[class*="inp-"]::-ms-clear { display:none; }
textarea[class*="inp-"] { display:block; max-height:200px; padding-top:14px; line-height:22px; overflow-y:hidden; border-color:1px #e0e0e0 solid; }
textarea[class*="inp-"]::placeholder { line-height:22px; }
/* textarea[class*="inp-"]:focus { padding:12px 15px 13px; } */

label { position:relative; display:inline-block; font-size:14px; }

input[type="radio"],
input[type="checkbox"] { position:absolute; bottom:0; left:0; width:0; height:0; opacity:0; }
input[type="password"] { font-family:Arial, sans-serif; }

/* headline style */
.h1 { font-weight:300; }
.h2 { font-weight:300; }
.h1 strong,
.h2 strong { font-weight:800; }

/* font color */
.fc-primary { color:#EC008C !important; }
.fc-secondary { color:#2041bf !important; }
.fc-sub { color:#757575 !important; }
.fc-warn { color:#f9a825 !important; }
.fc-bad { color:#d32f2f !important; }
.fc-good { color:#4caf50 !important; }
.fc-black { color:#000000 !important; }


/* keep-all */
.keep-word { word-break:keep-all !important; }

/* a */
a.link { color:#003cff; text-decoration:underline; }

/* hide/hidden style */
.a11y-hidden,
.hide  { overflow:hidden; position:absolute; left:-999px; top:-999px; width:1px; height:1px; margin-left:-1px; border:0 none !important; background:transparent !important; color:transparent !important; }

/* float */
.fclear::after { content:''; display:block; clear:both; }
.f-left { float:left !important; }
.f-right { float:right !important; }

.ellips {  overflow:hidden; display:block; text-overflow:ellipsis; white-space:nowrap; } /* 한줄 처리 */

/* scroll box */ 
.scroll-box { }
.scroll-box .scroll-list { overflow:hidden; overflow-x:auto; webkit-overflow-scrolling:touch;  white-space:nowrap; }
.scroll-box .scroll-list::-webkit-scrollbar {display:none;}

/* button */
.btn { display:inline-block; font-size:16px; text-align:center; border-radius:8px; }
.btn.primary { min-width:240px; font-size:20px; font-weight:600; color:#fff; line-height:64px; background-color:#EC008C; }
.btn.base { min-width:160px; line-height:46px; font-weight:600; font-size:16px; border:1px solid #222; }
.btn.base.primary { min-width:160px; background:none; border-color:#EC008C; color:#EC008C; }
.btn.base.cancel { color:#74757E; border-color:#74757E; }


.btn-group { margin-top:30px; text-align:center; line-height:0; font-size:0; } 
.btn-group button { margin:0 5px; }

/* sns */
.sns-gruop { text-align:center; }
.sns-gruop a { position:relative; display:inline-block; margin:0 17px; padding-top:48px; min-width:40px; line-height:21px; }
.sns-gruop a:after { content:''; position:absolute; top:0; left:50%; margin-left:-20px; width:40px; height:40px; background:url(/kiam/moWeb/resources/images/common/ico_comm.png) no-repeat -310px -120px; background-size:500px auto;  }
.sns-gruop a.sns-face:after { background-position-x:-360px; }
.sns-gruop a.sns-url:after { background-position-x:-410px; }
 
/* skip ********************************************************/
#skipNavi{position:absolute;width:100%;margin:0 auto;z-index:999}
#skipNavi a{text-indent:0;background:#000;position:absolute;z-index:1;top:-500px;width:100%;padding:5px 0;text-align:center}
#skipNavi a:active,
#skipNavi a:focus{top:0;color:#fff}

.fund-flags { line-height:0; font-size:0; }
.fund-flags span { display:inline-block; height:20px; padding:4px 8px 0 8px; font-size:10px; border:1px solid #1A399E; color:#1A399E; border-radius:4px 0px 4px 4px; }
.fund-flags span + span { margin-left:4px; }
.fund-flags span.high { color:#EC008C; border-color:#EC008C; }

/* tab */
.ui-tab-pnl, .ui-tab-pnl[aria-hidden=true] { display:none; visibility:hidden; opacity:0; }
.ui-tab-pnl.selected,
.ui-tab-pnl[aria-hidden='false'] { display:block; visibility:visible; position:relative; opacity:1; z-index:1; transition:opacity 0.3s linear 2s; }

/* list */
.ui-list li { position:relative; margin-top:4px; padding-left:6px; font-size:12px; line-height:18px; color:#74757E; }
.ui-list li:before { content:''; display:block; position:absolute; top:8px; left:0; width:2px; height:2px; background:#73737A; border-radius:50%; }

/* inputs */
.lb-item { position:relative; display:inline-block; margin-right:8px; box-sizing:border-box; white-space:nowrap; vertical-align:middle; }
.lb-radio,
.lb-checkbox { position:relative; display:inline-flex; vertical-align:top; align-items:center; min-height:24px; padding:0 0 0 32px; margin:0; cursor:pointer; line-height:1; }
.lb-radio::before{ content:""; display:block; flex:0; width:24px; height:24px; border-radius:50%; border:1px solid #BFC0C5; background-color:#fff; position:absolute; top:0; left:0; box-sizing:border-box; }
.lb-checkbox::before { content:""; display:block; flex:0; position:absolute; top:0; left:0; width:24px; height:24px; border-radius:4px; border:1px solid #B9BBC6; border-radius:4px; }
.lb-checkbox::after,
.lb-radio::after { content:""; display:block; position:absolute; top:0; left:0; width:24px; height:24px; background:url(/kiam/pcWeb/resources/images/common/ico_form_chk.png) no-repeat 1px 0; }
.lb-checkbox.no-label.checked:before,
.lb-checkbox:hover:before,
.lb-radio:hover:before { border-color:#EC008C; }
.lb-radio:hover:after,
.lb-checkbox:hover:after { background-position-x:-47px; }
.lb-checkbox:hover:disabled:before,
input:checked + .lb-checkbox::before,
input:checked + .lb-radio::before { background:#EC008C; border-color:#EC008C; }
input:checked + .lb-radio::after,
input:checked + .lb-checkbox::after { background-position-x:-24px; }
input:disabled + .lb-checkbox:before,
input:disabled + .lb-radio:before { background:#DFE0E2; border-color:#B9BBC6; }
input:disabled + .lb-radio:after,
input:disabled + .lb-checkbox:after { display:none; }
input:checked:disabled + .lb-checkbox:before,
input:checked:disabled + .lb-radio:before { background:#B9BBC6; border-color:#B9BBC6; }
input:checked:disabled + .lb-radio:after,
input:checked:disabled + .lb-checkbox:after { display:block; background-position-x:-24px; }
.lb-checkbox.no-label { padding-left:20px; }
.lb-checkbox.no-label.checked { padding-left:20px; }

.tab-tog { position:relative; }
.tab-tog .lb-txts { position:absolute; top:4px; left:50px; color:#74757E; }
.tab-tog .tab-chkbox { display:inline-block; position:relative; width:auto; min-width:44px; height:24px; cursor:pointer; box-sizing:border-box; vertical-align:top;}
.tab-tog input + .tab-chkbox:before, 
.tab-tog input + .tab-chkbox:after { content:''; display:block; position:absolute; }
.tab-tog input + .tab-chkbox:before { left:0; top:0; width:100%; height:24px; background:#d7dbe1; border-radius:24px; transition:all 0.2s ease-in-out; box-sizing:border-box; }
.tab-tog input + .tab-chkbox:after { left:2px; top:2px; width:21px; height:21px; background:url(/kiam/pcWeb/resources/images/common/common.png) no-repeat -300px -120px; }
.tab-tog input:checked + .tab-chkbox:after { left:calc(100% - 23px); background-position-x:-342px;  }
.tab-tog input:checked + .tab-chkbox:before { background:#EC008C; }
.tab-tog input:focus + .tab-chkbox { outline:1px solid #000; }

select[class], textarea[class], input[class] { appearance:none; -moz-appearance:none; -webkit-appearance:none; -webkit-border-radius:0; }

/* range */
.ui-range { width:100%; height:6px; padding:0 0; display:flex; flex-direction:column; justify-content:flex-start; align-items:center; position:relative; -webkit-user-select:none; -moz-user-select:none; -ms-user-select:none; user-select:none; }
.ui-range[data-type=single] .ui-range-bar { right:auto; }
.ui-range[data-type=single] .ui-range-point::before { content:"code" !important; }
.ui-range[data-orient=vertical] { transform-origin:5rem 5rem; transform:rotate(-90deg); margin-left:6rem;}
.ui-range[data-orient=vertical] .ui-range-point { justify-content:center; }
.ui-range[data-orient=vertical] .ui-range-point .n { display:none; top:2.4rem; left:2rem; transform:rotate(90deg); transform-origin:0 0; }
.ui-range .ui-range-inp { width:100%; top:0; left:0; position:absolute; border:none; z-index:10; -webkit-appearance:none; -moz-appearance:none; appearance:none; opacity:0; padding:0; margin:0; height:2rem; border:1px solid red; cursor:pointer; }
.ui-range .ui-range-inp:focus ~ .ui-range-track { outline:1px solid #000; }
.ui-range .ui-range-inp.on { z-index:11; }
.ui-range .ui-range-inp::-webkit-slider-thumb { pointer-events:all; -webkit-appearance:none; appearance:none; background-color:red; width:2rem; height:2rem; margin-top:0rem; }
.ui-range .ui-range-inp[data-range=from]:focus ~ .ui-range-track .left.ui-range-point { outline:2px solid #000; pointer-events:none; }
.ui-range .ui-range-inp[data-range=to]:focus ~ .ui-range-track .right.ui-range-point { outline:2px solid #000; pointer-events:none; }
.ui-range .ui-range-track { position:relative; width:100%; height:6px; background-color:#BDBDBD; pointer-events:none; z-index:12; }
.ui-range .ui-range-bar { position:absolute; pointer-events:none; top:0; bottom:0; left:0; right:0; border-radius:0.5rem; background-color:#1E66F2; }
.ui-range .ui-range-point { position:absolute; top:50%; width:20px; height:20px; border-radius:50%; border:4px solid #fff; background-color:#1E66F2; pointer-events:all; z-index:13; cursor:pointer; box-shadow:0px 4px 4px rgba(0, 0, 0, 0.1); }
.ui-range .ui-range-point:hover { pointer-events:none; }
.ui-range .ui-range-point.on { z-index:1; }
.ui-range .ui-range-point.left { left:0; transform:translate(-10px, -50%); }
.ui-range .ui-range-point.right { right:0; transform:translate(10px, -50%); }
.ui-range .ui-range-point .n { display:none; position:absolute; top:-1.8rem; text-align:center; font-size:1.2rem; pointer-events:none; }
.ui-range .ui-range-marks { position:absolute; display:block; width:100%; padding:0 10px; height:20px; top:25px; }
.ui-range .ui-range-marks option { position:absolute; top:0; font-size:14px; font-weight:500; padding:0; text-align:center; }
.range-risk .ui-range option[value='0'] { left:0; color:#003975; }
.range-risk .ui-range option[value='1'] { left:calc(20% - 50px); width:100px; text-align:center; color:#1E66F2; }
.range-risk .ui-range option[value='2'] { left:calc(40% - 50px); width:100px; text-align:center; color:#58B84A; }
.range-risk .ui-range option[value='3'] { left:calc(60% - 50px); width:100px; text-align:center; color:#E9B63E; }
.range-risk .ui-range option[value='4'] { left:calc(80% - 50px); width:100px; text-align:center; color:#F46931; }
.range-risk .ui-range option[value='5'] { right:10px; color:#F2303C; }
.range-return .ui-range option[value='0'] { left:0; color:#003975; }
.range-return .ui-range option[value='1'] { left:calc(16.666% - 50px); width:100px; color:#003975; }
.range-return .ui-range option[value='2'] { left:calc(33.333% - 50px); width:100px; color:#003975; }
.range-return .ui-range option[value='3'] { left:calc(50% - 50px); width:100px; color:#003975; }
.range-return .ui-range option[value='4'] { left:calc(66.666% - 50px); width:100px; color:#F2303C; }
.range-return .ui-range option[value='5'] { left:calc(83.333% - 50px); width:100px; color:#F2303C; }
.range-return .ui-range option[value='6'] { right:0; color:#F2303C; }

/* modal */
.scroll-no { overflow:hidden; /*padding-right:1.7rem;*/ }
.ui-modal.n1 { z-index:201; }
.ui-modal.n2 { z-index:202; }
.ui-modal.n3 { z-index:203; }
.ui-modal.n4 { z-index:204; }
.ui-modal.n5 { z-index:205;}
.ui-modal { display:none; position:fixed; top:0; left:0; z-index:200; width:100%; height:100%; overflow:hidden; overflow-y:hidden; }
.ui-modal.ready { display:flex; overflow-y:hidden; }
.ui-modal.ready .ui-modal-dim { content:""; display:block; position:fixed; top:0; left:0; width:100%; height:100%; opacity:0; background:rgba(0, 0, 0, .5); }
.ui-modal.open .ui-modal-dim{ opacity:1; }
.ui-modal.ps-center.is-over { align-items:flex-start; }
.ui-modal.ps-center.is-over .ui-modal-wrap { margin:20px 0; }
.ui-modal.ps-center { justify-content:center; align-items:center; }
.ui-modal.ps-top { justify-content:center; align-items:flex-start; overflow:hidden; }
.ui-modal.ps-bottom { justify-content:center; align-items:flex-end; overflow:hidden; }
.ui-modal-wrap { position:relative; max-width:100%; opacity:0; z-index:10; background:#fff; border-radius:8px; }

.ui-modal .icon-close { position:absolute; top:0; right:0; width:40px; height:40px; }
.ui-modal .icon-close:after { content:''; position:absolute; display:block; width:16px; height:16px; top:50%; left:50%; margin:-8px 0 0 -8px;  background:url(/kiam/moWeb/resources/images/common/ico_comm.png) no-repeat -60px 0; background-size:500px auto; }
.ui-modal.ps-center .ui-modal-wrap { width:calc(100% - 48px); }
.ui-modal.ps-center .ui-modal-header { padding:24px 24px 0 24px;  }
.ui-modal.ps-center .ui-modal-header h1 { font-size:16px; line-height:24px; font-weight:700; }
.ui-modal.ps-center .ui-modal-body { padding:20px 24px 40px 24px;  }

.ui-modal.ps-bottom .ui-modal-wrap { max-height:calc(100vh - 120px); padding:0 24px; border-radius:24px 0 0 0; }
.ui-modal.ps-bottom .ui-modal-header { position:relative; padding:40px 0 0 0;  }
.ui-modal.ps-bottom .ui-modal-header .icon-close { display:none;  }

.ui-modal.type-full-mobile .ui-modal-wrap { border-radius:0; }
.ui-modal.type-full-mobile .ui-modal-header { position:relative; padding:16px 0 0 0; text-align:center; font-size:16px; font-weight:600; }
.ui-modal.type-full-mobile .ui-modal-header .icon-close { top:18px; right:16px; width:24px; height:24px; background:url(/kiam/moWeb/resources/images/common/ico_common.png) no-repeat -240px -20px; background-size:500px auto;  }
.ui-modal.type-full-mobile .ui-modal-header .icon-close:before,
.ui-modal.type-full-mobile .ui-modal-header .icon-close:after { display:none; }

.ui-modal.open .ui-modal-wrap { opacity:1; }
.type-full .ui-modal-wrap,
.size-mobile .ui-modal.ps-top .ui-modal-wrap,
.size-mobile .ui-modal.ps-bottom .ui-modal-wrap { margin:0; width:100%; }
.ui-modal.ps-top .ui-modal-wrap { margin-top:1rem; }
.ui-modal.ps-bottom .ui-modal-wrap { margin-bottom:1rem; }
.ui-modal.ready.open { animation-name:fadeIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards;}
.ui-modal.ready.close { animation-name:fadeOut; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.type-full.open.ps-center .ui-modal-wrap { animation-name:slideUpIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.type-full.close.ps-center .ui-modal-wrap { animation-name:slideUpOut; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.open.ps-center .ui-modal-wrap { animation-name:scaleIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.close.ps-center .ui-modal-wrap { animation-name:scaleOut; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.close.ps-top .ui-modal-wrap { animation-name:slideDownOut; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.open.ps-top .ui-modal-wrap { animation-name:slideDownIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.close.ps-bottom .ui-modal-wrap { animation-name:slideUpOut; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.open.ps-bottom .ui-modal-wrap { animation-name:slideUpIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards;}

.ui-modal.close.ps-center.type-system .ui-modal-wrap { animation-name:slideDownOut; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.open.ps-center.type-system .ui-modal-wrap { animation-name:slideDownIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ie9 .ui-modal.open { display:block; }
.ie9 .ui-modal.open .ui-modal-wrap { opacity:1; margin:0 auto; transform:translate(0, -50%); top:50% }
.ui-modal.type-system .ui-modal-wrap { width:auto; text-align:center; font-size:14px; }
.ui-modal.type-system .ui-modal-body,
.ui-modal.type-system .ui-modal-footer { max-width:100% !important; background:#6200ee; color:#fff; }
.ui-modal.type-system .ui-modal-wrap strong { font-weight:900; }

.ui-modal-header { position:relative; top:0; left:0; width:100%; min-height:36px; }
.ui-modal-header h1 { position:relative; display:inline-block; font-weight:700; font-size:20px; line-height:30px; }
.ui-modal-header .icon-close:hover:after { background-color:#000; }

.ui-modal-body { position:relative; padding-top:32px; }

/* .ui-modal-body.ui-scrollbar { height:550px !important; } */
.ui-modal-body .ui-scrollbar-item { width:calc(100% - 10px) !important; }


.ui-m .scroll-no {overflow:hidden;padding-right:0;}
.ui-m .ui-modal-wrap {margin:2rem;}
.ui-m .ui-modal.ps-top .ui-modal-wrap {margin-top:0;}
.ui-m .ui-modal.ps-bottom .ui-modal-wrap {margin-bottom:0;}
.ui-m .ui-modal.type-full.ps-center {justify-content:center;align-items:flex-start;}
.ui-m .ui-modal.type-full .ui-modal-wrap {margin:0;width:100%;height:auto;min-height:100%;position:relative;left:0;top:auto;bottom:-10vh;transition:opacity 0.2s ease-in, bottom 0.2s ease-in;overflow:hidden;border-radius:0;opacity:0;}
.ui-m .ui-modal.open.type-full .ui-modal-wrap {bottom:0;opacity:1;top:auto;}
.ui-m .ui-modal-footer {padding:0.8rem 1.6rem;}
.ui-m .ui-modal.ps-top .ui-modal-wrap,.ui-m .ui-modal.ps-bottom .ui-modal-wrap {margin:0;}
.ui-m .ui-modal.open.ps-top .ui-modal-wrap {top:0;}
.ui-m .ui-modal.open.ps-bottom .ui-modal-wrap {bottom:0;}
.ui-m .ui-modal.ps-center.is-over .ui-modal-wrap {margin:0;}
.size-mobile .ps-bottom .ui-modal-wrap,.size-mobile .ps-top .ui-modal-wrap,.size-mobile .type-full-mobile .ui-modal-wrap {margin:0;width:100%;max-width:100%;}



/* ui scroll bar :base */
.ui-scrollbar { position:relative; overflow:auto; }
.ui-scrollbar-item { position:absolute; top:0; left:0; }
.ui-scrollbar.ready.view-y .ui-scrollbar-item.scroll-y-padding { width:calc(100% - 5px) !important; }
