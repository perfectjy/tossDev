/* .fund-detail {margin-top:56px;} */
.hide + .fund-detail { margin-top:0px; }
.fund-detail { margin-top:56px;}
.fund-detail .h3 { margin-bottom: 16px; font-size: 16px; line-height: 24px; font-weight: 700; }
.fund-detail .contents-box strong{ display: block; margin-bottom: 8px; color: #1A399E; font-size: 14px; font-weight: 700;}
.fund-detail .contents-box strong:not(:first-child){ margin-top: 16px; }
.fund-detail .contents-box strong + p{ color:#73737A; font-size: 14px; font-weight: 500; }
.fund-detail .contents-box .info-list { display:flex; position:relative; flex-direction: row; flex-wrap: wrap; border-top: 1px solid #222; }
.fund-detail .contents-box .info-list div { width:50%; padding: 16px 16px 16px 0; border-bottom: 1px solid #DFE0E2; }
.fund-detail .contents-box .info-list div.full { width:100%; }
.fund-detail .contents-box .info-list div dt { margin-bottom: 4px; color:#73737A; font-size:12px; line-height: 18px; font-weight: 400; }
.fund-detail .contents-box .info-list div dd { color:#222; font-size:14px; line-height: 21px; font-weight: 600; }

.btn-box { margin:16px 0 56px 0; text-align: right; }

.hide + .sub-title { margin-top:0; }
.sub-title { position:relative; margin:40px 0 16px 0; }
.sub-title .h3 { font-size:16px; line-height: 24px; font-weight: 700; margin: 0 !important; }
.sub-title .msg { font-size:10px; color:#9D9DA5; }
.sub-title > .msg { position:absolute; top:8px; right:0; }

/* table */
.tbl{ position: relative; width: 100%; border-top: 1px solid #222; background-color: #FFF; }
.tbl thead tr { background:#fff; }
.tbl thead th { padding: 14px 12px; font-size: 14px; font-weight: 600; color: #222; vertical-align: middle; line-height: 1.5; border-bottom: 1px solid #B9BBC6; }
.tbl tbody td { height:53px; padding: 8px 16px; font-size: 12px; font-weight: 500; text-align: center; border-bottom: 1px solid #DFE0E2; vertical-align: middle; line-height: 1.5; word-break: break-all; }
.tbl-scroll { margin: 0 -20px 0px 0; overflow-x: auto;  -webkit-overflow-scrolling: touch; }
.tbl-scroll .tbl { border-collapse:collapse; white-space: nowrap; }
.tbl-nodate{ font-size: 14px; font-weight: 500; padding: 152px 0; text-align: center; }
thead.is-colgroup th[scope="colgroup"]{ padding: 10px; border-bottom:1px solid #DFE0E2; }
thead.is-colgroup tr + tr th{ padding: 10px; }

.symbol { content: ''; display:block; width:16px; height:16px; margin:0 auto; }
.symbol[data-num="1"] { background:url(/kosef/pcWeb/resources/images/common/ico_conts.png) 0 0; }
.symbol[data-num="2"] { background:url(/kosef/pcWeb/resources/images/common/ico_conts.png) -20px 0; }
.symbol[data-num="3"] { background:url(/kosef/pcWeb/resources/images/common/ico_conts.png) -40px 0; }
.symbol[data-num="3/2"] { width:42px; background:url(/kosef/pcWeb/resources/images/common/ico_conts.png) -60px 0; }
.symbol[data-num="4=2-3"] { width:74px; background:url(/kosef/pcWeb/resources/images/common/ico_conts.png) -110px 0; }
.symbol[data-num="4/1"] { width:42px; background:url(/kosef/pcWeb/resources/images/common/ico_conts.png) -190px 0; }

.ui-acco { margin-top:56px !important; }

/*text star*/
.txt-list.star, .txt-list.star:not(ol):not(ul){ margin-top:8px; color: #73737A; }
.txt-list.star li{ position: relative; padding-left:10px; font-size: 14px; line-height: 18px; }
.txt-list.star li:not(:first-child) { margin-top:4px; }
.txt-list.star li::before, .txt-list.star:not(ol):not(ul)::before{ content:'*'; position:absolute; top: 2px; left: 0; }

