/* ui scroll bar :base */
.ui-scrollbar { position:relative; overflow:auto; }
.ui-scrollbar-item { position:absolute; top:0; left:0; }
.ui-scrollbar.ready.view-y .ui-scrollbar-item.scroll-y-padding { width:calc(100% - 5px) !important; }
.ui-scrollbar.ready.view-x .ui-scrollbar-item.scroll-x-padding { height:calc(100% - 5px) !important; }
.ui-scrollbar.ready { overflow:hidden !important; /* padding:0 !important; */ }
.ui-scrollbar.ready>.ui-scrollbar-item { position:relative; width:100%; height:100% !important; overflow:scroll !important; }
.ui-scrollbar.ready>.ui-scrollbar-item { -ms-overflow-style:none; scrollbar-width:none; }
.ui-scrollbar.ready>.ui-scrollbar-item::-webkit-scrollbar { display:none; }
.ui-scrollbar.ready .ui-scrollbar-item::-webkit-scrollbar,
.ui-scrollbar.ready .ui-scrollbar::-webkit-scrollbar { width:0; height:0; }
.ie .ui-scrollbar.ready>.ui-scrollbar-item { width:calc(100% + 17px) !important; height:calc(100% + 5px) !important; }
.ui-scrollbar-barwrap { opacity:0; }
.ui-scrollbar-barwrap.type-y { display:none; position:absolute; top:0; right:0; width:5px; height:100%; background:rgba(255, 255, 255, 0.1); z-index:100; border-top: 0.2px solid transparent; border-bottom: 2px solid transparent;}
.ui-scrollbar-barwrap.type-x { display:none; position:absolute; bottom:0; left:0; width:100%; height:10px; background:rgba(255, 255, 255, 0.1); z-index:100; border-left: 2px solid transparent; border-right: 2px solid transparent;}
.type-y .ui-scrollbar-bar { position:absolute; top:0; right:0; width:5px; margin:0; height:20px; min-height: 20px; opacity:0.5; border-radius:2px; background:#E0E0E0; }
.type-x .ui-scrollbar-bar { position:absolute; top:2px; left:0; width:20px; min-width: 20px; margin:0; height:6px; opacity:1; border-radius:2px; opacity:0.5; background:#E0E0E0; }
.ui-scrollbar:hover .ui-scrollbar-bar { opacity:1; box-shadow:0 0 5px rgba(0,0,0,0.1); }
.view-scrollbar>.ui-scrollbar-barwrap { animation-name:fadeIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.view-y>.ui-scrollbar-barwrap.type-y { display:block; } 
.view-x>.ui-scrollbar-barwrap.type-x { display:block; }

/* drop box */
.ui-drop-wrap {}
.ui-drop-wrap > div { opacity:0; transition:all 0.33s cubic-bezier(0.685, 0.0473, 0.346, 1) 0s; }
.ui-drop-wrap .ui-drop-box { display:none; }
.ui-drop-wrap.on .ui-drop-box { display:block; }
.ui-drop-wrap.on > div { opacity:1; transition:all 0.33s cubic-bezier(0.685, 0.0473, 0.346, 1) 0s; }

