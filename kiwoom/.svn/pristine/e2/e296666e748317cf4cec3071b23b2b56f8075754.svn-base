@charset "utf-8";

/* common */
* { box-sizing:border-box; padding:0; margin:0; line-height:1; }
*:after,
*:before { box-sizing:border-box; }
html, body { background:#fff; user-select:none; -webkit-user-select:none; -ms-user-select:none; -webkit-user-select:none; -webkit-tap-highlight-color:transparent; }
body { padding:0; margin:0; font-family:"Pretendard", Sans-serif; font-size:14px; font-weight:500; color:#222; word-break:break-all; letter-spacing:0; }

h1, h2, h3, h4, h5, h6 { font-weight:400; color:#222; }
a { text-decoration:none; outline-offset:1px; color:inherit; }
a:hover { text-decoration:none; }
p { line-height:1.5; }
dl, ol, ul, dt, dd, li { display:block; list-style:none; }
table { width:100%; border-collapse:collapse; border-spacing:0; table-layout:fixed; }
caption { height:0; opacity:0; }
th, td { border-collapse:collapse; vertical-align:middle; }
button{overflow:visible}
button{margin:0;padding:0;border:0;background:none;width:auto;cursor:pointer;overflow:visible;vertical-align:middle;background:transparent;cursor:pointer; font-family:"Pretendard", Sans-serif; }
button span, button:active span, button:focus span{position:relative}
select, textarea, input { -webkit-appearance:none; border:0; }
select { height:40px; padding:0 10px 0 10px; }
input { border:0; }
em, i { font-style:normal; text-decoration:none;}
hr { position:relative; display:block; height:1px; border:0; background:#eee; margin:30px 0; }
img { border:0; }
.hanja { font-family:'맑은 고딕', '돋움', '굴림'; }

.lock {height:100%; overflow:hidden;}

/* margin only top */
.mgt0 { margin-top:0 !important; }
.mgt10 { margin-top:10px !important; }
.mgt15 { margin-top:15px !important; }
.mgt20 { margin-top:20px !important; }
.mgt25 { margin-top:25px !important; }
.mgt30 { margin-top:30px !important; }
.mgt40 { margin-top:40px !important; }
.mgt50 { margin-top:50px !important; }
.mgt80 { margin-top:80px !important; }
.mgt100 { margin-top:100px !important; }

/* input box block type */
[class*="inp-"] { position:relative; display:inline-block; width:100%; height:50px; font-size:15px; color:#222; line-height:1; padding:12px 15px; border:1px #bdbdbd solid; background:#fff; font-family:"Pretendard", Sans-serif; outline:0; -webkit-appearance:none; -webkit-border-radius:0; box-sizing:border-box;   font-weight: 600;}
[class*="inp-"]:focus { border-color:#0091ea; }
[class*="inp-"]::placeholder { font-family:"Pretendard", Sans-serif; color:#B9BBC6 ; }
[class*="inp-"]::-webkit-input-placeholder { font-family:"Pretendard", Sans-serif; color:#B9BBC6 ; }
[class*="inp-"]:-ms-input-placeholder { font-family:"Pretendard", Sans-serif; color:#B9BBC6 ; }
[class*="inp-"]:focus::placeholder { color:transparent; }
[class*="inp-"]:focus::-webkit-input-placeholder { color:transparent; }
[class*="inp-"]:focus:-ms-input-placeholder { color:transparent; }
[class*="inp-"][readonly]:focus { border-color:#e5e5e5; }
[class*="inp-"][disabled],
[class*="inp-"][readonly] { background:#f2f2f2; border-color:#e5e5e5; }
[class*="inp-"][readonly]::placeholder,
[class*="inp-"][disabled]::placeholder { color:#9e9e9e; }
[class*="inp-"][readonly]::-webkit-input-placeholder,
[class*="inp-"][disabled]::-webkit-input-placeholder { color:#9e9e9e; }
[class*="inp-"][readonly]:-ms-input-placeholder,
[class*="inp-"][disabled]:-ms-input-placeholder { color:#9e9e9e; }
[class*="inp-"][type="password"] { -webkit-text-security:disc; }
[class*="inp-"]::-ms-clear { display:none; }
textarea[class*="inp-"] { display:block; max-height:200px; padding-top:14px; line-height:22px; overflow-y:hidden; border-color:1px #e0e0e0 solid; }
textarea[class*="inp-"]::placeholder { line-height:22px; }
/* textarea[class*="inp-"]:focus { padding:12px 15px 13px; } */
.ui-input-txt { display:inline-block; position:relative; }
.ui-input-txt .btn-clear { position:absolute; top:50%; right:10px; margin-top:-12px;  }
.btn-clear { display:inline-block; width:24px; height:24px; right:20px; background:url(/kiam/moWeb/resources/images/common/ico_comm.png) no-repeat -120px -20px; background-size:500px auto; }

.inp-date { display:inline-block; width:auto; height:auto; font-family:"Pretendard", Sans-serif; font-size:14px; font-weight:600; padding:0;  }
.inp-date::-webkit-calendar-picker-indicator { border:1px solid red; } 

label { position:relative; display:inline-block; font-size:14px; }

input[type="radio"],
input[type="checkbox"] { position:absolute; bottom:0; left:0; width:0; height:0; opacity:0; }
input[type="password"] { font-family:Arial, sans-serif; }

/* headline style */
.h2 { font-weight:700; font-size:40px; line-height:56px; }
.h3 { margin:80px 0 25px 0; font-weight:700; font-size:20px; line-height:30px; }
.h4 { margin:25px 0 15px 0; font-weight:700; font-size:16px; line-height:24px;  }
.h4-dot { margin:40px 0 16px 0; font-weight:700; }
h3.dot,
h2.dot,
.h4-dot { position:relative; padding-left:10px; font-weight:700; font-size:24px; line-height:36px;  }
h2.dot:before,
h3.dot:before,
.h4-dot:before { content:''; position:absolute; top:6px; left:0; width:8px; height:8px; background:#EC008C; border-radius:50%;  }
.h4-dot { font-size:16px; line-height:24px;  }
.h4-dot:before { width:4px; height:4px; }

.h3-sub { position:relative; margin:56px 0 16px 0; color:#222; font-size:16px; line-height: 24px; font-weight: 700; }
.h3-sub.dot { padding-left: 8px; font-size:16px; line-height:24px; font-weight:700;}
.h3-sub.dot::before {content:''; position: absolute; left:0; top:8px; width: 4px; height:4px; border-radius: 50%; background:#EC008C;}

/* font color */
.fc-primary { color:#EC008C !important; }
.fc-secondary { color:#2041bf !important; }
.fc-sub { color:#757575 !important; }
.fc-warn { color:#f9a825 !important; }
.fc-bad { color:#d32f2f !important; }
.fc-good { color:#4caf50 !important; }
.fc-black { color:#000000 !important; }

/* keep-all */
.keep-word { word-break:keep-all !important; }

/* a */
a.link { color:#003cff; text-decoration:underline; }

/* hide/hidden style */
.a11y-hidden,
.hide  { position: absolute !important; width: 1px !important; height: 1px !important; color: transparent !important; overflow: hidden !important; clip: rect(1px, 1px, 1px, 1px); -webkit-clip-path: inset(50%); clip-path: inset(50%); margin: 0 !important; padding: 0 !important; }

/* float */
.fclear::after { content:''; display:block; clear:both; }
.f-left { float:left !important; }
.f-right { float:right !important; }

.ellips {  overflow:hidden; display:block; text-overflow:ellipsis; white-space:nowrap; } /* 한줄 처리 */

hr.sub-line { margin:24px 0; height:10px; background:#F2F3F5; }
.contents hr.sub-line { margin-left:-24px; margin-right:-24px; }

/* scroll box */ 
.scroll-box { }
.scroll-box .scroll-list { overflow:hidden; overflow-x:auto; webkit-overflow-scrolling:touch;  white-space:nowrap; }
.scroll-box .scroll-list::-webkit-scrollbar {display:none;}

/* button */
.btn { display:inline-block; font-size:16px; text-align:center; color:#222; }
.btn.primary { min-width:240px; font-size:20px; font-weight:600; color:#fff; line-height:64px; background-color:#EC008C; }
.btn.base { min-width:160px; line-height:46px; font-weight:600; font-size:16px; border:1px solid #222; }
.btn.base.primary { min-width:160px; background:none; border-color:#EC008C; color:#EC008C; }
.btn.base.cancel { color:#74757E; border-color:#74757E; }


/* mo button sticky*/
.btn-area{ display: flex; flex-direction: row; flex-wrap: nowrap; }
.btn-area.sticky{ position: fixed; left: 0; right: 0; bottom: 0; margin: 0; z-index: 99; }
.btn-area.sticky [class^=btn-mo-] { flex-grow:1; width: 50%; min-width: auto; min-height: 64px; font-size: 16px; font-weight: 600; }
.btn-area.sticky .btn-mo-agree{ color: #FFF; background-color: #1A399E;}
.btn-area.sticky .btn-mo-agree:active{ background-color: #1D3480; }
.btn-area.sticky .btn-mo-cancel{ color: #B9BBC6; border-top: 1px solid #DFE0E2; background-color: #FFF;}
.btn-area.sticky .btn-mo-cancel:active{ color: #73737A; }

/*buttons*/
.btn { width: 100%; line-height: inherit !important; }/*차후지움*/
.btn { display:inline-flex; justify-content: center; align-items: center; min-width: 100%; min-height: 48px; padding: 0 40px; font-size: 16px; border-radius: 32px; background-color:#FFF; text-align: center;}
.btn:disabled { color: #B9BBC6 !important; border: 1px solid #DFE0E2 !important; background-color: #DFE0E2 !important; cursor: default; }
.btn:disabled:active { color: #B9BBC6; border: 1px solid #DFE0E2; background-color: #DFE0E2; }
.btn.primary { color: #EC008C; font-weight: 600; border: 1px solid #EC008C; background-color: #FFF;}
.btn.primary:active { color: #FFF; background-color: #EC008C; }
.btn.secondary { color: #222; border: 1px solid #222; }
.btn.secondary:active { background-color: #F7F8F8; }
.btn.tertiary { color: #9D9DA5; border: 1px solid #9D9DA5;}
.btn.tertiary:active { background-color: #F7F8F8; }
.btn.fill { color: #FFF; background-color: #EC008C;}
.btn.fill:active { background-color: #C02882; }
.btn.lg { min-width: 200px; min-height: 56px; font-size: 20px; }

.btn-icon{ display:inline-block; min-height:40px; padding:0 24px; border: 1px solid #222; border-radius:40px;}
.btn-icon span{ position:relative; display:inline-block; font-size:12px; font-weight:600px; }
.btn-icon span::after{ content: ''; position:absolute; top:0; right:0; width:16px; height:16px; background: url('../images/common/ico_comm.png') -120px 0 no-repeat; background-size: 500px 500px;}
.btn-icon.box { border-radius:8px; }

.btn-icon.go { background:#EC008C; border-color:#EC008C; color:#fff; }
.btn-icon.go span:after { background-position-x:-410px;}

.btn-icon.icon-link { min-width:auto; min-height:auto; padding:0 20px; }
.btn-icon.icon-link span { position:relative; padding:6px 20px 6px 8px; line-height:18px; }
.btn-icon.icon-link span:before { content:''; display:block; position:absolute; top:10px; left:0;  width:4px; height:4px; border-radius:50%; background:#EC008C; }
.btn-icon.icon-link span:after { display:block; position:absolute; top:7px; right:0; width:16px; height:16px; background-position:-40px 0;  }
.btn-icon.icon-dw { padding:0 10px; }
.btn-icon.icon-dw span { position:relative; padding:10px 20px 0 0;  line-height:18px; }
.btn-icon.icon-dw span:after { content:''; display:block; position:absolute; top:11px; right:0; width:16px; height:16px; background-position:-260px 0; }
.btn-icon.icon-go { padding:0 14px 0 16px; }
.btn-icon.icon-go span { position:relative; padding:10px 16px 0 0; line-height:18px; }
.btn-icon.icon-go span:after { content:''; display:block; position:absolute; top:11px; right:0; width:16px; height:16px; background-position:-40px 0; }

.btn-sns { display:inline-block; width:20px; height:20px; background:url(/kiam/moWeb/resources/images/common/ico_comm.png) no-repeat -450px -20px; background-size:500px auto; }

.btn-group { margin-top:30px; text-align:center; line-height:0; font-size:0; } 
.btn-group button { margin:0 5px; }
.btn-group.btn-r { margin-top:16px;  text-align:right; }

.btn-txt { display:inline-block; position:relative; font-size:12px; line-height:18px; padding-right:16px; color:#73737A; }
.btn-txt:after { content:''; position:absolute; background-image:url(/kiam/moWeb/resources/images/common/ico_comm.png); background-repeat:no-repeat ; background-size:500px auto;  }
.btn-txt.link:after { top:1px; right:0; width:16px; height:16px; background-position:-40px 0; opacity:0.5;   }
.btn-txt:hover { color:#222; font-weight:500;  }
.btn-txt.link { color:#73737A; text-decoration:none;}
.btn-txt.link:hover:after { opacity:1; }
.btn-txt.reset { padding-right:24px; }
.btn-txt.reset:after { top:3px; right:0; width:16px; height:16px; background-position:-320px 0; opacity:0.5;  }
.btn-txt.reset:hover:after { opacity:1; }
.btn-txt.down { padding-right:24px; font-size:16px; line-height:24px; }
.btn-txt.down:after { top:4px; right:0; width:16px; height:16px; background-position:-300px 0; opacity:0.5;  }
.btn-txt.down:hover:after { opacity:1; }

.btn-sort { position:relative; display:inline-block; padding-right:20px; line-height:24px; color:#73737A; }
.btn-sort:after { content:''; position:absolute; top:3px; right:0; width:16px; height:16px; opacity:0.7; background:url(/kiam/moWeb/resources/images/common/ico_comm.png) no-repeat -300px 0; background-size:500px auto;   }

.btn-txt-arrow { display:inline-block; position:relative; padding-right:20px; color:#73737A; font-size:14px; line-height:21px; }
.btn-txt-arrow:after { content:""; position:absolute; top:2px; right:0; width:16px; height:16px; opacity:0.5; background:url(/kiam/pcWeb/resources/images/common/ico_comm.png) no-repeat -40px 0; }

.btn-search { display:block; width:100%; height:40px; border:1px solid #222; border-radius:8px; }
.btn-search span { line-height:18px; font-size:12px;  }

.btn-more{ width: 100%; background-color: #FFF;}
.btn-more > span { position:relative; display:inline-block; color: #9D9DA5; font-size:12px; line-height:18px; padding:16px 20px 16px 0; }
.btn-more > span:after { content:''; position:absolute; top:50%; right:0; transform:translateY(-50%);  width:16px; height:16px; opacity:0.5; background:url(/kiam/moWeb/resources/images/common/ico_comm.png) no-repeat 0 0; background-size:500px auto;   }

.btn-unline span { padding-left:2px; color:#222; font-size:12px; font-weight: 500; }
.btn-unline.icon-dw::after { content:''; position:relative; top:4px; display:inline-block; width:16px; height:16px; margin-left:2px; background:url(/kiam/moWeb/resources/images/common/ico_comm.png) -280px 0; background-size: 500px auto; }

.btn-group.flex { display:flex; margin-left:-4px; margin-right:-4px;  }
.btn-group.flex a { flex:1 1; margin:0 4px;  }

.btn-noline{padding: 4px;}
.btn-noline span{ display: inline-flex; align-items: center; color: #222; font-size: 14px; font-weight: 500;}
.btn-noline.dw span::after{ content: ''; display: inline-block; width: 16px; height: 16px; margin-left: 4px; background: url('../images/common/ico_comm.png') -260px 0 no-repeat; background-size: 500px 500px;}

.btn-right {text-align:right;}
.btn.sub {display:inline-block; width:auto; min-width:80px; min-height:40px; padding:0 20px; font-weight:600; border-radius:8px; font-size:12px; line-height:38px; text-align:center; border:1px solid #222; color:#222;}

/* sns */
.sns-gruop { text-align:center; }
.sns-gruop a { position:relative; display:inline-block; margin:0 20px; padding-top:48px; min-width:40px; line-height:21px; }
.sns-gruop a:after { content:''; position:absolute; top:0; left:50%; margin-left:-20px; width:40px; height:40px; background:url(/kiam/moWeb/resources/images/common/ico_comm.png) no-repeat -310px -120px; background-size:500px auto;  }
.sns-gruop a.sns-face:after { background-position-x:-360px; }
.sns-gruop a.sns-url:after { background-position-x:-410px; }
 
/* skip ********************************************************/
#skipNavi{position:absolute;width:100%;margin:0 auto;z-index:999}
#skipNavi a{text-indent:0;background:#000;position:absolute;z-index:1;top:-500px;width:100%;padding:5px 0;text-align:center}
#skipNavi a:active,
#skipNavi a:focus{top:0;color:#fff}

.fund-flags { line-height:0; font-size:0; }
.fund-flags span { display:inline-block; height:20px; padding:4px 8px 0 8px; font-size:10px; border:1px solid #1A399E; color:#1A399E; border-radius:4px 0px 4px 4px; }
.fund-flags span + span { margin-left:4px; }
.fund-flags span.high { color:#EC008C; border-color:#EC008C; }

/* tab */
.ui-tab-pnl, .ui-tab-pnl[aria-hidden=true] { display:none; visibility:hidden; opacity:0; }
.ui-tab-pnl.selected,
.ui-tab-pnl[aria-hidden='false'] { display:block; visibility:visible; position:relative; opacity:1; z-index:1; transition:opacity 0.3s linear 2s; }

/* list */
.txt-list{ text-align:left; }
.txt-list > li,
.txt-list:not(ol):not(ul){ position:relative; font-size:14px; font-weight:500; margin-top:8px; padding-left:6px; line-height:21px; color:#74757E;  }
.txt-list > li:first-child{ margin-top:0 !important; }
.txt-list > li::before,
.txt-list:not(ol):not(ul)::before{ position: absolute; }

.txt-list.disc li::before, .txt-list.disc:not(ol):not(ul)::before{ content:''; top:8px; left: 0; width:2px; height:2px; border-radius:100%; background-color:#73737A; }

.txt-list.dot li:first-child { margin:0; }
.txt-list.dot li { margin-top:4px; padding-left:6px; font-weight:500; font-size:14px; color:#74757E; line-height:21px;  }
.txt-list.dot li:after { content:''; display:block; position:absolute; top:8px; left:0; width:2px; height:2px; background:#74757E; }

.txt-list.star,
.txt-list.star:not(ol):not(ul){ color: #73737A; }
.txt-list.star li{ font-size: 14px;}
.txt-list.star li::before, .txt-list.star:not(ol):not(ul)::before{ content:'*'; top: 2px; left: 0;}

.txt-lineh{ line-height: 24px; }


/* .ui-list li { position:relative; margin-top:4px; padding-left:6px; font-size:12px; line-height:18px; color:#74757E; }
.ui-list li:before { content:''; display:block; position:absolute; top:8px; left:0; width:2px; height:2px; background:#73737A; border-radius:50%; } */

dl.info-list { display:flex; position:relative; flex-direction: row; flex-wrap: wrap; border-top: 1px solid #222; }
dl.info-list > div { position:relative;  width:50%; padding: 16px 16px 16px 0; border-bottom: 1px solid #DFE0E2; }
dl.info-list > div.full { width:100%; }
dl.info-list > div.full:before { content:''; display:block; position:absolute; top:-1px; left:0; right:0; height:1px; background:#DFE0E2; }
dl.info-list > div dt { margin-bottom: 4px; color:#73737A; font-size:12px; line-height:21px;}
dl.info-list > div dd { color:#222; font-size:14px; line-height: 21px; font-weight: 600; }
dl.info-list:after { content:''; position:absolute; bottom:0; left:0; right:0; height:1px; background:#DFE0E2; }

/* inputs */
.lb-item { position:relative; display:inline-block; margin-right:8px; box-sizing:border-box; white-space:nowrap; vertical-align:middle; }
.lb-radio,
.lb-checkbox { position:relative; display:inline-flex; vertical-align:top; align-items:center; min-height:24px; padding:0 0 0 32px; margin:0; cursor:pointer; line-height:1; }
.lb-radio::before{ content:""; display:block; flex:0; width:24px; height:24px; background-color:#fff; position:absolute; top:0; left:0; background:url(/kiam/moWeb/resources/images/common/ico_comm.png) no-repeat -150px -300px; background-size:500px auto; }
.lb-checkbox::before { content:""; display:block; flex:0; position:absolute; top:0; left:0; width:24px; height:24px; background:url(/kiam/moWeb/resources/images/common/ico_comm.png) no-repeat 0 -300px; background-size:500px auto;}
.lb-checkbox.no-label.checked:before {background-position-x:-30px;}
.lb-radio:hover:before {background-position-x:-210px;}
.lb-checkbox:hover:disabled:before {background-position-x:-90px;}
input:checked + .lb-checkbox::before {background-position-x:-30px;}
input:checked + .lb-radio::before {background-position-x:-180px;}
input:disabled + .lb-checkbox:before {background-position-x:-90px;}
input:disabled + .lb-radio:before {background-position-x:-240px;}
input:checked:disabled + .lb-checkbox:before {background-position-x:-120px;}
input:checked:disabled + .lb-radio:before {background-position-x:-270px;}
.lb-checkbox.no-label { padding-left:24px; }
.lb-checkbox.no-label.checked { padding-left:24px; }

.tab-tog { position:relative; }
.tab-tog .lb-txts { position:absolute; top:4px; left:50px; color:#222; }
.tab-tog .tab-chkbox { display:inline-block; position:relative; width:auto; min-width:44px; height:24px; cursor:pointer; box-sizing:border-box; vertical-align:top;}
.tab-tog input + .tab-chkbox:before, 
.tab-tog input + .tab-chkbox:after { content:''; display:block; position:absolute; }
.tab-tog input + .tab-chkbox:before { left:0; top:0; width:100%; height:24px; background:#d7dbe1; border-radius:24px; transition:all 0.2s ease-in-out; box-sizing:border-box; }
.tab-tog input + .tab-chkbox:after { left:2px; top:2px; width:20px; height:20px; background:url(/kiam/moWeb/resources/images/common/ico_comm.png) no-repeat -120px -50px; background-size:500px auto;  }
.tab-tog input:checked + .tab-chkbox:after { left:calc(100% - 22px); background-position-x:-150px; }
.tab-tog input:checked + .tab-chkbox:before { background:#EC008C; }

select[class], textarea[class], input[class] { font-family:"Pretendard", Sans-serif; appearance:none; -moz-appearance:none; -webkit-appearance:none; -webkit-border-radius:0; }
select { font-family:"Pretendard", Sans-serif; width:100%; height:24px; line-height:18px; padding: 0 20px 0 0; color:#73737A; font-size:14px; background:url(/kiam/moWeb/resources/images/common/sel-sub.png) no-repeat 100% center; background-size:16px auto; }
.ui-select select { width:auto; }

/* range */
.ui-range { width:100%; height:30px; padding:10px 0; display:flex; flex-direction:column; justify-content:flex-start; align-items:center; position:relative; -webkit-user-select:none; -moz-user-select:none; -ms-user-select:none; user-select:none; }
.ui-range.same .ui-range-point {pointer-events:none !important;}
.ui-range[data-type=single] .ui-range-bar { right:auto; }
.ui-range[data-type=single] .ui-range-point::before { content:"code" !important; }
.ui-range[data-orient=vertical] { transform-origin:5rem 5rem; transform:rotate(-90deg); margin-left:6rem;}
.ui-range[data-orient=vertical] .ui-range-point { justify-content:center; }
