@charset "utf-8";

@keyframes fadeIn {
    0% {
        opacity: 0;
    }

    100% {
        opacity: 1;
    }
}

@keyframes fadeOut {
    0% {
        opacity: 1;
    }

    100% {
        opacity: 0;
        visibility: hidden;
    }
}

@keyframes scaleIn {
    0% {
        opacity: 0;
        transform: scale(0.9);
    }

    100% {
        opacity: 1;
        transform: scale(1);
    }
}

@keyframes scaleOut {
    0% {
        opacity: 1;
        transform: scale(1);
    }

    100% {
        opacity: 0;
        transform: scale(0.9);
        visibility: hidden;
    }
}

@keyframes slideDownIn {
    0% {
        display: block;
        opacity: 0;
    }

    1% {
        top: -10rem;
        opacity: 0;
    }

    100% {
        top: 0;
        opacity: 1;
    }
}

@keyframes slideDownOut {
    0% {
        top: 0;
        opacity: 1;
    }

    99% {
        top: -10rem;
        opacity: 0;
    }

    100% {
        display: none;
        opacity: 0;
    }
}

@keyframes slideUpIn {
    0% {
        display: block;
        opacity: 0;
        z-index: 0;
        top: auto;
    }

    1% {
        bottom: -10rem;
        opacity: 0;
        top: auto;
    }

    100% {
        bottom: 0;
        opacity: 1;
        top: auto;
    }
}

@keyframes slideUpOut {
    0% {
        bottom: 0;
        opacity: 1;
    }

    99% {
        bottom: -10rem;
        opacity: 0;
    }

    100% {
        opacity: 0;
        bottom: -10rem;
        visibility: hidden;
    }
}

@keyframes slideLeftIn {
    0% {
        display: block;
    }

    1% {
        left: -10rem;
        opacity: 0;
    }

    100% {
        left: 0;
        opacity: 1;
    }
}

@keyframes slideLeftOut {
    0% {
        left: 0;
        opacity: 1;
    }

    99% {
        left: -10rem;
        opacity: 0;
    }

    100% {
        left: -10rem;
        opacity: 0;
        visibility: hidden;
    }
}

@keyframes slideRightIn {
    0% {
        display: block;
    }

    1% {
        right: -10rem;
        opacity: 0;
    }

    100% {
        right: 0;
        opacity: 1;
    }
}

@keyframes slideRightOut {
    0% {
        right: 0;
        opacity: 1;
    }

    99% {
        right: -10rem;
        opacity: 0;
    }

    100% {
        right: -10rem;
        opacity: 0;
        visibility: hidden;
    }
}

@keyframes clockAni {
    0% {
        transform: rotate(0)
    }

    100% {
        transform: rotate(360deg)
    }
}

@keyframes orbitLeftAni {
    0% {
        left: 0;
        z-index: 2;
        opacity: 1;
    }

    50% {
        left: calc(100% - 20px);
        z-index: 2;
        opacity: 1;
    }

    51% {
        left: calc(100% - 20px);
        z-index: 1;
        opacity: 1;
    }

    100% {
        left: 0;
        z-index: 1;
        opacity: 1;
    }
}

@keyframes orbitRightAni {
    0% {
        left: calc(100% - 20px);
        z-index: 1;
        opacity: 1;
    }

    50% {
        left: 0;
        z-index: 1;
        opacity: 1;
    }

    51% {
        left: 0;
        z-index: 2;
        opacity: 1;
    }

    100% {
        left: calc(100% - 20px);
        z-index: 2;
        opacity: 1;
    }
}

@keyframes orbitScaleShowAni {
    0% {
        width: 0;
        height: 0;
        ;
    }

    100% {
        width: 150vmax;
        height: 150vmax;
    }
}

@keyframes orbitScaleHideAni {
    0% {
        width: 150vmax;
        height: 150vmax;
    }

    100% {
        width: 0;
        height: 0;
        ;
    }

}

@keyframes fadeInTop {
    0% {
        opacity: 0;
        bottom: -4rem;
    }

    100% {
        opacity: 1;
        bottom: 0;
    }
}

@keyframes fadeOutTop {
    0% {
        opacity: 1;
        bottom: 0;
    }
    99% {
        opacity: 0;
        bottom: -4rem;
    }
    100% {
        opacity: 0;
        bottom: -4rem;
        visibility: hidden;
    }
}

@keyframes datepickerUp {
    0% {
        display: block;
        z-index: 0;
    }

    1% {
        bottom: -100vw;
        opacity: 0;
    }

    100% {
        bottom: 0;
        opacity: 1;
    }
}

@keyframes datepickerDown {
    0% {
        bottom: 0;
        opacity: 1;
    }

    99% {
        bottom: -100vw;
        opacity: 0;
    }

    100% {
        bottom: -100vw;
        opacity: 0;
        visibility: hidden;
    }
}



/* ui-loading */
.ui-loading {
    display: none;
    position: fixed;
    z-index: 100;
    width: 100%;
    height: 100%;
    opacity: 0;
    top: 0;
    left: 0;
    overflow: hidden;

    animation-name: fadeOut;
    animation-duration: .5s;
    animation-iteration-count: 1;
    animation-timing-function: linear;
    animation-fill-mode: forwards;

    &.type-area{
        position: absolute;
    }
    &.type-area::after{
        position: absolute !important;
    }
    &.visible {
        display: block;
        animation-name: fadeIn;
        animation-duration: .5s;
        animation-iteration-count: 1;
        animation-timing-function: linear;
        animation-fill-mode: forwards;
    }

    .ui-loading-message {
        font-size: 14px;
        color: #fff;
        width: 120px;
        position: absolute;
        left: 50%;
        bottom: -30px;
        margin-left: -60px;
        text-align: center;
    
        span {
            display: block;
            padding: 2px;
        }
    }
}
/* ui-loading : clock */
.ui-loading.clock{
    &::after {
        content: "";
        display: block;
        position: fixed;
        z-index: 0;
        width: 100%;
        height: 100%;
        opacity: 1;
        top: 0;
        left: 0;
        background: rgba(0, 0, 0, 0.5);

        animation-name: fadeIn;
        animation-duration: 0.5s;
        animation-iteration-count: 1;
        animation-timing-function: ease-in-out;
        animation-fill-mode: forwards;
    }
    &.close::after {
        animation-name: fadeOut;
        animation-duration: 0.25s;
        animation-iteration-count: 1;
        animation-timing-function: ease-in;
        animation-fill-mode: forwards;
    }

    .ui-loading-wrap {
        position: absolute;
        z-index: 1;
        width: 80px;
        height: 80px;
        top: 50%;
        left: 50%;
        margin: -40px 0 0 -40px;
        border-radius: 50%;
        border: 1px solid rgba(255, 255, 255, 0.5);
        background: rgba(0, 0, 0, 0.1);
        box-shadow: 0 0 8px rgba(0, 0, 0, 0.2);
    
        &::after {
            content: "";
            display: block;
            position: absolute;
            z-index: 1;
            left: 50%;
            top: 40px;
            margin-left: -20px;
            border-radius: 1px;
            width: 40px;
            height: 1px;
            background: linear-gradient(to right, rgba(255, 255, 255, 0) 0%, rgba(255, 255, 255, 0) 50%, rgba(255, 255, 255, 1) 100%);

            animation-name: clockAni;
            animation-duration: 12s;
            animation-iteration-count: infinite;
            animation-timing-function: linear;
        }

        &::before {
            content: "";
            display: block;
            position: absolute;
            z-index: 1;
            left: 50%;
            top: 40px;
            margin-left: -30px;
            border-radius: 1px;
            width: 60px;
            height: 1px;
            background: linear-gradient(to right, rgba(255, 255, 255, 0) 0%, rgba(255, 255, 255, 0) 50%, rgba(255, 255, 255, 1) 100%);

            animation-name: clockAni;
            animation-duration: 1.2s;
            animation-iteration-count: infinite;
            animation-timing-function: linear;
        }
    }
}
/* ui-loading : orbit */
.ui-loading.orbit {
    &::after {
        content: "";
        display: block;
        position: absolute;
        ;
        z-index: 0;
        width: 0;
        height: 0;
        top: 50%;
        left: 50%;
        background: rgba(0, 0, 0, 0.5);
        transform: translate(-50%, -50%);
        border-radius: 50%;

        animation-name: orbitScaleShowAni;
        animation-duration: 0.5s;
        animation-iteration-count: 1;
        animation-timing-function: ease-in-out;
        animation-fill-mode: forwards;
    }
    &.close:after {
        animation-name: orbitScaleHideAni;
        animation-duration: 0.25s;
        animation-iteration-count: 1;
        animation-timing-function: ease-in;
        animation-fill-mode: forwards;
    }

    .ui-loading-wrap {
        position: absolute;
        width: 80px;
        height: 80px;
        top: 50%;
        left: 50%;
        margin: -40px 0 0 -40px;
        z-index: 1;

        &::after {
            content: "";
            display: block;
            position: absolute;
            left: 0;
            top: calc(50% - 10px);
            border-radius: 50%;
            width: 20px;
            height: 20px;
            background: #F5F5F5;

            animation-name: orbitLeftAni;
            animation-duration: 1.5s;
            animation-iteration-count: infinite;
            animation-timing-function: ease-in-out;
            animation-direction: reverse;
        }
        &::before {
            content: "";
            display: block;
            position: absolute;
            right: 0;
            top: calc(50% - 10px);
            border-radius: 50%;
            width: 20px;
            height: 20px;
            background: #212121;

            animation-name: orbitRightAni;
            animation-duration: 1.5s;
            animation-iteration-count: infinite;
            animation-timing-function: ease-in-out;
            animation-direction: reverse;
        }
        
    }
}




/* ui-acco */
.ui-acco {
    position: relative;
    margin: 0;
    &::after{
        content: "";
        display: block;
        width: 100%;
        height: 1px;
        position: absolute;
        bottom:0;
        left:0;
        border-bottom: 1px solid $gray400;
    }

    .ui-acco-wrap {
        position: relative;
        width: 100%;
        padding: 0;
        margin: 0;
    }
    
    .ui-acco-tit {
        position: relative;
        display: flex;
        justify-content: space-between;
        align-items: center;
        width: 100%;
        height: 100%;
        margin: -1px 0 0;
        padding: 0;
        min-height: $x6;
        border-top: 1px solid $gray400;
        border-bottom: 1px solid $gray400;

        background: $white;
    }
    .ui-acco-txt {
        position: relative;
        padding: $x1 $x5 $x1 $x1;
        z-index:2;

        & + .ui-acco-btn{
            position: absolute;
        }
    }
    .ui-acco-btn {
        cursor: pointer;
        position: relative;
        z-index: 1;
        top: 0;
        right: 0;
        width: 100%;
        height: 100%;
        padding: $x1 $x5 $x1 $x1;
        letter-spacing: 0;
        text-align: left;
        color: $gray700;
        font-size: 1.4rem;
    
        &[aria-expanded="true"]::after {
            content: "keyboard_arrow_up";
        }
    
        &::after {
            content: "keyboard_arrow_down";
            font-family: 'Material Icons';
            display: block;
            position: absolute;
            right: 10px;
            top: 50%;
            margin-top: -10px;
            width: 18px;
            height: 20px;
            float: right;
            overflow: hidden;
            font-size: 20px;
            line-height: 1;
            color: $gray700;
        }
    }
    
    a.ui-acco-btn {
        display: inline-block;
        text-decoration: none;
        text-decoration: underline;
        color: #6296df;
    
        &::after {content: "keyboard_arrow_right";}
        .ui-acco-arrow {transform: rotate(-90deg);}
    }
    
    .ui-acco-pnl {
        position: relative;
        visibility: visible;
        transition: height .3s ease;
        height:auto;
        overflow: hidden;
        background: $gray50;   
        
        &[aria-hidden="true"] {
            visibility: visible;
            height: 0;
            transition: height .3s ease;

            * {
                pointer-events: none; 
                // display: none;
            }
        }
    }
    
    .ui-acco-pnl-wrap{
        padding: $x3;
        transition: height .3s ease;
    }
    
    .ui-acco-tit-wrap {
        position: relative;
        display: inline-block;
        z-index: 2;
        width: auto;
        text-decoration: none;
        padding: 10px;
        color: #000;
    
        & + .ui-acco-btn {
            position: absolute;
            color: transparent;
            overflow: hidden;
            font-size: xx-small;
            line-height: 0;
        }
    }
}









/* ui-draggable */
.ui-draggable {
    position: relative;
    width: 100%;
}

.ui-draggable-item {
    position: absolute;
    z-index: 1;
}

.ui-draggable-item.clone {
    position: absolute;
    z-index: 0;
    opacity: 0.5;
}

.ui-draggable-item:hover {
    z-index: 2;
}

.ui-draggable-area {
    position: absolute;
}





/* ui-floating : base */
.ui-floating {
    border: 1px dashed rgb(230, 230, 230);
}

.ui-floating-wrap {
    width: 100%;
    position: relative;
}

.ui-floating[data-ps="bottom"][data-state="fix"] .ui-floating-wrap {
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
}

.ui-floating[data-ps="top"][data-state="fix"] .ui-floating-wrap {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 999;
}

.ui-floating-range {
    display: inline-block;
    vertical-align: middle;
    height: 500px;
    width: 300px;
    border: 1px dashed #ccc;
}

.ui-floating-range-item {
    position: relative;
    top: 0;
    left: 0;
    width: 100%;
    transform: translate(0,0);
    transition:transform .2s;
}



