/* focus loop */
[class*="ui-focusloop-"]:focus {
    height: auto;
}

[class*="ui-focusloop-"] {
    position: absolute;
    left: 0;
    width: 100%;
    height: 0;
    overflow: hidden;
}

[class*="ui-focusloop-"] span {
