/* ui select */

.ui-select {
    position: relative;
    display: inline-block;
    width: 100%;
}

.ui-select select {
    width: 100%;
}

.ui-select select.off {
    position: absolute;
    z-index: -1;
}

.ui-select select.off:focus+.ui-select-btn {
    border-color: #000000;
}

.ui-select select.off:focus+.ui-select-btn:after {
    content: "keyboard_arrow_up";
}

.ui-select-btn {
    padding: 0 0 0 1rem;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    height: auto;
    min-height: 4rem;
    width: 100%;
}

.ui-select-btn:after {
    content: "keyboard_arrow_down";
    font-family: 'Material Icons';
    display: block;
    position: absolute;
    right: 0.6rem;
    top: 50%;
    margin-top: -1.2rem;
    width: 2.4rem;
    height: 2.4rem;
    float: right;
    overflow: hidden;
    line-height: 1;

    font-size: 2.2rem;
    
    color: #424242;
    
}

.ui-select-btn span {
    display: inline-block;
    width: calc(100% - 4.5rem);
    text-align: left;
    word-break: nowrap;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    text-align: left;
    line-height: 1.2;
    margin: .8rem 0;
}

.ui-select-btn[aria-expanded="true"],
.ui-select-btn:focus,
.ui-select-btn:active {
    border-color: #E0E0E0;
}

.ui-select-btn[aria-expanded="true"]:after,
.ui-select-btn:focus:after,
.ui-select-btn:active:after {
    content: "keyboard_arrow_up";
}

.ui-select-wrap.on {
    display: block
}

.ui-select-wrap.top {
    bottom: 3.9rem
}

.ui-select-wrap.bottom {
    top: 3.9rem
}

.ui-select-wrap {
    display: none;
    position: absolute;
    z-index: 100;
    left: 0;
    width: auto;
    overflow-y: auto;
    max-height: 16.2rem;
    -webkit-overflow-scrolling: touch;

    border: 1px solid #212121;
    background: #FFFFFF;
    
}

.ui-select-wrap.ui-scrollbar {
    position: absolute;
    min-height: 16.2rem;
}

.ui-select-opts {
    overflow: hidden;
    width: 100%;
}

.ui-select-opt {
    display: block;
    float: left;
    width: 100%;
    padding: .5rem 2rem .5rem 1rem;
    height: 4rem;
    text-align: left;
    white-space: nowrap;

    background: #FFFFFF;
    
    font-size: 1.4rem;
    
}

.ui-select-opt.selected {
    font-weight: 700;
    text-decoration: underline;

    background: #EEEEEE;
    
    color: #000000;
    
}

.ui-select-opt.disabled {
    opacity: 0.5;
}

.ui-select-title {
    display: none;
    position: absolute;
    top: 0;
    left: 0;
}

.ui-select.type-tel {
    width: 15rem
}

.ui-select.type-bank {
    width: 20rem
}

.ui-select.type-card {
    width: 20rem
}

.ui-select-cancel,
.ui-select-confirm {
    display: none;
}

.ui-m .ui-select-wrap.on {
    display: block;
}

.ui-m .ui-select-cancel,
.ui-m .ui-select-confirm,
.ui-m .ui-select-title,
.ui-m .ui-select-wrap.top,
.ui-m .ui-select-wrap.bottom {
    bottom: -30rem;
    top: auto;
    animation-name: slideUpIn;
    animation-duration: .2s;
    animation-iteration-count: 1;
    animation-timing-function: ease-in-out;
    animation-fill-mode: forwards;
}

.ui-m .ui-select-wrap {
    display: none;
    position: fixed;
    z-index: 100;
    left: 0;
    bottom: -30rem;
    width: 100%;
    border: 0;
    overflow-y: auto;
    max-height: 24rem;

    background: #FFFFFF;
    
}

.ui-m .ui-select-title {
    position: fixed;
    margin-bottom: 20rem;
    left: 0;
    top: auto;
    display: block;
    width: 100%;
    height: 4rem;
    padding: .9rem 1rem .4rem;
    text-align: center;
    z-index: 10;

    background: #212121;
    
    color: #FFFFFF;
    
}

.ui-m .ui-select-opts {
    padding: 12rem 0 8rem;
}

.ui-m .ui-select-opts:after {
    content: "";
    display: block;
    position: fixed;
    bottom: 11.8rem;
    width: 100%;
    height: 1px;
    z-index: 0;

    border-top: 1px solid #F5F5F5;
    
}

.ui-m .ui-select-opts:before {
    content: "";
    display: block;
    position: fixed;
    bottom: 7.8rem;
    width: 100%;
    height: 1px;
    z-index: 0;

    border-top: 1px solid #F5F5F5;
}

.ui-m .ui-select-opt {
    text-align: center;
    z-index: 1;
    position: relative;
    background: transparent;

    color:$gray900;
    
}

.ui-m .ui-select-opt.selected {
    color: #000000;
    
    background-color: #EEEEEE;
}

.ui-m .ui-select-cancel,
.ui-m .ui-select-confirm {
    position: fixed;
    margin-bottom: 20rem;
    right: 0;
    display: block;
    width: 5rem;
    height: 4rem;
    padding: .7rem 1rem .6rem;
    text-align: center;
    z-index: 11;

    color: #FFFFFF;
    
    font-size: 1.4rem;
    
    
}

.ui-m .ui-select-cancel {
    left: 0;
    right: auto;
}
