/* range */
.ui-range {

    $rangeWidth:2rem;

	width: 100%;
	height: 3rem;
	padding: 1.2rem 1rem;
	display: flex;
	flex-direction: column;
	justify-content: flex-start;
	align-items: center;
	position: relative;
    -webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;

    &[data-type="single"]  {
        .ui-range-bar {
            right: auto;
        }
        .ui-range-point{
            // pointer-events:none !important;
            &::before {
                content: "code" !important;
            }
        }
    }

    &[data-orient="vertical"] {
        transform-origin: 5rem 5rem;
        transform: rotate(-90deg);
        margin-left: 6rem;

        .ui-range-point {
            justify-content: center;

            .n {
                top: 2.4rem;
                left: 2rem;
                transform: rotate(90deg);
                transform-origin: 0 0;
            }
        }
    }
        
    .ui-range-inp {
        width: 100%;
        top: .5rem;
        left: 0;
        position: absolute;
        border: none;
        // pointer-events: none;
        z-index: 10;
        appearance: none;
        opacity: 0;
        padding: 0;
        margin: 0;
        height: $rangeWidth;
        border:1px solid red;
        cursor: pointer;

        &:focus ~ .ui-range-track{
            outline: 1px solid #000;
        }

        &.on {
            z-index: 11;
        }
        &::-webkit-slider-thumb {
            pointer-events: all;
            /* appearance, background-color는 지워도 됨 */
            appearance: none;
            background-color: red;
            width: $rangeWidth;
            height: $rangeWidth;
            margin-top: 0rem;
        }
        &[data-range="from"]:focus ~ .ui-range-track .left.ui-range-point {
            outline: 2px solid #000;
            pointer-events: none;
        }
        &[data-range="to"]:focus ~ .ui-range-track .right.ui-range-point {
            outline: 2px solid #000;
            pointer-events: none;
        }
    }

    .ui-range-track {
        position: relative;
        width: 100%;
        height: 0.6rem;
        border-radius: 0.3rem;
        background-color: #BDBDBD;
        pointer-events: none;
        z-index: 12;
    }

    .ui-range-bar {
        position: absolute;
        pointer-events: none;
        top: 0;
        bottom: 0;
        left: 0;
        right: 0;
        border-radius: 0.5rem;
        background-color: $primary;
    }

    .ui-range-point {
        position: absolute;
        top: 50%;
        display: flex;
        justify-content: center;
        align-self: center;
        width: $rangeWidth;
        height: $rangeWidth;
        border-radius: 50%;
        background-color:$primary;
        pointer-events:all;
        z-index: 13;
        cursor: pointer;

        &:hover{
            pointer-events:none;
        }

        &.on {
            z-index: 1;
        }
        &::before {
            font-family: 'Material Icons';
            line-height: 2rem;
            color: #fff;
            font-size: 1.2rem;
            pointer-events: none;
        }
        &.left {
            left: 0;
            transform: translate(-1rem, -50%);

            &::before {content: 'arrow_left';}
        }
        &.right {
            right: 0;
            transform: translate(1rem, -50%);

            &::before {content: 'arrow_right';}
        }

        .n {
            position: absolute;
            top: -1.8rem;
            text-align: center;
            font-size: 1.2rem;
            pointer-events: none;
        }
    }

    .ui-range-marks {
        position: relative;
        width: 100%;
        position: absolute;
        display: flex;
        justify-content: space-between;
        top:100%;

        option {
            display: flex;
            justify-content: center;
            font-size: 1.2rem;
            width: $rangeWidth;
            padding:0;
            position: relative;
            text-align: center;
            padding-top:$x1;

            &::before{
                content:"";
                display:block;
                width:1px;
                height: $x0_5;
                background-color: $black;
                left:50%;
                position: absolute;
                transform: translate(-50%,0);
                top:0;
            }
        }
    }
}





/* ui codinglist */
.ui-codinglist {
    width: 100%;
    padding:1rem;

    &[data-state] tbody tr.srch-hidden,
    &[data-pub] tbody tr.srch-hidden{display: none !important;}
    
    &[data-state] tbody tr,
    &[data-pub] tbody tr{display: none;}
    &[data-pub] tbody tr.worker-view{display: table-row;}
    &[data-state] tbody tr.state-view{display: table-row;}

    &[data-state][data-pub] tbody tr.state-view,
    &[data-state][data-pub] tbody tr.worker-view{display: none;}
    &[data-state][data-pub] tbody tr.worker-view.state-view{display: table-row;}

    tr[data-state="제외"] {
        td {background-color: $gray100;}
        * {text-decoration-line: line-through; pointer-events: none; color: #ccc !important;}
    }

    .srch-area{
        display: flex;
    }
    .btn-outlined{
        margin-left:.4rem;
    }
    .srch-hidden {
        display:none;
    }
    .sch_warn {
        .state span,
        .state:before{
            color: #f44336 !important;
        }
    }
    .state {
        span{
            display:inline-block;
