    padding:1rem;
    position:fixed;
    left:0;
    margin:1rem;
    transition: all .3s;

    font-size:1.2rem;
    color:#FFFFFF;
   
    background:#6200ee;
}

.ui-toast-ready .toast{
    display:block;
    opacity: 0;
    bottom:-10rem;
}

.ui-toast-show .toast{
    opacity: 1;
    bottom: 0;
}

.ui-toast-ready .toast.top{
    display:block;
    opacity: 0;
    bottom:auto;
    top:-10rem;
}

.ui-toast-show .toast.top{
    opacity: 1;
    bottom:auto;
    top: 0;
