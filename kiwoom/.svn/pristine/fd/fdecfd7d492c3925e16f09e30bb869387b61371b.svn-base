/* ui scroll bar : base */
.ui-scrollbar {
    position: relative;
    overflow: auto;
}

.ui-scrollbar-item {
    position: absolute;
    top: 0;
    left: 0;
}

.ui-scrollbar.ready.view-y .ui-scrollbar-item.scroll-y-padding {
    width: calc(100% - 1rem) !important;
}

.ui-scrollbar.ready.view-x .ui-scrollbar-item.scroll-x-padding {
    height: calc(100% - 1rem) !important;
}

.ui-scrollbar.ready {
    overflow: hidden !important;
    /* padding: 0 !important; */
}

.ui-scrollbar.ready>.ui-scrollbar-item {
    position: relative;
    width: 100%;
    height: 100% !important;
    overflow: scroll !important;
}

.ui-scrollbar.ready>.ui-scrollbar-item {
    -ms-overflow-style: none;
    scrollbar-width: none;
}

.ui-scrollbar.ready>.ui-scrollbar-item::-webkit-scrollbar {
    display: none;
}

.ui-scrollbar.ready .ui-scrollbar-item::-webkit-scrollbar,
.ui-scrollbar.ready .ui-scrollbar::-webkit-scrollbar {
    width: 0;
    height: 0;
}

.ie .ui-scrollbar.ready>.ui-scrollbar-item {
    width: calc(100% + 1.7rem) !important;
    height: calc(100% + 1.7rem) !important;
}

.ui-scrollbar-barwrap {
    opacity: 0;
}

.ui-scrollbar-barwrap.type-y {
    display: none;
    position: absolute;
    top: 0;
    right: 0;
    width: 1rem;
    height: 100%;
    background: rgba(255, 255, 255, 0.1);
    z-index: 100;
    border-top: .2rem solid transparent;
    border-bottom: .2rem solid transparent;
}

.ui-scrollbar-barwrap.type-x {
    display: none;
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 1rem;
    background: rgba(255, 255, 255, 0.1);
    z-index: 100;
    border-left: .2rem solid transparent;
    border-right: .2rem solid transparent;
}

.type-y .ui-scrollbar-bar {
    position: absolute;
    top: 0;
    right: .2rem;
    width: .6rem;
    margin: 0;
    height: 2rem;
    opacity: 0.5;
    border-radius: .2rem;

    background: #E0E0E0;
    
}

.type-x .ui-scrollbar-bar {
    position: absolute;
    top: .2rem;
    left: 0;
    width: 2rem;
    margin: 0;
    height: 6px;
    opacity: 1;
    border-radius: .2rem;
    opacity: 0.5;

    background: #E0E0E0;
    
}

.ui-scrollbar:hover .ui-scrollbar-bar {
    opacity: 1;
    box-shadow: 0 0 .5rem rgba(0,0,0,0.1);
}

.view-scrollbar>.ui-scrollbar-barwrap {
    animation-name: fadeIn;
    animation-duration: .2s;
    animation-iteration-count: 1;
    animation-timing-function: linear;
    animation-fill-mode: forwards;
}

.view-y>.ui-scrollbar-barwrap.type-y {
    display: block;
}

.view-x>.ui-scrollbar-barwrap.type-x {
    display: block;
}


/* ui scroll box : base */
.ui-parallax-wrap {
    position: relative;
}


/* ui-drop : base */
.ui-drop {
	position: relative;
	display: inline-block;
}

.ui-drop-pnl {
	display: none;
	position: absolute;
	z-index: 100;
}

.ui-drop-pnl.on {
	display: block;
}

.ui-drop-box {

    background: #fff;
	
}

/* ui-drop : netvie UI style */
.ui-drop-box {
	padding: 10px;

    border: 1px solid #EEEEEE;
    
}

.ui-drop-pnl .ui-drop-close {
	position: absolute;
	top: 6px;
	right: 6px;
}



/* tab */
.ui-tab {
	position: relative;
	margin: 0
}

.ui-tab-btns.ui-scrollbar .wrap-group {
	overflow-x: initial;
}

.ui-tab-btns .wrap-group {
	display: flex;
	flex-wrap: nowrap;
	margin: 0;
	overflow-x: auto;
}

.ui-tab-btns {
	position: relative;
	overflow: hidden;
	z-index: 1
}

.ui-tab-btns:after {
	content: "";
	display: block;
	position: absolute;
	bottom: 0;
	left: 0;
	z-index: 1;
	clear: both;
	width: 100%;
	height: 1px;

    background: #EEEEEE;
	
}

.ui-tab-btn {
	box-sizing: border-box;
	float: left;
	position: relative;
	z-index: 0;
	flex: 1;
	white-space: nowrap;
}

.ui-tab-btn.selected {
	z-index: 2;
}

.ui-tab-pnls {
	box-sizing: border-box;
	position: relative;
	width: 100%;
}

.ui-tab-pnl {
	box-sizing: border-box;
	top: 0;
	left: 0;
}

.ui-tab-pnl,
.ui-tab-pnl[aria-hidden='true'] {
	visibility: hidden;
	position: absolute;
	opacity: 0;
	display: none
}

.ui-tab-pnl.selected,
.ui-tab-pnl[aria-hidden='false'] {
	visibility: visible;
	position: relative;
	opacity: 1;
	z-index: 1;
	display: block
}

/* ui-tab :base style */
.ui-tab-btn {
	padding: 0 1rem;
	min-height: 4rem;
	border-left: 0;

    background: #FAFAFA;
    
    border: 1px solid #EEEEEE;
	
}

.ui-tab-btn:first-child {
    border-left: 1px solid #EEEEEE;
	
}

.ui-tab-btn.selected {
	background: #fff;
    
	color:$gray900;
    
	border-bottom: 1px solid #fff;
	
}

.ui-tab-pnl {
	width: 100%;
	padding: 1rem;

    background: #fff;
	
}

/* ui-tab : eff */
.ui-tab.eff-fade>.ui-tab-pnls>.ui-tab-pnl {
	animation-name: fadeIn;
	animation-duration: .2s;
	animation-iteration-count: 1;
	animation-timing-function: linear;
	animation-fill-mode: forwards;
}

.ui-tab.eff-fade>.ui-tab-pnls>.ui-tab-pnl[aria-hidden="false"] {
	animation-name: fadeIn;
	animation-duration: .2s;
	animation-iteration-count: 1;
	animation-timing-function: linear;
	animation-fill-mode: forwards;
}

/* ui-fixtd */
.ui-fixtd-btn {
	position: absolute;
	top: 50%;
	transform: translate(0, -50%);
	vertical-align: middle;
	display: inline-block;
    background: #FFFFFF;
	
	width: 1.8rem;
	height: 1.8rem;
	line-height: 1;

	border: 1px solid #E0E0E0;
    
}

.ui-fixtd-btn.prev {
	left: -1rem;
}

.ui-fixtd-btn.next {
	right: -1rem;
}

.ui-fixtd-btn.prev::after {
	content: "keyboard_arrow_left";
	font-family: 'Material Icons';
	font-size: 1.6rem;
}

.ui-fixtd-btn.next::after {
	content: "keyboard_arrow_right";
	font-family: 'Material Icons';
	font-size: 1.6rem;
}

[class*="ui-fixtd-n"] {
	display: none;
	position: relative;
}

[data-current="1"] col.ui-fixtd-n1,
[data-current="2"] col.ui-fixtd-n2,
[data-current="3"] col.ui-fixtd-n3,
[data-current="4"] col.ui-fixtd-n4,
[data-current="5"] col.ui-fixtd-n5,
[data-current="6"] col.ui-fixtd-n6,
[data-current="7"] col.ui-fixtd-n7,
[data-current="8"] col.ui-fixtd-n8,
[data-current="9"] col.ui-fixtd-n9,
[data-current="10"] col.ui-fixtd-n10,
[data-current="11"] col.ui-fixtd-n11 {
	display: block
}

[data-current="1"] .ui-fixtd-n1,
[data-current="2"] .ui-fixtd-n2,
[data-current="3"] .ui-fixtd-n3,
[data-current="4"] .ui-fixtd-n4,
[data-current="5"] .ui-fixtd-n5,
[data-current="6"] .ui-fixtd-n6,
[data-current="7"] .ui-fixtd-n7,
[data-current="8"] .ui-fixtd-n8,
[data-current="9"] .ui-fixtd-n9,
[data-current="10"] .ui-fixtd-n10,
[data-current="11"] .ui-fixtd-n11 {
	display: table-cell
}

/* ui-print*/
body.ui-print-ing>* {
    display: none
}

body.ui-print-ing .ui-print {
    display: flex;
    width: 100%;
    height: 100%;
    justify-content: center;
    align-items: center;
    position: fixed;
    opacity: 0.5
}

@media print {
    body.ui-print-ing>* {
        display: none
    }

    body.ui-print-ing .ui-print {
        display: block;
        opacity: 1;
        position: relative;
    }
}


/* ui tooltip:z-index 300 ~ 399 */
.ui-tooltip {
    visibility: hidden;
    position: absolute;
    z-index: 300;
    padding: 1rem;
    font-size: 1.2rem;
    border: 1px solid #E0E0E0;
    background: #FAFAFA;
    box-shadow: 0 .2rem 1rem rgba(0,0,0,0.1);
    text-align: justify;
}

.ui-tooltip[aria-hidden="false"]{
    visibility: visible;
    animation-name: fadeIn;
	animation-duration: .2s;
	animation-iteration-count: 1;
	animation-timing-function: linear;
	animation-fill-mode: forwards;
}
.ui-tooltip[aria-hidden="true"]{
    visibility: visible;
    animation-name: fadeOut;
	animation-duration: .2s;
	animation-iteration-count: 1;
	animation-timing-function: linear;
	animation-fill-mode: forwards;
}

.ui-tooltip.hover {
    z-index: 301;
}

.ui-tooltip.fix {
    display: block !important;
}

.ui-tooltip-btn {
    cursor: help;
}

button.ui-tooltip-btn {
    display: inline-block;
    position: relative;
    width: 1.4rem;
    height: 1.4rem;
    min-width: 1.4rem;
    min-height: 1.4rem;
    max-width: 1.4rem;
    max-height: 1.4rem;
    border-radius: .7rem;
    color: transparent;
    overflow: hidden;
    font-size: xx-small;

    border: 1px solid #424242;
    
    background: #fff;
    
}

button.ui-tooltip-btn:after {
    content: "?";
    position: absolute;
    top: -1px;
    left: 0;
    display: block;
    width: 100%;
    height: 100%;
    font-size: 1rem;
    font-weight: 700;

    color: #424242;
    
}

.ui-tooltip .ui-tooltip-arrow {
    content: "";
    position: absolute;
    display: block;
    width: 1rem;
    height: 1rem;

    background: #FAFAFA;
    
    border-top: 1px solid  #E0E0E0;
    
    border-right: 1px solid  #E0E0E0;
    
}

.ui-tooltip[data-ps="top"] .ui-tooltip-arrow {
    left: 0;
    bottom: -.6rem;
    transform: rotate(135deg);
    margin-left: -.6rem
}
.ui-tooltip[data-ps="bottom"] .ui-tooltip-arrow {
    left: 0;
    top: -.6rem;
    transform: rotate(-45deg);
    margin-left: -.6rem
}

.ui-tooltip.ps-lt .ui-tooltip-arrow {
    left: 0;
    bottom: -.6rem;
    transform: rotate(135deg);
    margin-left: 1rem
}

.ui-tooltip.ps-rt .ui-tooltip-arrow {
    right: 0;
    bottom: -.6rem;
    transform: rotate(135deg);
    margin-right: 1rem
}

.ui-tooltip.ps-lb .ui-tooltip-arrow {
    left: 0;
    top: -.6rem;
    transform: rotate(-45deg);
    margin-left: 1rem
}

.ui-tooltip.ps-rb .ui-tooltip-arrow {
    right: 0;
    top: -.6rem;
    transform: rotate(-45deg);
    margin-right: 1rem
}


/* ui date picker scss ------------------------------------------------------------------------------------- */
.ui-datepicker {
    display: flex;
    justify-content: flex-start;
    position: relative;

    &.ui-datepicker-view .datepicker-footer{display: none;}
    &.type-dual {
        .ui-datepicker-wrap {
            width: 460px;
    
            .tbl-datepicker {
                width: 210px;
                float: left;
    
                & +.tbl-datepicker {
                    float: right;
                }
            }
        }
    }

    &.type-month {
        .ui-datepicker-wrap {
            width: auto;
        }
        .datepicker-head-month {
            width: 460px;
            padding: 1rem 0;
            margin: 1px 0 0 1px;
    
            button {
                margin: -1px 0 0 -1px;
            }
        }
    }

    &.visible .ui-datepicker-wrap {
        opacity: 1;
        top: 4rem;
    }

    .icon-close {
        position: absolute;
        top: .6rem;
        right: .6rem;
        z-index: 1;

        &::after {
            color:$gray900;
        }
    }

    .inp-base{
        padding-right:$x4;

        &::after {
            content: 'date_range';
    
            position: absolute;
            top:50%;
            right:$x1;
            transform: translate(0,-50%);
            font-family: 'Material Icons';
            font-feature-settings: 'liga';
            word-break: keep-all;
            line-height: 1.6;
        } 
    }
}

.ui-datepicker-btn {
    border: 0;
    padding: 0;
    text-align: right;
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 1;
    background-color: transparent;

    &:focus~.inp-base,
    &:hover~.inp-base {
        border: 1px solid #1f1f1f;
        background: transparent;
    }
    &:disabled {
        opacity: 0.5;
        &:disabled:focus,
        &:disabled:hover {
            border: 0;
            background: transparent;
            color: #666;
        }
    }
    
}

.datepicker {
    width: 100%;
    max-width: 23rem;

    .wrap-group {
        margin: 0;
    }
    :disabled {
        opacity: .8 !important;
    }
}

.datepicker-header {
    position: relative;
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-wrap: wrap;
    
    & > button {
        width: 2.4rem;
        height: 2.4rem;
        font-family: 'Material Icons';
        position: relative;
        font-size: 2.2rem;
        &::before {
            display: block;
            line-height: 2.6rem;
        }
        &.ui-prev-y::before {content: "chevron_left";}
        &.ui-next-y::before {content: "chevron_right";}
        &.ui-prev-m::before {content: "chevron_left";}
        &.ui-next-m::before {content: "chevron_right";}
    }

    .ui-today {
        width: 2.5rem;
        height: 2.5rem;
        line-height: 1;
        position: absolute;
        top: 0;
        right: 0;
        &::before {
            content: "today";
        }
    }
    .datepicker-title {
        width: 100%;
        text-align: center;
        font-size: 1.4rem;
    }
}

.datepicker-body {
    margin: 1rem 0;

    th {font-size: 1.2rem;}
    .datepicker-date td {
        text-align: center
    }
    .week-word{display: none;} 
    .datepicker-day {
        width: 2.5rem;
        height: 2.5rem;
        border-radius: 50%;
        margin: 0.3rem 0;
        line-height: 1;
        font-size: 1.2rem;
    
        &.selected-end,
        &.selected-start,
        &.selected {
            background: #6200ee;
            color: #fff;
        }
    
        &.today.selected-end,
        &.today.selected-start {
            background: #6200ee;
            color: #fff;
        }
    
        &.today {
            background: #fff;
            border: 1px solid #6200ee;
            color: #6200ee;
        }
    }
    .during .datepicker-day {
        background: #EEEEEE !important;
    }
}
