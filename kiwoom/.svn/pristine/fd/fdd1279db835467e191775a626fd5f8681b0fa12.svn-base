.eng-conts.eng-activity dl > div:before { content:''; display:block; position:absolute; width:48px; height:48px; top:0; left:50%; transform:translateX(-50%); background:url(/kiam/moWeb/resources/images/common/ico_conts.png) no-repeat 0 0; background-size:500px auto; }
