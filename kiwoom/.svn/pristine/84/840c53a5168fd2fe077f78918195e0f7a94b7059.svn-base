.sub-title {position:relative; margin:40px 0 16px 0;}
.sub-title .tbl-title {font-size:16px; font-weight: 700; margin: 0 !important;}
.sub-title .msg {font-size:10px; color:#9D9DA5; }
.sub-title > .msg {position:absolute; top:4px; right:0; }

.tbl{position: relative; width: 100%; border-top: 1px solid #222; background-color: #FFF; }
.tbl thead tr {background:#fff;}
.tbl thead th {padding: 14px 12px; font-size: 14px; font-weight: 600; color: #222; vertical-align: middle; line-height: 1.5; border-bottom: 1px solid #B9BBC6;} 
.tbl tbody td {height:53px; padding: 8px 16px; font-size: 12px; font-weight: 500; text-align: center; border-bottom: 1px solid #DFE0E2; vertical-align: middle; line-height: 1.5; word-break: break-all;}

