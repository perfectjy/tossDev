.fund-detail:not(:first-child) { margin-top:56px;}
.fund-detail .h3 { margin-bottom: 16px; font-size: 16px; line-height: 24px; font-weight: 700; }
.fund-detail .contents-box strong{ display: block; margin-bottom: 8px; color: #1A399E; font-size: 14px; font-weight: 700;}
.fund-detail .contents-box strong:not(:first-child){ margin-top: 16px; }
.fund-detail .contents-box strong + p{ color:#73737A; font-size: 14px; font-weight: 500; }
/* .fund-detail .contents-box .info-list { display:flex; position:relative; } */
/* .fund-detail .contents-box .info-list:before { content:''; position:absolute; left:0; bottom:0; width:100%; background:#DFE0E2; height:1px; } */
.contents-box {border-top: 1px solid #222;}
.contents-box .info-list { display:flex; width:100%; border: 1px solid #000; justify-content: flex-start; align-items: flex-start; flex-direction: row; }
.contents-box .info-list div { width:100%; padding: 16px 16px 16px 0; border-bottom: 1px solid #DFE0E2; }
.contents-box .info-list div.half { width:50%; border: 1px solid #000;}
.contents-box .info-list div dt { margin-bottom: 4px; color:#73737A; font-size:12px; line-height: 18px; font-weight: 400; }
.contents-box .info-list div dd { color:#222; font-size:14px; line-height: 21px; font-weight: 600; }

.tbl-nodate{ font-size: 14px; font-weight: 500; padding: 152px 0; text-align: center;}
.fund-info-text h4{ color: #222; font-size: 16px; font-weight: 700; }
.fund-info-text strong{ display: block; color: #1A399E; font-size: 14px; margin-top: 16px; }
.fund-info-text p{ color: #73737A; font-size: 14px; margin-top: 16px; }

.sub-title {position:relative; margin:40px 0 16px 0;}
.sub-title .tbl-title {font-size:16px; font-weight: 700; margin: 0 !important;}
.sub-title .msg {font-size:10px; color:#9D9DA5; }
.sub-title > .msg {position:absolute; top:4px; right:0; }

.tbl{position: relative; width: 100%; border-top: 1px solid #222; background-color: #FFF; }
.tbl thead tr {background:#fff;}
.tbl thead th {padding: 14px 12px; font-size: 14px; font-weight: 600; color: #222; vertical-align: middle; line-height: 1.5; border-bottom: 1px solid #B9BBC6;} 
.tbl tbody td {height:53px; padding: 8px 16px; font-size: 12px; font-weight: 500; text-align: center; border-bottom: 1px solid #DFE0E2; vertical-align: middle; line-height: 1.5; word-break: break-all;}

