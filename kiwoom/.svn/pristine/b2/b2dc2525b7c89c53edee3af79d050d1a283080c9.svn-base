/* pc */
.tbl-flex {display:flex;  justify-content: flex-start; align-items: flex-start; margin-top: 80px;}
.tbl-flex .title-box {width: 308px;}
.tbl-flex .title-box h4 {font-size: 24px; font-weight: 700; line-height: 36px; color:#222;}
.tbl-flex .contents-box {width: 892px; }

.info-list {display: flex; justify-content: flex-start; flex-wrap: wrap; border-top: 1px solid #222;}
.info-list > li { width:50%; padding: 24px 16px 24px 16px; border-bottom: 1px solid #DFE0E2; }
.info-list > li.flex1 {width:100%;}

.dl-items {text-align: left;}
.dl-items div dt {font-size: 14px; line-height: 21px; font-weight: 400; color:#73737A; margin-bottom: 8px;}
.dl-items div dd {font-size: 16px; font-weight: 600; line-height: 24px; color:#222;}

.btn-box {margin-top:16px; text-align: right;}
.btn-box + .tbl {margin-top:16px;}
.tbl + .ui-acco {margin-top:80px;}

.symbol {position: relative; border: 1px solid #000;}
.symbol::before {content: ''; position: absolute; left:50%; top:0; transform: translateX(-50%); width:16px; height: 16px; border-radius: 50%; background:#9D9DA5;}
.symbol.a::before {}
.symbol.b::before {}
.symbol.c::before {}
.symbol.d::before {}
.symbol.e::before {}
.symbol.f::before {}





/* Mobile */
.ui-tab.t1 .ui-tab-btns {display:flex; padding:0 12px; border-bottom:1px solid #DFE0E2; font-size:0;}
.ui-tab.t1 .ui-tab-btn {flex:1;  height:42px; font-size:14px; font-weight:500; color:#73737A;}
.ui-tab.t1 .ui-tab-btn.selected {border-bottom:2px solid #EC008C; color:#222; font-weight:700;}
.ui-tab.t1 .ui-tab-pnls {padding: 18px 24px 0 24px;}
