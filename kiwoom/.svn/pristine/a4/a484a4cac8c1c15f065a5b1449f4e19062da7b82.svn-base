/* animation */
.fadeIn {-webkit-animation-name:fadeIn; animation-name: fadeIn; }
.fadeInUp {-webkit-animation-name:fadeInUp; animation-name: fadeInUp; }
.isIn .ani-top{-webkit-animation:slide-top 1s cubic-bezier(.25,.46,.45,.94) both; animation:slide-top 1s cubic-bezier(.25,.46,.45,.94) both }
.isIn .ani-top2{-webkit-animation:slide-top2 1s cubic-bezier(.25,.46,.45,.94) both; animation:slide-top2 1s cubic-bezier(.25,.46,.45,.94) both }
.isIn .ani-spin { animation-name: spin; animation-duration: 20000ms; animation-iteration-count: infinite; animation-timing-function: linear; }
.isIn .ani-digonal { animation: ani-digonal 3s linear infinite alternate; }
.isIn .ani-fade { -webkit-animation:fadeIn 1s cubic-bezier(.25,.46,.45,.94) both; animation:fadeIn 1s cubic-bezier(.25,.46,.45,.94) both  }

.ani02{animation-delay: .3s !important;-webkit-animation-delay: .3s !important;}
.ani03{animation-delay: .6s !important;-webkit-animation-delay: .6s !important;}
.ani04{animation-delay: .9s !important;-webkit-animation-delay: .9s !important;}
.ani05{animation-delay: 1.2s !important;-webkit-animation-delay: 1.2s !important;}
.ani06{animation-delay: 1.5s !important;-webkit-animation-delay: 1.5s !important;}
.ani07{animation-delay: 1.8s !important;-webkit-animation-delay: 1.8s !important;}
.ani08{animation-delay: 2.1s !important;-webkit-animation-delay: 2.1s !important;}
.ani09{animation-delay: 2.4s !important;-webkit-animation-delay: 2.4s !important;}
.ani10{animation-delay: 2.7s !important;-webkit-animation-delay: 2.7s !important;}
