.fund-detail { position:relative; padding:80px 0 0 305px; }
.fund-detail .h3 { position:absolute; top:80px; left:0; margin:0; }
.fund-detail .tooltip { position:absolute; top:110px; left:0; font-size:14px; line-height: 21px; font-weight: 500; color: #73737A; margin:0;}
.fund-detail .tooltip::before { content:''; position: absolute; right:25px; top:7px; border: 1.4px solid #73737A; }
.fund-detail .tooltip button {margin-left: 20px;}
.fund-detail .contents-box .info-list { display:flex; position:relative; }
.fund-detail .contents-box .info-list:before { content:''; position:absolute; left:0; bottom:0; width:100%; background:#DFE0E2; height:1px; }
.fund-detail .contents-box .info-list > div { width:50%; padding:24px 16px 24px 16px; border-bottom:1px solid #DFE0E2;   }
.fund-detail .contents-box .info-list > div.full { width:100%;}
.fund-detail .contents-box dt { font-size: 14px;line-height: 21px; font-weight: 400;  color: #73737A; margin-bottom: 8px; }
.fund-detail .contents-box dd { font-size: 16px; font-weight: 600; line-height: 24px; color: #222; }
.fund-detail .contents-box .info-list.flex { display:flex; }
.fund-detail .contents-box .info-list.flex > div { position:relative; flex:1 1; text-align:center; }
.fund-detail .contents-box .info-list.flex .sum:after { content:'+';  position:absolute; top:50%; right:-5px; margin-top:-12px; font-size:28px; font-weight:300;  }
.fund-detail .btn-group { display:flex; justify-content: space-between; }
.fund-detail .btn-group.fund .btn { width:211px; }

.tbl-flex {display:flex;  justify-content: flex-start; align-items: flex-start; margin-top: 80px;}
.tbl-flex .title-box {width: 308px;}
.tbl-flex .title-box h4 {font-size: 24px; font-weight: 700; line-height: 36px; color:#222;}
.tbl-flex .contents-box {width: 892px; }

.info-list {display: flex; justify-content: flex-start; flex-wrap: wrap; border-top: 1px solid #222;}
.info-list > li { width:50%; padding: 24px 16px 24px 16px; border-bottom: 1px solid #DFE0E2; }
.info-list > li.flex1 {width:100%;}

.dl-items {text-align: left;}
.dl-items div dt {font-size: 14px; line-height: 21px; font-weight: 400; color:#73737A; margin-bottom: 8px;}
.dl-items div dd {font-size: 16px; font-weight: 600; line-height: 24px; color:#222;}

.btn-box {margin-top:16px; text-align: right;}
.btn-box + .tbl {margin-top:16px;}
.tbl + .ui-acco {margin-top:80px;}

.symbol {position: relative; border: 1px solid #000;}
.symbol::before {content: ''; position: absolute; left:50%; top:0; transform: translateX(-50%); width:16px; height: 16px; border-radius: 50%; background:#9D9DA5;}


