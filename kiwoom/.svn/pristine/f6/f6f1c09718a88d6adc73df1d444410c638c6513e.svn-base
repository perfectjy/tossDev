/*table*/
.tbl{ position: relative; width: 100%; border-top: 1px solid #222; background-color: #FFF; }
.tbl thead tr {background:#F7F8F8;}
.tbl thead th { padding: 28px 16px; font-size: 16px; font-weight: 600; color: #222; vertical-align: middle; line-height: 1.5; } 
.tbl tbody td { padding: 24px 16px 25px ; font-size: 16px; font-weight: 500; text-align: center; border-bottom: 1px solid #DFE0E2; vertical-align: middle; line-height: 1.5;}
.tbl .txtl{ text-align: left; padding-left: 16px;}
thead.is-colgroup th[scope="colgroup"]{padding: 16px; border-bottom:1px solid #DFE0E2;}
thead.is-colgroup tr + tr th{padding: 16px;}