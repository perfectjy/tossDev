@charset "utf-8";
@import 'common.css'; 
@import 'fonts.css'; 

html, body { height:100%; }

.mgt10 { margin-top:10px; }
.mgt25 { margin-top:25px; }

.txt-center { text-align:center; }

.ico-req { }
.ico-req:after { content:'*'; padding-left:5px; color:#F2303C; }
.error-info { position:relative; padding-left:25px; margin-top:10px; color:#F2303C; }
.error-info:before { content:''; position:absolute; top:3px; left:0; width:16px; height:16px; background:#F2303C; color:#fff; border-radius:50%; }
.error-info:after { content:'!'; position:absolute; top:1px; left:6px; color:#fff; font-size:14px;  }
.tbl-base .error { position:absolute; right:0; bottom:7px; margin-top:10px; font-size:12px; color:#F2303C;}

.kiam-amin-wrap {position: relative; padding:16px 16px 0 16px; height:100%; }
.kiam-amin-wrap.login {background:linear-gradient(to right, #1A399E, #4D38CE); overflow:hidden;}
.kiam-amin-wrap.login::before {content:''; position:absolute; right:-82px; bottom:-170px; width:646px; height:642px; background:url(/admin/resource/images/bg_login.png)no-repeat; }
.gnb-wrap { position:fixed; top:0; left:0; right:0; background:#fff; padding:16px 16px 0 16px; z-index:100;  }
.gnb-wrap > div { background:linear-gradient(to right, #1A399E, #4D38CE); display:flex; align-items: center; justify-content:space-between; height:72px; padding:0 23px; border-radius:16px 0px 16px 16px;}
.gnb-wrap h1 a { display:inline-block; width:163px; height:28px; background:url(/admin/resource/images/kiam_logo.png);  }
.gnb-wrap .links a { position:relative; display:inline-block; padding-right:20px; font-size:14px; color:#fff;  } 
.gnb-wrap .links a:after { content:''; display:block; position:absolute; top:50%; right:0; margin-top:-8px;  width:16px; height:16px; background:url(/admin/resource/images/icons.png) no-repeat -260px 0;  }
.gnb-wrap .links a + a { margin-left:50px; }
.gnb-wrap .links .logout { position:relative; border: 1px solid #fff; border-radius: 8px 0 8px 8px; font-size:14px; color:#fff; padding: 12px 36px 12px 16px; margin-left: 48px;}
.gnb-wrap .links .logout::before {content:''; position:absolute; right:16px; top:11px; width:15px; height:15px; background:url(/admin/resource/images/ico_logout.png)no-repeat center;}
footer { padding:25px 0 25px 248px; color:#B9BBC6; font-size:14px; } 

.main-wrap { position:relative; margin-top:32px; padding:80px 0 80px 248px; }
.main-wrap .sub-nav { position:fixed; top:88px; left:16px; bottom:0; background: #F7F8F8; border-right:1px solid #1A399E; border-radius:16px 0 0 0; z-index:10; }
.main-wrap .sub-nav > ul { padding:20px 0 0 34px; }
.main-wrap .sub-nav > ul > li:first-child { padding-top:0; }
.main-wrap .sub-nav > ul > li { padding:5px 0; }
.main-wrap .sub-nav > ul > li > a {position:relative; display:block; width:184px; height:40px; font-size:16px; font-weight:600; line-height:24px; padding:8px 16px;}
.main-wrap .sub-nav > ul > li > a.on { color:#fff; background:#1A399E; border-radius:8px 0 0 8px; }
.main-wrap .sub-nav > ul > li > a.mMenu:after { content:''; position:absolute; right:16px; top:10px; width:20px; height:20px; background:url(/admin/resource/images/sp_menu_arrow.png)no-repeat -20px -20px;}
.main-wrap .sub-nav > ul > li > a.on::after {background-position: 0px 0px;}
.main-wrap .sub-nav > ul > li > a:not(.on):hover {color:#fff; background:#1A399E; border-radius:8px 0 0 8px; }
.main-wrap .sub-nav > ul > li > a:not(.on):hover::after {background-position: 0 -20px;}
.main-wrap .sub-nav .sub { display:none; padding:8px 0; }
.main-wrap .sub-nav .sub li { margin: 8px 0;}
.main-wrap .sub-nav .sub li a  { display:block; font-size:14px; line-height:21px; padding: 8px 0 8px 24px; color: #74757E;}
.main-wrap .sub-nav .sub li a:hover { color:#1A399E; }
.main-wrap .sub-nav .sub li.current a {position:relative; color:#000; font-weight:700;}
.main-wrap .sub-nav .sub li.current a span {position:relative;}
.main-wrap .sub-nav .sub li.current a span::before { content: ''; display: block; position: absolute; top: 1px; right: -10px; width: 6px; height: 6px; border-radius: 50%; background: #EC008C;}
.main-wrap .sub-nav .sub.on {position:relative; display: block; background: #fff; border-radius: 8px 0 0 8px; border: 1px solid #1a399e; border-right:0; box-shadow: 0px 2px 4px rgb(26 57 158 / 16%); margin: 4px -1px 0 0;}
.main-wrap .sub-nav .sub.on::after {content:''; position:absolute; right:-2px; top:0; width:4px; height: 100%; background:#fff;}
.btn-wrap { margin-top:40px; text-align:center; }

.main-conts { position:relative; min-width:1200px; }
.main-conts h2 { position:relative; display:inline-block; padding-left:20px; font-size:32px; font-weight:700; line-height:42px; } 
.main-conts h2:before { content:''; display:block; position:absolute; top:5px; right:-12px; width:8px; height:8px; border-radius:50%; background:#EC008C; } 
.main-conts h3 { font-weight:700; font-size:20px; }

.page-nav { position:absolute; top:0; right:0; line-height:0; font-size:0; }
.page-nav li { display:inline-block; position:relative;  }
.page-nav li:first-child:after { display:none; }
.page-nav li:after { content:''; position:absolute; top:3px; left:0; width:1px; height:8px; background:#B9BBC6;  }
.page-nav li a { color:#73737A; padding:0 16px; font-size:14px; }
.page-nav li.current a { color:#222; }

.sub-title-util { position: absolute; right:0; top:0; }
.sub-title-util .msg { display:inline-block; }
.sub-title-util .msg + .lb-item { margin-left:10px; }

.sub-title { position:relative; margin-top:40px; }
.sub-title .msg { font-size:14px; color:#73737A; }
.sub-title > .msg { position:absolute; top:-5px; right:0; }
.sub-title .msg.error-info { color:#F2303C; }
.sub-title .msg.error-info:before { display:none; }
.sub-title .count {font-size:14px; font-weight: 400;}
.sub-title .count span {font-weight: 500;}

.tbl-base { border:1px solid #222; border-width:1px 0; }
.tbl-base table { width:100%; table-layout:fixed; border-collapse:collapse; border-spacing:0; font-size:16px; font-weight:400; }
.tbl-base table th {color:#74757E; font-weight:500; }
.tbl-base table td {color:#222; font-weight: 600;}
.tbl-base table th,
.tbl-base table td { padding:10px 0; height:66px; border-top:1px solid #dfe0e2; }
.tbl-base table tbody tr:first-child th,
.tbl-base table tbody tr:first-child td { border-top:0; }

/* .tbl-base table td */
.tbl-base.type2 table th  {height:56px; color:#222;}
.tbl-base.type2 table {text-align: center; font-size:14px;}
.tbl-base.type2 table thead {font-weight: 600; border-bottom: 1px solid #222; height:56px;}
.tbl-base.type2 table tbody td {height:56px; font-weight: 500; }
.tbl-base.type2 table tbody td:nth-child(2) {text-align:left; padding:0 0 0 20px; }
.tbl-base.type2 table tbody td a:hover {color:#1A399E;}
.apply-btn {display:inline-block; width:56px; height:24px; background:#1A399E; border-radius: 4px; color:#fff; font-size:12px; padding:3px 0; margin-lefT:8px; }

/* .modal-conts .tbl-base {border: 1px solid #000;} */

.frm-area { position:relative; line-height:0; font-size:0; padding:5px 0;  }
.frm-area * { vertical-align:middle; }
.frm-area [class*="inp-"] + [class*="inp-"] { margin-left:8px; }
.frm-area [class*="inp-"] + .btn-base { margin-left:8px; }
.frm-area .txt-error { position:absolute; bottom:0; right:0; color:#F2303C; font-size:14px; }
.frm-area .note { margin-top:10px; font-size:14px; color:#74757E; }
.frm-area.multi { display:flex; }
.frm-area.multi .lb-item { flex:1 1; }
.frm-area.multi .lb-item.type2 {display:flex;}
.frm-area.type2 {display:flex; justify-content: space-between; align-items: center;}
.frm-area.type2 .name {font-size:14px; width:60px; color:#74757E; font-weight: 500;}
.frm-area.type2 .frm-area {display:inline-block; width:100px; margin-left:16px;}
.frm-area.report { overflow:hidden; overflow-y:scroll; padding:0; height:353px; }
.frm-area.report .lb-item { position:relative; display:block; width:100%; margin:0 !important; text-align: center; height:40px; line-height: 40px; background: #F7F8F8;}
.frm-area.report .lb-item label { display:block; }
.frm-area.report .lb-item label:before,
.frm-area.report .lb-item label:after { display:none; }
.frm-area.report .lb-item label span { display:inline-block; position:relative; padding-left:32px; line-height:40px; }
.frm-area.report .lb-item label span:before { content:""; display:block; flex:0; position:absolute; top:50%; left:0; margin-top:-12px; width:24px; height:24px; border-radius:4px; border:1px solid #B9BBC6; border-radius:4px;}
.frm-area.report .lb-item input:checked + label { background:#fff; }
.frm-area.report .lb-item input:checked + label span:before { background:#EC008C; border-color: #EC008C; }
.frm-area.report .lb-item input:checked + label span:after { content: ""; display: block; position: absolute; top:50%; left:0; margin-top:-12px; width: 24px;height: 24px; background: url(/kiam/pcWeb/resources/images/common/ico_form_chk.png) no-repeat -24px 0; }

.frm-area .btn-del { position:relative; display:inline-block; width:16px; height:16px; text-indent:-9999px; border-radius:15px; background:url(/admin/resource/images/ico_comm.png) no-repeat -105px 0; vertical-align:middle; margin-left:5px; }
.frm-area .btn-text { display:inline-block; font-size:14px; padding-left:10px; line-height:21px;  }
.frm-area.add-conts { display:table; table-layout:fixed; width:100%; text-align: left;}
.frm-area.add-conts > div { display:table-cell; }
.frm-area.add-conts > div:first-child { width:100px; }

.login-top .links {display:flex; justify-content: space-between; align-items: center; margin-right:-40px;}
.login-top .links .kiam a { display:block; width:160px; height:26px; background:url(/admin/resource/images/kiam_logo2.png) no-repeat 0 0; }
.login-top .links .kosef a { display:block; width:120px; height:18px; background:url(/admin/resource/images/kosef_logo.png) no-repeat 0 0;}
.login-btn { margin-top:20px; }
.login-btn .btn.primary { width:100%; }

.iod-wrap { display:inline-block; margin-left:18px; }
.iod-wrap .btn { position:relative; min-width:40px; min-height:40px; background-repeat: no-repeat; background-position: center; padding:0; border-radius: 0; }
.iod-wrap .btn + .btn {margin-left:18px;}
.iod-wrap .btn.plus {background-image:url(/admin/resource/images/ico_plus.png); }
.iod-wrap .btn.minus {background-image:url(/admin/resource/images/ico_minus.png);}

.iod-wrap.type2 .btn { display:inline-block; margin: 0 auto;}
.iod-wrap.type2 .btn + .btn {margin-top:8px;}

.ui-tab-btns { display:flex; border-bottom:1px solid #DFE0E2; }
.ui-tab-btns:after { content:''; display:block; clear:both; }
.ui-tab-btn { position:relative; flex:1 1; }
.ui-tab-btn { display:block; height:64px; font-size:20px; color:#73737A; }
.ui-tab-btn.selected { font-weight:700; color:#222; }
.ui-tab-btn.selected:after { content:''; display:block; position:absolute; bottom:-1px; left:0; width:100%; height:2px; background:#EC008C; }
.ui-tab-pnl, .ui-tab-pnl[aria-hidden=true] { display:none; visibility:hidden; opacity:0; }
.ui-tab-pnl.selected,
.ui-tab-pnl[aria-hidden='false'] { display:block; visibility:visible; position:relative; opacity:1; z-index:1; transition:opacity 0.3s linear 2s; }

.tab-info { margin-top:10px; text-align:right; font-size:14px; color:#73737A;  }
.tab-info .msg { position:relative; display:inline-block; }
.tab-info .msg:before { content:'*'; }

.edit-area { height:353px; background:#F7F8F8; padding:16px; margin-top:8px; overflow-y:scroll;}


/* popup */
.modal-conts .info-title .msg {text-align:right; font-size:12px; color:#73737A;}
.frm-area.file-register {position: relative; display:flex;}
.file-btn {width:96px; height:40px; padding:0 8px; margin-left:8px; font-size: 14px; color: #222;  text-align: center; line-height:38px;  border:1px solid #222; border-radius:4px; cursor: pointer;}
.file-btn .file-inp {overflow: hidden; position: absolute; width: 1px; height: 1px; margin: -1px; padding: 0; clip: rect(0,0,0,0); border: 0;}
.file-text {font-size:14px; color:#222; padding:3px 0 0; line-height:21px;}
.file-del-btn {position: relative; width:20px; height: 16px; font-size: 0;}
.file-del-btn::before,
.file-del-btn::after {content:''; display:block; position:absolute; left:50%; top:0; width:14px; height:1px; margin-top:7px; margin-left:-7px; background:#222;}
.file-del-btn::before {transform: rotate(45deg);}
.file-del-btn::after {transform: rotate(135deg);}
.file-text i {display:inline-block;  margin-left:20px; color:#73737A;}
.modal-conts .error-info {padding-left:8px; margin:4px 0 0; font-size:12px;}
.modal-conts .error-info::before {display:none;}
.modal-conts .error-info::after {top:0; left:0; color:#F2303C; font-size:12px;}