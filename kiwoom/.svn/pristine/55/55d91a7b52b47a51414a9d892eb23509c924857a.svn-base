.main-horizontal-slide > ul li a .txt-box{ background-color: #FFF; padding: 32px 32px 80px 32px;}
.main-horizontal-slide > ul li a .stxt{ color: #74757E; font-size: 14px; font-weight: 400;}
.main-horizontal-slide > ul li a strong{ display: block; color: #222; font-weight: 600; margin-top: 8px; line-height: 24px;}
.main-horizontal-slide > ul li a::after{ content: ''; position: absolute; bottom: 0; right: 20px; width: 96px; height: 70px;}
.main-horizontal-slide > ul li a:hover p, .main-horizontal-slide ul li a:active p {color: #1A399E; }

.main-horizontal-slide > ul li:nth-of-type(1) a::after{ width: 96px; height: 72px; background: url(/kosef/pcWeb/resources/images/main/01.svg) no-repeat center center;}
.main-horizontal-slide > ul li:nth-of-type(2) a::after{ width: 115px; height: 72px; background: url(/kosef/pcWeb/resources/images/main/02.svg) no-repeat center center;}
.main-horizontal-slide > ul li:nth-of-type(3) a::after{ width: 118px; height: 72px; background: url(/kosef/pcWeb/resources/images/main/03.svg) no-repeat center center;}
.main-horizontal-slide > ul li:nth-of-type(4) a::after{ width: 121px; height: 72px; background: url(/kosef/pcWeb/resources/images/main/04.svg) no-repeat center center;}
.main-horizontal-slide > ul li:nth-of-type(5) a::after{ width: 117px; height: 72px; background: url(/kosef/pcWeb/resources/images/main/05.svg) no-repeat center center;}
