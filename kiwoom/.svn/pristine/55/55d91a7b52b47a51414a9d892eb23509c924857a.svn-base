.history-wrap .history-contents { border-top: none; }
.history-wrap .history-contents::before{ content: ''; width: 100%; height: 1px; position: absolute; top: 3px; background-color: #1A399E;}
.history-contents .history-item::before{ top: -20px; left: 0;}
.history-contents .slick-arrow{ position: absolute; top:-152px; right: 360px; width: 64px; height: 64px; border-radius: 50%; text-indent: -9999px; z-index: 1;}
.history-contents .slick-arrow.slick-disabled::before{content: ''; position: absolute; top: 0; left: 0; width: 64px; height: 64px; opacity: 0.5; background-color: #FFF; border-radius: 50%;}

.history-contents .slick-prev{ background: url(/kosef/moWeb/resources/images/circle_right.svg); transform: rotate(180deg); right: 440px;}
.history-contents .slick-next{ background: url(/kosef/moWeb/resources/images/circle_right.svg); }

.history-contents .slick-dots{ width: 100%; text-align:left; top:-3px;}
.history-contents .slick-dots li{ width: calc(100%/3); margin: 0; padding: 0;}
/* .history-contents .slick-dots li.slick-active:first-child::before{content: ''; display: block; width: 100%; height: 3px; background-color: #1A399E; animation: none;} */
.history-contents .slick-dots li.active.slick-active::before{content: ''; display: block; width: 100%; height: 3px; background-color: #1A399E; animation: full 1s forwards}
@keyframes full {
    0% {
        width: 0%;
    }
    100% {
        width: 100%;
    }
}
.history-contents .slick-dots li button::before{display: none;}
.history-contents .slick-dots li.active::before{ content: ''; display: block; width: 100%; height: 3px; background-color: #1A399E;} 
.history-contents .slick-dots li.stop:first-child::before{ content: ''; display: block; width: 100%; height: 3px; background-color: #1A399E;}
.history-contents .slick-dots li.stop:nth-of-type(2)::before{ content: ''; display: block; width: 100%; height: 3px; background-color: #1A399E;}
.history-contents .slick-dots li.stop.slick-active + .stop::before{content: ''; display: block; height: 3px; animation: unfull 1s forwards; background-color: #1A399E;}

@keyframes unfull {
    0% {
        width: 100%;
    }
    100% {
        width: 0%;
    }
}


