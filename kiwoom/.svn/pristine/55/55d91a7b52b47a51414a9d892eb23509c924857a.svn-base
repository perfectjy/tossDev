/*메인*/
.kosef-main{position: relative; min-width:1600px; }
.kosef-main .gnb-wrap{ position: fixed; width: 100%; }
.kosef-main .gnb-wrap h1 a{ width: 101px; height: 16px; background: url(/kosef/pcWeb/resources/images/common/ico_comm.png) -430px -380px no-repeat; }
.kosef-main .gnb-wrap .gnb:before{background: none;}
.kosef-main .gnb-wrap .gnb nav .menu > li > a{background: none; color: #FFF; font-weight: 600;}
.kosef-main .gnb-wrap .util .go-kiam{color: #FFF;}
.kosef-main .gnb-wrap .util .go-kiam::after{ background: url(/kosef/pcWeb/resources/images/common/ico_comm.png) -140px -20px no-repeat; }
.kosef-main .gnb-wrap .ui-drop-wrap .ui-drop-btn{color: #B9BBC6;}
.kosef-main main{ height: 100%; animation: zoom 30s ease-out both infinite; background: #10507f url(/kosef/pcWeb/resources/images/main/main_02.jpg) center center no-repeat fixed; opacity: 1;}
.kosef-main main::before{ content: ''; position: absolute; left: 0; top:0; width: 100%; height: 100%; background-size: 100% auto; }
.kosef-main.main-bg01 main{ background: url(/kosef/pcWeb/resources/images/main/main_01.jpg) center center no-repeat fixed; } 
.kosef-main.main-bg02 main{ background: url(/kosef/pcWeb/resources/images/main/main_02.jpg) center center no-repeat fixed; } 
.kosef-main.main-bg03 main{ background: url(/kosef/pcWeb/resources/images/main/main_03.jpg) center center no-repeat fixed; animation: zoom2 18s ease-out both infinite; }
.kosef-main main .contents::before{ content: ''; position: absolute; left: 80px; width: 1px; height: 100%; background-color: rgb(255 255 255 / 20%);}
.kosef-main .gnb-wrap.show h1 a, .kosef-main .gnb-wrap.scroll h1 a{ background-position-y: -360px; }
.kosef-main .gnb-wrap.show .gnb:before, .kosef-main .gnb-wrap.scroll .gnb:before{background: #FFF;}
.kosef-main .gnb-wrap.show .gnb nav .menu, .kosef-main .gnb-wrap.scroll .gnb nav .menu{background-color: #FFF; }
.kosef-main .gnb-wrap.show .gnb nav .menu > li > a, .kosef-main .gnb-wrap.scroll .gnb nav .menu > li > a{ color:#292929 }
.kosef-main .gnb-wrap.show .util .go-kiam, .kosef-main .gnb-wrap.scroll .util .go-kiam{ color:#222; }
.kosef-main .gnb-wrap.show .util .go-kiam::after, .kosef-main .gnb-wrap.scroll .util .go-kiam::after{ background-position-y: 0; }
.kosef-main .gnb-wrap.show .ui-drop-wrap .ui-drop-btn{ color: #9D9DA4; }
.kosef-wrap .gnb-wrap.scroll { position: fixed; width: 100%; box-shadow: 0px 4px 8px rgba(34, 34, 34, 0.05);  }

/* .gnb-wrap .top-sch-group .btn-sch:after, .gnb-wrap .top-sch-group .btn-global:after{background: none;} */
@keyframes zoom{
    0%{
        background-size: 150% auto;
    }
    100%{
        background-size: 100% auto;
    }
}

@keyframes zoom2{
    0%{
        background-size: 115% auto;
    }
    100%{
        background-size: 100% auto;
    }
}


.gnb-wrap .util .top-sch-group .btn-sch{margin-left: 0;}

.kosef-main .contents{ border-top:solid 1px transparent;}
.kosef-main .contents-top{ position: relative; margin: 208px 0 0 360px; }
.kosef-main .contents-top .tit p{ color: #FFF; font-size: 20px; font-weight: 600; } 
.kosef-main .contents-top .tit strong{ display: block; width: 486px; height: 80px; margin-top: 24px; background: url(/kosef/pcWeb/resources/images/main_text.svg) no-repeat 0 0; }
.kosef-main .contents-top .tit  span{ display: block; color: rgba(255, 255, 255, 0.8); margin-top: 40px; line-height: 24px;}
.kosef-main .gnb-wrap .util .btn-sch::after{ background-position-x: -480px; background-position-y: 0px; }

@media (max-width: 1835px) {
    /* .kosef-main{ position: relative !important; } */
    .main-media{ padding: 40px 0 0 40px !important; }
    .kosef-main .gnb-wrap .gnb{ padding: 0 220px 0 150px; min-width: 580px;}
    .kosef-main .contents-top{ margin: 208px 0 0 100px; }
    .kosef-main .contents_bottom{ padding: 72px 0 120px 100px !important;}
    .kosef-main .gnb-wrap .gnb nav .menu > li{ min-width: 200px; }
    .gnb-wrap .util{right: 54px !important;}
    .kosef-main main{background-size: auto auto !important; min-width: 1400px;}
    .gnb-wrap .gnb{ padding: 0 220px 0 150px; min-width: 580px; }
}

/* 메인 검색*/
.gnb-wrap .top-sch-group { z-index: 100; display: inline-block; width:40px; height:40px; position: absolute; top: -10px; right: -56px; vertical-align: middle; }
.gnb-wrap .top-sch-group .sch-cont { display:none; position:fixed; left:0; right:0; top:0; z-index:901;  height:375px; background:#fff; }
.gnb-wrap .top-sch-group .search-area { width:780px; margin:0 auto; padding:120px 0; }
.gnb-wrap .top-sch-group .btn-close { position:absolute; top:0; right:0; width:120px; height:120px;  background: #1A399E;  }
.gnb-wrap .top-sch-group .btn-close:after { content: ''; display:block; position:absolute; top:50%; left:50%; right: inherit; transform: translate(-50%, -50%);  width: 48px; height: 48px; background: url(/kosef/pcWeb/resources/images/common/ico_comm.png) no-repeat -540px -330px; }
.gnb-wrap.scroll .util .btn-sch::after{background: url(/kosef/pcWeb/resources/images/common/ico_comm.png)  0 -40px no-repeat;}

.search-area { width:790px; margin:0 auto; padding:60px 0 0 0;  } 
.search-area .sch-top { position:relative; height:72px; padding:0 40px; border:2px solid #1A399E; border-radius:100px; }
.search-area .sch-top.active .btn-sch::before{display: none;}
.search-area .sch-top input { width:calc(100% - 60px); height:100%; font-size:20px; line-height:30px; font-weight:500; background:transparent; border:0;  }
.search-area .sch-top input::placeholder { opacity:0.6; }
.search-area .sch-top .btn-sch { position:absolute; top:50%; right:20px; margin-top:-20px; width:40px; height:40px; background:url(/kosef/pcWeb/resources/images/common/ico_comm.png) no-repeat -260px -70px;  }

.search-area .sch-top .btn-clear { right:0; }
.search-area .sch-tips { margin-top:30px; text-align:center; display:flex; align-items:center; justify-content:center; }
.search-area .sch-tips dt { margin-right:20px; color:#74757E; }

.search-area .sch-tips button { display:inline-block; height:40px; margin:0 4px; padding:0 24px; background:#fff; border:1px solid #B9BBC6; font-size:14px; border-radius:100px;  }
.search-area .sch-tips button:hover { border-color:#EC008C; color:#EC008C; }
.search-area .sch-tips button.active { background:#EC008C; border-color:#EC008C; color:#fff;  }

.gnb-wrap .top-sch-group:before { content: ''; display: block; position: fixed; left: 0;  top: 0;  right: 0;  bottom: 0;  z-index: 900;  height: 0;  background: rgba(0, 0, 0, .5); opacity: 0; transition: opacity 0.3s ease, height 0s 0.3s;  overflow: auto;  -webkit-overflow-scrolling: touch; }
.gnb-wrap .top-sch-group.active:before { display: block; opacity: 1; height: 100%; transition-delay: 0s, 0s;  }


.maia-sch-wrap{ position: relative; width: 384px; height: 56px; border-bottom: 1px solid #fff; margin-top: 40px;}
.maia-sch-wrap input[type="text"]{ width: 100%; color: #FFF; font-size: 20px; font-weight: 700; padding: 13px 50px 13px 0; background-color:transparent; }
.maia-sch-wrap .btn-sch{ position: absolute; top: 50%; right: 0; width: 40px; height: 40px; transform:translateY(-50%); background: url(/kosef/pcWeb/resources/images/main/main_circle_search.svg) center center no-repeat;}

.main-slide{ position: absolute; top:56px; right: 158px; width: 712px;}
.main-slide > ul{position: relative;}
.main-slide > ul li a{ display: flex; }
.main-slide > ul li a .slide-con{ position: relative; display: flex; flex-direction:column; width: 580px; padding: 48px 40px;}
.main-slide > ul li a .slide-con::after{ content: ''; width: 100%; position: absolute; top: 0; left: 0; height: 100%; background-image: url(/kosef/pcWeb/resources/images/main/bg_blur.png);}
.main-slide > ul li a .slide-con span{ color:#FFF; font-size: 14px; font-weight: 500;}
.main-slide > ul li a .slide-con strong{ color:#FFF; font-size: 32px; margin-top: 12px; }
.main-slide > ul li a .slide-con p{ color: #FFF; font-size: 16px; font-weight: 400; margin-top: 18px;}
.main-slide .slick-arrow{ position: absolute; top: 50%; transform:translateY(-50%);  width: 40px; height: 40px; text-indent: -9999px;}
.main-slide .slick-prev{ left: -50px; background: url(/kosef/pcWeb/resources/images/main/main_arrow_left.svg) center center no-repeat; }
.main-slide .slick-next{ right: -50px; background: url(/kosef/pcWeb/resources/images/main/main_arrow_right.svg) center center no-repeat; }
.main-slide .control{ display: flex; position: absolute; top: -28px; right: 0;}
.main-slide .control > button{ width: 16px; height: 16px; margin-left: 4px;}
.main-slide .control .play{ background: url(/kosef/pcWeb/resources/images/main/play.svg) center center no-repeat; }
.main-slide .control .pause{ background: url(/kosef/pcWeb/resources/images/main/pause.svg) center center no-repeat;}




.main-slide .slick-dots{position: relative;  display: flex !important; bottom: inherit;}
.main-slide .slick-dots li{ position: relative; margin: 0 2px; border-radius: 50%;}
.main-slide .slick-dots li::after{ content: ''; position: absolute; top:50%; left:50%; transform: translate(-50%, -50%); width: 6px; height: 6px; background-color: #FFF; opacity: 0.4; border-radius: 50%;}
.main-slide .slick-active::after{ opacity: 1 !important; }
.main-slide .slick-dots li button{width: 16px; height: 16px; border-radius: 50%;}
.main-slide .slick-dots li button span{ display: inline-block; text-indent: -9999px;}

.main-slide .slick-active .spinner{position: absolute; left: 1px; top:1px;}

.main-slide .slick-active .spinner .path{ stroke-dasharray: 500; stroke-dashoffset: 0; transform: rotate(-90deg); animation: rotate 19s; stroke: #FFF; transform-origin: center;}
@keyframes rotate {
    0% {
        stroke-dashoffset: 500;
    }
    100% {
        stroke-dashoffset: 0;
    }
}
.kosef-main .contents_bottom{ display: flex; position: relative; border-top: 1px solid rgb(255 255 255 / 20%);  margin-top: 160px; padding: 72px 0 120px 360px;}
.kosef-main .contents_bottom::before{ content: ''; position: absolute; top: 292px; left: 80px; width: 1px; background-color: rgb(255 255 255 / 20%); z-index: 2; }
.kosef-main .contents_bottom .choice{ display: flex; flex-direction:column; width: 306px; z-index: 2;}
.kosef-main .contents_bottom .choice .tit{ color:rgba(255, 255, 255, 0.8); font-size: 16px; line-height: 24px; margin-top: 60px;}

.sub-wrap{ min-width: 1400px; }
.sub-wrap.no-overflow{ overflow: inherit; }
.is-overflow{ overflow: hidden; }

.contents_bottom .choice .ui-select.first{ margin-top: 12px; color: #FFF;}
.contents_bottom .choice .ui-select.first .ui-select-btn{ color: #FFF; font-size: 32px; font-weight: 700; border: none; padding: 0 18px 0 0;}
.contents_bottom .choice .ui-select.first .ui-select-btn:after{ width: 16px; height: 16px; background: url(/kosef/pcWeb/resources/images/common/ico_comm.png) no-repeat -168px -88px; opacity: 1;}
.contents_bottom .choice .ui-select.on.first .ui-select-btn{border: none;}
.contents_bottom .choice .ui-select.on.first .ui-select-btn:after{ transform: rotate(180deg) translateY(50%); }
.contents_bottom .choice .ui-select.second{ width: 160px; margin-top: 32px; }
.contents_bottom .choice .ui-select.second .ui-select-btn{ color: #FFF; font-size: 16px; font-weight: 600; padding: 0 18px 0 26px;  border-radius: 24px; }
.contents_bottom .choice .ui-select.second .ui-select-btn:after{ width: 16px; height: 16px; background: url(/kosef/pcWeb/resources/images/common/ico_comm.png) no-repeat -424px -43px; opacity: 1;}
.contents_bottom .choice .ui-select.on.second .ui-select-btn{border-color: #FFF;}
.contents_bottom .choice .ui-select.on.second .ui-select-btn:after{ transform: rotate(180deg) translateY(50%); }
.contents_bottom .result{ position: relative; width: calc(100% - 306px); height: 462px; z-index: 2;}
.contents_bottom .result ul{ display: flex; justify-content: flex-end;}
.contents_bottom .result ul li{ position: relative; }
.contents_bottom .result ul li a{ display: flex; flex-direction: column; position: relative; width: 376px; height: 400px; padding: 48px; margin: 62px 0 0 24px; background-color: #FFF; z-index: 1; }
.contents_bottom .result ul li::after{ content: ''; position: absolute; top: 0; right: 0; width: 109px; height: 96px; background: url(/kosef/pcWeb/resources/images/common/ico_conts.png) no-repeat 0px -400px; }
.contents_bottom .result ul li:nth-of-type(1)::after{ background-position-y: -400px; }
.contents_bottom .result ul li:nth-of-type(2)::after{ width:122px; background-position-X: -118px; }
.contents_bottom .result ul li:nth-of-type(3)::after{ width:125px; background-position-X: -264px; }
.contents_bottom .result ul li a:hover, .contents_bottom .result ul li a:active{ background: linear-gradient(114.62deg, #394DE4 6.07%, #953DEE 96.44%); }
.contents_bottom .result a:hover .tag-wrap .t-local, .contents_bottom .result a:active .tag-wrap .t-local{ color: #FFF; border-color: #FFF;}
.contents_bottom .result a:hover .tag-wrap .t-bond, .contents_bottom .result a:active .tag-wrap .t-bond{ color: #B9BBC6; border-color: #B9BBC6;}
.contents_bottom .result a:hover .tag-wrap .t-high, .contents_bottom .result a:active .tag-wrap .t-high{ color: #F083C7; border-color: #F083C7;}
.contents_bottom .result a:hover .tit, .contents_bottom .result a:active .tit{ color: #FFF; }
.contents_bottom .result a:hover p, .contents_bottom .result a:active p{ color: #FFF; }
.contents_bottom .result a:hover span strong, .contents_bottom .result a:active span strong{ color: #FFF; }
.contents_bottom .result a:hover span .sp-txt, .contents_bottom .result a:active span .sp-txt{ color: #FFF; }
.contents_bottom .result a:hover .info > span, .contents_bottom .result a:active .info > span{ color: #B9BBC6;}


.contents_bottom .result .tit{ color: #222; font-size: 24px; margin-top: 24px; line-height: 36px;} 
.contents_bottom .result p{ color: #222; font-size: 20px; font-weight: 400; margin-top: 8px;}
.contents_bottom .result .info{ display: flex; flex-direction: column; position: absolute; bottom: 48px; width: calc(100% - 48px * 2); color: #74757E; font-size: 14px; font-weight: 400;}
.contents_bottom .result .info > span{ display: flex; justify-content: space-between; align-items: center; }
.contents_bottom .result .info > span:not(:first-child){ margin-top: 16px; }
.contents_bottom .result .info > span .sp-txt span{ font-size: 20px; line-height: 30px; margin-left: 2px;}
.contents_bottom .result .info > span strong{ display: flex; align-items: center; color: #222; font-size: 24px; }
.contents_bottom .result .info > span strong span{font-size: 20px; font-weight: 600; margin-left: 2px;}
.contents_bottom .result .info .txt-up{ color: #EC008C; font-size: 32px;}
.contents_bottom .result .info .txt-drop{ color: #1E66F2; font-size: 32px;}
.main .ui-select-wrap{ border: none; background: #222; padding: 0; margin-top: 8px; border-radius: 0px;}
.main .ui-select-wrap .ui-select-opt{ color:#FFF; background-color: #222; }
.main .ui-select-wrap .ui-select-opt.selected {background-color: rgba(146, 176, 255, 0.16);}
.main .ui-select-wrap .ui-select-opt:hover {background-color: rgba(146, 176, 255, 0.16);}

.tab-nav{ position: fixed; left: 50%; bottom: 24px; transform: translateX(-50%); display: flex; justify-content: center; padding: 8px 12px; background: rgba(116, 117, 126, 0.82); border-radius: 32px; z-index: 100;}
.tab-nav ul{ display: flex; justify-content: center; align-items: center;}
.tab-nav li button{ min-width: 110px; color: #DFE0E2; font-size: 16px; font-weight: 600; padding: 12px 32px;}
.tab-nav li button:hover{color: #FFF;}

.tab-nav li .on{ background-color: #222; border-radius: 24px;}


main.selected, .main-media.selected{display: block;}
.kosef-main.fix{ position: fixed; width: 100%; z-index:1000; }
.main-media{ display: none; position: fixed ; top: 88px; left: 80px; width: calc(100% - 80px); height: calc(100% - 88px); padding: 80px 0 0 280px; background-color: #F7F8F8; z-index: 902; overflow-y: scroll;}
.main-media::before{ content: ''; display: block; position: fixed; left: 0;  top: 0;  right: 0;  bottom: 0; height: 0; opacity: 0; transition: opacity 0.3s ease, height 0s 0.3s;  overflow: auto;  -webkit-overflow-scrolling: touch; }
.main-media.selected::before{ display: block; opacity: 1; height: 100%; transition-delay: 0s, 0s; }
.main-media .tit-wrap .tit{ color:#222;  font-size: 48px; font-weight: 700; line-height: 57px; }
.main-media .tit-wrap { position: relative; width: 1200px; }
.main-media .tit-wrap .main-tab-wrap{position: absolute; top:26px ; right: 0; }
.main-media .main-tab-wrap ul{ display: flex; border: none;}
.main-media .main-tab-wrap ul li{margin-left: 24px;}
.main-media .main-tab-wrap ul li .main-tab-btn{ position: relative; color: #B9BBC6;font-size: 24px; font-weight: 700; padding: 6px 0; height: inherit;}
.main-media .main-tab-wrap ul li .main-tab-btn.selected{ color:#222; }
.main-media .main-tab-wrap ul li .main-tab-btn.selected::after{ content: ''; position: absolute; bottom: 0; left: 0; width: 100%; height: 2px; background-color: #222;; }


.main-media .main-close{ position: absolute; right: 24px; bottom: 24px; width: 64px; height: 64px; background: url(/kosef/pcWeb/resources/images/main/circle_close.svg) no-repeat 0 0; z-index: 10;}


.hash-tag-wrap .ui-tab-btns{ display: inline-flex; }
.hash-tag-wrap .ui-tab-btns .ui-tab-btn{ flex: none; }
.hash-tag-wrap .ui-tab-btns .ui-tab-btn.on{color:#fff; font-weight:700; background-color:#EC008C;  border-color:#EC008C;}
.main-media .hash-tag-wrap{ position: relative; width: 1200px; margin-top: 48px;}
.main-media .hash-tag-wrap .mlink{ display: flex; align-items:center; position: absolute; top:0; right: 0; height: 40px; background: linear-gradient(114.62deg, #394DE4 6.07%, #953DEE 96.44%); border-radius: 20px; padding: 5px 32px 5px 47px;}
.main-media .hash-tag-wrap .mlink.mg::before{ content: ''; position: absolute; left: 12px; width: 24px; height: 24px; background: url(/kosef/pcWeb/resources/images/main/money_bag.svg) 0 0 no-repeat; margin-right: 8px; animation: wbtni .6s forwards; z-index: 10;}
.main-media .hash-tag-wrap .mlink.tv::before{ content: ''; position: absolute; left: 12px; width: 24px; height: 24px; background: url(/kosef/pcWeb/resources/images/main/cash.svg) 0 0 no-repeat; margin-right: 8px; animation: wbtni .6s forwards; z-index: 10;}
.main-media .hash-tag-wrap .mlink span{ color: #DFE0E2; font-size: 14px; font-weight: 500; }
.main-media .hash-tag-wrap .mlink .ctxt{ position: relative; width: 138px; animation: wbtn .5s forwards; height: 14px; overflow: hidden;}
.main-media .hash-tag-wrap .mlink .txt{ width: inherit; color:#FFF; font-size: 14px; font-weight: 700; margin-left: 12px; }
.main-media .hash-tag-wrap .mlink .txt::after{ content: ''; position: absolute; top: 12px; right: 12px; width: 16px; height: 16px; background: url(/kosef/pcWeb/resources/images/main/ch_right.svg) 0 0 no-repeat;}
@keyframes wbtn {
    0%{
        opacity: 0;
        width: 0px;
    }
    80%{
        opacity: 0.9;
    }
    100%{
        opacity: 1;
        width: 138px;
    }
}

@keyframes wbtni {
    0%{
        left: 70px;
    }
    100%{
        left: 12px;
    }
}



.main-horizontal-slide{ margin-top: 48px; }
.main-horizontal-slide > ul li{ margin-right: 24px;}
.main-horizontal-slide > ul li.slick-active{position: relative; }
.main-horizontal-slide > ul li:nth-child(1){animation: slidet .5s forwards; animation-delay: 0; bottom:-120px}
.main-horizontal-slide > ul li:nth-child(2){animation: slidet .7s forwards; animation-delay: 0; bottom:-120px}
.main-horizontal-slide > ul li:nth-child(3){animation: slidet .9s forwards; animation-delay: 0; bottom:-120px}
.main-horizontal-slide > ul li:nth-child(4){animation: slidet 1.1s forwards; animation-delay: 0; bottom:-120px}
@keyframes slidet {
    0%{
        bottom: -320px;
    }
    100%{
        bottom: 0;
    }
}
.main-horizontal-slide > ul li a{ display: flex; flex-direction: column; position: relative; height: 440px;}
.main-horizontal-slide > ul li a .vd{display: inline-flex; position: relative;}
.main-horizontal-slide > ul li a .vd img{ width: 100%; }
