@charset "utf-8";

/* tab */
.ui-tab-wrap {font-size:0;}
.ui-tab-list {display:flex; flex-wrap:nowrap; white-space:nowrap;  border-bottom:1px solid #DFE0E2;}
.ui-tab-list li {position:relative; flex:1; min-width:auto;}
.ui-tab-list li .ui-tab-btn {display:flex; width:100%; height:64px; padding:17px 24px; justify-content:center; align-items:center; text-align:center;}
.ui-tab-content {margin:40px auto 0;}
.ui-tab-list.sub {margin:-16px 0 40px; border-bottom:0;}
.ui-tab-list.sub li {display:inline-block; flex-shrink:0; flex-grow:0; margin-right:8px; }
.ui-tab-list.sub li .ui-tab-btn {display:block; min-width:160px; height:40px;  padding:0 24px; line-height:38px; font-size:14px; color:#222; border:1px solid #B9BBC6; background-color:#fff; border-radius:20px;}
.ui-tab-list.sub li .ui-tab-btn::after {display:none;}
.ui-tab-list.sub li .ui-tab-btn.selected {color:#fff; background-color:#EC008C; border:transparent;}

.ui-drop-input .ui-select {flex-shrink:0; width:152px;}
.ui-drop-input .ui-input-txt {flex-shrink:0; width:333px;}
.ui-drop-input .inp {border-radius:0 8px 8px 0; }

/* sub header */
.sub-wrap.lounge .sub-head-wrap, .sub-wrap.talent .sub-head-wrap, .sub-wrap.announce .sub-head-wrap, .sub-wrap.pension .sub-head-wrap {height:271px; padding:88px 0 0 0; text-align:center; color:#222;  background:url(/kiam/pcWeb/resources/images/bg_lounge.png) no-repeat center top ;   border-radius:56px 0 56px 56px;}
.sub-wrap.lounge .sub-head-wrap .head-info, .sub-wrap.talent .sub-head-wrap .head-info, .sub-wrap.announce .sub-head-wrap .head-info {margin-top:16px; color:#74757E; font-size:20px; line-height:30px;  } 
.sub-wrap.talent .sub-head-wrap {background-image:url(/kiam/pcWeb/resources/images/bg_lounge_talent.png)}
.sub-wrap.esg .sub-head-wrap {background-image:url(/kiam/pcWeb/resources/images/bg_esg.png)}
.sub-wrap.company .sub-head-wrap {background-image:url(/kiam/pcWeb/resources/images/bg_company.png)}
.sub-wrap.announce .sub-head-wrap {background-image:url(/kiam/pcWeb/resources/images/bg_announce.png)}
.sub-wrap.pension .sub-head-wrap {background-image:url(/kiam/pcWeb/resources/images/sub/bg_pension1.png)}
.sub-wrap.pension.bg1 .sub-head-wrap {background-image:url(/kiam/pcWeb/resources/images/sub/bg_pension2.png)}
.sub-wrap.pension.bg2 .sub-head-wrap {background-image:url(/kiam/pcWeb/resources/images/sub/bg_pension3.png)}
.sub-wrap.pension.bg3 .sub-head-wrap {background-image:url(/kiam/pcWeb/resources/images/sub/bg_pension4.png)}
.sub-wrap.pension.bg4 .sub-head-wrap {background-image:url(/kiam/pcWeb/resources/images/sub/bg_pension5.png)}
.sub-wrap.pension.bg5 .sub-head-wrap {background-image:url(/kiam/pcWeb/resources/images/sub/bg_pension6.png)}
.sub-wrap.pension.bg6 .sub-head-wrap {background-image:url(/kiam/pcWeb/resources/images/sub/bg_pension7.png)}

/* lounge 키움라운지 */
.list-thumb {margin:24px 0 0; border-top:1px solid #222;}
.list-thumb:after {content:''; display:block; clear:both;}
.list-thumb .list-item {position:relative; float:left; width:384px; margin:24px 0 0 24px;}
.list-thumb .list-item::after {content:''; display:block; clear:both; }
.list-thumb .list-item a {overflow:hidden; display:block; position:relative; width:100%; height:100%; border:1px solid #dfe0e2; border-radius:24px 0 24px 24px;}
.list-thumb .list-item .imgs {overflow:hidden; position:relative; display:block; padding-bottom:56.25%;}
.list-thumb .list-item:nth-child(3n+1) {margin-left:0;}
.list-thumb .list-item .imgs img {position:absolute; left:0; right:0; bottom:0; top:0; width:100%; transition:all .15s ease-out;}
.list-thumb .list-item .video::before {z-index:1; content:''; position:absolute; left:0; right:0; top:0; bottom:0; width:100%; height:100%; background-color:#222; opacity:0.2; }
.list-thumb .list-item .imgs.video:after {z-index:10; content:''; position:absolute; top:50%; left:50%; width:48px; height:48px; background:url(/kiam/pcWeb/resources/images/common/ico_comm.png) no-repeat -460px -70px; background-size:600px auto; transform:translate(-50%,-50%); }
.list-thumb .list-cont > * {display:block;}
.list-thumb .list-cont {display:block; padding:24px 32px;}
.list-thumb .list-cont .stxt {margin-bottom:8px; font-size:14px; line-height:21px; color:#74757E;}
.list-thumb .list-cont .tit {overflow:hidden; margin-bottom:8px; font-size:16px; height:48px; line-height:24px; color:#222; font-weight:600;  display:-webkit-box; -webkit-box-orient: vertical; -webkit-line-clamp:2; text-overflow:ellipsis; word-break: break-word; }
.list-thumb .list-cont .date {font-size:14px; line-height:21px; color:#74757E; font-weight:400;} 
.list-thumb .list-item .d-day {z-index:10; position:absolute; display:block; top:0px; right:0; min-width:61px; height:40px; padding:0 16px; line-height:40px; font-size:16px; font-weight:600; background-color:#1a399e; color:#fff; text-align:center; border-radius:0px 0px 0px 8px;}
.list-thumb .list-item a:hover,
.list-thumb .list-item a:focus {border-color:#1A399E;}
.list-thumb .list-item a:hover .imgs img {transform:scale(1.1);}
.list-thumb .list-item a:hover .tit,
.list-thumb .list-item a:focus .tit {color:#1A399E;}
.list-thumb .list-item.event-end .tit,
.list-thumb .list-item.event-end .date {color:#9D9DA4;}
.list-thumb .list-item.event-end .imgs:after {display:block; content:''; position:absolute; top:0; right:0; left:0; bottom:0; background:rgba(157, 157, 164, 0.8);}
.list-thumb .list-item.event-end a:hover,
.list-thumb .list-item.event-end a:focus {border-color:#dfe0e2;}
.list-thumb .list-item.event-end a:hover .tit,
.list-thumb .list-item.event-end a:focus .tit {color:#9D9DA4;}
.sub-wrap.lounge .ui-tab .ui-tab-btns + .ui-tab-pnls {margin:40px auto 0;}

