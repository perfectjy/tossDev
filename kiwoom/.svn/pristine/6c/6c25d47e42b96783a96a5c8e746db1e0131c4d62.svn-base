.file-box .btn-file span::before {content:''; display:inline-block; width:24px; height:24px; margin-right:8px; background:url(/kiam/pcWeb/resources/images/common/ico_comm.png) no-repeat -510px -40px; background-size:600px auto; vertical-align:middle;}
