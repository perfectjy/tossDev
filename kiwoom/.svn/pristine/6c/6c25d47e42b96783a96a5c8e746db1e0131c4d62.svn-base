.ocio-fund-wrap {width:100%; padding:80px 118px; margin:0 0 24px; background-color:#F7F8F8; border-radius:56px; text-align:center;}
.ocio-fund-wrap .tit {margin-bottom:8px; font-size:24px; line-height:36px; font-weight:700;}
.ocio-box {width:800px; margin:40px auto;}
.ocio-box:after {display:block; content:''; clear:both;}
.ocio-box > dl {width:280px;}
.ocio-box .left {float:left; text-align:center;}
.ocio-box .right {float:right; text-align:center;}
.ocio-box dt {display:block; margin-bottom:8px; font-size:24px; line-height:36px;}
.ocio-box .left dt { color:#1A399E;}
.ocio-box dd {position:relative;}
.ocio-box dd:before {display:block; content:''; width:104px; height:104px; margin:0 auto; background:url(/kiam/pcWeb/resources/images/common/ico_funds.png) no-repeat -450px 0;}
.ocio-box .right dd:before {background-position-x:-570px;}
.ocio-box dd strong {display:block; margin:8px 0 4px; color:#222; font-size:16px; line-height:24px; font-weight:700;}
.ocio-box dd span {display:block; font-size:14px; line-height:21px; color:#74757E;}
.ocio-box .right dt {color:#EC008C}
.blue-box {width:100%; height:48px; margin:24px 0; color:#fff; text-align:center; line-height:48px; background:linear-gradient(100.59deg, #1A399E 62.77%, #4D38CE 103.34%); border-radius:24px;}
.list-box {display:flex; flex-wrap:wrap; padding-top:40px; border-radius:24px;}
.list-box > div {position:relative; flex:1; text-align:left;}
.list-box > div + div {margin-left:64px;}
.list-box > div:before {display:block; content:''; position:absolute; margin:0 28px 0 0; width:48px; height:48px; background-repeat:no-repeat; background-image:url(/kiam/pcWeb/resources/images/common/ico_funds.png); background-size:800px 300px;}
.list-box > div:nth-child(1):before {background-position:-150px -100px;}
.list-box > div:nth-child(2):before {background-position:-200px -100px;}
.list-box > div:nth-child(3):before {background-position:-250px -100px;}
.list-box > div:nth-child(4):before {background-position:-300px -100px;}
.list-box > div dt {padding-left:77px; line-height:24px; color:#222; font-weight:600;}
.list-box > div dd {padding-left:77px; font-size:14px; line-height:21px; color:#74757E;}
.link-wrap {position:relative; height:425px; padding:70px 120px 0; border-radius:56px; background:#FFF5FC url(/kiam/pcWeb/resources/images/sub/bg_phone.png) no-repeat right 120px bottom -19px / auto 389px; }
.link-wrap .tit {display:block; color:#EC008C; font-weight:700;}
.link-wrap .tit-s {margin:16px 0 40px; font-size:32px; line-height:42px; color:#222;}
.link-wrap .txt {margin-bottom:40px; font-size:16px; line-height:24px; color:#74757E; text-align:left;}
.txt-box {position:relative; margin:0 0 160px;}
.txt-box:after {content:''; display:block; width:100%; height:280px; margin:56px auto 0; background:url(/kiam/pcWeb/resources/images/sub/bg_annuity3.png) no-repeat center / 100%;}
.annuity3 .info-box.bg {flex-direction:initial; flex-wrap:wrap; margin:0;}
.annuity3 .info-box.bg li {min-width:435px; margin-bottom:24px;}
.annuity3 .info-box.bg li + li {margin-left:0;}
.annuity3 .info-box.bg li:nth-child(even) {margin-left:24px;}
.annuity3 .info-box.bg li:nth-child(1):before {background-position-x:-350px;}
.annuity3 .info-box.bg li:nth-child(2):before {background-position-x:-400px;}
.annuity3 .info-box.bg li:nth-child(3):before {background-position-x:-450px;}
.annuity3 .info-box.bg li:nth-child(4):before {background-position-x:-500px;}
.img-box.process .txt {margin:40px 0 0; font-size:16px; line-height:24px;}
.btn-box-wrap {font-size:0;}
.btn-box-wrap .btn.link {position:relative; padding:40px; min-width:435px; justify-content:flex-start; border:1px solid #DFE0E2; border-radius:24px 0px 24px 24px;}
.btn-box-wrap .btn.link + .btn.link {margin-left:24px;}
.btn-box-wrap .btn.link span {position:relative; display:inline-block; width:100%; text-align:left; font-size:20px; line-height:30px;}
.btn-box-wrap .btn.link span:before {display:inline-block; content:''; position:absolute; top:10px; right:0; width:40px; height:40px; background:url(/kiam/pcWeb/resources/images/common/ico_comm.png) no-repeat -310px -70px; opacity:.5;}
.btn-box-wrap .btn.link:hover, 
.btn-box-wrap .btn.link:active {border:2px solid #1A399E; box-shadow:0px 2px 4px rgba(26, 57, 158, .16);}
.btn-box-wrap .btn.link:hover span,
.btn-box-wrap .btn.link:active span {color:#1A399E;}
.btn-box-wrap .btn.link:hover span:before,
.btn-box-wrap .btn.link:active span:before {opacity:1;}
.personal1 .txt-box:after {background-image:url(/kiam/pcWeb/resources/images/sub/bg_annuity4.png);}
.personal1 .txt-box {margin:0;}
.divide-box {position:relative; padding:16px 64px 24px; background:linear-gradient(100.59deg, #1A399E 62.77%, #4D38CE 103.34%); border-radius:56px;}
.divide-box .tit {z-index:1; position:absolute; left:50%; top:50%; width:240px; height:240px; background-color:#fff; color:#1A399E; text-align:center; border:16px solid #1A399E; border-radius:50%; transform:translate(-50%, -50%);}
.divide-box .tit span {position:absolute; left:50%; top:50%; transform:translate(-50%, -50%); font-size:24px; line-height:36px; font-weight:700;}
.divide-box [class^=list-info]:before,
.divide-box [class^=list-info]:after {content:''; display:block; position:absolute; height:1px; opacity:0.2; background-color:#B9BBC6;}
.divide-box [class^=list-info]:before {top:50%; left:0;  width:1072px; transform:translateY(-50%);}
.divide-box [class^=list-info]:after {width:512px; left:280px; transform:rotate(90deg)translateY(-50%)}
.divide-box [class^=list-info] {position:relative; display:flex; flex-wrap:wrap; justify-content:space-between; align-items:center;}
.divide-box [class^=list-info] > div {width:382px; padding:88px 0 94px 0;}  
.divide-box [class^=list-info] > div:nth-child(odd) {padding-left:30px;} 
.divide-box [class^=list-info] > div dt:after {content:''; display:inline-block; width:40px; height:40px; margin-left:12px; vertical-align:sub; background:url(/kiam/pcWeb/resources/images/common/ico_funds.png) no-repeat 0 -160px / 800px auto;}
.divide-box [class^=list-info] dt {position:relative; margin-bottom:24px; color:#fff; font-size:32px; line-height:42px; text-align:center;}
.divide-box [class^=list-info] dd {color:#DFE0E2;}
.divide-box .list-info1 > div:nth-child(2) dt:after {background-position-x:-50px;}
.divide-box .list-info1 > div:nth-child(3) dt:after {background-position-x:-100px;}
.divide-box .list-info1 > div:nth-child(4) dt:after {background-position-x:-150px;}
.divide-box .list-info2 > div:nth-child(1) dt:after {background-position-x:-200px;}
.divide-box .list-info2 > div:nth-child(2) dt:after {background-position-x:-300px;}
.divide-box .list-info2 > div:nth-child(3) dt:after {background-position-x:-250px;}
.divide-box .list-info2 > div:nth-child(4) dt:after {background-position-x:-350px;}
.divide-box .txt-list.disc:before {background-color:#DFE0E2;} 
.divide-box .txt-list li {color:#B9BBC6;}
.divide-box p.txt-list.disc {margin-top:0; font-weight:500; color:#B9BBC6;}
.divide-box p.txt-list.disc::before {background-color:#DFE0E2;}
.btn-wrap {position:relative; font-size:0;}
.btn-wrap a.btn {margin-left:16px;}
.btn-wrap .share-sns { vertical-align:top;}
.btn-wrap .share-sns.active .tip-sns {top:60px;}
.letter .txts-wrap {position:relative; padding:35px 0 0; margin:80px 0 0; height:640px; background:url(/kiam/pcWeb/resources/images/sub/img_letter.png) no-repeat right bottom / 582px 640px; text-align:left;}
.letter .txts-wrap:before {content:''; display:block; width:211px; height:98px; margin:0 0 40px; background:url(/kiam/pcWeb/resources/images/common/ico_funds.png) no-repeat -570px -110px;}
.letter .txts-wrap .txt {margin-bottom:24px; text-align:left;}
.letter .txt.yellow {display:block; margin-bottom:40px; font-size:32px; line-height:42px; color:#e9b63e;}
.letter .txt strong {font-size:24px; line-height:36px;}
.letter .h2 {margin-bottom:24px; text-align:center;}
