            var fundSummarySliders = {                    
                1: {slider : '#summarySlide1'},
                2: {slider : '#summarySlide2'},
                3: {slider : '#summarySlide3'},
                4: {slider : '#summarySlide4'},
                5: {slider : '#summarySlide5'},
            };

            var mainSummarySlide = function () {
                $.each(fundSummarySliders, function() {
                    $(this.slider).slick({ 
                        dots: true,              
                        slidesToShow: 1,
                        variableWidth:true,
                        slidesToScroll:1
                    });
                });
            }
            mainSummarySlide();   

            // 상단 띠배너 보기
            KAUI.common.topBnr.show();
            KAUI.tab.init({ 
                id:'fundSummaryTab', 
                current:0,
                callback:() => {  
                    $.each(fundSummarySliders, function() {
                        $(this.slider).slick('unslick'); 	
                    });
                    mainSummarySlide();   
                } 
            }); // 20230321 추가    
                            
        })();        
        
    
