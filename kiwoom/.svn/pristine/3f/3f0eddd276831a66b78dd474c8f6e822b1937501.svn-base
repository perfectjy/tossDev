/* 자사 Main Tab */
.main-tab .ui-tab-btns {line-height:0; font-size:0;  border-bottom:none;}
.main-tab .ui-tab-btn { display:inline-block; height:48px; padding:0 25px; font-size:18px; background:#f7f7f7; color:#878c91; border-radius:24px;}
.main-tab .ui-tab-btn + .ui-tab-btn {margin-left:10px; }
.main-tab .ui-tab-btn.selected {color:#fff; background:#1A399E;}
.main-tab .ui-tab-pnls {margin-top:40px; }
.main-tab .ui-tab-btn.selected:after {display:none;}


/* Search Tab */
.main-tab.search {padding:20px; background-color:#1A399E;}
.main-tab.search .ui-tab-btn {height:40px; padding:0 20px; color:#fff; border:1px solid #fff; border-radius:100px; font-size:16px; background:transparent;}
.main-tab.search .ui-tab-btns {font-size:16px;}
.main-tab.search .ui-tab-btn.selected {background-color:#EC008C; border:none;}
.main-tab.search .ui-tab-pnls h3 {color:#fff;}


