@charset "utf-8";

/* common */
* { box-sizing:border-box; padding:0; margin:0; line-height:1; }
html, body { background:#fff; user-select:none; -webkit-user-select:none; -ms-user-select:none; -webkit-user-select:none; -webkit-tap-highlight-color:transparent; }
body { padding:0; margin:0; font-family: "Pretendard", Sans-serif; font-size:16px; font-weight:500; color:#222; word-break:break-all; letter-spacing:0; }

h1, h2, h3, h4, h5, h6 { font-weight:400; color:#000; }
a { text-decoration:none; outline-offset:1px; color:inherit; }
a:hover { text-decoration:none; }
p { line-height:1.5; }
dl, ol, ul, dt, dd, li { display:block; list-style:none; }
table { width:100%; border-collapse:collapse; border-spacing:0; }
caption { height:0; opacity:0; }
th, td { border-collapse:collapse; vertical-align:middle; }
button{overflow:visible}
button{margin:0;padding:0;border:0;background:none;width:auto;cursor:pointer;overflow:visible;vertical-align:middle;background: transparent;cursor:pointer; font-family: "Pretendard", Sans-serif; }
button span, button:active span, button:focus span{position:relative}
select, textarea, input { -webkit-appearance:none; border:0; }
select { height:40px; padding:0 10px 0 10px; }
input { border:0; }
em, i { font-style:normal; text-decoration:none;}
hr { position:relative; display:block; height:1px; border:0; background:#eee; margin:30px 0; }
img { border:0; }
.hanja { font-family:'맑은 고딕', '돋움', '굴림'; }


/* input box block type */
[class*="inp-"] { position:relative; display:inline-block; width:100%; height:40px; font-size:15px; color:#000; line-height:1; padding:12px 15px; border-radius:8px; background:#F7F8F8; outline:0; }
[class*="inp-"]:focus { border-color:#0091ea; }
[class*="inp-"]::placeholder { font-family:'NanumSquare', sans-serif; color:#9e9e9e; }
[class*="inp-"]::-webkit-input-placeholder { font-family:'NanumSquare', sans-serif; color:#9e9e9e; }
[class*="inp-"]:-ms-input-placeholder { font-family:'NanumSquare', sans-serif; color:#9e9e9e; }
[class*="inp-"]:focus::placeholder { color:transparent; }
[class*="inp-"]:focus::-webkit-input-placeholder { color:transparent; }
[class*="inp-"]:focus:-ms-input-placeholder { color:transparent; }
[class*="inp-"][readonly]:focus { border-color:#e5e5e5; }
[class*="inp-"][disabled],
[class*="inp-"][readonly] { background:#f2f2f2; border-color:#e5e5e5; }
[class*="inp-"][readonly]::placeholder,
[class*="inp-"][disabled]::placeholder { color:#9e9e9e; }
[class*="inp-"][readonly]::-webkit-input-placeholder,
[class*="inp-"][disabled]::-webkit-input-placeholder { color:#9e9e9e; }
[class*="inp-"][readonly]:-ms-input-placeholder,
[class*="inp-"][disabled]:-ms-input-placeholder { color:#9e9e9e; }
[class*="inp-"][type="password"] { -webkit-text-security:disc; }
[class*="inp-"]::-ms-clear { display:none; }
textarea[class*="inp-"] { display:block; max-height:200px; padding-top:14px; line-height:22px;  overflow-y:hidden; background:#F7F8F8; border-radius:8px; font-family: "Pretendard", Sans-serif; }
textarea[class*="inp-"]::placeholder { line-height:22px; }
/* textarea[class*="inp-"]:focus { padding:12px 15px 13px; } */

label { position:relative; display:inline-block; font-size:14px; }
 
input[type="radio"],
input[type="checkbox"] { position:absolute; bottom:0; left:0; width:0; height:0; opacity:0; }
input[type="password"] { font-family:Arial, sans-serif; }

input[type=file] { font-family: "Pretendard", Sans-serif; }
input[type=file]::file-selector-button { display:none; }
.btn-file { display:inline-block; width:120px; line-height:48px; text-align:center; font-weight:600; font-size:16px; border:1px solid #222; border-radius:8px; cursor:pointer;}

/* select */
select { min-width:120px; height:45px; font-size:14px; font-weight:700; padding:0 70px 0 24px; border-radius:8px; background:#F7F8F8 url(/admin/resource/images/ico_acco_arr.png) no-repeat calc(100% - 20px) 12px; background-size:24px; }
select:focus {border-color:#222;}

/* headline style */
.h1 { font-weight:300; }
.h2 { font-weight:300; }
.h1 strong,
.h2 strong { font-weight:800; }

/* font color */
.fc-primary { color:#EC008C !important; }
.fc-secondary { color:#2041bf !important; }
.fc-sub { color:#757575 !important; }
.fc-warn { color:#f9a825 !important; }
.fc-bad { color:#d32f2f !important; }
.fc-good { color:#4caf50 !important; }
.fc-black { color:#000000 !important; }

/* keep-all */
.keep-word { word-break:keep-all !important; }

/* a */
a.link { color:#003cff; text-decoration:underline; }

/* hide/hidden style */
.a11y-hidden,
.hide { overflow:hidden; position: absolute; left:-999px; top:-999px; width:1px; height:1px; margin-left:-1px; border: 0 none !important; background:transparent !important; color:transparent !important; }

/* float */
.fclear::after { content:''; display:block; clear:both; }
.f-left { float:left !important; }
.f-right { float:right !important; }

.ellips {  overflow:hidden; display:block; text-overflow:ellipsis; white-space:nowrap; } /* 한줄 처리 */자

/* inputs */
/* inputs */
.lb-item { position:relative; display:inline-block; margin-right:8px; box-sizing:border-box; white-space:nowrap; vertical-align:middle; }
.lb-radio,
.lb-checkbox { position:relative; display:inline-flex; vertical-align:top; align-items:center; min-height:24px; padding:0 0 0 32px; margin:0; cursor:pointer; line-height:1; }
.lb-radio::before{ content:""; display:block; flex:0; width:24px; height:24px; border-radius:50%; border:1px solid #BFC0C5; background-color:#fff; position:absolute; top:0; left:0; box-sizing:border-box; }
.lb-checkbox::before { content:""; display:block; flex:0; position:absolute; top:0; left:0; width:24px; height:24px; border-radius:4px; border:1px solid #B9BBC6; border-radius:4px; }
.lb-checkbox::after,
.lb-radio::after { content:""; display:block; position:absolute; top:0; left:0; width:24px; height:24px; background:url(/kiam/pcWeb/resources/images/common/ico_form_chk.png) no-repeat 1px 0; }
.lb-checkbox.no-label.checked:before,
.lb-checkbox:hover:before,
.lb-radio:hover:before { border-color:#EC008C; }
.lb-radio:hover:after,
.lb-checkbox:hover:after { background-position-x:-47px; }
.lb-checkbox:hover:disabled:before,
input:checked + .lb-checkbox::before,
input:checked + .lb-radio::before { background:#EC008C; border-color:#EC008C; }
input:checked + .lb-radio::after,
input:checked + .lb-checkbox::after { background-position-x:-24px; }
input:disabled + .lb-checkbox:before,
input:disabled + .lb-radio:before { background:#DFE0E2; border-color:#B9BBC6; }
input:disabled + .lb-radio:after,
input:disabled + .lb-checkbox:after { display:none; }
input:checked:disabled + .lb-checkbox:before,
input:checked:disabled + .lb-radio:before { background:#B9BBC6; border-color:#B9BBC6; }
input:checked:disabled + .lb-radio:after,
input:checked:disabled + .lb-checkbox:after { display:block; background-position-x:-24px; }
.lb-checkbox.no-label { padding-left:20px; }
.lb-checkbox.no-label.checked { padding-left:20px; }

/* FORM */
.frm-wrap.iod { display:flex; justify-content:space-between; }
.frm-wrap.iod .iod-wrap.type2 .btn + .btn { margin:0; }
.frm-wrap.iod .iod-wrap { display:flex; flex-direction:column; justify-content:center; width:40px; height:90px; margin-left:0; padding:5px 0 0 0; }
.frm-wrap.iod .frm-area { width:calc(100% - 50px);  }
.frm-wrap.iod .frm-area textarea { min-height:80px; }
.frm-area:not(.multi) .lb-item + .lb-item { margin-left:20px; }

/* BTN */
.btn { display:inline-block; font-size:16px; text-align:center; border-radius:32px; }
.btn.primary { min-width:240px; font-size:20px; font-weight:600; color:#fff; line-height:64px; background-color:#EC008C; }
.btn.primary:hover {}
.btn.primary:disabled {}
.btn.base { min-width:160px; line-height:46px; font-weight:600; font-size:16px; border:1px solid #222;  }
.btn.base.primary { min-width:160px; background:none; border-color:#EC008C; color:#EC008C; }
.btn.base.cancel { color:#74757E; border-color:#74757E;  }
.btn.base.file { }
.btn.base.tbl { line-height:38px; min-width:80px; font-size:14px; }

.btn.modify { display:inline-block; width:45px; height:45px; margin-left:16px;}
.btn.modify::before {content:''; display:block; width:14px; height: 14px;  background:url(../images/ico_pencil.png)no-repeat center;}

.tbl-base [class*="inp-"] + .btn { margin-left:8px; }

.tbl-btns { position: relative; margin-top:10px; overflow: hidden; height:48px; text-align:center; }
.tbl-btns > div { float:right; line-height:0; font-size:0; }
.tbl-btns > div .btn + .btn { margin-left:5px; }

.pager { margin-top:20px; text-align:center; line-height:0; font-size:0; }
.pager a { display:inline-block; width:40px; height:40px; font-size:16px; line-height:40px; text-align:center; color:#B9BBC6; vertical-align:middle; }
.pager a.on { color:#1A399E; border-radius:50%; background:#F1F7FE; }
.pager a.btn { width:16px; height:16px; background:url(/admin/resource/images/ico_comm.png) no-repeat -77px 0; }
.pager a.btn.next { background-position-x:-52px; }
.pager a.btn.end { background-position-x:-52px; }
.btn-group { margin-top: 30px; text-align: center; line-height: 0; font-size: 0; }
.btn-group .btn { margin:0 5px; }

.ui-modal-footer .pager {left:0; transform: none; }
.ui-modal-footer .pager a {font-size:14px;}

.file-wrap { position:relative;  }
.file-wrap + .file-wrap { margin-top:10px; }
.file-wrap .file-title { display:inline-block; width:60px; font-weight:600;  }
.file-wrap .btn-file { margin-right:10px; font-size:16px; }
.file-wrap input[type="file"] { font-size:16px; }

.file-wrap { padding-bottom:0 !important; display:flex; align-items:center; }
.file-wrap .inp-text { opacity:0; position:absolute; top:30px; left:0; height:auto; bottom:0; float:left; width:calc(100% - 100px); background:#fff; border-color:#bdbdbd; }
.file-wrap .btn { position:relative; width:90px; margin-left:0 !important; }
.file-wrap .btn input { z-index:1; position:absolute; top:0; left:0; width:100%; height:100%; opacity:0; cursor:pointer; }
.file-wrap .btn input:focus span { text-decoration:underline; }
.file-wrap .file-list { position:relative; width:calc(100% - 100px); padding:10px; }
.file-wrap .file-list .file { position:relative; word-break:break-all; font-size:14px; }
.file-wrap .file-list .file + .file { margin-top:8px; }
.file-wrap .file-list .btn-del { position:relative; display:inline-block; width:16px; height:16px; text-indent:-9999px; border-radius:15px; background:url(/admin/resource/images/ico_comm.png) no-repeat -105px 0; vertical-align:middle; margin-left:5px; }
.file-wrap.multi { display:block; }
.file-wrap.multi .file-list { padding:10px 0; }

/* list */
.list-dot li { position:relative; padding-left:15px; line-height:21px; margin-top:5px; }
.list-dot li:after { content:''; display:block; position: absolute; top:8px; left: 0; width: 4px;  height: 4px; background-color: #444; border-radius: 100%;    }
.tbl-base + .list-dot { margin-top:10px; }

/* login */
.admin-login-inner {position:absolute; left:50%; top:50%; width:768px; transform:translate(-50%, -50%); }
.login-top {display:flex; justify-content:space-between; align-items:center; margin-bottom:16px;}
.login-top h1 {font-size:32px; line-height:32px; font-weight:700; color:#F1F7FE; opacity: .5;}
/* .login-top .links{display:flex; align-items:center; border: 1px solid #000;} */
/* .login-top .links a {display:block;} */
/* .login-top .links a + a {margin-left:32px;} */
.login-box { width: 768px; /*height:448px;*/ padding:80px 64px; background:#fff; border:1px solid #DFE0E2; border-radius:38px 0 38px 38px; box-shadow:0px 3px 10px rgba(34, 34, 34, 0.1);}
.login-box label {display:block;}
.login-box label + label {margin-top:43px;}
.login-box input {width:100%; height:64px; font-size:24px; line-height:36px; border-bottom:2px solid #DFE0E2; text-indent:26px;}
.login-box input::-webkit-input-placeholder {font-size:24px; color:#DFE0E2; font-weight:500; letter-spacing:-2px;}
.login-box input:focus {border-color:#222; outline: none; font-weight:700;}
.login-box .login-btn {width:100%; height:64px; margin-top:53px; padding:0 80px; color:#fff; font-size:20px; font-weight:600; text-align:center; border-radius:8px; background-color:#EC008C;}

/* modal */
.scroll-no { overflow:hidden; padding-right:1.7rem; }
.ui-modal.n1 { z-index:201; }
.ui-modal.n2 { z-index:202; }
.ui-modal.n3 { z-index:203; }
.ui-modal.n4 { z-index:204; }
.ui-modal.n5 { z-index:205;}
.ui-modal { display:none; position:fixed; top:0; left:0; z-index:200; width:100%; height:100%; overflow:hidden; overflow-y:hidden; }
.ui-modal.ready { display:flex; overflow-y:hidden; }
.ui-modal.ready .ui-modal-dim { content:""; display:block; position:fixed; top:0; left:0; width:100%; height:100%; opacity:0; background:rgba(0, 0, 0, .5); }
.ui-modal.open .ui-modal-dim{ opacity:1; }
.ui-modal.ps-center.is-over { align-items:flex-start; }
.ui-modal.ps-center.is-over .ui-modal-wrap { margin:20px 0; }
.ui-modal.ps-center { justify-content:center; align-items:center; }
.ui-modal.ps-top { justify-content:center; align-items:flex-start; overflow:hidden; }
.ui-modal.ps-bottom { justify-content:center; align-items:flex-end; overflow:hidden; }
.ui-modal-wrap { position:relative; padding:40px; min-width:480px; max-width:900px; opacity:0; z-index:10; background:#fff; border-radius:24px 0px 24px 24px; }
.ui-modal.open .ui-modal-wrap { opacity:1; }
.type-full .ui-modal-wrap,
.size-mobile .ui-modal.ps-top .ui-modal-wrap,
.size-mobile .ui-modal.ps-bottom .ui-modal-wrap { margin:0; width:100%; }

/* .ui-modal-body { width:700px; } */

.ui-modal.ps-top .ui-modal-wrap { margin-top:1rem; }
.ui-modal.ps-bottom .ui-modal-wrap { margin-bottom:1rem; }
.ui-modal.ready.open { animation-name:fadeIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards;}
.ui-modal.ready.close { animation-name:fadeOut; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.type-full.open.ps-center .ui-modal-wrap { animation-name:slideUpIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.type-full.close.ps-center .ui-modal-wrap { animation-name:slideUpOut; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.open.ps-center .ui-modal-wrap { animation-name:scaleIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.close.ps-center .ui-modal-wrap { animation-name:scaleOut; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.close.ps-top .ui-modal-wrap { animation-name:slideDownOut; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.open.ps-top .ui-modal-wrap { animation-name:slideDownIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.close.ps-bottom .ui-modal-wrap { animation-name:slideUpOut; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear;  animation-fill-mode:forwards; }
.ui-modal.open.ps-bottom .ui-modal-wrap { animation-name:slideUpIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards;}

.ui-modal.close.ps-center.type-system .ui-modal-wrap { animation-name:slideDownOut; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.open.ps-center.type-system .ui-modal-wrap { animation-name:slideDownIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ie9 .ui-modal.open { display:block; }
.ie9 .ui-modal.open .ui-modal-wrap { opacity:1; margin:0 auto; transform:translate(0, -50%); top:50% }
.ui-modal.type-system .ui-modal-wrap { width:auto; text-align:center; font-size:14px; }
.ui-modal.type-system .ui-modal-body,
.ui-modal.type-system .ui-modal-footer { max-width:100% !important; background:#6200ee; color:#fff; }
.ui-modal.type-system .ui-modal-wrap strong { font-weight:900; }

.ui-modal-header { position:relative; top:0; left:0; width:100%; min-height:36px; }
.ui-modal-header h1 { position:relative; display:inline-block; padding-left:14px; font-weight:700; font-size:24px; line-height:36px; }
.ui-modal-header h1:before { content:''; display:block; position:absolute; top:3px; left:0; width:8px; height:8px; border-radius:50%; background:#EC008C; }
.ui-modal-header .icon-close { position:absolute; display:block; top:0; right:0; width:24px; height:24px; }
.ui-modal-header .icon-close:before,
.ui-modal-header .icon-close:after { content:''; position:absolute; display:block; top:0; left:11px; width:2px; height:26px; transform: rotate(-45deg); background-color:#878C91; }
.ui-modal-header .icon-close:after { left:inherit; right:11px; transform: rotate(45deg); }
.ui-modal-header .icon-close:hover:before,
.ui-modal-header .icon-close:hover:after { background-color:#000; }
.ui-modal-body { position:relative; padding-top:32px; }
.ui-modal-body.ui-scrollbar { height:550px !important; }
.ui-modal-body .ui-scrollbar-item { width:calc(100% - 10px) !important; }

.ui-modal-footer .btn-group { text-align:center; }


/* ui scroll bar :base */
.ui-scrollbar { position:relative; overflow:auto; }
.ui-scrollbar-item { position:absolute; top:0; left:0; }
.ui-scrollbar.ready.view-y .ui-scrollbar-item.scroll-y-padding { width:calc(100% - 5px) !important; }
.ui-scrollbar.ready.view-x .ui-scrollbar-item.scroll-x-padding { height:calc(100% - 5px) !important; }
.ui-scrollbar.ready { overflow:hidden !important; /* padding:0 !important; */ }
.ui-scrollbar.ready>.ui-scrollbar-item { position:relative; width:100%; height:100% !important; overflow:scroll !important; }
.ui-scrollbar.ready>.ui-scrollbar-item { -ms-overflow-style:none; scrollbar-width:none; }
.ui-scrollbar.ready>.ui-scrollbar-item::-webkit-scrollbar { display:none; }
.ui-scrollbar.ready .ui-scrollbar-item::-webkit-scrollbar,
.ui-scrollbar.ready .ui-scrollbar::-webkit-scrollbar { width:0; height:0; }
.ie .ui-scrollbar.ready>.ui-scrollbar-item { width:calc(100% + 1.7rem) !important;  height:calc(100% + 5px) !important; }
.ui-scrollbar-barwrap { opacity:0; }
.ui-scrollbar-barwrap.type-y { display:none; position:absolute; top:0; right:0; width:5px; height:100%; background:rgba(255, 255, 255, 0.1); z-index:100; border-top:.2rem solid transparent; border-bottom:.2rem solid transparent; }
.ui-scrollbar-barwrap.type-x { display:none; position:absolute; bottom:0; left:0; width:100%; height:1rem; background:rgba(255, 255, 255, 0.1); z-index:100; border-left:.2rem solid transparent; border-right:.2rem solid transparent; }
.type-y .ui-scrollbar-bar { position:absolute; top:0; right:0; width:5px; margin:0; height:2rem; opacity:0.5; border-radius:.2rem; background:#E0E0E0; }
.type-x .ui-scrollbar-bar { position:absolute; top:.2rem; left:0; width:2rem; margin:0; height:6px; opacity:1; border-radius:.2rem; opacity:0.5; background:#E0E0E0; }
.ui-scrollbar:hover .ui-scrollbar-bar { opacity:1; box-shadow:0 0 .5rem rgba(0,0,0,0.1); }
.view-scrollbar>.ui-scrollbar-barwrap { animation-name:fadeIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.view-y>.ui-scrollbar-barwrap.type-y { display:block; } 
.view-x>.ui-scrollbar-barwrap.type-x { display:block; }

.ui-file {position: relative;display: flex;justify-content: center;align-items: center;margin-right:8px; width: 160px;}
.ui-file input:active + .btn-base,.ui-file input:hover + .btn-base,.ui-file input:focus + .btn-base {border: 1px solid #616161;background: #FAFAFA;}
.ui-file-inp {position: absolute;top: 0;left: 0;margin: 0;opacity: 0;width: 100%;height: 100%;direction: ltr;cursor: pointer;font-size: 12px;z-index: 1;}
.ui-file-list {position: relative;padding: 0 32px 0 8px; margin-top: 0;display: none;}
.ui-file-list.on {display: block;}
.ui-file-list ul {padding: 0;margin: 0;}
.ui-file-list li {position: relative;padding: 8px 32px 0 0;display: flex; width:350px; justify-content: left;align-items: center;border-bottom: 1px solid #EEEEEE;}
.ui-file-list li p { white-space: nowrap; overflow: hidden; text-overflow: ellipsis; }
.ui-file-list img {display: none;}
.ui-file-list .ui-file-del {position: absolute;right: 5px;top: 1rem;line-height: 1;}
.ui-file-list .ui-file-del::after {content: "";display: block; position:absolute; right:-9px; top:0; transform: rotate(45deg);  width: 20px;height: 2px; background:#000;}
.ui-file-list .ui-file-del::before {content: "";display: block; position:absolute; right:0; top:-9px; transform: rotate(45deg); width: 2px;height: 20px; background:#000;}

/* animation keyframes */
@-webkit-keyframes fadeIn {0% {opacity:0;}100% {opacity:1;}}
@keyframes fadeIn {0% {opacity:0;}100% {opacity:1;}}
@-webkit-keyframes fadeOut {0% {opacity:1;}100% {opacity:0;visibility:hidden;}}
@keyframes fadeOut {0% {opacity:1;}100% {opacity:0;visibility:hidden;}}
@-webkit-keyframes scaleIn {0% {opacity:0;transform:scale(0.9);}100% {opacity:1;transform:scale(1);}}
@keyframes scaleIn {0% {opacity:0;transform:scale(0.9);}100% {opacity:1;transform:scale(1);}}
