.datepicker-header > button.ui-prev-m::before { content:""; }
.datepicker-header > button.ui-next-m::before { content:""; } 
.datepicker-header .ui-today { width:2.5rem; height:2.5rem; line-height:1; position:absolute; top:0; right:0; }
.datepicker-header .ui-today::before { content:""; }
.datepicker-header .datepicker-title { display:none; width:100%; text-align:center; font-size:1.4rem; }
.datepicker-body { margin:5px 0; }
.datepicker-body th { font-size:12px; padding:5px 0; }
.datepicker-body .datepicker-date td { text-align:center; }
.datepicker-body .week-word { display:none; }
.datepicker-body .datepicker-day { width:24px; height:24px; font-size:12px; border-radius:50%; margin:3px 0; line-height:1; }
.datepicker-body .datepicker-day.selected-end, .datepicker-body .datepicker-day.selected-start, .datepicker-body .datepicker-day.selected { background:#6B38FB; color:#fff; }
.datepicker-body .datepicker-day.today.selected-end, .datepicker-body .datepicker-day.today.selected-start { background:#6200ee; color:#fff; }
.datepicker-body .datepicker-day.today { background:#fff; border:1px solid #6200ee; color:#6200ee; }
.datepicker-body .during .datepicker-day { background:#EEEEEE !important; }
.datepicker-body .during-not .selected-end { background:#6200ee !important; }
.datepicker-body .during-not ~ td .datepicker-day { background:#fff !important; }
.ui-date-wrap:not(.disabled):hover { border-color:#EC008C; }
.ui-date-wrap:not(.disabled) .ui-datepicker:hover .inp-base,
.ui-date-wrap:not(.disabled) .ui-datepicker:hover .form-txt { color:#222; }
.ui-date-wrap:not(.disabled) .ui-datepicker:hover .inp-base::after { opacity:1; background-position-x:-390px; }
.ui-date-wrap .form-txt { padding:0 10px; color:#73737A; }
.ui-date-wrap.disabled { background:#DFE0E2; color:#B9BBC6; }
.ui-date-wrap.disabled .ui-datepicker .inp-base, 
.ui-date-wrap.disabled .form-txt { color:#B9BBC6; }
.ui-date-wrap.disabled .ui-datepicker .inp-base:after { opacity:0.2; }

/* sheet */
.sheet-bottom.datepicker .datepicker-wrap { padding:10px; background:#FAFAFA; border:1px solid #FAFAFA; box-shadow:0 0.3rem 0.3rem rgba(0, 0, 0, 0.1); box-shadow:0 0.3rem 0.3rem rgba(0, 0, 0, 0.1); position:relative; z-index:1; }
.sheet-bottom { position:absolute; top:0; left:0; opacity:0; z-index:301; transition:opacity 0.2s ease-in-out; }
.sheet-bottom.on {opacity:1; }
.size-mobile .datepicker.sheet-bottom { max-width:100%; top:auto !important; left:0 !important; bottom:0; position:fixed; }
.size-mobile .datepicker-body { max-width:23rem; margin:1rem auto; }
.size-mobile .datepicker.sheet-bottom { bottom:-50vh; }
.size-mobile .datepicker.sheet-bottom.on {
top:auto;
-webkit-animation-name:slideUpIn;
        animation-name:slideUpIn;
-webkit-animation-duration:0.2s;
        animation-duration:0.2s;
-webkit-animation-iteration-count:1;
        animation-iteration-count:1;
-webkit-animation-timing-function:ease-in-out;
        animation-timing-function:ease-in-out;
-webkit-animation-fill-mode:forwards;
        animation-fill-mode:forwards;
}
.size-mobile .datepicker.sheet-bottom.off {
top:auto;
-webkit-animation-name:slideUpOut;
        animation-name:slideUpOut;
-webkit-animation-duration:0.2s;
        animation-duration:0.2s;
-webkit-animation-iteration-count:1;
        animation-iteration-count:1;
-webkit-animation-timing-function:ease-in-out;
        animation-timing-function:ease-in-out;
-webkit-animation-fill-mode:forwards;
        animation-fill-mode:forwards;
}
.sheet-dim {position:fixed; top:0; left:0; width:100%; height:100%; z-index:0; background:rgba(0, 0, 0, 0.3); opacity:0; transition:opacity 0.2s ease-in-out; }
.sheet-dim.on { opacity:1; transition:opacity 0.2s ease-in-out; }

/* 게시판 스타일 */
.notice-wrap { position:relative; }
.notice-wrap span { display:block; }
.notice-wrap span.pic { overflow:hidden; margin-bottom:30px; border-radius:16px; }
.notice-wrap span.pic img { width:100%; }
.notice-wrap dl { }
.notice-wrap dt .category { font-size:16px; line-height:24px; color:#73737A; }
.notice-wrap dt strong { font-size:20px; color:#222; font-weight:700; line-height:30px; }
.notice-wrap dd { margin-top:5px; line-height:21px; color:#73737A; }


