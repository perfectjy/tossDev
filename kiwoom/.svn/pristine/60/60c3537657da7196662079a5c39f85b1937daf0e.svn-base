    <script>
    (() => {
        'use strict';
        var $infoTop = $('.info-top');

                $infoTop.each(function(){
                    var target = $(this),
                        $btn = $(this).find('.btn-sch');
                        
                    $btn.off('click.schBtn').on('click.schBtn', function(e) {
                        e.preventDefault();
                        if(!!!target.hasClass('active')){
                            target.addClass('active');
                        } else {
                            target.removeClass('active');
                        }
                    });
                });
        })();
