/* tab link */
.tab-btn { position:fixed; bottom:10px; left:50%; transform:translateX(-50%); display:flex; justify-content:center; z-index:10;}
.tab-btn ul { display:flex; justify-content:space-between; align-items:center; width:518px; height:64px; padding:8px 12px; border-radius:100px; background-color:#73737A; opacity:0.8 }
.tab-btn ul li button { width:110px; height:48px; font-size:16px; color:#DFE0E2; border-radius:100px; background-color:#73737A; }
.tab-btn ul li.on button { background:#222; }

/* switch button*/
.toggle-btn { display:inline-block; }
.toggle-btn input + label { display:inline-block; position:relative; width:44px; height:24px; border-radius:12px; background-color:#B9BBC6; }
.toggle-btn input + label::after { display:inline-block; content:''; position:absolute; left:2px; top:2px; width:20px; height:20px; border-radius:12px; background-color:#FFF;  transition:.4s; }
.toggle-btn input:checked + label { background-color:#EC008C; }
.toggle-btn input:checked + label::after { content:'';  left:22px; }

/*buttons*/
.btn { line-height:inherit !important; }/*차후지움*/
.btn { display:inline-flex; justify-content:center; align-items:center; min-width:160px; min-height:48px; padding:0 40px; font-size:16px; border-radius:32px; background-color:#FFF; text-align:center;}
.btn:disabled { color:#B9BBC6; border:1px solid #DFE0E2; background-color:#DFE0E2; cursor:default; }
.btn:disabled:hover, .btn:disabled:active { color:#B9BBC6; border:1px solid #DFE0E2; background-color:#DFE0E2; }
.btn.primary { color:#EC008C; font-weight:600; border:1px solid #EC008C; background-color:#FFF;}
.btn.primary:hover, .btn.primary:active { color:#FFF; background-color:#EC008C; }
.btn.secondary { color:#222; border:1px solid #222; }
.btn.secondary:hover, .btn.secondary:active { background-color:#F7F8F8; }
.btn.tertiary { color:#9D9DA5; border:1px solid #9D9DA5;}
.btn.tertiary:hover, .btn.tertiary:active { background-color:#F7F8F8; }
.btn.fill { color:#FFF; background-color:#EC008C;}
.btn.fill:hover, .btn.fill:active { background-color:#C02882; }
.btn.lg { min-width:200px; min-height:56px; font-size:20px; }
.btn.search{ min-width: 90px; min-height: 48px; padding :0 20px; margin-left: 8px; color: #EC008C; font-size: 14px; font-weight: 600; border: 1px solid #EC008C; border-radius: 8px;}
.btn.search:hover, .btn.search:active{ color: #FFF; background-color: #EC008C;}

/*icon buttons*/
.btn-icon { display:inline-block; min-height:48px; padding:0 24px; border:1px solid #222; border-radius:40px; background:#fff;  }
.btn-icon:hover, .btn-icon:active {background-color:#F7F8F8;} 
.btn-icon span { position:relative; color:#222; font-size:14px; font-weight:500; text-align:center; padding-right:21px; }
.btn-icon span::after { content:''; position:absolute; top:0; right:0; display:inline-block; width:16px; height:16px;}
.btn-icon.icon-dw span::after{ background:url(/kiam/pcWeb/resources/images/common/ico_comm.png) no-repeat -300px 0;}
.btn-icon.icon-more span::after{ background:url(/kiam/pcWeb/resources/images/common/ico_comm.png) no-repeat -340px 0;}
.btn-icon.box{ border-radius: 8px; }

/*more button*/
.btn-more{ width:100%; margin-top: 24px; background-color:#FFF; }
.btn-more span{ display:inline-flex; color:#73737A; font-size:14px; font-weight:500; padding:16px 0;}
.btn-more span::after{ content:''; display:inline-block; width:16px; height:16px; padding-left:4px; background:url(/kiam/pcWeb/resources/images/common/ico_comm.png) no-repeat 2px 0; opacity:0.6;}
.btn-more:hover{ background-color:#F7F8F8; }
.btn-more:hover span{ color:#222; }
.btn-more:hover span::after{ opacity:1; }

/*table*/
.tbl{ position: relative; width: 100%; border-top: 1px solid #222; background-color: #FFF; }
.tbl thead tr {background:#F7F8F8;}
.tbl thead th { padding: 28px 16px; font-size: 16px; font-weight: 600; color: #222; vertical-align: middle; line-height: 1.5; } 
.tbl tbody th { font-size:14px; font-weight:600; text-align:center; color:#222; background-color:#F1F7FE; vertical-align:middle; border-bottom:1px solid #B9BBC6; }
.tbl tbody tr.line td{border-bottom:1px solid #B9BBC6;}
.tbl tbody tr td a {color:#73737A;}
.tbl tbody tr td a:hover {color:#1A399E;}
.tbl tbody tr td { padding: 24px 16px 25px ; font-size: 16px; font-weight: 500; text-align: center; border-bottom: 1px solid #DFE0E2; vertical-align: middle; line-height: 1.5;}
.tbl tbody.txtl tr td:not(:first-of-type) {text-align: left; padding-left: 16px;}
.tbl .txtl{ text-align: left; padding-left: 16px;}


thead.is-colgroup th[scope="colgroup"]{padding: 16px; border-bottom:1px solid #DFE0E2;}
thead.is-colgroup tr + tr th{padding: 16px;}

.tbl-tit{ display:block; font-size:16px; font-weight:700; color:#222; margin-bottom:22px;}
.tbl-row { position:relative; width:100%; border-top:1px solid #000; border-bottom:1px solid #DFE0E2; background-color:#FFF;}
.tbl-row tbody th{ height:56px; padding:4px; font-size:14px; font-weight:500; color:#73737A; background-color:#F1F7FE; vertical-align:middle; }
.tbl-row tbody td{ height:56px; padding:4px; font-size:16px; font-weight:700; text-align:center; vertical-align:middle; }
.tbl-row tbody tr td:not(:first-child){border-left:1px solid #DFE0E2;}

.tbl-box { position:relative; width:100%; padding:0 48px; border:1px solid #DFE0E2; border-radius:24px 0 24px 24px; }
.tbl-box li:not(:first-child) { border-top:1px solid #DFE0E2; }
.tbl-box li a { display:flex; justify-content:space-between; align-items:center; padding:32px 0; }
.tbl-box li .txt { color:#222; font-size:16px; font-weight:500; }
.tbl-box li .date { color:#73737A; font-size:14px; font-weight:400; }
.tbl-box li a:hover .txt{ color:#1A399E; }

.tbl-list { position:relative; width:100%; border-top:1px solid #000; }
.tbl-list li { border-bottom:1px solid #DFE0E2}
.tbl-list li a{ display:flex; justify-content:space-between; align-items:center; padding:40px 24px; }
.tbl-list li a:hover div{ color:#1A399E; }
.tbl-list li a div{ font-size:20px; font-weight:600; }
.tbl-list li a div span{ color:#1A399E; }
.tbl-list li a .date{ font-size:14px; font-weight:400; }

.sub-title { position:relative; margin-top:80px;}
.sub-title .tbl-title {font-size:24px; font-weight: 700; margin-bottom: 24px;}
.sub-title .msg { font-size:14px; color:#73737A; }
.sub-title > .msg { position:absolute; top:5px; right:0; }

.tbl-nodate{ font-size: 24px; font-weight: 600; padding: 120px 0; text-align: center;}

.tbl + .txt-list{ margin-top: 24px; }

.txt-lineh{ line-height: 24px; }


/*text list*/
.txt-list{ text-align:left; word-break:keep-all; display: block; margin-top:24px;}
.txt-list > li,
.txt-list:not(ol):not(ul){ position: relative; font-size: 16px; font-weight: 600; margin-top: 8px; padding-left: 12px; line-height: 24px; }
.txt-list > li:first-child{ margin-top:0 !important; }
.txt-list > li::before,
.txt-list:not(ol):not(ul)::before{ position: absolute; }

/*text disc*/
.txt-list.disc li::before, .txt-list.disc:not(ol):not(ul)::before{ content:''; top:10px; left: 0; width:4px; height:4px; border-radius:100%; background-color:#73737A; }

/*text dash*/
/* .txt-list.dash li::before, p.txt-list.dash:not(ol):not(ul)::before{ content:''; top:10px; left: 0; width:4px; height:1px; background-color:#222; } */

/*text star*/
.txt-list.star,
.txt-list.star:not(ol):not(ul){ color: #73737A; }
.txt-list.star li{ font-size: 14px;}
.txt-list.star li::before, .txt-list.star:not(ol):not(ul)::before{ content:'*'; top: 2px; left: 0;}


/*KOP02010100M*/
.kosef-btn-tab{ text-align: center; margin-top: -90px; }
.kosef-tab-item{display: inline-flex;}
.kosef-tab-item:not(:first-child){ margin-left: 10px; }
.kosef-tab-item .tab-ckb{ display: flex; justify-content: center; align-items: center; flex-direction: column; width: 158px; height: 144px; border-radius: 8px; border: 1px solid #DFE0E2; box-sizing: border-box;  box-shadow: 0px 4px 8px rgba(34, 34, 34, 0.05); background-color: #FFF; cursor: pointer;}
.kosef-tab-item .tab-ckb em{ display: flex; font-size: 20px; font-weight: 600; margin-top: 12px;}
.kosef-tab-item .tab-ckb span{ width: 32px; height: 32px; background-position-x: center; background-repeat: no-repeat;}
.kosef-tab-item .tab-ckb .on{ display: none; }
.kosef-tab-item .tab-ckb:hover{ color: #1A399E; border: 1px solid #1A399E; box-sizing: border-box;}
.kosef-tab-item input:checked + label{ color: #FFF; background: linear-gradient(114.62deg, #394DE4 6.07%, #953DEE 96.44%); box-shadow: 0px 2px 4px rgba(26, 57, 158, 0.16);}
.kosef-tab-item input:checked + label span{display: none;}
.kosef-tab-item input:checked + label span.on{display: block;}
.kosef-tab-item input:checked:hover + label{ color: #FFF !important; border: 1px solid #394DE4; box-sizing: border-box; }


/*KOP02010200M*/
.term-wrap{ position: relative; display: flex; justify-content: space-between; align-items: center; margin-bottom: 40px;}
.term-wrap .tit{ color: #222; font-size: 16px; font-weight: 500; margin-right: 48px;}
.term-btn{ display: flex;  }
.term-btn > button{ min-width: 104px; color: #B9BBC6; font-size: 14px; font-weight: 500; padding: 16px; border: 1px solid #B9BBC6; border-radius: 8px; margin-left: 8px; }
.term-btn button:first-child{margin-left: 0;}
.term-btn > button:hover,
.term-btn > button:active,
.term-btn > button.on{ color: #222; border: 1px solid #222;}
.term-wrap .ui-date-wrap{ margin-left: 32px; }
.sub-graph{ border-top: 1px solid #222; border-bottom: 1px solid #222; }
.sub-graph .sub-graph-info{ margin-bottom: 32px; }
.sub-graph .sub-graph-info ul{ display: flex; justify-content: flex-end;}
.sub-graph .sub-graph-info ul li{ margin-left: 64px; }
.sub-graph .sub-graph-info span{ position: relative; padding-left: 20px; color: #73737A; font-size: 14px; font-weight: 400;}
.sub-graph .sub-graph-info span::before{content: ''; position: absolute; top: 50%; left: 0; transform: translateY(-50%); width: 12px; height: 12px; border-radius: 50%; }
.sub-graph .sub-graph-info .blue::before{ background-color: #1A399E; }
.sub-graph .sub-graph-info .pink::before{ background-color: #EC008C; }
.sub-graph .sub-graph-info .green::before{ background-color: #58B84A; }