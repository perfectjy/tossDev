@charset "utf-8";

.main-wrap { padding:0 40px; }
.main-wrap .top-visual { }
.main-wrap .sch-box { width:1200px; margin:70px auto 0 auto; text-align:center; }
.main-wrap .sch-box .sch-top { position:relative; width:720px; height:80px; text-align:left; margin:0 auto; padding:20px 80px 20px 40px; border-radius:40px; border:2px solid #113585; }
.main-wrap .sch-box .btn-sch { position:absolute; top:18px; right:20px; width:40px; height:40px; background:url(/kiam/pcWeb/resources/images/common/ico_comm.png) no-repeat -260px -70px;}
.main-wrap .sch-box input[type="text"]{ width:100%; font-size:20px; line-height:38px; border:0; padding:0; height:auto;   }
.main-wrap .sch-box input[type="text"]::placeholder { color:#1A399E; font-family:"Pretendard", Sans-serif; }
.main-wrap .sch-box .btn-clear { position:absolute; top:50%; right:70px; margin-top:-10px; }
.main-wrap .sch-box .sch-tips { margin:5px 0 0 0; line-height:0; font-size:0; }
.main-wrap .sch-box .sch-tips a { display:inline-block; padding:12px 20px; color:#222; font-size:16px; line-height:24px; }
.main-conts { width:1200px; margin:0 auto; }

.main-visual .main-items { height:516px; }
.main-visual .items { position:relative; }
.main-visual .items .product { width:1200px; height:480px; margin:0 auto; padding:120px 0 0 0; height:480px; z-index:1; }
.main-visual .items .product .txts { display:block; color:#878c91; font-size:24px; }
.main-visual .items .product .title { display:block; margin:25px 0 0 0; color:#222; font-size:54px; font-weight:700; }
/* .main-visual .btns { position:absolute; top:200px; left:300px; } */
.main-visual .btns a { display:inline-block; height:48px; margin-right:5px; padding:14px 25px 0 25px; font-weight:500; border:1px solid #000; border-radius:24px; color:#000;  }
.main-visual .btns a.btn-kosef { border:0; color:#fff; padding-top:15px; background:linear-gradient(to right, #524ae6, #ca3eee); }
.main-visual .items .bg { position:absolute; top:0; right:5%; z-index:0; }


.main-conts { position:relative; margin-top:80px; padding-bottom:80px; }
.main-conts:before { content:''; display:block; position:absolute; top:0; left:820px; bottom:80px; width:1px; background:#dbdfe1; }
.main-conts:after { content:''; display:block; clear:both; }
.main-conts .major { float:left; width:820px; padding-right:60px; }
.main-conts aside { float:right; width:calc(100% - 820px); padding-left:60px;  }
.main-conts aside .main-bnr { margin-top:32px; }
.main-conts aside .main-bnr a.bnr-kakao { position:relative; height:106px; display:flex; align-items: center; padding-left:32px; background:#FEEE79; border-radius: 24px 0px 24px 24px; }
.main-conts aside .main-bnr a.bnr-kakao span { display:block; line-height:24px; font-weight:600; color:#222; }
.main-conts aside .main-bnr a.bnr-kakao:after { content:''; display:block; position:absolute; top:50%; right:32px; width:61px; height:41px; transform:translateY(-50%); background:url(/kiam/pcWeb/resources/images/common/ico_comm.png) no-repeat -500px -175px; }

.main-conts article { margin-top:80px; }
.main-conts article h2 { margin-bottom:16px; font-weight:700; font-size:24px; color:#222; line-height:36px; }
.main-conts article h2 a { display:inline-block; position:relative; padding-right:24px; color:inherit; line-height:36px; }
.main-conts article h2 a:after { content:''; display:inline-block; display:block; position:absolute; top:50%; right:0; margin-top:-12px; width:24px; height:24px; background:url(/kiam/pcWeb/resources/images/common/ico_comm.png) no-repeat -120px -40px;  }
.main-conts aside > article:first-child { margin-top:0; }
.main-conts aside > article { margin-top:78px; }
.main-conts aside > article h2 a { padding-left:14px; }
.main-conts aside > article h2 a:before { content:''; display:block; position:absolute; top:5px; left:0; width:8px; height:8px; border-radius:50%; background:#ec3690; }
article.fund-summary { margin-top:0; }
article.fund-summary h2 { font-size:32px; font-weight:700; }
article.fund-summary h2 span { display:inline-block; position:relative; padding-left:14px;  }
article.fund-summary h2 span:after { content:''; display:block; position:absolute; left:0; top:0; width:8px; height:8px; border-radius:50%; background:#ec3690;  }

.pic-list li { position:relative; margin:32px 0 0 0; padding-top:208px; }
.pic-list li:first-child { margin-top:0; }
.pic-list li a { color:#969a9e; }
.pic-list li span,
.pic-list li strong { display:block; font-size:14px; line-height:24px; }
.pic-list li .pic { overflow:hidden; position:absolute; top:0; left:0; right:0; height:192px; border-radius: 24px 0px 24px 24px; background:#f2e9d3; }
.pic-list li .pic .tag { position:absolute;  }
.pic-list li .pic .tag.live { position: absolute; z-index:2; display:inline-block; top:140px; left:20px; font-weight:700; height:30px; padding:3px 15px 0 28px; color:#fff; font-size:12px; background:#F2303C; border-radius:15px; }
.pic-list li .pic .tag.live:after { content:''; display:block; position:absolute; top:12px; left:15px; width:5px; height:5px; background:#fff; opacity:0; border-radius:50%; animation:liveDot  1s 1s infinite linear; }
.pic-list li .pic img { position: absolute; left: 50%; top: 50%; transform: translate(-50%,-50%); width: 100%; height: 100%; z-index:1; }
.pic-list li .pic.no-img { background:#f7f7f7 url(/kiam/pcWeb/resources/images/common/img_logo_off.png) no-repeat center center; }
.pic-list li .title { overflow:hidden; margin:8px 0; color:#222; font-size:16px; line-height:24px; font-weight:600; display:-webkit-box; -webkit-box-orient: vertical; -webkit-line-clamp:2; text-overflow:ellipsis;   } 
.pic-list.tv li .pic:after { content:''; z-index:2; display:block; position:absolute; width:48px; height:48px; top:50%; left:50%; transform:translate(-50%, -50%); background:url(/kiam/pcWeb/resources/images/common/ico_comm.png) no-repeat -460px -70px;  }
.pic-list.tv li .pic.no-img:after { display:none; }

.fund-summary .fund-tab { margin-top:50px; }
.fund-summary .fund-tab .ui-tab-list { border-bottom:0; display:block;  }
.fund-summary .fund-tab .ui-tab-btns { display:block; line-height:0; font-size:0; border-bottom:0; }
.fund-summary .fund-tab .ui-tab-btn { display:inline-block; min-width:119px; height:48px; padding:0 40px; font-weight:600; font-size:20px; line-height:30px;  background:#f7f7f7; color:#878c91; border-radius:26px; }
.fund-summary .fund-tab .ui-tab-btn + .ui-tab-btn { margin-left:8px; }
.fund-summary .fund-tab .ui-tab-btn.selected { color:#fff; background:#1A399E; }
.fund-summary .fund-tab .ui-tab-btn.selected:after { display:none; }
.fund-summary .fund-tab .ui-tab-pnls { margin-top:46px; }
/* .fund-summary .fund-tab .ui-tab-pnls .ui-tab-pnl { display:block; visibility:visible; opacity:1; margin-top:46px; } */

.fund-list li { margin-top:15px; line-height:0; font-size:0; }
.fund-list li a { position:relative; display:block; height:188px; padding:40px 200px 0 48px; border:1px solid #DFE0E2; border-radius:20px 0 20px 20px; box-shadow:0 6px 12px 0 rgb(0 0 0 / 4%) }
.fund-list li a:hover { border-color:#1A399E; }
.fund-list li a .title { overflow:hidden; display:block; margin:16px 0 0 0; font-size:24px; line-height:36px; color:#222; text-overflow:ellipsis; white-space:nowrap; }
.fund-list li a > .txts { overflow:hidden; display:block; margin-top:8px; font-size:16px; color:#878c91;  text-overflow:ellipsis; white-space:nowrap; }
.fund-list li.pick a { background:#F1F7FE; border-color:#113585; }
.fund-list li.pick a:after { content:''; display:block; position:absolute; top:-6px; right:50px; width:40px; height:48px; background:url(/kiam/pcWeb/resources/images/common/ico_comm.png) no-repeat -320px -140px;  }
.fund-list li .rate { position:absolute; bottom:42px; right:45px; font-size:20px; text-align:right; }
.fund-list li .rate .points { letter-spacing:1px; font-weight:700; font-size:18px; }
.fund-list li .rate em { padding-right:5px; font-size:38px; font-family: inherit; } 
.fund-list li .rate .txts { display:block; font-size:14px; margin-top:4px; color:#74757E; line-height:21px; }
.fund-list li .rate .txt-up,
.fund-list li .rate .txt-drop { font-size:32px; line-height:42px; font-weight:700; }
.fund-list .tag-wrap { display:block; }

.noti-list { border:1px solid #DFE0E2; padding:0 48px; border:1px solid #DFE0E2; border-radius:24px 0px 24px 24px; }
.noti-list li { border-top:1px solid #e9e9e9; }
.noti-list li:first-child { border-top:0; }
.noti-list li a { display:flex; justify-content: space-between; align-items: center; padding:34px 0; }
.noti-list li a span.txts { width:calc(100% - 150px); line-height:24px; }
.noti-list li a span.date { font-size:14px; color:#878c91; line-height:24px; }

.main-slick-wrap { position:relative; min-width:1200px; height:515px; margin:0 auto; }
.main-slick-wrap:after,
.main-slick-wrap:before { content:''; position:absolute; top:0; left:0; height:480px;  width:56px; background:url(/kiam/pcWeb/resources/images/bg_visual_left.png) no-repeat 0 0;  }
.main-slick-wrap:before { left:inherit; right:0; background:url(/kiam/pcWeb/resources/images/bg_visual_right.png) no-repeat 0 0; z-index:10; }
.main-visual .main-slick { overflow:hidden; height:515px; padding:0; border-radius:0; margin:0;  }
.main-slick .items { position:relative; height:515px; }
.main-slick .items .item-inner { position:relative; min-width:1280px; height:515px; margin:0 auto; overflow:hidden; }
.main-slick .items .item-inner img { width:100%; min-width:1280px; height:515px; object-fit:cover; }
.main-slick .items .item-txts .txt1 { color:#fff; font-size:20px; line-height:30px; font-weight:400; }
.main-slick .items .item-txts .txt2 { margin-top:16px; color:#fff; font-weight:700; font-size:56px; line-height:67px; }
.main-visual .btns { line-height:0; font-size:0;  }
.main-visual .btns a { display:inline-block; color:#fff; border: 1px solid #fff; padding:13px 40px 0 40px; border-radius: 100px; }
.main-visual .btns a.btn-kosef { background: linear-gradient(98.43deg, #263BDC 26.72%, #B629D6 135.94%);  }
.main-visual .btns a.btn-kosef span { padding:0 22px 0 85px; }
.main-visual .btns a.btn-kosef span:after { content:''; display:block; position:absolute; top:1px; left:0; width:78px; height:12px; background:url(/kiam/pcWeb/resources/images/common/ico_comm.png) no-repeat -431px -320px;  }
.main-visual .btns a span { position:relative; line-height:21px; font-size:14px; padding:0 22px 0 0; }
.main-visual .btns a span:before { content:''; display:block; position:absolute; top:3px; right:0; width:19px; height:10px; background:url(/kiam/pcWeb/resources/images/common/ico_comm.png) no-repeat -460px -22px;     }
.main-visual .btns a + a { margin-left:16px; }
.main-visual .visual-utils { position:relative; width:1280px; margin:0 auto; }
.main-visual .visual-utils .btns { position:absolute; left:50px; bottom:180px; width:500px; text-align:left;  }
.main-visual .visual-utils .slick-info { position:absolute; left:50px; bottom:250px; width:100%; }
.main-visual .visual-utils .slick-info .play { display:none; }
.main-visual .visual-utils .slick-info .page { display:inline-block; vertical-align:top; }
.main-visual .visual-utils .slick-info .slick-control { display:inline-block; vertical-align:top;  }
.main-visual .visual-utils .slick-info .slick-dots { position:static; font-size:0; line-height:0; }
.main-visual .visual-utils .slick-info .slick-dots .slick-dots li { position: relative; margin: 0 2px; border-radius: 50%; }
.main-visual .visual-utils .slick-info .slick-dots li button:before { background:#fff; opacity:0.5; }
.main-visual .visual-utils .slick-info .slick-dots li.slick-active button:before { background:#fff; opacity:0; }
.main-visual .visual-utils .slick-info .slick-dots {position: relative;  display: flex !important; bottom: inherit;}
.main-visual .visual-utils .slick-info .slick-dots li{ position: relative; margin: 0 2px; border-radius: 50%;}
.main-visual .visual-utils .slick-info .slick-dots li::after{ content: ''; position: absolute; top:50%; left:50%; transform: translate(-50%, -50%); width: 6px; height: 6px; background-color: #FFF; opacity: 0.4; border-radius: 50%;}
.main-visual .visual-utils .slick-info .slick-dots .slick-active::after{ opacity: 1 !important; }
.main-visual .visual-utils .slick-info .slick-dots li button{width: 16px; height: 16px; border-radius: 50%;}
.main-visual .visual-utils .slick-info .slick-dots li button span{ display: inline-block; text-indent: -9999px;}
.main-visual .visual-utils .slick-info .slick-active .spinner{position: absolute; left: 1px; top:1px;}
.main-visual .visual-utils .slick-info .slick-active .spinner .path{ stroke-dasharray: 500; stroke-dashoffset: 0; transform: rotate(-90deg); animation: rotate 19s; stroke: #FFF; transform-origin: center;}
@keyframes rotate {
    0% {
        stroke-dashoffset: 500;
    }
    100% {
        stroke-dashoffset: 0;
    }
}


@keyframes revealCircle {
    to {
      transform: rotate(180deg);
    }
  }
@keyframes liveDot { 0% {opacity:0;} 100% {opacity:1;} }