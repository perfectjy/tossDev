@charset "utf-8";

/* mo Tab */
.ui-tab.scroll-tab.t1 .ui-tab-btns {padding:0 24px; font-size:0; border-bottom:1px solid #DFE0E2; }
.ui-tab.scroll-tab.t1 .ui-tab-btn {position:relative; padding:0 8px; color:#73737A;}
.ui-tab.scroll-tab.t1 .ui-tab-btn.selected {color:#222; font-weight:700;}
.ui-tab.scroll-tab.t1 .ui-tab-pnls {padding:24px 24px 0 24px;}
.ui-tab.scroll-tab.t1 .ui-tab-btn.selected {background-color:#fff;}
.ui-tab.scroll-tab.t1 .ui-tab-btn {background-color:#fff;}
.ui-tab.scroll-tab.t1 .ui-tab-btn.selected {color: #222; font-weight:700; }

.ui-tab.scroll-tab .ui-tab-btns:after { content:''; display:block; clear:both;}
.ui-tab.scroll-tab .ui-tab-btn { display:inline-block; font-size:14px; height:47px; color:#73737A; }
.ui-tab.scroll-tab .ui-tab-btn.selected:after { content:''; display:block; position:absolute; bottom:0; left:0; width:100%; height:2px; background:#EC008C; }
.ui-tab.scroll-tab .ui-tab-pnls { padding:40px 24px; }
.sub-conts-wrap .ui-tab.scroll-tab .ui-tab-btn.selected {background:#fff ;}


.main-tab.search {position:relative; font-size:0;}
.main-tab.search .ui-tab-btn {display:inline-block; height:32px; padding:0 16px; margin-top:8px; margin-right:8px; color:#73737A; font-size:14px; background-color:#fff; border:1px solid #DFE0E2; border-radius:40px; vertical-align:top;}
.main-tab.search .ui-tab-btn:last-child {margin-right:0;}
.main-tab.search .ui-tab-btn.selected {color:#fff; background-color:#EC008C; border-color:#EC008C;}



/* ui select */
.ui-select { position:relative; display:inline-block; width:100%; }
.ui-select select { width:100%; }
.ui-select select.off { position:absolute; z-index:-1; opacity:0; }
.ui-select select.off:focus + .ui-select-btn { border-color:#000000; }
.ui-select select.off:focus + .ui-select-btn:after { border-color:#000000; }
.ui-select-btn { padding:0 40px 0 26px; display:flex; align-items:center; justify-content:flex-start; width:100%; height:48px; font-size:14px; color:#73737A; border:1px solid #BFC0C5; border-radius:8px;}
.ui-select-btn:after { content:""; display:block; position:absolute; right:20px; top:50%; margin-top:0; width:16px; height:16px; opacity:0.5; background:url(/kiam/pcWeb/resources/images/common/ico_comm.png) no-repeat 0 0; transform:translateY(-50%);}
.ui-select-btn span { display:inline-block; width:calc(100% - 40px); text-align:left; word-break:nowrap; text-overflow:ellipsis; white-space:nowrap; overflow:hidden; text-align:left; line-height:1.2; }
.ui-select-btn[aria-expanded=true],
.ui-select-btn:hover,
.ui-select-btn:focus,
.ui-select-btn:active { border-color:#BFC0C5; color:#222; }
.ui-select-btn[aria-expanded=true]:after,
.ui-select-btn:hover:after,
.ui-select-btn:focus:after,
.ui-select-btn:active:after { opacity:1; }
.ui-select-wrap.on { display:block; }
.ui-select-wrap.top { bottom:3.9rem; }
.ui-select-wrap.bottom { top:40px;}
.ui-select-wrap { display:none; position:absolute; z-index:100; left:0; width:100%; overflow-y:auto; padding:10px 0; max-height:200px; -webkit-overflow-scrolling:touch; border:1px solid #BFC0C5; border-radius:0 0 8px 8px; border-top:0; background:#FFFFFF; }
.ui-select-wrap.ui-scrollbar { position:absolute; }
.ui-select-opts { overflow:hidden; width:100%; }
.ui-select-opt { display:block; float:left; width:100%; padding:0 40px 0 26px; font-size:14px; height:40px; text-align:left; white-space:nowrap; background:#FFF; }
.ui-select.on .ui-select-btn { border-radius:8px 8px 0 0; border-color:#EC008C; border-bottom:1px solid #fff; }
.ui-select.on .ui-select-btn:after { background-position-x:-380px; opacity:1; }
.ui-select.on .ui-select-wrap { border-color:#EC008C; }
.ui-select.error .ui-select-btn { border-color:#F2303C; color:#222; }

.ui-select-opt.selected { font-weight:700; text-decoration:underline; background:#F1F7FE; color:#1A399E; }
.ui-select-opt.disabled { opacity:0.5; }
.ui-select-title { display:none; position:absolute; top:0; left:0; }
.ui-select.type-tel { width:15rem; }  
.ui-select.type-bank { width:20rem;}
.ui-select.type-card { width:20rem; }
.ui-select-cancel,
.ui-select-confirm { display:none; }

.ui-select-btn:disabled { border-color:#B9BBC6; background:#DFE0E2; color:#B9BBC6; }
.ui-select-btn:disabled:after { opacity:0.3; }


/* 키자매거진 */
/* 공통 */
