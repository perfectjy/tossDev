.kosef-tab-item{ display: inline-flex; position:relative; }
.kosef-tab-item:not(:first-child){ margin-left: 10px; }
.kosef-tab-item .tab-ckb{ display: flex; justify-content: center; align-items: center; flex-direction: column; word-break: keep-all; white-space: initial; text-align: center; width: 80px; height: 80px; border-radius: 8px; border: 1px solid #DFE0E2; box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.04); box-sizing: border-box; background-color: #FFF; cursor: pointer;}
.kosef-tab-item .tab-ckb em{ display: flex; font-size: 12px; font-weight: 600; margin-top: 6px; color:#74757E; }
.kosef-tab-item .tab-ckb span{ width: 24px; height: 24px; background-position-x: center; background-repeat: no-repeat; background-size: 100%;}
.kosef-tab-item .tab-ckb .on{ display: none; }
.kosef-tab-item .tab-ckb:hover{ color: #1A399E; border: 1px solid #1A399E; box-sizing: border-box;}
.kosef-tab-item input:checked + label{ color: #FFF; background: linear-gradient(114.62deg, #394DE4 6.07%, #953DEE 96.44%); box-shadow: 0px 2px 4px rgba(26, 57, 158, 0.16); }
.kosef-tab-item input:checked + label span{ display: none; }
.kosef-tab-item input:checked + label span.on{ display: block; }
.kosef-tab-item input:checked:hover + label{ border: 1px solid #394DE4; box-sizing: border-box; }
.kosef-tab-item input:checked + label em { color:#fff;}


.txt-list{ text-align: left; word-break: keep-all; display: block; }


/*KOP02010200M*/
.head-group .dline{ max-height:112px }
.tbl-nodate{ font-size: 14px; font-weight: 500; padding: 152px 0; text-align: center;}
.sub-graph .sub-graph-info{ margin-bottom: 16px; }
.sub-graph .sub-graph-info ul{ display: flex; justify-content: flex-end;}
.sub-graph .sub-graph-info ul li{ margin-left: 12px; }
.sub-graph .sub-graph-info ul li:first-child{ margin-left: 0;}
.sub-graph .sub-graph-info span{ position: relative; padding-left: 12px; color: #74757e; font-size: 12px; font-weight: 400;}
.sub-graph .sub-graph-info span::before{content: ''; position: absolute; top: 50%; left: 0; transform: translateY(-50%); width: 8px; height: 8px; border-radius: 50%; }
.sub-graph .sub-graph-info .blue::before{ background-color: #1A399E; }
.sub-graph .sub-graph-info .pink::before{ background-color: #EC008C; }
.sub-graph .sub-graph-info .green::before{ background-color: #58B84A; }


/*KOM03010000M*/
.report-detail-wrap .sub-head-wrap{ background-color:#ECF1FF; padding-top: 8px; padding-bottom: 24px;}
.report-detail-wrap .sub-head-wrap .title::before{ content: ''; display: block; width: 40px; height: 40px; background:url(/kosef/moWeb/resources/images/common/ico_conts.png) no-repeat -350px 0;  background-size: 500px; margin-bottom: 16px;}
.sub-head-wrap .txt{ color: #74757e; font-size: 14px; font-weight: 500; text-align: left; }
.room-wrap{ padding: 40px 20px 56px 20px;}
.room-wrap .title{ display: flex; align-items: center; margin-bottom:8px; }
.room-wrap .title strong{ margin-right: 10px; }
.room-wrap .download{ display: flex; align-items: center; margin-top: 16px;}
.room-wrap .download button{ width:100%; }
.room-wrap .download .btn-icon span { padding: 0 !important;}
.room-wrap .download .btn-icon.icon-dw span:after { position:static; }

/*KOM04020201T*/
.ui-tab.scroll{ position: relative; }
.ui-tab.on .ui-tab-list.sublink{ margin-bottom: 64px;}
.ui-sub-tab .ui-select{ position: absolute; top:50px; right: 0; display: none; }
.ui-sub-tab .ui-select.on{ display: block;}


/*KOM04030000T*/
.etf-point-wrap{ height: 623px; }
.etf-point-wrap.on{ height: 2300px; }
.etf-point-wrap.on .point-box{ position: fixed; top:74px; height: 628px; width: 100%; }
.etf-point-wrap.on .etf-card-list{ position: fixed; margin-top: 170px; left: calc(50% - 204px);}
.etf-point-wrap.on .etf-card-list .card-item{position: inherit; left: calc(50% - 163.5px); margin-top: 180px;}
.etf-point-wrap.on .etf-card-list .card-item{ translate: none; rotate: none; scale: none; transform: translate(0px, 0px); opacity: 1; }
.etf-point-wrap .etf-card-list .card-item.ing{ animation:card_on 1s forwards;}
@keyframes card_on{
    0%{
        translate: none; rotate: none; scale: none; transform: translate(0px, 0px); opacity: 1;
    }
    100%{
        translate: none; rotate: none; scale: none; transform: translate3d(397.766px, -198.883px, 0px) rotate(34.8045deg) scale(0.8011, 0.8011); opacity: 0.0056;
    }
}

.is-eft-wrap{ position: relative; }
.is-eft-circle .txt{ position: absolute; top:50%; left: 50%; transform: translate(-50%, -50%); color: #F2F3F5; font-weight: 700; font-size: 64px; line-height: 76px; z-index: -1;}
.is-eft-origin{width: 327px; height: 327px; position: absolute;}
.is-eft-origin.lt{ animation: rotatelt01 18s ease-in-out infinite; z-index: 2;}
.is-etf-item.stock{ transform: rotate(0deg); animation: rotatelt02 18s ease-in-out infinite;}
@keyframes rotatelt01 {
    0% { 
        transform: rotate(0deg); 
    }
    15%{
        transform: rotate(-40deg); 
    }
    24%{
        transform: rotate(-40deg); 
    }
    45%{
        transform: rotate(0deg); 
    }
    60%{
        transform: rotate(5deg); 
    }
    100%{
        transform: rotate(0deg); 
    }
}
@keyframes rotatelt02 {
    0% { 
        transform: rotate(0deg); 
    }
    15%{
        transform: rotate(40deg); 
    }
    24%{
        transform: rotate(40deg); 
    }
    45%{
        transform: rotate(0deg); 
    }
    60%{
        transform: rotate(-5deg); 
    }
    100%{
        transform: rotate(0deg); 
    }
}
.is-eft-origin.rt{ animation: rotatert01 22s ease-in-out infinite; z-index: 2;}
.is-etf-item.fund{ animation: rotatert02 22s ease-in-out infinite; }
@keyframes rotatert01 {
    0% { 
        transform: rotate(0deg); 
    }
    15%{
        transform: rotate(42deg); 
    }
    22%{
        transform: rotate(42deg); 
    }
    45%{
        transform: rotate(0deg); 
    }
    60%{
        transform: rotate(10deg); 
    }
    100%{
        transform: rotate(0deg); 
    }
} 
@keyframes rotatert02 {
    0% { 
        transform: rotate(0deg); 
    }
    15%{
        transform: rotate(-42deg); 
    }
    22%{
        transform: rotate(-42deg); 
    }
    45%{
        transform: rotate(0deg); 
    }
    60%{
        transform: rotate(-10deg); 
    }
    100%{
        transform: rotate(0deg); 
    }
}
.is-eft-circle .b-circle{ position: absolute; top:30px; left: 30px; width: 265px; height: 268px; background: url(/kosef/moWeb/resources/images/main/b_circle.png) 0 0 no-repeat; background-size: 265px 268px; animation: b-circle 6s infinite; transform: rotate(0deg);}
@keyframes b-circle{
    0%{
        transform: rotate(0deg);
    }
    40%{
        transform: rotate(-2deg);
    }
    70%{
        transform: rotate(2deg);
    }
    90%{
        transform: rotate(2deg);
    }
    100%{
        transform: rotate(0deg);
    }
}
.is-eft-circle .r-circle{ position: absolute; top:30px; left: 30px; width: 265px; height: 265px; background: url(/kosef/moWeb/resources/images/main/r_circle.png) 0 0 no-repeat; background-size: 265px 265px; animation: r-circle 30s infinite;}
@keyframes r-circle {
    0%{
        transform: rotate(0deg);
    }
    100%{
        transform: rotate(360deg);
    }
}
.is-eft-circle .s-circle{position: absolute; bottom: 0; right: 13px;  width: 72px; height: 72px; background: url(/kosef/moWeb/resources/images/main/s_circle.png) 0 0 no-repeat; background-size: 72px 72px; animation: s-circle 6s infinite; transform: rotate(0deg);}
@keyframes s-circle{
    0%{
        transform: rotate(0deg);
    }
    40%{
        transform: rotate(2deg);
    }
    70%{
        transform: rotate(-2deg);
    }
    90%{
        transform: rotate(-2deg);
    }
    100%{
        transform: rotate(0deg);
    }
}




