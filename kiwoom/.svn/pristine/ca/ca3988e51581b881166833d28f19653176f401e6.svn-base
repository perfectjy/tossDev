.main-popup .mlink{ display: flex; align-items:center; position: fixed; bottom: 24px; right: 24px; width: 138px;  height: 40px; background: linear-gradient(114.62deg, #394DE4 6.07%, #953DEE 96.44%); border-radius: 20px; padding: 5px 16px 5px 16px; z-index: 10; animation: wbtnl 1s forwards;}
.main-popup .mlink.mg::before{ content: ''; position: absolute; left: 14px; width: 24px; height: 24px; background: url(/kosef/moWeb/resources/images/main/money_bag.svg) 0 0 no-repeat; margin-right: 8px; animation: wbtni 0.3s forwards; z-index: 10;}
.main-popup .mlink.tv::before{ content: ''; position: absolute; left: 18px; width: 24px; height: 24px; background: url(/kosef/moWeb/resources/images/main/cash.svg) 0 0 no-repeat; margin-right: 8px; animation: wbtni 0.3s forwards; z-index: 10;}
.main-popup .mlink span{ color: #DFE0E2; font-size: 14px; font-weight: 500; }
.main-popup .mlink .ctxt{ position: relative; animation: wbtn .25s forwards; height: 14px; overflow: hidden; opacity: 0; margin-left: 32px; padding-right: 70px;}
.main-popup .mlink .txt{ position: absolute; right: 0; color:#FFF; font-size: 12px; font-weight: 700; padding-right: 40px; }
.main-popup .mlink .txt::after{ content: ''; position: absolute; top: -2px; right: 12px; width: 16px; height: 16px; background: url(/kosef/moWeb/resources/images/main/ch_right.svg) 0 0 no-repeat;}

@keyframes wbtnl {
    0%{
        width: 140px;
    }
    100%{
        width: 298px;
    }
}

@keyframes wbtn {
    0%{
        width: 0px;
        opacity: 0;
    }
    90%{
        width: 0px;
        opacity: 0;
    }
    100%{
        opacity: 1;
        width: 238px;
    }
}

@keyframes wbtni {
    0%{
        left: 70px;
    }
    100%{
        left: 12px;
    }
}



