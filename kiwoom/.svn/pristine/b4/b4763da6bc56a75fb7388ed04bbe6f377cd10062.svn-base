@charset "utf-8";

/* common */
* { box-sizing:border-box; padding:0; margin:0; line-height:1; }
html, body { background:#fff; user-select:none; -webkit-user-select:none; -ms-user-select:none; -webkit-user-select:none; -webkit-tap-highlight-color:transparent; }
body { padding:0; margin:0; font-family: "Pretendard", Sans-serif; font-size:16px; font-weight:500; color:#222; word-break:break-all; letter-spacing:0; }

h1, h2, h3, h4, h5, h6 { font-weight:400; color:#000; }
a { text-decoration:none; outline-offset:1px; color:inherit; }
a:hover { text-decoration:none; }
p { line-height:1.5; }
dl, ol, ul, dt, dd, li { display:block; list-style:none; }
table { width:100%; border-collapse:collapse; border-spacing:0; }
caption { height:0; opacity:0; }
th, td { border-collapse:collapse; vertical-align:middle; }
button{overflow:visible}
button{margin:0;padding:0;border:0;background:none;width:auto;cursor:pointer;overflow:visible;vertical-align:middle;background: transparent;cursor:pointer; font-family: "Pretendard", Sans-serif; }
button span, button:active span, button:focus span{position:relative}
select, textarea, input { font-family: "Pretendard", Sans-serif; -webkit-appearance:none; border:0; }
select { height:40px; padding:0 10px 0 10px; }
input { border:0; }
em, i { font-style:normal; text-decoration:none;}
hr { position:relative; display:block; height:1px; border:0; background:#eee; margin:30px 0; }
img { border:0; }
.hanja { font-family:'맑은 고딕', '돋움', '굴림'; }

/* BTN */
.btn { display: inline-flex; justify-content: center; align-items: center; min-width: 160px; min-height: 48px; padding: 0 40px; font-size: 16px; border-radius: 32px; background-color:#FFF; text-align: center;}
.btn.base { min-width:160px; line-height:46px; font-weight:600; font-size:16px; border:1px solid #222; }
.btn.base.cancel { color:#74757E; border-color:#74757E; }
.btn.base:hover {background:#F7F8F8;}
.btn:disabled { color: #B9BBC6; border: 1px solid #DFE0E2; background-color: #DFE0E2; cursor: default; }
.btn:disabled:hover, .btn:disabled:active { color: #B9BBC6; border: 1px solid #DFE0E2; background-color: #DFE0E2; }
.btn.primary { color: #EC008C; font-weight: 600; border: 1px solid #EC008C; background-color: #FFF;}
.btn.primary:hover, .btn.primary:active { color: #FFF; background-color: #EC008C; }
.btn.secondary { color: #222; border: 1px solid #222; }
.btn.secondary:hover, .btn.secondary:active { background-color: #F7F8F8; }
.btn.tertiary { color: #9D9DA5; border: 1px solid #9D9DA5;}
.btn.tertiary:hover, .btn.tertiary:active { background-color: #F7F8F8; }
.btn.fill { color: #FFF; background-color: #EC008C;}
.btn.fill:hover, fill:active { background-color: #C02882; }
.btn.lg { min-width: 200px; min-height: 56px; font-size: 20px; }
.btn.base.tbl { height:40px; padding:0 10px; min-height:40px; min-width:70px; border-radius:8px; font-size:14px;  }

.btn-icon { display: inline-flex; min-width: 118px; min-height: 48px; padding: 0 24px; border: 1px solid #222; border-radius: 8px; align-items: center; justify-content:center;}
.btn-icon:hover, .btn-icon:active {background-color: #F7F8F8;} 
.btn-icon span { display: inline-flex; align-items: center; position: relative; color: #222; font-size: 14px; font-weight: 500; }
.btn-icon span::after { content:''; display: inline-block; width: 16px; height: 16px; margin-left: 4px; }
.btn-icon.icon-dw span::after{ background:url(/admin/resource/images/ico_comm.png) no-repeat -280px 0;}
.btn-icon.icon-more span::after{ background:url(/admin/resource/images/ico_comm.png) no-repeat -300px 0;}
.btn.square { min-width:80px; min-height:40px; padding:0 24px; font-size:14px; line-height:21px; border-radius: 4px; white-space: nowrap; }
.btn.modify { display:inline-block; min-width:45px; min-height:45px; padding:0; border-radius: 0; }
.btn.modify::before {content:''; display:block; width:16px; height: 16px;  background:url(/admin/resource/images/ico_comm.png) no-repeat -360px 0; margin:0 auto;}

.iod-wrap { display:inline-block; margin-left:18px; }
/* .iod-wrap .btn { position:relative; min-width:40px; min-height:40px; background-repeat: no-repeat;padding:0; border-radius: 0; }
.iod-wrap .btn + .btn {margin-left:18px;}
.iod-wrap .btn.plus::before {content:''; width:16px; height:16px; background:url(/admin/resource/images/ico_comm.png) -140px 0; }
.iod-wrap .btn.minus::before {content:''; width:16px; height:16px; background:url(/admin/resource/images/ico_comm.png) -160px 0; } */

.iod-wrap.type2 .btn { display:inline-block; margin: 0 auto;}
.iod-wrap.type2 .btn + .btn {margin-top:8px;}


/* input box block type */
[class*="inp-"] { position:relative; display:inline-block; width:100%; height:40px; font-size:14px; color:#222; line-height:1; padding:9px 24px; border-radius:8px; font-family:"Pretendard", Sans-serif; background:#F7F8F8; outline:0; }
[class*="inp-"].text-arror {border: 1px solid #F2303C;}
[class*="inp-"]:focus { border-color:#0091ea; }
[class*="inp-"]::placeholder { font-family: "Pretendard", Sans-serif; color:#9e9e9e; }
[class*="inp-"]::-webkit-input-placeholder { font-family: "Pretendard", Sans-serif; color:#9e9e9e; }
[class*="inp-"]:-ms-input-placeholder { font-family: "Pretendard", Sans-serif; color:#9e9e9e; }
[class*="inp-"]:focus::placeholder { font-family: "Pretendard", Sans-serif; color:transparent; }
[class*="inp-"]:focus::-webkit-input-placeholder { font-family: "Pretendard", Sans-serif; color:transparent; }
[class*="inp-"]:focus:-ms-input-placeholder { font-family: "Pretendard", Sans-serif; color:transparent; }
[class*="inp-"][readonly]:focus { border-color:#e5e5e5; }
[class*="inp-"][disabled],
[class*="inp-"][readonly] { background:#DFE0E2; }
[class*="inp-"][readonly]::placeholder,
[class*="inp-"][disabled]::placeholder { color:#9e9e9e; }
[class*="inp-"][readonly]::-webkit-input-placeholder,
[class*="inp-"][disabled]::-webkit-input-placeholder { color:#9e9e9e; }
[class*="inp-"][readonly]:-ms-input-placeholder,
[class*="inp-"][disabled]:-ms-input-placeholder { color:#9e9e9e; }
[class*="inp-"][type="password"] { -webkit-text-security:disc; }
[class*="inp-"]::-ms-clear { display:none; }
textarea[class*="inp-"] { display:block; max-height:200px; line-height:22px; overflow-y:hidden; background:#F7F8F8; border-radius:8px; font-family: "Pretendard", Sans-serif; }
textarea[class*="inp-"]::placeholder { line-height:22px; }
/* textarea[class*="inp-"]:focus { padding:12px 15px 13px; } */

label { position:relative; display:inline-block; font-size:14px; }
 
input[type="radio"],
input[type="checkbox"] { position:absolute; bottom:0; left:0; width:0; height:0; opacity:0; }
input[type="password"] { font-family:Arial, sans-serif; }

input[type=file] { font-family: "Pretendard", Sans-serif; }
input[type=file]::file-selector-button { display:none; }
.btn-file { display:inline-block; width:120px; line-height:48px; text-align:center; font-weight:600; font-size:16px; border:1px solid #222; border-radius:8px; cursor:pointer;}

/* select */
select { min-width:120px; height:40px; color:#000; font-size:14px; font-weight:600; border:1px solid #F7F8F8;  padding:0 70px 0 24px; border-radius:8px; background:#F7F8F8 url(/admin/resource/images/ico_acco_arr.png) no-repeat calc(100% - 20px) 8px; background-size:24px; opacity: 0.5;}
select:hover { border:1px solid #1A399E;  background-color:#fff;  }



/* headline style */
.h1 { font-weight:300; }
.h2 { font-weight:300; }
.h1 strong,
.h2 strong { font-weight:800; }

/* font color */
.fc-primary { color:#EC008C !important; }
.fc-secondary { color:#2041bf !important; }
.fc-sub { color:#757575 !important; }
.fc-warn { color:#f9a825 !important; }
.fc-bad { color:#d32f2f !important; }
.fc-good { color:#4caf50 !important; }
.fc-black { color:#000000 !important; }

/* keep-all */
.keep-word { word-break:keep-all !important; }

/* a */
a.link { color:#003cff; text-decoration:underline; }

/* hide/hidden style */
.a11y-hidden,
.hide  { position: absolute !important; width: 1px !important; height: 1px !important; color: transparent !important; overflow: hidden !important; clip: rect(1px, 1px, 1px, 1px); -webkit-clip-path: inset(50%); clip-path: inset(50%); margin: 0 !important; padding: 0 !important; }

/* float */
.fclear::after { content:''; display:block; clear:both; }
.f-left { float:left !important; }
.f-right { float:right !important; }

.ellips {  overflow:hidden; display:block; text-overflow:ellipsis; white-space:nowrap; } /* 한줄 처리 */

/* inputs */

.lb-title,
.lb-item { position:relative; display:inline-block; margin:0 5px; box-sizing:border-box; white-space:nowrap; vertical-align:middle; }
.lb-radio,
.lb-checkbox { position:relative; display:inline-flex; vertical-align:top; align-items:center; min-height:24px; padding:0 0 0 32px; margin:0; cursor:pointer; line-height:1; }
.lb-checkbox::after,
.lb-radio::after { content:""; display:block; position:absolute; top:0; left:0; width:24px; height:24px; background:url(/admin/resource/images/ico_comm.png) no-repeat 0 -80px; }
.lb-radio::after { background-position-x:-150px; }
.lb-radio:hover:after { background-position-x:-180px; }
.lb-checkbox:hover:after { background-position-x:-30px; }
.lb-checkbox:hover:disabled:before { background-position-x:-19px; }
input:checked + .lb-radio::after { background-position-x:-180px; }
input:checked + .lb-checkbox::after { background-position-x:-30px; }
input:disabled + .lb-radio:after { background-position-x:-240px; }
input:disabled + .lb-checkbox:after { background-position-x:-90px; }
input:checked:disabled + .lb-radio:after { background-position-x:-270px; }
input:checked:disabled + .lb-checkbox:after { background-position-x:-120px; }
.lb-checkbox.no-label { padding-left:20px; }
.lb-checkbox.no-label.checked { padding-left:20px; }
/* .lb-title { position:absolute; display:inline-block; min-height: 24px; margin:0 20px; font-size:14px; line-height:24px; padding-left: 103px; } */

/* FORM */
/* .frm-wrap.iod { display:flex; justify-content:space-between; } */
/* .frm-wrap.iod .iod-wrap.type2 .btn + .btn { margin:0; } */
/* .frm-wrap.iod .iod-wrap { display:flex; flex-direction:column; justify-content:center; width:40px; height:90px; margin-left:0; padding:5px 0 0 0; } */
/* .frm-wrap.iod .frm-area { width:calc(100% - 50px);  } */
.frm-wrap.iod .frm-area textarea { width:100%; min-height:80px; }

.tbl-base [class*="inp-"] + .btn { margin-left:8px; }
.tbl-btns { position: relative; margin-top:16px; overflow: hidden; height:48px; }
.tbl-btns > div { float:right; line-height:0; font-size:0; }
.tbl-btns > div .btn + .btn { margin-left:5px; }
.tbl-btns .pager {float: none; text-align: center; margin-top:3px;}

.pager { margin-top:20px; text-align:center; line-height:0; font-size:0; }
.pager a { display:inline-block; width:40px; height:40px; font-size:16px; line-height:40px; text-align:center; color:#B9BBC6; vertical-align:middle; }
.pager a.on { color:#1A399E; border-radius:50%; background:#F1F7FE; }
.pager a.btn {position:relative; min-width:40px; max-width:40px; min-height:40px; max-height:40px; padding:0; border-radius: 0; margin:0 !important;}
.pager a.btn::before { content:''; position:absolute; left:50%; top:50%; transform:translate(-50%, -50%);  width:16px; height:16px; background:url(/admin/resource/images/ico_comm.png) no-repeat; opacity: 0.5;}
.pager a.btn.first::before {background-position: -460px 0;}
.pager a.btn.end {transform:rotate(180deg);}
.pager a.btn.end::before { background-position: -460px 0; }
.pager a.btn.prev::before { background-position:-420px 0 }
.pager a.btn.next::before { background-position:-40px 0 }
.btn-group { margin-top: 24px; text-align: center; line-height: 0; font-size: 0; }
.btn-group .btn { margin:0 4px; }

.ui-modal-footer .pager {left:0; transform: none; }
.ui-modal-footer .pager a {font-size:14px;}

.file-wrap { position:relative; }
.file-wrap + .file-wrap { margin-top:10px; }
.file-wrap .file-title { display:inline-block; min-width:60px; color:#74757E; font-weight:600;  }
.file-wrap .btn-file { margin-right:10px; font-size:16px; }
.file-wrap input[type="file"] { font-size:16px; }
.file-wrap { padding-bottom:0 !important; }
.file-wrap .input-item { display:inline-block; }
.file-wrap .input-item .inp-text { opacity:0; position:absolute; top:30px; left:0; height:auto; bottom:0; float:left; width:calc(100% - 100px); background:#fff; border-color:#bdbdbd; }
.file-wrap .btn { position:relative; margin-left:0 !important; }
.file-wrap .btn input { z-index:1; position:absolute; top:0; left:0; width:100%; height:100%; opacity:0; cursor:pointer; }
.file-wrap .btn input:focus span { text-decoration:underline; }
.file-wrap .file-list.size { display:inline-block;  font-size:14px; }
.file-wrap .file-list.size .file { display:inline-block; margin-left:10px;  }
.file-wrap .file-list { display:inline-block; position:relative; padding:8px; }
.file-wrap .file-list .file { position:relative; word-break:break-all; font-size:14px;  }
.file-wrap .file-list .file .file-name { overflow:hidden; display:inline-block; text-overflow:ellipsis; white-space:nowrap; vertical-align:middle; }
.file-wrap .file-list .file + .file { margin-top:8px; }
.file-wrap .file-list .btn-del { position:relative; display:inline-block; width:16px; height:16px; text-indent:-9999px; border-radius:15px; background:url(/admin/resource/images/ico_comm.png) no-repeat -60px 0; vertical-align:middle; margin-left:5px; }
.file-wrap.multi { display:block; }
.file-wrap.multi .file-list { padding:10px 0; width:100%; }
.file-wrap.multi .file-list .file-name { max-width:calc(100% - 25px); }
.file-wrap.input { display:block; }
.file-wrap .file-ipt { padding-right:100px; }
.file-wrap .file-ipt .btn { position:absolute; top:0; right:0; width:auto; }
.file-wrap .file-info { padding-left:10px; color:#74757E; }
.file-wrap .file-info + .file-name { padding-left:10px; }
.file-wrap.input.img .file-ipt { padding-right:120px; }

/* tab */
.ui-tab-btns { display:flex; border-bottom:1px solid #DFE0E2; }
.ui-tab-btns:after { content:''; display:block; clear:both; }
.ui-tab-btn { position:relative; flex:1 1; }
.ui-tab-btn { display:block; height:64px; font-size:20px; color:#74757E; }
.ui-tab-btn.selected { font-weight:600; color:#222; }
.ui-tab-btn.selected:after { content:''; display:block; position:absolute; bottom:-1px; left:0; width:100%; height:2px; background:#EC008C; }
.ui-tab-pnl, .ui-tab-pnl[aria-hidden=true] { display:none; visibility:hidden; opacity:0; }
.ui-tab-pnl.selected,
.ui-tab-pnl[aria-hidden='false'] { display:block; visibility:visible; position:relative; opacity:1; z-index:1; transition:opacity 0.3s linear 2s; }
.ui-tab-pnls { padding-top:20px; }

.tab-info { margin-top:20px; text-align:right; font-size:12px; color:#74757E;  }
.tab-info .msg { position:relative; display:inline-block; }
.tab-info .msg:before { content:'*'; }

/* list */
.list-dot li { position:relative; padding-left:13px; line-height:18px; margin-top:4px; font-size:14px; color:#74757E;  }
.list-dot li:after { content:''; display:block; position: absolute; top:6px; left: 0; width:4px;  height:4px; background-color:#74757E; border-radius: 100%;    }
.tbl-base + .list-dot { margin-top:10px; }

/* login */
.admin-login-inner {position:absolute; left:50%; top:50%; width:768px; transform:translate(-50%, -50%); }
.login-top {display:flex; justify-content:space-between; align-items:center; margin-bottom:16px;}
.login-top h1 {font-size:32px; line-height:32px; font-weight:700; color:#F1F7FE; opacity: .5;}
.login-box { width: 768px; padding:80px 64px; background:#fff; border:1px solid #DFE0E2; border-radius:40px 0 40px 40px; box-shadow:0px 3px 10px rgba(34, 34, 34, 0.1);}
.login-box .login-item { position:relative; padding-bottom:39px;}
.login-box .login-item .error-info { position:absolute; top:74px; }
.login-box label {display:block;}
.login-box label + label {margin-top:43px;}
.login-box input {width:100%; height:64px; font-size:24px; line-height:36px; border-bottom:2px solid #DFE0E2; text-indent:26px; font-family: "Pretendard", Sans-serif; }
.login-box input:focus {border-color:#F2303C; outline: none; font-weight:500;}
.login-box .login-btn {width:640px; height:64px; margin-top:24px; color:#fff; font-size:20px; font-weight:600; text-align:center; }
.login-box .login-btn .btn {min-width:100%; min-height: 64px; padding:0; color:#fff; font-size: 20px; background:#EC008C; border-radius: 40px; }
.login-box .login-btn .btn:hover {background:#C02882;}

/* modal */
.ui-modal.ps-top .ui-modal-drag {top: auto;bottom: 0;}
.ui-modal-drag {position: absolute;background-color: aqua;opacity: 0.5;width: 100%;height: 2rem;z-index: 3;}
.scroll-no { overflow:hidden; padding-right:1.7rem; }
.ui-modal.n1 { z-index:201; }
.ui-modal.n2 { z-index:202; }
.ui-modal.n3 { z-index:203; }
.ui-modal.n4 { z-index:204; }
.ui-modal.n5 { z-index:205;}
.ui-modal { display:none; position:fixed; top:0; left:0; z-index:200; width:100%; height:100%; overflow:hidden; overflow-y:hidden; }
.ui-modal.ready { display:flex; overflow-y:hidden; }
.ui-modal.ready .ui-modal-dim { content:""; display:block; position:fixed; top:0; left:0; width:100%; height:100%; opacity:0; background:rgba(0, 0, 0, .5);  transition: opacity .4s ease-in-out ;}
.ui-modal.open.current .ui-modal-dim{ opacity:1; }
.ui-modal.ps-center.is-over { align-items:flex-start; }
.ui-modal.ps-center.is-over .ui-modal-wrap { margin:20px 0; }
.ui-modal.ps-center { justify-content:center; align-items:center; }
.ui-modal.ps-top { justify-content:center; align-items:flex-start; overflow:hidden; }
.ui-modal.ps-bottom { justify-content:center; align-items:flex-end; overflow:hidden; }
.ui-modal-wrap { position:relative; padding:40px; min-width:480px; max-width:900px; min-height: 248px; opacity:0; z-index:10; background:#fff; border-radius:24px 0px 24px 24px; }
.ui-modal-wrap.flex { display:flex; justify-content: flex-end; align-items: center; flex-direction: column; }
.ui-modal.open .ui-modal-wrap { opacity:1; }
.type-full .ui-modal-wrap,
.size-mobile .ui-modal.ps-top .ui-modal-wrap,
.size-mobile .ui-modal.ps-bottom .ui-modal-wrap { margin:0; width:100%; }
.type-small .ui-modal-wrap { width:560px; max-height:600px; }
.type-large .ui-modal-wrap { width:960px; max-width:960px; height:800px; }


/* .ui-modal-body { width:700px; } */

.ui-modal.ps-top .ui-modal-wrap { margin-top:1rem; }
.ui-modal.ps-bottom .ui-modal-wrap { margin-bottom:1rem; }
.ui-modal.ready.open { animation-name:fadeIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards;}
.ui-modal.ready.close { animation-name:fadeOut; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.type-full.open.ps-center .ui-modal-wrap { animation-name:slideUpIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.type-full.close.ps-center .ui-modal-wrap { animation-name:slideUpOut; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.open.ps-center .ui-modal-wrap { animation-name:scaleIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.close.ps-center .ui-modal-wrap { animation-name:scaleOut; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.close.ps-top .ui-modal-wrap { animation-name:slideDownOut; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.open.ps-top .ui-modal-wrap { animation-name:slideDownIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.close.ps-bottom .ui-modal-wrap { animation-name:slideUpOut; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear;  animation-fill-mode:forwards; }
.ui-modal.open.ps-bottom .ui-modal-wrap { animation-name:slideUpIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards;}

.ui-modal.close.ps-center.type-system .ui-modal-wrap { animation-name:slideDownOut; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ui-modal.open.ps-center.type-system .ui-modal-wrap { animation-name:slideDownIn; animation-duration:.2s; animation-iteration-count:1; animation-timing-function:linear; animation-fill-mode:forwards; }
.ie9 .ui-modal.open { display:block; }
.ie9 .ui-modal.open .ui-modal-wrap { opacity:1; margin:0 auto; transform:translate(0, -50%); top:50% }
.ui-modal.type-system .ui-modal-wrap { width:480px; text-align:center; font-size:14px; }
.ui-modal.type-system .ui-modal-body,
.ui-modal.type-system .ui-modal-footer { max-width:100% !important; background:#fff; }
.ui-modal.type-system .ui-modal-wrap strong { font-weight:600; }
.ui-modal.type-system .ui-modal-body .alert-msg .info { display:block; color:#74757E; font-size:14px; line-height:21px; font-weight: 500; }
.ui-modal.type-system .ui-modal-body .alert-msg .txts { display:block; margin-top:4px; color:#222; font-size:20px; line-height:30px; font-weight: 600; }
.ui-modal.type-system .ui-modal-body .alert-msg .txts em { color:#F2303C; }
.ui-modal.type-system .ui-modal-body { padding-bottom:40px; }
.ui-modal.type-system .ui-modal-footer .wrap-group .btn + .btn { margin-left:10px; }

