/* sheet */
.sheet-bottom.datepicker .datepicker-wrap { padding:20px 15px 15px; background:#fafafa; border-radius:16px;  box-shadow: 0 0.3rem 0.3rem rgba(0, 0, 0, 0.1); box-shadow: 0 0.3rem 0.3rem rgba(0, 0, 0, 0.1); position: relative; z-index: 1; }
.sheet-bottom { position: absolute; top: 0; left: 0; opacity: 0; z-index: 301; transition: opacity 0.2s ease-in-out; }
.sheet-bottom.on {opacity: 1; }
.size-mobile .datepicker.sheet-bottom { max-width: 100%; top: auto !important; left: 0 !important; bottom: 0; position: fixed; }
.size-mobile .datepicker-body { max-width: 23rem; margin: 1rem auto; }
.size-mobile .datepicker.sheet-bottom { bottom: -50vh; }
.size-mobile .datepicker.sheet-bottom.on {
top: auto;
-webkit-animation-name: slideUpIn;
        animation-name: slideUpIn;
-webkit-animation-duration: 0.2s;
        animation-duration: 0.2s;
-webkit-animation-iteration-count: 1;
        animation-iteration-count: 1;
-webkit-animation-timing-function: ease-in-out;
        animation-timing-function: ease-in-out;
-webkit-animation-fill-mode: forwards;
        animation-fill-mode: forwards;
}
.size-mobile .datepicker.sheet-bottom.off {
top: auto;
-webkit-animation-name: slideUpOut;
        animation-name: slideUpOut;
-webkit-animation-duration: 0.2s;
        animation-duration: 0.2s;
-webkit-animation-iteration-count: 1;
        animation-iteration-count: 1;
-webkit-animation-timing-function: ease-in-out;
        animation-timing-function: ease-in-out;
-webkit-animation-fill-mode: forwards;
        animation-fill-mode: forwards;
}
.sheet-dim {position: fixed; top: 0; left: 0; width: 100%; height: 100%; z-index: 0; background: rgba(0, 0, 0, 0.3); opacity: 0; transition: opacity 0.2s ease-in-out; }
.sheet-dim.on { opacity: 1; transition: opacity 0.2s ease-in-out; }

