    <script>
        $(function(){
            var familyIdx = 1;
            $(document).on("click",".btnAddFamily",function(e){
                e.preventDefault();
                var family = "";
                familyIdx = familyIdx+1;
                family += '<div class="inputTable titleInput" id="family'+familyIdx+'">';
                family += '    <div class="title">';
                family += '        <h3 class="tit"><span class="family">가족'+familyIdx+'</span><span class="fontLevel2 fz14">(피보험자 / 만19세 이상 가입 시 본인명의 휴대폰 인증) </span></h3>';
                family += '        <div class="rightArea">';
                family += '             <a href="#" class="btn radius btnDel"><span>삭제</span></a>';
                family += '        </div>';
                family += '    </div>';
                family += '    <div class="inputSet">';
                family += '        <ul class="inputArea">';
                family += '            <li class="item">';
                family += '                <div class="selectItem">';
                family += '                    <select name="select_family'+familyIdx+'" id="select_family'+familyIdx+'" title="선택">';
                family += '                        <option value="1">선택1</option>';
                family += '                        <option value="2">선택2</option>';
                family += '                        <option value="3">선택3</option>';
                family += '                    </select>';
                family += '                </div>';
                family += '            </li>';
                family += '        </ul>';
                family += '        <ul class="inputArea">';
                family += '            <li class="item">';
                family += '                <div class="inputBase">';
                family += '                    <label for="name_family'+familyIdx+'">이름</label>';
                family += '                    <input type="text" id="name_family'+familyIdx+'">';
                family += '                </div>';
                family += '            </li>';
                family += '        </ul>';
                family += '        <ul class="inputArea harf73">';
                family += '            <li class="item">';
                family += '                <div class="inputBase">';
                family += '                    <label for="bday_family'+familyIdx+'">생년월일(ex. 19780802)</label>';
                family += '                    <input type="text" id="bday_family'+familyIdx+'">';
                family += '                </div>';
                family += '            </li>';
                family += '            <li class="item">';
                family += '                <div class="sexSelect dataSelect">';
                family += '                    <ul>';
                family += '                        <li><a href="#" data-media-value="M">남</a></li>';
                family += '                        <li><a href="#" data-media-value="F">여</a></li>';
                family += '                    </ul>';
                family += '                    <input type="hidden" id="sex_family'+familyIdx+'">';
                family += '                </div>';
                family += '            </li>';
                family += '        </ul>';
                family += '        <div class="btnArea">';
                family += '            <a href="#" class="btn middle plus btnAddFamily"><span>추가</span></a>';
                family += '        </div>';
                family += '    </div>';
                family += '</div>';
                $(this).closest(".tabCon").append(family);
                selectChg();
                inputAppend();
            })

            $(document).on("click",".btnDel",function(e){
                e.preventDefault();
                $(this).closest(".inputTable").remove();
            })
//            $(".ui-datepicker-trigger").eq(0).click();
        })

    </script>
