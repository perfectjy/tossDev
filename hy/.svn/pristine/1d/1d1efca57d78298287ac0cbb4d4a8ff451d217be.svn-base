    <script>
        $(function(){
            gnb(2,0,0);
            $(".objArea").eq(0).addClass("play");
            var mainTopTxt = []
            $(".mainTopArea .swiper-slide").each(function(){
                mainTopTxt.push($(this).attr("data-media-value"));
            })

            //2017-12-13 수정 : s
            $(".swiper-container-wrap").each(function(){
                var _this = $(this);
                var target = $(this).find(".swiper-container");
                if(target.find(".swiper-slide").length > 1){
                    target.closest(".swiper-container-wrap").find(".swiper-paginationWrap").show();
                    var swiper = new Swiper(target, {
                        nextButton: _this.find('.btn_next'),
                        prevButton: _this.find('.btn_prev'),
                        pagination: _this.find('.swiper-pagination'),
                        loop:true,
                        paginationClickable : true,
                        paginationElement : "button",
                        autoplay: 5000,
                        autoplayDisableOnInteraction: true,
                        onInit : function(){
                            setTimeout(function(){
                                layerPopOpen("mainEventBanner20171227");
                            },3000)
                        },
                        onSlideChangeStart : function(swiper){
                            target.find(".swiper-slide").find(".objArea").removeClass("play");
                        },
                        onSlideChangeEnd : function(swiper){
                            if(target.hasClass("mainTopArea")){
                                target.find(".swiper-slide.swiper-slide-active").find(".objArea").eq(0).addClass("play");
                                var prev_num = Number(target.find(".swiper-slide.swiper-slide-active").attr("data-swiper-slide-index"))-1;
                                var next_num = Number(target.find(".swiper-slide.swiper-slide-active").attr("data-swiper-slide-index"))+1;
                                if(prev_num < 0){
                                    prev_num = mainTopTxt.length-1
                                }
                                if(next_num > mainTopTxt.length-1){
                                    next_num = 0
                                }
                                _this.find('.btn_prev').text(mainTopTxt[prev_num]);
                                _this.find('.btn_next').text(mainTopTxt[next_num]);
                            }
                        }
                    });
                    _this.find(".btnStop").click(function(e){
                        e.preventDefault();
                        if(!$(this).hasClass("on")){
                            $(this).addClass("on");
                            $(this).text("재생");
                            swiper.stopAutoplay();
                        }else{
                            $(this).removeClass("on")
                            $(this).text("정지");
                            swiper.startAutoplay();
                        }
                    })

                    _this.on("mouseenter foucsin",function(){
                        if(!_this.find(".btnStop").hasClass("on")){
                            swiper.stopAutoplay();
                        }
                    })
                    _this.on("mouseleave foucsout",function(){
                        if(!_this.find(".btnStop").hasClass("on")){
                            swiper.startAutoplay();
                        }
                    })
                    target.find(".swiper-slide a").on("focus",function(){
                        $(this).css("position","fixed");
                        //swiper.slideTo(Number($(this).attr("data-swiper-slide-index"))-1)
                        $(this).css("position","relative");
                    })
                }
            })
            //2017-12-13 수정 : s
            $(".fullBannerWrap").slideDown()

        })
    </script>
