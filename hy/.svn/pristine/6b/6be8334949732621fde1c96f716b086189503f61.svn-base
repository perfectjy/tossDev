<!DOCTYPE html>
<html lang="ko">
<head>
<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=yes">
<meta name="format-detection" content="telephone=no, address=no, email=no">
<title>현대해상다이렉트</title>
<script type="text/javascript" src="../_ui_include/head.js"></script><!-- //Include: head -->
<style>
	.layoutBody{position: relative;background-color: #fff;}
	.layoutBody .label{padding: 4px 10px 5px;background-color: #0886c5; color: #fff; font-weight: normal;font-size: 14px;}
	.layoutBody > .label,
	[data-id] > .label{background-color: #0886c5 !important; color: #fff !important;}
	[data-class] > .label{background-color: #9ac4de; color: #222;}
	.layoutBody .etc{position: absolute;top: 3px;right: 10px;font-size: 12px;font-weight: normal;color: #fff;}
	[data-id] > .etc{color: #fff !important;}
	[data-class] > .etc{color: #222;}
	[data-depth]{position: relative; margin:10px 20px 15px; border: 1px solid #666; border-radius: 4px;overflow: hidden;}
</style>
<script>
	jQuery(document).ready(function($) {
		if($('body').hasClass('layoutBody') || $('body').find('.layoutBody').length < 1){
			$('body').removeClass('layoutBody').wrapInner('<div class="layoutBody"></div>');
		}
		$('.layoutBody').each(function(){
			var idx = 0;
			$(this).attr('data-depth', idx);

			$(this).find('div').each(function(){
				var $this = $(this);
				var idStr = $this.attr('id');
				var classStr = $this.attr('class');

				if(!!idStr) $this.attr('data-id', idStr).removeAttr('id');
				if(!!classStr) $this.attr('data-class', classStr).removeAttr('class');

				if($this.siblings('div').attr('data-depth') != undefined) idx = parseInt($this.siblings('div').attr('data-depth'));
				else idx = parseInt($this.parent('div').attr('data-depth')) + 1;

				if(idx % 2) $this.css({'background' : '#fff', 'box-shadow' : '4px 4px 4px 0 rgba(0,0,0,0.'+Math.ceil(idx/1.5)+')'});
				else $this.css({'background' : '#efefef', 'box-shadow' : '4px 4px 4px 0 rgba(0,0,0,0.'+Math.ceil(idx/1.5)+')'});

				$this.attr('data-depth', idx);

				if(idStr){
					if(classStr) $this.prepend('<div class="label">#'+idStr+'.'+classStr+'</div>');
					else $this.prepend('<div class="label">#'+idStr+'</div>');
				}else $this.prepend('<div class="label">.'+classStr+'</div>');

			});

			$(this).prepend('<div class="label">body</div>');
		});
	});
</script>
</head>

<body data-channel="pc" data-login="true" data-category="guide" data-category-sub="layout">
	<div id="skip"></div>
	<!-- wrap -->
	<div id="wrap">
		<div id="header">
			<div id="gnb"></div>
		</div>

		<div id="container">
			<!-- content -->
			<div id="content">
				<div class="breadcrumb"></div>
				<div class="section">
					<div class="governing"></div>
				</div>
			</div>
			<!-- //content -->
		</div>


		<!-- <script type="text/javascript" src="../_ui_include/quick.js"></script> -->
		<div id="quick"></div>
		<!-- //Include: quick -->

		<div id="footer" class="footer">
			<!-- <script type="text/javascript" src="../_ui_include/footer.js"></script> -->
		</div>
	</div>
	<!-- //wrap -->
</body>
</html>