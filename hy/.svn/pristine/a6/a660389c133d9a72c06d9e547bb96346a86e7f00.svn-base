//Guide CSS: module.svg.html 사용
.guide_svg{
	ul{@include grid(10, 8px, 8px);
		justify-content: normal!important;
		li{position: relative; height:160px; border: 1px solid #ccc; border-radius: 12px; text-align: center;
			i{display: block; height: 90%;}
			.name{position: absolute; bottom: 10px; left: 4px; right: 4px; font-size: 12px;}
			&.black{background-color: #909090; color: #fff;}
		}
		&.big{
			@include grid(2, 10px, 10px);
			li{height:200px;}
		}
	}
}
@media (max-width: 1800px){.guide_svg ul{@include grid(8, 8px, 8px);} }
@media (max-width: 1600px){.guide_svg ul{@include grid(7, 8px, 8px);} }
@media (max-width: 1400px){.guide_svg ul{@include grid(6, 8px, 8px);} }
@media (max-width: 1200px){.guide_svg ul{@include grid(5, 8px, 8px);} }
@media (max-width: 1000px){.guide_svg ul{@include grid(4, 8px, 8px);} }
@media (max-width: 800px){.guide_svg ul{@include grid(3, 8px, 8px);} }
@media (max-width: 600px){.guide_svg ul{@include grid(2, 8px, 8px);} }
@media (max-width: 400px){.guide_svg ul{@include grid(1, 8px, 8px);} }


$svg_test:"data:image/svg+xml,%0A%3Csvg width='72px' height='72px' viewBox='0 0 72 72' version='1.1' xmlns='http://www.w3.org/2000/svg' xmlns:xlink='http://www.w3.org/1999/xlink'%3E%3Cg id='Symbols' stroke='none' stroke-width='1' fill='none' fill-rule='evenodd'%3E%3Cg id='icon_error_72'%3E%3Crect id='box' x='0' y='0' width='72' height='72'%3E%3C/rect%3E%3Crect id='Rectangle' fill='%23D9DADC' x='6' y='6' width='60' height='60' rx='30'%3E%3C/rect%3E%3Cg id='Group' transform='translate(34.000000, 23.000000)' fill='%23FFFFFF'%3E%3Ccircle id='Oval' cx='2' cy='24' r='2'%3E%3C/circle%3E%3Crect id='Rectangle' x='0' y='0' width='4' height='16' rx='2'%3E%3C/rect%3E%3C/g%3E%3C/g%3E%3C/g%3E%3C/svg%3E";
i.svg_test{background:url($svg_test) no-repeat center center;}


//Test Function
$svg_test_func: svg_color($svg_test, '#3b92e5', '%23D9DADC');
$svg_test_func2: svg_color($svg_test_func, '#9900cc', '%23FFFFFF');
