urrent\r\n\t\t// active : over\r\n\t\t> li.on > a,\r\n\t\t> li.on:hover > a{ border:1px solid #bbb; border-bottom:1px solid #ddd; color:#fff; /*font-weight:bold;*/ background:#bbb}\r\n\r\n\t\t> li.active > a,\r\n\t\t> li.hover > a,\r\n\t\t> li:hover > a{background:#efefef;} //2Depths over 시 1Depths 활성화\r\n\r\n\t\t//1Depths Hover Line Motion\r\n\t\t> li > a:after{position:absolute;content:''; bottom:-2px;left:50%;right:50%;height:0;background-color:#fff;transition: backgorund ease-in-out 0.2s;}\r\n\t\t> li.active > a:after,\r\n\t\t> li:hover > a:after{bottom:0;left:0;right:0;height:3px;background-color:#bbb;}\r\n\t\t> li.on.active >a {background-color:#bbb;}\r\n\t}\r\n}\r\n\r\n\r\n\r\n//2Depth : sub Nav;\r\n.tab_nav{\r\n\tli.active > .subNav{display:block;}\r\n\r\n\t//가로 메뉴\r\n\t.subNav{display:none;position:absolute;left:0;right:0;bottom:0;margin-left:10px;z-index:999; \r\n\t\t> ul{position:relative;padding:0;\r\n\t\t\t&:after{content:''; clear:both; display:table;}\r\n\r\n\t\t\tli{float:left;}\r\n\t\t\tli a{position:relative;display:block;padding:5px 7px 7px;font-size:13px;color:#666; text-decoration:none; letter-spacing:-0.6px}\r\n\t\t\tli a:after{content:''; position:absolute;bottom:0px;left:50%;width:0%;height:2px;background-color:#bbb}\r\n\t\t\tli a:hover{color:#555;}\r\n\t\t\tli a:hover:after\r\n\t\t\t, li.active a:after{left:0;width:100%;transition:all ease-in-out 0.2s}\r\n\t\t} \r\n\t}\r\n\r\n\t//세로메뉴\r\n\t&.vertical{\r\n\t\t& > ul > li{position: relative;}\r\n\r\n\t\t.subNav{left:0;top:40px; margin-left:0px;  min-width:150px;padding: 10px;\r\n\t\t\tbackground: #fff; border: 1px solid #ddd; border-top: none;\r\n\t\t\tbox-shadow: 0 1px 2px 0 rgba(0,0,0,.2);\r\n\t\t\t> ul {\r\n\t\t\t\tli{float:none;}\r\n\t\t\t\tli a{padding:2px 7px 4px;font-size:12px;color:#999;letter-spacing:-0.8px}\r\n\t\t\t\tli a:hover:after\r\n\t\t\t\t,li.active a:after{left:0;width:0;transition:none;}\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n} \r\n\r\n\r\n@mixin nav_xscroll(){\r\n\t&.headerFloating 