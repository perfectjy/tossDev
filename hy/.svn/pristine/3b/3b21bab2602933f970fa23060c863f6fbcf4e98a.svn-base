      return false;
    } else if(resData.resultCode == "201") {
      alert('즉시이체 계좌의 예금주명 조회에 실패하였습니다.\n잠시후 다시 사용하십시오.');
      return;
    } else if(resData.resultCode == "202") {
      alert('즉시이체 계좌의 예금주명과 입력하신 예금주명이 틀립니다.\n확인후 다시 사용하십시오.');
      return;
    } else if(resData.resultCode == "203") {
      alert('은행대기오류 오류로 인해 즉시이체 처리가 실패하였습니다.\n처음 부터 다시 이용해 주세요.');
      return;
    } else if(resData.resultCode == "204") {
      alert('즉시이체 처리중 오류가 발생했습니다 .\n처음 부터 다시 이용해 주세요.');
      return;
    } else if(resData.resultCode == "301") {
      alert('자동이체 계좌의 예금주명 조회에 실패하였습니다.\n잠시후 다시 사용하십시오.');
      return;
    } else if(resData.resultCode == "302") {
      alert('즉시이체 계좌의 예금주명과 입력하신 예금주명이 틀립니다.\n확인후 다시 사용하십시오.');
      return;
      
    } else if(resData.resultCode == "303") {
      alert('자동이체 처리중 오류가 발생했습니다 .\n처음 부터 다시 이용해 주세요.');
      return;
    } else if(resData.resultCode == "402") {
      alert('직접서명에 실패하였습니다.\n처음 부터 다시 이용해 주세요.');
      return;
    } else if(resData.resultCode == "501") {
      //alert("보험료 설계 확정중 오류가 발생했습니다.");
      //openLayerCompleteFail();
      openLayer('completeFailLayer');
      return;
