    <script>
        $(function(){
            $(document).on("click",".rowItem .price a",function(e){
                e.preventDefault();
                var idx = $(this).closest(".rowItem").index();
                $(this).closest(".priceInfomationArea").addClass("openPop");
                if(!$(this).closest(".rowItem").hasClass("on")){
                    $(this).closest(".priceInfomationArea").find(".openPopContentArea").hide().removeClass("on");
                    $(this).closest(".priceInfomationArea").find(".rowItem").removeClass("on").eq(idx).addClass("on");
                    var target = $(this).closest(".rowItem").find(".openPopContentArea");
                    var top = ($(this).closest(".rowItem").height() * idx)-(idx+1);
                    target.css("top",-top).show();
                    setTimeout(function(){
                        target.addClass("on")
                        selectChg()
                    },1)

                }
            })
            $(document).on("click",".openPopContentArea .btnClose",function(e){
                e.preventDefault();
                $(this).closest(".priceInfomationArea").removeClass("openPop");
                $(this).closest(".priceInfomationArea").find(".openPopContentArea").removeClass("on");
                $(this).closest(".priceInfomationArea").find(".rowItem").removeClass("on")
                setTimeout(function(){
                    $(".openPopContentArea").hide()
                },200)

            })
            $(document).on("change",".typeSelect",function(){
                var val = $(this).val();
                var target = $(this).closest(".inScrollArea");
                target.find(".tabCon").removeClass("on");
                target.find(".tabCon."+val).addClass("on");

            })
            /*tabWrap*/
            $(document).on("click",".tabWrap .tabList li a",function(e){
                e.preventDefault();
                var idx = $(this).parent().index();
                if(idx == 0){
                    $(this).closest(".openPopContentArea").find(".etcCon").show();
                }else{
                    $(this).closest(".openPopContentArea").find(".etcCon").hide();
                }
            })
        })
    </script>
