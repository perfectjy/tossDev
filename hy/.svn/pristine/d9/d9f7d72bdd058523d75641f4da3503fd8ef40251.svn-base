<script>
    $(function(){
        $(document).on("click",".tabCon .zipList a",function(e){
            e.preventDefault();
            var val = $(this).find(".addrdata").text();
            var $target = $(this).closest(".tabCon");
            $target.find(".zipStep").hide();
            $target.find(".zipStep2").show();
            $target.find(".zipStep2 .addr").val(val);
        })

        $(".wrap_zipcode li a").on("click",function(){
            var idx = $(this).parent().index();
            var target = $(this).closest("ul");
            target.addClass("on");
            target.find("li").removeClass("on").eq(idx).addClass("on")
        })
    })
</script>
