<script>
	$(function() {
		UI.init();

    $('.button_filter li').eq(0).addClass('on');
    $('.sub_filter .tab_wrap').eq(0).show().find('li').eq(0).addClass('on');
    $('.button_filter a').on('click',function(){
      let idx = $(this).parent().index();
      $('.button_filter li').removeClass('on');
      $(this).parent().addClass('on');

      $('.sub_filter .tab_wrap').hide();
      $('.sub_filter .tab_wrap li').removeClass('on');
      $('.sub_filter .tab_wrap').eq(idx).show().find('li').eq(0).addClass('on');
    })
    $('.sub_filter a').on('click',function(){
      $('.sub_filter .tab_wrap li').removeClass('on');
      $(this).parent().addClass('on');
    })
	});
</script>
