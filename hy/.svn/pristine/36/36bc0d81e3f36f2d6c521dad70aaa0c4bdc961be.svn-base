.listStyle.number > li:nth-child(12):before{content:'⑫'}
.listStyle.number > li:nth-child(13):before{content:'⑬'}

.listStyle.number2 > li{ padding-left:17px;color:#555555;}
.listStyle.number2 > li:before{ display:block; content:''; position:absolute; left:0; top:-1px;}
