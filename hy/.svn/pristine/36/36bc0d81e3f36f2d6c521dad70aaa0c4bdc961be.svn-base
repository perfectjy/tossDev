/* 20180125 아래 내용 신규 */
/* Layout */
.skip_navi{position:relative;z-index:9999;}
.skip_navi a{display:inline-block;width:200px;position:absolute;left:0px;top:-30px;background:silver}
.skip_navi a:focus{top:0;}
#wrap{height:100%;}
#header{border-top:solid 2px #f68b1f;background:url('https://direct.hi.co.kr/images/common/bg_header1.gif') repeat-x left bottom;padding-bottom:4px;position:relative;}
.main #header{background:none}
#header .header_inn{height:94px;}
.header_utile{text-align:right;padding-top:10px;height:20px}
.header_utile .user_sign{display: inline-block}
.header_utile .user_sign span{background:url('https://direct.hi.co.kr/images/common/icon_usersign1.gif') no-repeat left center;padding-left:19px;}
.header_utile .user_sign strong{color:#d04a00;}
.header_utile .user_sign a{display:inline-block}
.header_utile ul{display:inline-block;margin-top:1px}
.header_utile li{display:inline-block;background:url('https://direct.hi.co.kr/images/common/bg_bar01.gif') no-repeat 2px center; padding-left:11px}
.header_utile li{.display:inline;}
.header_utile li:first-child{background:none}
.header_utile li.all_menu a{display:inline-block;background:url('https://direct.hi.co.kr/images/common/icon_all_menu.gif') no-repeat left center;padding-left:17px;margin-left:3px}
.header_utile li.all_menu{padding-left:12px}
#contents{float:left;min-height:300px;width:770px;line-height:1.65em}
#footer{clear:both;min-height:150px;margin-top:30px;border-top:solid 1px #dddddd}
#header .header_inn,
#container,
#footer .footer_inn{width:1080px; margin:0 auto;}
#footer .footer_inn{padding:20px 0;color:#666;font-size:11px}
.footer_gomenu:after{content:'';display:block;clear:both}
.footer_gomenu{overflow:hidden}
.footer_gomenu span{float:left;}
.footer_gomenu .goMenu{float:left;margin-top:5px;}
.footer_gomenu .goMenu li{background:url('https://direct.hi.co.kr/images/common/bg_bar01.gif') no-repeat left center; padding:0 7px 0 8px;letter-spacing:-1px;font-size:12px}
.footer_gomenu .goMenu li:first-child{background:none}
.footer_gomenu .goMenu li.em{color:#555555}
.footer_gomenu .siteP{float:right}
.footer_gomenu ul{overflow:hidden}
.footer_gomenu ul li{float:left;font-size:11px}
.footer_gomenu .info_mobile{float:right}

.footer_address{margin-top:18px;overflow:hidden}
.footer_address p{line-height:1.6em;margin-bottom:2px}
.footer_address p span{padding:0 5px;color:#d5d5d4}
.footer_address_inn{float:left;}
.siteP{float:right}
.siteP li{margin-left:8px;float:left}
.siteP li:first-child{margin-left:0px}


.location{text-align:right;color:#a3a3a3;font-size:11px; padding:12px 0 16px 0}
.location span{color:#686868}
.location em{color:#444}

