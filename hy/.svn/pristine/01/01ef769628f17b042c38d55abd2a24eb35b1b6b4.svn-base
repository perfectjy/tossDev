<script>
    $(function(){

        $(document).on("change",".noHaveSelect",function(){
            var $target = $(this).closest(".layerPopWrap");
            if($(this).val()=="noHave"){
                $(".nohaveInput").show();
            }else{
                $(".nohaveInput").hide();
            }
        })
        $(document).on("click",".countrySearch .btnSubmit",function(){
            var $target = $(this).closest(".layerPopWrap");
            var countryTarget = $(this).closest(".layerPopWrap").attr("data-media-target");
            var value = $(this).closest(".layerPopWrap").find("input[type=radio]:checked").val();
            $target.find(".btnClose").click();
            $(".countryItem .country."+countryTarget).addClass("insert")
            $(".countryItem .country."+countryTarget).find(".con").text(value)
        })
    })
</script>
