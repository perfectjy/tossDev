    <script>
        var countryType = true;
        $(function(){
            $(document).on("change",".bankChk",function(){
                if($(this).val()=="yes"){
                    $(".bankChkCon").slideDown();
                }else{
                    $(".bankChkCon").slideUp();
                }
            })


            $(document).on("change",".koreaOther",function(e){
                if($(this).is(":checked")){
                    $(".bankChkCon .country2").show();
                    $(".bankChkCon .country3").show();
                }else{
                    $(".bankChkCon .country2").hide();
                    $(".bankChkCon .country3").hide();
                }
            })

            $(document).on("click",".country .btnDel",function(e){
                e.preventDefault();
                $(this).closest(".country").removeClass("insert");
                $(this).closest(".insertCon").find(".con").text("");
            })
            $(document).on("click",".btnCountry",function(e){
                e.preventDefault();
                var idx = $(this).parent().index()+1;
                layerPopOpen("countrySearch","country"+idx);
            })

            $(document).on("countryInsert",".country",function(e,target){
                e.preventDefault();
                console.log(target)
            })

        })
    </script>
