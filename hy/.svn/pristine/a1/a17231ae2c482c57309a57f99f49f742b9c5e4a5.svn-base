<script>
	$(function(){
	    $(".popupContentArea .tabCon").addClass("on").css({"height":0,"overflow":"hidden"});
	    $(".popupContentArea .tabCon").eq(0).css({"height":"auto","overflow":"hidden"});
//	    2017-12-04 수정
	    $(".popSlidimgArea").each(function(){
	        var _this = $(this);
	        var target = $(this);
            target.find(".popSlidePrev").on("click",function(){
                if(target.find(".swiper-slide-active").index() == 0){
                    var idx = $(".popSlidTab li.on").index()-1;
                    if(idx >= 0){
                        $(".popSlidTab li").eq(idx).find("a").click();
					}

				}
            })
            target.find(".popSlideNext").on("click",function(){
                if(target.find(".swiper-slide-active").index() == target.find(".swiper-slide").length-1){
                    var idx = $(".popSlidTab li.on").index()+1;
                    $(".popSlidTab li").eq(idx).find("a").click();
                }
            })
            var swiper = new Swiper(target, {
                nextButton: _this.find('.popSlideNext'),
                prevButton: _this.find('.popSlidePrev'),
                pagination: _this.find('.popSlideBtn'),
                wrapperClass: 'swiper-wrapper',
                slideClass: 'swiper-slide',
                paginationElement: 'a',
                paginationClickable : true
            });

		})
//	    2017-12-04 수정 : e
		setTimeout(function(){
            $(".popupContentArea .tabCon").removeClass("on").css({"height":"auto","overflow":"hidden"}).eq(0).addClass("on");
		},500)

	})
</script>
