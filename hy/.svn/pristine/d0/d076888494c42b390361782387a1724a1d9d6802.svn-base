    <script>
        $(function(){
            $(document).on("change",".cardSalePoint",function(){
                var target = $(this).val();
                $(".cardSalePointCon").slideUp();
                $(".cardSalePointCon."+target).slideDown();
                selectChg();
            })
            $(document).on("click",".cardKind .select-title",function(){
                var target = $(this).closest(".cardKind");
                if(!$(this).hasClass("active")){
                    $(this).addClass("active");
                    target.find(".select-option").slideDown();
                }else{
                    $(this).removeClass("active");
                    target.find(".select-option").slideUp()
                }

            })
            $(document).on("click",".cardKind .select-option li a",function(){
                var target = $(this).closest(".cardKind");
                var val = $(this).attr("data-media-value");
                var txt = $(this).find("img").attr("alt");
                target.find("li").removeClass("active");
                $(this).parent().addClass("active");
                target.find(".select-title").removeClass("active").addClass("value").find("strong").text(txt);
                target.find(".select-option").slideUp();

                $(".cardSaleInfo").hide();
                $(".cardSaleInfo."+val).show();
            })
        })
    </script>
