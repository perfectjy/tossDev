/* Common */

body,p,h1,h2,h3,h4,h5,h6,ul,ol,li,dl,dt,dd,table,th,td,form,fieldset,legend,input,textarea,button,select,menu,footer{margin:0;padding:0}

body{font-size:12px;line-height:1.3em;font-family:/*NanumBarunGothic,*/"돋움",Dotum,"굴림",Gulim,"Apple SD Gothic Neo",AppleGothic,Sans-serif;}

/*p{line-height:1.6em}*/



html, body{height:100%}



body{position:relative;background-color:#fff;color:#333;}

.hide, .hide_title{overflow:hidden;position:absolute;top:0;left:-9999px;width:1px;height:1px;background:none;line-height:0} /* 웹접근성1704 */

.none{display:none !important}

.disb{display:block !important}

iframe{border:0}

pre {font-family:'NanumGothic';white-space:pre-line;text-align:left;}



/*table{border-collapse:collapse;width:100%}*/

table{border-collapse:separate;width:100%;border-spacing:0}

img,fieldset{border:0}

ul,ol{list-style:none}

em,address{font-style:normal}

a{color:inherit;text-decoration:none}



input[type=text],

input[type=password],

input[type=tel],

input[type=number],

input[type=date],

input[type=email], 

input[type=search], 

textarea{border:solid 1px #cecece;padding:0.4em 0.4em;/*padding:0.45em 0.4em;*/resize:none;color:#666}

select{border:solid 1px #cecece;padding:4px 6px; height:26px;/*padding:0.3em 0.3em 0.5em 0.4em;height:2.4em;*/line-height:1.5em;color:#666}

@media \0screen {select {height:24px;padding:3px 0.41em 3px;}}

select, input, textarea{font-size:12px}

caption{overflow:hidden;position:absolute;top:0;left:-9999px;width:1px;height:1px;background:none;line-height:0} /* 웹접근성1704 */

input[type='checkbox']:focus, input[type='radio']:focus {outline:2px dotted #a5c7fe;}

/* a:focus{outline:2px dotted #a5c7fe;} 웹접근성1704 */





input[type=text],

input[type=tel],

input[type=number],

input[type=date],

input[type=password], input[type=email], input[type=search]{color:#666;width:127px;}

input[readonly=readonly], select[readonly=readonly] {background-color:#f5f5f5 !important;border:solid 1px #aaa !important;color:#242424 !important}

input[type='text']:disabled, select[disabled=disabled] {background-color:#f5f5f5 !important;border:solid 1px #aaa !important;color:#242424 !important;opacity:1 !important}

.bgRed, input[readonly=readonly].bgRed, input[disabled].bgRed {background:#fff6ec !important;}

input[type=radio]:disabled {background:none !important;border:1px #aaa solid !important;border-radius:100px;color:#242424 !important;opacity:0.5 !important;width:1em;height:1em;}



 

input[type=radio],

input[type=checkbox]{margin-right:0.1em;vertical-align:top;}

input[type=radio]{display:inline-block}



input[type=radio] + label,

input[type=checkbox] + label{margin-right:0.5em}

input[type=checkbox]{margin-right:0.1em}



/* 폼요소 디자인

input[type="radio"], input[type="checkbox"] {width:1.1em; height:1.1em; -webkit-appearance:none;outline:0; -moz-appearance:none;outline:0;border:0}

input[type="radio"] {background:url('../img/common/radio_off.png') no-repeat center center;background-size:1em;}

input[type="radio"]:checked { background:url('../img/common/radio_on.png') no-repeat center center;background-size:1em;}

input[type="checkbox"] {background:url('../img/common/check_off.png') no-repeat left top;background-size:0.99em}

input[type="checkbox"]:checked {background:url('../img/common/check_on.png') no-repeat left top;background-size:0.99em}



::-moz-placeholder{font-size:0.8em;line-height:2em;}

::-webkit-input-placeholder{font-size:0.8em;line-height:2em;}

table td ::-moz-placeholder{font-size:1em;line-height:1.3em}

table td ::-webkit-input-placeholder{font-size:1em;line-height:1.3em}

*/

select, input[type="text"], input[type="password"], input[type="checkbox"], input[type="date"] {vertical-align:middle;}





