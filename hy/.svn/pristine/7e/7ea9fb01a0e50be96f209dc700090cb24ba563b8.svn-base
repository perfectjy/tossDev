// 게시판 상세
.board_view{
  &_header{
    margin-bottom:32px;
    padding-bottom:24px;
    border-bottom:1px solid #EFEFF1;
  }
  &_title{
    margin-top:40px;
    font-size:19px;
    font-weight:700;
    line-height:1.4em;
    letter-spacing:-0.02em;
    color:#1F1F21;
  }
  .benefits_title{
    margin-top:24px;
  }
  .btn_area{
    margin-top:32px;
  }
}

// 컨텐츠 이용안내
.content_use_info{
  margin-top:32px;
  padding-top:24px;
  border-top:1px solid #EFEFF1;
  .title{
    font-size:14px;
    font-weight:500;
    line-height:1.5em;
    letter-spacing:-0.04em;
    color:#3F3F41;
    &~.title{
      margin-top:16px;
    }
  }
  ul,ol{
    margin-top:8px;
    li{
      position:relative;
      margin-top:6px;
      padding-left:10px;
      font-size:13px;
      line-height:1.5em;
      letter-spacing:-0.04em;
      color:#555557;
      &:first-child{
        margin-top:0;
      }
    }
  }
  ul{
    li{
      &:after{
        content:'';
        position:absolute;
        top:10px;
        left:1px;
        width:2px;
        height:2px;
        border-radius:50%;
        background-color:#555557;
      }
    }
  }
  ol{
    li{
      padding-left:17px;
      .number{
        position:absolute;
        top:0;
        left:0;
      }
    }
  }
}
.donation_box{
  display:flex;
  @include box(gray,24px 20px);
  justify-content:space-between;
  align-items:center;
  &+.donation_box{
    margin-top:10px;
  }
  .left{
    display:flex;
    align-items:center;
    i{
      display:block;
      width:32px;
      height:32px;
      margin-right:10px;
      background-repeat:no-repeat;
      background-position:50% 50%;
      background-size:100% 100%;
    }
    span{
      font-weight:500;
      line-height:1.5em;
      letter-spacing:-0.04em;
      color:#1F1F21;
    }
  }
  .right{
    strong{
      @include fz17_700();
    }
  }
}
.donation{
  &_list{
    margin-top:32px;
  }
  &_item{
    padding:24px;
    background: #FFFFFF;
    box-shadow: 0px 3px 10px rgba(0, 0, 0, 0.1);
    border-radius: 16px;
    margin-top:10px;
    &:first-child{
      margin-top:0;
    }
    &.compact{
      display:flex;
      align-items:center;
      .donation_image{
        position:relative;
        width:80px;
        height:80px;
        img{
          position:absolute;
          top:0;
          bottom:0;
          left:50%;
          width:auto;
          height:100%;
          transform:translateX(-50%);
        }
      }
      .donation_contents{
        margin-top:0;
        padding-left:14px;
        width:calc(100% - 80px);
      }
      .d_day.end{
        border-color:#DFDFE1;
        color:#767678;
      }
      .donation_title{
        margin-top:6px;
        @include line_clamp(1.5em,42,2);
        font-weight:400;
      }
    }
    &.end{
      .donation_title{
        color:#767678;
      }
    }
  }
  &_image{
    overflow:hidden;
    border-radius:16px;
    img{
      width:100%;
    }
  }
  &_contents{
    margin-top:20px;
  }
  &_info{
    font-size:0;
    .d_day{
      display:inline-block;
      padding:0 6px;
      font-size:10px;
      font-weight:700;
      line-height:18px;
      vertical-align:top;
      letter-spacing:-0.04em;
      color:#F8670D;
      border:1px solid rgba(248, 103, 13, 0.4);;
      border-radius:28px;
    }
    strong{
      margin-top:1px;
      padding-left:8px;
      font-size:12px;
      line-height:1.5em;
      letter-spacing:-0.04em;
      color:#767678;
      vertical-align:top;
      @include ellipsis(calc(100% - 40px));
    }
  }
  &_title{
    margin-top:8px;
    font-size:15px;
    font-weight:500;
    line-height:1.5em;
    letter-spacing:-0.04em;
    word-break:keep-all;
    color:#1F1F21;
    &+.status{
      margin-top:20px;
    }
  }
  &_date{
    margin-top:12px;
    font-size:13px;
    line-height:1.5em;
    letter-spacing:-0.04em;
    color:#767678;
    &+.status{
      margin-top:24px;
    }
  }
  .btn_area{
    .btn{
      color:#1F1F21;
    }
  }
  &+.btn_area.more{
    .btn.more{
      margin-top:24px;
    }
  }
  &_big{
    margin-top:40px;
    padding-bottom:24px;
    border-bottom:1px solid #EFEFF1;
    .donation_contents{
      position:relative;
      margin-top:24px;
    }
    .donation_title{
      margin-top:24px;
      font-size:19px;
      font-weight:700;
      line-height:1.4em;
      letter-spacing:-0.02em;
    }
    .status .status_foot{
      margin-top:12px;
      .left{
        font-size:17px;
      }
    }
    .btn.share{
      position:absolute;
      top:-2px;
      right:0;
      min-width:0;
      width:16px;
      height:16px;
      padding:0;
      border:none;
      &:before{
        content:'';
        position:absolute;
        top:50%;
        left:50%;
        width:36px;
        height:36px;
        margin:-18px 0 0 -18px;
      }
    }
  }
  &_detail{
    margin-top:32px;
    padding-bottom:32px;
    border-bottom:1px solid #EFEFF1;
  }
  &_amount{
    @include box(gray);
    text-align:center;
    span,strong{
      display:block;
    }
    span{
      font-weight:500;
      line-height:1.5em;
      letter-spacing:-0.04em;
      color:#1F1F21; 
    }
    strong{
      margin-top:4px;
      font-size:22px;
      font-weight:700;
      line-height:1.4em;
      letter-spacing:-0.02em;
      color:#F8670D;
    }
  }
  &_slide{
    margin-top:40px;
    .swiper-slide{
      width:200px;
      border:1px solid #EFEFF1;
      a{
        display:block;
      }
      img{
        width:100%;
      }
    }
  }
}
.sub_filter{
  .tab_wrap{
    display:none;
  }
}

.review{
  &_item{
    padding:24px;
    border-radius:16px;
    background-color:#FFFFFF;
    box-shadow:0px 3px 10px rgba(0, 0, 0, 0.1);
    &+.review_item{
      margin-top:10px;
    }
    .category{
      font-size:13px;
      font-weight:500;
      line-height:1.5em;
      letter-spacing:-0.04em;
      color:#F8670D;
    }
    .text{
      margin-top:6px;
      font-size:15px;
      line-height:1.5em;
      letter-spacing:-0.04em;
      color:#1F1F21;
    }
    .info{
      margin-top:8px;
      font-size:0;
      span{
        position:relative;
        margin-left:6px;
        padding-left:9px;
        font-size:13px;
        line-height:1.5em;
        letter-spacing:-0.04em;
        color:#767678;
        &:after{
          content:'';
          position:absolute;
          top:50%;
          left:0;
          width:3px;
          height:3px;
          margin-top:-1px;
          border-radius:50%;
          background-color:#C8C8CA;
        }
        &:first-child{
          margin-left:0;
          padding-left:0;
          &:after{
            display:none;
          }
        }
      }
    }
    .score{
      height:16px;
      span:not(.blind){
        display:inline-block;
        width:16px;
        height:16px;
        background:url($svg_icon_review) 50% 50% no-repeat;
        background-size:100% 100%;
        &.on{
          background-image:url($svg_icon_review_on);
        }
      }
    }
  }
}

.direct_tv{
  a{
    display:block;
    overflow:hidden;
    position:relative;
    border-radius:16px;
    img{
      width:100%;
    }
    &:before{
      content:'';
      position:absolute;
      top:0;
      left:0;
      z-index:1;
      width:100%;
      height:100%;
      background:linear-gradient(360deg, rgba(0, 0, 0, 0.5) 0%, rgba(255, 255, 255, 0) 100%);;
    }
    &:after{
      content:'';
      position:absolute;
      top:50%;
      left:50%;
      z-index:1;
      width:56px;
      height:56px;
      margin:-28px 0 0 -28px;
      background:url($svg_icon_video) 50% 50% no-repeat;
      background-size:100% 100%;
    }
    strong{
      position:absolute;
      z-index:3;
      right:12px;
      bottom:12px;
      left:12px;
      font-weight:700;
      line-height:1.5em;
      letter-spacing:-0.02em;
      color:#FFFFFF;
    }
  }
  &_list{
    margin-top:24px;
    padding-top:24px;
    border-top:1px solid #EFEFF1;
    .item{
      border-radius:16px;
      background-color:#ffffff;
      box-shadow:0 4px 16px rgba(0, 0, 0, 0.08);
      &+.item{
        margin-top:10px;
      }
      a{
        display:flex;
        padding:16px;
        align-items:center;
      }
      .thumb{
        overflow:hidden;
        border-radius:8px;
        width:103px;
        img{
          width:100%;
        }
      }
      .content{
        margin-left:14px;
        strong,span{
          display: block;
        }
        strong{
          color:#1F1F21;
        }
        span{
          position:relative;
          margin-top:6px;
          padding-left:16px;
          font-size:11px;
          line-height:1.4em;
          letter-spacing:-0.04em;
          color:#767678;
          &:after{
            content:'';
            position:absolute;
            top:50%;
            left:0;
            width:12px;
            height:12px;
            margin-top:-6px;
            background:url($svg_icon_video_02) 50% 50% no-repeat;
            background-size:100% 100%;
          }
        }
      }
    }
  }
}
