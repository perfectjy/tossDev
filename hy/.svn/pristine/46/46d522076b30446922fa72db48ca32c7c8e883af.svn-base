sc {\n  display: block;\n  padding-left: 34px;\n  color: #555557;\n  font-size: 12px;\n  font-weight: 400;\n}\n.sticky_area .plan_list .plan_list_item .form_area {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n}\n.sticky_area .plan_list .plan_list_item .form_area .form_wrap {\n  flex: 1;\n  justify-content: center;\n  padding: 10px 8px;\n  color: #767678;\n  font-weight: 400;\n  text-align: center;\n  border-bottom: 1px solid #EFEFF1;\n}\n.sticky_area .plan_list .plan_list_item .form_area .form_wrap select {\n  height: auto;\n  color: #767678;\n  font-size: 15px;\n  font-weight: 400;\n  text-align: center;\n  background: url(\"data:image/svg+xml,%3Csvg width='17' height='16' viewBox='0 0 17 16' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath d='M12.5 7L8.5 11L4.5 7' stroke='%23909090' stroke-width='1.2' stroke-linecap='round' stroke-linejoin='round'/%3E%3C/svg%3E%0A\") no-repeat 100% 50%;\n  padding-right: 0;\n}\n.sticky_area .plan_list .plan_list_item .form_area .form_wrap + .form_wrap {\n  margin-top: 0;\n}\n.sticky_area .plan_list .plan_list_item .form_area .form_wrap::after {\n  display: none;\n}\n.sticky_area .plan_list .plan_list_item .form_area .on {\n  color: #1F1F21;\n  font-size: 15px;\n  font-weight: 500;\n  border-bottom-color: #FFF4EB;\n  background-color: #FFF4EB;\n}\n.sticky_area .plan_list .plan_list_item .form_area .on select {\n  color: #1F1F21;\n  font-size: 15px;\n  font-weight: 500;\n  background: url(\"data:image/svg+xml,%3Csvg width='24' height='24' viewBox='0 0 24 24' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath d='M6 10L12 16L18 10' stroke='black' stroke-width='1.5' stroke-linecap='round' stroke-linejoin='round'/%3E%3C/svg%3E%0A\") no-repeat 100% 50%;\n  background-size: 17px 16px;\n}\n.sticky_area .plan_list .plan_list_item:first-child {\n  margin-top: 0;\n}","@charset \"utf-8\";\r\n\r\n//=======================================================\r\n// Util.mixins\r\n//=======================================================\r\n\r\n\r\n//------------------------------------\r\n// shadow\r\n//------------------------------------\r\n@mixin shadow($type){\r\n\t@if $type =='lg'{ // 0 ~ 1280\r\n\t\t$penumbra-opacity: 0.14 !default;\r\n\t\t$ambient-opacity: 0.12 !default;\r\n\t\t$umbra-opacity: 0.2 !default;\r\n\t\tbox-shadow: 0 16px 38px -12px rgba(0, 0, 0, $penumbra-opacity * 4),\r\n\t\t0  4px 25px 0px rgba(0, 0, 0, $ambient-opacity),\r\n\t\t0  8px 10px -5px rgba(0, 0, 0, $umbra-opacity);\r\n\t}\r\n\t@else if $type =='md'{\r\n\t\tbox-shadow: 0px 3px 6px 0px rgba(0, 0, 0, .2),\r\n\t\t0 1px 10px -5px rgba(#000, 0.08);\r\n\t}\r\n\t@else if $type =='sm'{\r\n\t\tbox-shadow: 0 1px 10px 0px rgba(0, 0, 0, .04),\r\n\t\t0 1px 10px -5px rgba(#000, 0.02);\r\n\t}\r\n\t@else if $type =='none'{\r\n\t\tbox-shadow: none;\r\n\t}\r\n\t@content;\r\n}\r\n\r\n\r\n//------------------------------------\r\n// transition\r\n//------------------------------------\r\n@mixin transitions($prop, $time, $type){\r\n\ttransition: $prop $time $type;\r\n}\r\n\r\n\r\n@mixin common-transition($obj : all, $time : 0s){\r\n\t$transition : cubic-bezier(0.685, 0.0473, 0.346, 1);\r\n\ttransition:$obj .33s $transition $time;\r\n}\r\n\r\n@mixin rotate($value) {\r\n\ttransform: rotate($value);\r\n}\r\n\r\n//svgString 색상변경\r\n//@ svg \r\n//@ background positionX\r\n//@ background positionY\r\n//@ svg rotate\r\n//@ new_color\r\n// @ find_color\r\n@mixin background($svg, $posx:center, $posy:center, $rotate:0, $new_color:#222, $find_color :'%23xxx'){\r\n\t$svg : str_replace(str_replace($svg, $find_color,  $new_color), '#', '%23');\r\n\ttransform : rotate(#{$rotate}deg);\r\n\tbackground:url($svg) no-repeat $posx $posy; \r\n}\r\n\r\n@mixin svg_color($svg, $new_color:#222, $find_color :'%23xxx'){\r\n\t$svg : svg_color($svg, $new_color, $find_color);\r\n\tbackground-image:url($svg); \r\n\t// background:url($svg) no-repeat 0 0;\r\n}\r\n\r\n//SVG icon Align Center\r\n@mixin bg_center {\r\n\tcontent:'';position:absolute;top:50%;left:50%;transform:translate(-50%, -50%);\r\n}\r\n\r\n//------------------------------------\r\n// mediaQuery\r\n//------------------------------------\r\n// $breakpoint_xg : 1920px;\r\n$breakpoint_xg_gutter: 1800px;//세로 스크롤 영역 제외\r\n// $breakpoint_xg_margin : 80px;\r\n\r\n// $screen_width: 1280px;//gap: 40, content: 1200\r\n// $screen_max_width: 1920px;// gap: 40, content: 1840\r\n\r\n@mixin mediaQuery($swidth){\r\n\t//Under : 미만 \r\n\t@if $swidth =='md_under'{ // 0 ~ 1280\r\n\t\t@media (min-width: 0) and (max-width: $screen_width){\r\n\t\t\t@content;\r\n\t\t}\r\n\t}\r\n\r\n\t@else if $swidth =='xg_under'{\r\n\t\t@media (min-width: 0) and (max-width: $breakpoint_xg_gutter){\r\n\t\t\t@content;\r\n\t\t}\r\n\t}\r\n}\r\n\r\n\r\n//사용안함\r\n@mixin headerHeight($attr:height){\r\n\t#{$attr} : $gnb_dep1_height_wide;\r\n\t@include mediaQuery('xg_under'){\r\n\t\t#{$attr} : $gnb_dep1_height;\r\n\t}\r\n}\r\n\r\n//사용안함\r\n@mixin responsive($attr, $value1, $value2){\r\n\t#{$attr}: $value1;\r\n\t@include mediaQuery('xg_under'){\r\n\t\t#{$attr}: $value2;\r\n\t}\r\n}\r\n\r\n//사용안함\r\n@mixin responsive_md($attr, $value1, $value2){\r\n\t@include mediaQuery('xg_under'){\r\n\t\t#{$attr}: $value1;\r\n\t}\r\n\t@include mediaQuery('md_under'){\r\n\t\t#{$attr}: $value2;\r\n\t}\r\n}\r\n\r\n//------------------------------------\r\n// Grid\r\n//------------------------------------\r\n// flex grid\r\n@mixin grid($_grid:2, $_gutter:1, $_hgap:0){\r\n\t$gutter: $_gutter;\r\n\t$hgap :$_hgap;\r\n\t$width1: calc(100% / $_grid);\r\n\t$width: calc( #{$width1} - #{$gutter} );\r\n\r\n\t& {display:flex; flex-wrap: wrap;gap: $hgap $gutter; \r\n\t\tjustify-content: space-between;\r\n\t}\r\n\t& > *{width: $width;}\r\n\r\n\t&.no_pad{gap:0;}\r\n\t&.no_pad > *{width: $width1; }\r\n\t// &.no_gap > *{margin-top: 0;}\r\n}\r\n\r\n@mixin grid_old($_grid:2, $_gutter:1, $_hgap:0){\r\n\t$gutter: $_gutter;\r\n\t$hgap :$_hgap;\r\n\t$width1: calc(100% / $_grid);\r\n\t$width: calc( #{$width1} - #{$gutter} );\r\n\t$mgutter: calc((100% - ( ((100% / #{$_grid}) - #{$gutter}) * #{$_grid})) / (#{$_grid} - 1));\r\n\r\n\t@if $_grid == 1 {$mgutter : 0 }\r\n\r\n\t& {display:flex; flex-wrap: wrap;}\r\n\t& > * {width: $width; margin-top: #{$hgap}; margin-right: $mgutter;}\r\n\t& > *:nth-of-type(#{$_grid}n) {margin-right: 0; }\r\n\t@for $c from 1 through $_grid{\r\n\t\t& > *:nth-of-type(#{$c}) {margin-top: 0;}\r\n\t}\r\n\r\n\t&.no_pad > *{width: $width1; margin-right: 0;}\r\n\t&.no_gap > *{margin-top: 0;}\r\n}\r\n\r\n\r\n@mixin div($_grid:2, $_gutter:2%, $_hgap:20px){\r\n\t$gutter: calc($_gutter);\r\n\t$first: calc(($_grid / $grid_column) * 100%) - calc($gutter / 2);\r\n\t$second: calc(100% - ($first + $gutter));\r\n\t$first_nopad: calc(($_grid / $grid_column) * 100%);\r\n\t$second_nopad: calc(100% - $first) - calc($gutter / 2);\r\n\r\n\t@if $_grid == 1 {$_gutter : 0 }\r\n\r\n\t& {display:flex; flex-wrap: wrap;justify-content: space-between;}\r\n\t& > *:nth-of-type(odd) {width: $first; margin-top: #{$_hgap};}\r\n\t& > *:nth-of-type(even) {width: $second; margin-top: #{$_hgap};}\r\n\t& > *:nth-of-type(1) {margin-top: 0;}\r\n\t& > *:nth-of-type(2) {margin-top: 0;}\r\n\t&.no_pad > *:nth-of-type(odd) {width: $first_nopad;}\r\n\t&.no_pad > *:nth-of-type(even) {width: $second_nopad;}\r\n\t&.no_gap > *{margin-top: 0;}\r\n}\r\n\r\n//usage: @include div_px(calc(30% - 4px), 4px);\r\n@mixin div_px($_firstMW:100px, $_gutter:4px){\r\n\t// & > *:nth-of-type(1) {flex:none;min-width: $_firstMW; margin-right:$_gutter}\r\n\t// & > *:nth-of-type(2) {flex:1; margin-left:$_gutter}\r\n\tdisplay:flex;\r\n\t& > *:first-child {flex:none;min-width: $_firstMW; margin-right:$_gutter}\r\n\t& > *:last-child {flex:1; margin-left:$_gutter}\r\n}\r\n\r\n\r\n//float_grid\r\n@mixin float_grid($_grid:2, $_gutter:1, $_hgap:0){\r\n\t@include clearfix();\r\n\t$gutter: $_gutter;\r\n\t$hgap :$_hgap;\r\n\t$width1: calc(100% / $_grid);\r\n\t$width: calc( #{$width1} - #{$gutter} );\r\n\t$mgutter: calc((100% - ( ((100% / #{$_grid}) - #{$gutter}) * #{$_grid})) / (#{$_grid} - 1));\r\n\r\n\t@if $_grid == 1 {$mgutter : 0 }\r\n\r\n\t& > * {\r\n\t\tbox-sizing: border-box;\r\n\t\tmargin-left: 0 !important;\r\n\t\tmargin-right: 0 !important;\r\n\t\tpadding-left: 0 !important;\r\n\t\tpadding-right: 0 !important;\r\n\t\tfloat:left;\r\n\t\twidth: $width;\r\n\t\tmargin-right: $mgutter !important;\r\n\t\tmargin-top: #{$hgap};\r\n\t}\r\n\r\n\t@if $_grid == 2 {\r\n\t\t$gutter2: calc($gutter / 2);\r\n\t\t$width: calc( #{$width1} - #{$gutter2} );\r\n\r\n\t\t& > * {\r\n\t\t\twidth: $width;\r\n\t\t\tfloat: left;\r\n\t\t\tmargin-right: 0 !important;\r\n\t\t}\r\n\r\n\t\t& > *:nth-child(2n) {\r\n\t\t\tfloat:right;\r\n\t\t}\r\n\t}\r\n\t@else{\r\n\t\t& > *:nth-of-type(#{$_grid}n) {\r\n\t\t\tfloat:right;\r\n\t\t\tmargin-right: 0 !important;\r\n\t\t}\r\n\t}\r\n\r\n\t//hgap\r\n\t@for $c from 1 through $_grid{\r\n\t\t& > *:nth-of-type(#{$c}) {\r\n\t\t\tmargin-top: 0;\r\n\t\t}\r\n\t}\r\n}\r\n\r\n\r\n//One Page 2등분 : 마지막 item 바닥에 붙게;\r\n@mixin grid_between($height : calc(100vh - 202px), $align : left) {\r\n\tdisplay: flex ;flex-direction: column;min-height: $height;\r\n\r\n\t& > :last-child{\r\n\t\tdisplay: flex; flex-grow:1; flex-direction: column; justify-content: flex-end; align-items:$align;\r\n\t}\r\n}\r\n\r\n\r\n// Accordion, Tab 요소 width Full 사용시\r\n\r\n\r\n\r\n//------------------------------------\r\n// Box\r\n//------------------------------------\r\n@mixin box($type: white, $pad: 24px, $radius:16px){\r\n\tposition:relative;\r\n\tbox-sizing: border-box;\r\n\tpadding:$pad;\r\n\tborder-radius:$radius;\r\n\r\n\t@if $type == white {\r\n\t\tborder:1px solid #EAEBED;\r\n\t\tbackground-color:#fff;\r\n\t}\r\n\t@if $type == white_shadow {\r\n\t\tbackground-color:#fff;\r\n\t\tbox-shadow:0px 4px 16px rgba(0, 0, 0, 0.08);\r\n\t}\r\n\r\n\t@if $type == gray {\r\n\t\tborder-color:#F9F9FB;\r\n\t\tbackground-color: #F9F9FB;\r\n\t}\r\n\r\n\t@if $type == gray_line {\r\n\t\tborder-color:#EFEFF1;\r\n\t\tbackground-color: #F9F9FB;\r\n\t}\r\n\r\n\t@if $type == blue {\r\n\t\tborder-color:#F3F9FF;\r\n\t\tbackground-color:#F3F9FF;\r\n\t}\r\n\r\n\t@content;\r\n}\r\n\r\n//레이아웃 \r\n@mixin layoutFull(){\r\n\tmargin-left: -20px;margin-right: -20px;\r\n\t.modal.bottom &{\r\n\t\tmargin-left: -24px;margin-right: -24px;\r\n\t}\r\n}\r\n\r\n\r\n@mixin hr($hr_height: 12px, $hr_hgap: 32px){\r\n\t& {\r\n\t\tposition: relative;\r\n\t\theight: $hr_height;\r\n\t\tpadding-top: $hr_height;//추가\r\n\t\t&:before{\r\n\t\t\tcontent:'';\r\n\t\t\tposition: absolute;\r\n\t\t\tleft: -20px; right: -20px; top: 0;\r\n\t\t\theight: $hr_height;\r\n\t\t\tbackground: #F9F9FB;\r\n\t\t}\r\n\r\n\t\t&.mtb{margin-top: $hr_hgap;margin-bottom: $hr_hgap; padding-top: 0;}\r\n\t\t&.mtb_sm{margin-top: calc($hr_hgap / 2);margin-bottom: calc($hr_hgap / 2); padding-top: 0;}\r\n\t}\r\n\r\n\t& + .comp{margin-top: 0;}\r\n\t&.section,\r\n\t&.comp{\r\n\t\t// margin-top: $hr_height;\r\n\t\theight: auto;\r\n\t\t&:before{\r\n\t\t\t// top: - $hr_height;\r\n\t\t\ttop:0;\r\n\t\t}\r\n\t\t&.mtb{margin-top: calc($hr_hgap + $hr_hgap + $hr_height); //margin-bottom:0;\r\n\t\t\t&:before{\r\n\t\t\t\ttop: - calc($hr_hgap + $hr_height);\r\n\t\t\t}\r\n\t\t}\r\n\t\t&:last-child{\r\n\t\t\tmargin-bottom:0;\r\n\t\t}\r\n\t}\r\n}\r\n\r\n//76\r\n//44\r\n\r\n@mixin hr_top(){\r\n\tposition: relative;padding-top: 44px; \r\n\t&:after{content:''; position: absolute;  left: -20px; right: -20px; top:0; height: 12px; background: #F9F9FB;\r\n\t}\r\n}\r\n\r\n@mixin hr_bottom(){\r\n\tposition: relative;padding-bottom: 44px; \r\n\t&:after{content:''; position: absolute;  left: -20px; right: -20px; bottom:0; height: 12px; background: #F9F9FB;\r\n\t}\r\n}\r\n\r\n@mixin border_bottom() {\r\n\tborder-bottom: 1px solid #efeff1;\r\n}\r\n\r\n\r\n//------------------------------------\r\n// icon\r\n//------------------------------------\r\n@mixin icon_set($width, $height) {\r\n\tcontent: '';\r\n\tposition: absolute;\r\n\twidth: $width;\r\n\theight: $height;\r\n\tbackground-position: 50% 50%; \r\n\tbackground-size: 100% 100%;\r\n\tbackground-repeat: no-repeat; \r\n}\r\n\r\n@mixin icon_left($top, $left, $translateX) {\r\n\ttop: $top;\r\n\tleft: $left;\r\n\ttransform: translate($translateX, -50%);\r\n}\r\n@mixin icon_right($top, $right, $translateX) {\r\n\ttop: $top;\r\n\tright: $right;\r\n\ttransform: translate($translateX, -50%);\r\n}\r\n@mixin icon_center() {\r\n\tleft: 50%;\r\n\ttransform: translateX(-50%);\r\n}\r\n\r\n//위험등급\r\n@mixin risk {\r\n\tposition: relative;\r\n\tflex: 1;\r\n\twidth: 100%;\r\n\theight: 10px;\r\n\tmargin-top: 30px;\r\n\ttext-align: center;\r\n\r\n\t&.on:after {\r\n\t\tcontent: '';\r\n\t\tposition: absolute;\r\n\t\ttop: 50%;\r\n\t\tleft: 50%;\r\n\t\tdisplay: inline-block;\r\n\t\twidth: 10px;\r\n\t\theight: 10px;\r\n\t\tborder: 3px solid $risk1;\r\n\t\tborder-radius: 50%;\r\n\t\tbackground: #fff;\r\n\t\ttransform: translate(-50%, -50%);\r\n\t}\r\n\t&:before {\r\n\t\tcontent: '';\r\n\t\tposition: absolute;\r\n\t\ttop: 0;\r\n\t\tleft: 0;\r\n\t\tdisplay: inline-block;\r\n\t\twidth: 2px;\r\n\t\theight: 10px;\r\n\t\tbackground: #fff;\r\n\t}\r\n\t&:first-child {\r\n\t\tborder-radius: 10px 0 0 10px;\r\n\t\t&:before {display: none;}\r\n\t}\r\n\t&:last-child {border-radius: 0 10px 10px 0;}\r\n\tspan {position: absolute;top: -30px;left: 50%;transform: translateX(-50%);}\r\n}\r\n@mixin risk1() {\r\n\tbackground-color: $risk1;\r\n\t&.on {\r\n\t\tcolor: $risk1;\r\n\t\t&:after {border-color: $risk1;}\r\n\t}\r\n}\r\n@mixin risk2() {\r\n\tbackground-color: $risk2;\r\n\t&.on {\r\n\t\tcolor: $risk2;\r\n\t\t&:after {border-color: $risk2;}\r\n\t}\r\n}\r\n@mixin risk3() {\r\n\tbackground-color: $risk3;\r\n\t&.on {\r\n\t\tcolor: $risk3;\r\n\t\t&:after {border-color: $risk3;}\r\n\t}\r\n}\r\n@mixin risk4() {\r\n\tbackground-color: $risk4;\r\n\t&.on {\r\n\t\tcolor: $risk4;\r\n\t\t&:after {border-color: $risk4;}\r\n\t}\r\n}\r\n@mixin risk5() {\r\n\tbackground-color: $risk5;\r\n\t&.on {\r\n\t\tcolor: $risk5;\r\n\t\t&:after {border-color: $risk5;}\r\n\t}\r\n}\r\n\r\n// etc\r\n@mixin font_set($color,$size,$weight) {\r\n\tcolor: $color;\r\n\tfont-size: $size;\r\n\tfont-weight: $weight;\r\n}\r\n\r\n@mixin beforeBul() {\r\n\tcontent: '';\r\n\tposition: absolute;\r\n\ttop: 50%;\r\n\tleft: 0;\r\n\tdisplay: inline-block;\r\n\twidth: 3px;\r\n\theight: 3px;\r\n\tbackground: #C8C8CA;\r\n\tborder-radius: 50%;\r\n\ttransform: translateY(-50%);\r\n}\r\n@mixin afterBul() {\r\n\tcontent: '';\r\n\tposition: absolute;\r\n\ttop: 50%;\r\n\tright: 0;\r\n\tdisplay: inline-block;\r\n\twidth: 3px;\r\n\theight: 3px;\r\n\tbackground: #C8C8CA;\r\n\tborder-radius: 50%;\r\n\ttransform: translateY(-50%);\r\n}","@charset \"utf-8\";\r\n\r\n//=======================================================\r\n// Util.varibles\r\n//=======================================================\r\n\r\n$channel: \"mobile\";\r\n\r\n\r\n\r\n\r\n// * 경로\r\n//-----------------\r\n$path:      '../images/new/';\r\n$font_path: './new_fonts/';\r\n\r\n\r\n\r\n$screen: (\r\n  \"pc_width\":          1280px,\r\n  \"pc_max_width\":      1920px,\r\n  \"pc_margin\":         40px, //1280 이하\r\n  \"pc_max_margin\":     50px, //1280 이상\r\n  \"mobile_width\":      100%,\r\n  \"mobile_max_width\":  100%,\r\n  \"mobile_margin\":     20px,\r\n  \"mobile_max_margin\": 0\r\n) !default;\r\n\r\n\r\n\r\n\r\n//=================\r\n// * Layout\r\n//-----------------\r\n//screen\r\n$screen_width:        map-get($screen, \"#{$channel}_width\");//gap: 40, content: 1200\r\n$screen_max_width:    map-get($screen, \"#{$channel}_max_width\");// gap: 40, content: 1840\r\n$screen_margin:       map-get($screen, \"#{$channel}_margin\");\r\n$screen_max_margin:   map-get($screen, \"#{$channel}_max_margin\");\r\n\r\n//layout\r\n// $header_height:       100px;\r\n// $breadcrumb_height:   40px;\r\n// $footer_height:       342px;\r\n// $footer_width:        $screen_width; \r\n\r\n//layout MO\r\n$header_height:       52px;\r\n$sidebar_top_height:  52px;\r\n$footer_height:       58px;//메인에만 있음(추후 수정)\r\n\r\n// content\r\n$content_top_space:       40px;//content 상단 여백\r\n$content_bottom_space:    64px; //content 하단 여백\r\n// $btn_bottom_fixed_height: 58px;// 하단 고정형 버튼 높이\r\n$content_footer_height: 90px;// 하단 고정형 버튼 높이\r\n\r\n\r\n//section\r\n$section_space_h:     40px;//section 상단 영백\r\n$comp_space_h:        32px;//comp 상단 영백\r\n\r\n\r\n//Grid\r\n$grid_column: 12;\r\n$grid_gutter: 2%; \r\n$grid_hgap: 20px;\r\n\r\n// * Z-index\r\n//-----------------\r\n// $zindex_skip:         900 !default;\r\n// $zindex_header:       500 !default;\r\n// $zindex_breadcrumb:   409 !default;\r\n// $zindex_quick:        400 !default;\r\n// $zindex_footer:       300 !default;\r\n\r\n$zindex_header:       500 !default;\r\n$zindex_sidebar:      600 !default;\r\n// $zindex_breadcrumb:   409 !default;\r\n$zindex_footer:       300 !default;\r\n$zindex_content_footer: 209 !default;\r\n$zindex_quick:        400 !default;\r\n\r\n$zindex_modal_up:     8300 !default;//alert confirm\r\n$zindex_modal:        8010 !default; //full popup (8010 ~ 8199 까지 팝업위 팝업 가능)\r\n\r\n//modal dimm\r\n$zindex_modal_backdrop: 8000 !default;//full popup\r\n\r\n\r\n\r\n// * GNB \r\n//-----------------\r\n$gnb_dep1_height: 79px;\r\n\r\n\r\n\r\n// * Font\r\n//-----------------\r\n$font_family: 'Noto Sans', 'Malgun Gothic',Dotum,Helvetica,'Apple SD Gothic Neo',Sans-serif;\r\n\r\n//fw\r\n$fw_light:    300 !default;\r\n$fw_regular:  400 !default;\r\n$fw_medium:   500 !default;\r\n$fw_bold:     700 !default;\r\n$fw_base:     $fw_medium;\r\n\r\n// Size\r\n$heding1:     22px;\r\n\r\n\r\n\r\n// * Color\r\n//-----------------\r\n$point_color: #F8670D;//orange \r\n$terms_point_color: #FF2D55; //red\r\n$error_color: #f93636; //red\r\n$quarernary_color : #028CED; //blue\r\n\r\n// Grey text\r\n$grey_130: #F9F9FB;\r\n$grey_150: #F5F5F7;\r\n$grey_170: #F3F4F6;\r\n$grey_190: #F1F1F3;\r\n$grey_200: #EFEFF1;\r\n$grey_250: #E8E8EA;\r\n$grey_300: #DFDFE1;\r\n$grey_350: #C8C8CA;\r\n$grey_400: #B7B7B9;\r\n$grey_500: #909090;\r\n$grey_650: #767678;\r\n$grey_700: #555557;\r\n$grey_750: #3F3F41;\r\n$grey_850: #1F1F21;\r\n\r\n// Grey bg\r\n\r\n\r\n// * Transition\r\n//-----------------\r\n// $transition: transform 0.33s cubic-bezier(0.685, 0.0473, 0.346, 1) 0s !default;\r\n$transition_base: .33s cubic-bezier(0.685, 0.0473, 0.346, 1) 0s;//-- 이름 변경 건\r\n\r\n// * Dimmed \r\n//-----------------\r\n$dimmed_alpha: 0.5;\r\n\r\n\r\n\r\n\r\n// * button\r\n//------------------------------------\r\n// $btn-transition:color .15s ease-in-out, background-color .15s ease-in-out, border-color .15s ease-in-out, box-shadow .15s ease-in-out !default;\r\n$btn-focus-box-shadow:  0 0 0 .2rem rgba(#FF7B02, .25) ;\r\n$btn-active-box-shadow: inset 0 3px 5px rgba(#000, .125) ;\r\n\r\n\r\n\r\n\r\n\r\n// * Table \r\n//-----------------\r\n$table_bgc: #f9f9f9; //bg\r\n$table_bdc: #ccc; //border_color\r\n$table_bdc_strong: #aaa;\r\n$table_bdbc: #bbb; //bottom_color\r\n\r\n\r\n//위험 등급\r\n$risk1: #028CED;\r\n$risk2: #5CB531;\r\n$risk3: #FEBF00;\r\n$risk4: #F8670D;\r\n$risk5: #FF2D55;","@charset \"utf-8\";\r\n\r\n//=======================================================\r\n// Util.include\r\n//=======================================================\r\n\r\n@mixin blind() {\r\n\tposition:absolute;display:inline-block;font-size:1px;width:1px;height:1px;padding:0;margin:-1px;visibility:hidden;overflow:hidden;border:0;clip:rect(0, 0, 0, 0);\r\n}\r\n@mixin clearfix() {\r\n\t&::after {display: block; content: \"\"; clear: both;}\r\n}\r\n@mixin ellipsis($width: 100%) {\r\n\tdisplay: inline-block; max-width: $width; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; word-wrap: normal;\r\n}\r\n@mixin line_clamp($lineHeight, $height, $line){\r\n\toverflow:hidden;\r\n\ttext-overflow:ellipsis;\r\n\tmax-height:$height + px;\r\n\tline-height:$lineHeight;\r\n\tword-wrap:break-word;\r\n\tword-break:keep-all;\r\n\t-webkit-line-clamp:$line; \r\n\t-webkit-box-orient:vertical;\r\n\tdisplay:-webkit-box;\r\n}\r\n\r\n//SVG icon Align Center\r\n@mixin bg_center {\r\n\tcontent:'';position:absolute;top:50%;left:50%;transform:translate(-50%, -50%);\r\n}\r\n\r\n\r\n\r\n\r\n// * Font\r\n//--------------------\r\n\r\n@mixin fz27{font-weight: 500; font-size: 27px; line-height: 37px; letter-spacing: -0.02em; color: #1F1F21; }\r\n@mixin fz27_700{font-weight: 700; font-size: 27px; line-height: 37px; letter-spacing: -0.02em; color: #1F1F21; }\r\n@mixin fz22{font-weight: 700; font-size: 22px; line-height: 1.5em; letter-spacing: -0.02em; color: #1F1F21; }\r\n@mixin fz18{font-weight: 700; font-size: 18px; line-height: 1.5em; letter-spacing: -0.02em; color: #1F1F21; }\r\n@mixin fz17{font-weight: 500; font-size: 17px; line-height: 23px; letter-spacing: -0.02em; color: #1F1F21; }\r\n@mixin fz17_700{@include fz17(); font-weight: 700; }\r\n\r\n//-------------------\r\n@mixin fz16{font-weight: 500; font-size: 16px; line-height: 24px; letter-spacing: -0.02em; color: #1F1F21; }\r\n@mixin fz16_700{@include fz16();font-weight: 700; }\r\n//-------------------\r\n@mixin fz15{font-weight: 500; font-size: 15px; line-height: 21px; letter-spacing: -0.04em; color: #1F1F21; }\r\n@mixin fz15_700{@include fz15(); font-weight: 700; }\r\n@mixin fz15_400{@include fz15(); font-weight: 400; }\r\n//-------------------\r\n@mixin fz14{font-weight: 500; font-size: 14px; line-height: 21px; letter-spacing: -0.04em; color: #1F1F21; }\r\n@mixin fz14_700{@include fz14(); font-weight: 700; color: #1F1F21; }\r\n@mixin fz14_400{@include fz14(); font-weight: 400; color: #555557;}\r\n//-------------------\r\n@mixin fz13_400{font-weight: 400; font-size: 13px; line-height: 1.5em; letter-spacing: -0.02em; color: #767678; }\r\n@mixin fz13{font-weight: 500; font-size: 13px; line-height: 1.5em; letter-spacing: -0.02em; color: #767678; }\r\n@mixin fz13_700{font-weight: 700; font-size: 13px; line-height: 19px; letter-spacing: -0.04em; color: #1F1F21; }\r\n//-------------------\r\n@mixin fz12{font-weight: 500; font-size: 12px; line-height: 1.5em; letter-spacing: -0.04em; color: #767678; }\r\n@mixin fz12_400{font-weight: 400; font-size: 12px; line-height: 1.5em; letter-spacing: -0.04em; color: #767678; }\r\n//-------------------\r\n@mixin fz11{font-weight: 400; font-size: 11px; line-height: 1.5em; letter-spacing: -0.04em; color: #555557; }\r\n\r\n@mixin product_tit {font-weight: $fw_medium; font-size: 16px; line-height: 1.5em; letter-spacing: -0.04em; color: $grey_850;}\r\n\r\n\r\n// 디자인: H1, H2 ~> 퍼블: H2, H3 ~ (1++)\r\n@mixin h2{@include fz22();}//Governing\r\n@mixin h3{@include fz17_700(); color: #3F3F41;}\r\n@mixin h4{@include fz15(); color: #3F3F41;}\r\n@mixin h5{@include fz14(); color: #000;}//[4/5]유의사항만 사용되어지고 있음\r\n\r\n\r\n\r\n\r\n\r\n//=======================================================\r\n// List Style\r\n//=======================================================\r\n@mixin bul_dot(){\r\n\tposition: relative;padding-left: 10px; //line-height: 20px; \r\n\t&:before{content: \"\"; position: absolute; left: 1px; top: 10px; width:2px; height:2px; background-color: #909090; border-radius: 50%;}\r\n}\r\n\r\n@mixin bul_bar(){\r\n\tposition: relative;padding-left: 10px; //line-height: 20px;\r\n\t&:before{\r\n\t\tposition: absolute; content: ''; top: 12px; left: 0; width: 4px; height: 1px;background: #767678; \r\n\t}\r\n}\r\n\r\n@mixin bul_imp(){\r\n\tposition: relative;padding-left:16px;\r\n\t&:before{\r\n\t\tposition: absolute; content: '※'; top: 0; left: 0; font-size:11px;\r\n\t}\r\n\t& small{font-size: 13px;}\r\n}\r\n\r\n@mixin bul_tip(){\r\n\tposition: relative;padding-left:20px;\r\n\t@include font_set($grey_850,15px,$fw_medium);\r\n\t&:before{\r\n\t\tposition: absolute; content: ''; top: 2px; left: 0; width: 16px; height: 16px; background: url($svg_icon_notice_red) no-repeat center center;\r\n\t}\r\n}\r\n\r\n\r\n// EAEBED\r\n//기본폰트: body inherit: @include fz14_400();\r\n@mixin uol_list(){\r\n\ttable{margin-top: 24px;}\r\n\ttable+table, table+p {margin-top: 16px;}\r\n\ttable + .bul_imp{margin-top: 12px;}\r\n\r\n\tp{margin-bottom: 4px;}\r\n\tp + p{margin-top: 12px;}\r\n\t.bul_imp{margin-top: 16px;color: #767678;}\r\n\r\n\tp, div{text-indent: 0;}\r\n\r\n\tul + p,\r\n\tol + p,\r\n\tp + ul,\r\n\tp + ol{margin-top: 12px;}\r\n\r\n\tul + ul,\r\n\tol + ol,\r\n\tul + ol,\r\n\tol + ul{margin-top: 12px;}\r\n\r\n\tstrong{color: #1F1F21; font-weight: 500;}\r\n\r\n\t//list-type: \r\n\t// li: fz14 \r\n\t// li > li : fz13\r\n\tul, ol, &.fz_sm ul, &.fz_sm ol{ \r\n\t\tli{position: relative; margin-top: 8px; color: #3F3F41;word-break: break-all;\r\n\t\t\tp{@include fz13_400(); margin-top: 6px;}\r\n\t\t\tul, ol{margin-top: 6px; \r\n\t\t\t\tli{\t@include fz13_400();color: #767678;margin-top: 6px;\r\n\t\t\t\t\tp{@include fz12_400(); margin-top: 6px;}\r\n\t\t\t\t}\r\n\t\t\t} \r\n\t\t}\r\n\t\tli:first-child{margin-top: 0; }\r\n\t}\r\n\r\n\tol{\r\n\t\tli{margin-top:8px;padding-left: 19px;text-indent:-19px; \r\n\t\t\t&:first-letter {letter-spacing: 3px;}\r\n\t\t\tli{padding-left: 15px;text-indent:-15px; }\r\n\t\t\tul{\r\n\t\t\t\tli{@include bul_bar(); \r\n\t\t\t\t\tpadding-left: 16px;text-indent:0px; \r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t\tol{\r\n\t\t\t\tli{padding-left: 12px;text-indent:-12px;\r\n\t\t\t\t\t&:first-letter {letter-spacing: 0px;}\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n\r\n\tul > li{@include bul_dot();\r\n\t\t> ul >li{@include bul_bar();}\r\n\t}\r\n\r\n\t//page에 따라 bar, dot 가 Change 됨에 따라 개별은 초기화\r\n\tul.bul_bar > li{@include bul_bar();}\r\n\t.bul_bar{padding-left: 0;\r\n\t\t&:before{content:''; width: 0;height: 0;}\r\n\t}\r\n\t.bul_dot{padding-left: 0;\r\n\t\t&:before{content:''; width: 0;height: 0;}\r\n\t}\r\n\r\n\t\r\n\tul, ol > li{\r\n\t\t.bul_imp{font-size: 13px;margin-left: -10px; margin-top: 4px;}\r\n\t\t> ul, ol >li{\r\n\t\t\t.bul_imp{font-size: 12px;}\r\n\t\t}\r\n\t}\r\n\r\n\tol > li{\r\n\t\t.bul_imp{margin-left: -19px;}\r\n\t}\r\n\r\n\r\n\tul, ol{\r\n\t\tli{\r\n\t\t\t.btn_area{margin-top: 6px; text-align: left;}\r\n\t\t\t[class*=btn_]{text-indent:0}\r\n\t\t}\r\n\t}\r\n\r\n\tp + [class*='btn_']{margin-top: 8px;}\r\n\tul + [class*='btn_'],\r\n\tol + [class*='btn_']{margin-top: 8px;}\r\n\tli{\r\n\t\tbr + [class*='btn_']{margin-top: 8px;}\r\n\t}\r\n}\r\n\r\n\r\n//약관/안내 페이지 \r\n//기본폰트: body inherit: @include fz14_400();\r\n@mixin base_list(){\r\n\t@include uol_list();\r\n\r\n\th2{@include h2();}//fz22\r\n\th3{@include h3();}//fz17_700\r\n\th4{@include h4();}//fz15 (500)\r\n\r\n\t// h1\r\n\th1,h2,h3,h4,h5,h6{\r\n\t\tmargin-top: 40px;\r\n\t\t&:first-child{margin-top: 32px;}\r\n\t\t& + ul, & + ol, & + div, & + P{margin-top: 24px;}\r\n\t}\r\n\r\n\th4{margin-top: 24px;\r\n\t\t& + ul, & + ol, & + div, & + P{margin-top: 8px;}//16px;\r\n\t}\r\n\th3 + h3 {margin-top: 24px;}\r\n\tul li, ol li {\r\n\t\tul li, ol li {\r\n\t\t\tfont-size: 14px;\r\n\t\t}\r\n\t}\r\n\t//테이블 보조 및 하단\r\n\th3.subtit + .table {margin-top: 16px;}\r\n}\r\n\r\n\r\n@mixin table(){\r\n\t//[Style based]: border-left, border-bottom\r\n\t$table_border_top: 1px solid #EFEFF1;\r\n\t$table_border_bottom: 1px solid #EFEFF1;\r\n\t$table_border_side: none;\r\n\r\n\t$thead_th_border_bottom: 1px solid #EFEFF1;\r\n\t$thead_th_border_left: 1px solid #EFEFF1;\r\n\t$thead_td_border_bottom: 1px solid #EFEFF1;\r\n\t$thead_last_border_bottom: 1px solid #EFEFF1;\r\n\r\n\t$tbody_th_border_bottom: 1px solid #EFEFF1;\r\n\t// $tbody_th_border_left: 1px solid #EFEFF1;\r\n\t$tbody_th_border_left: 1px solid #F9F9FB;\r\n\t$tbody_td_border_bottom: 1px solid #EFEFF1;\r\n\t$tbody_td_border_left: 1px solid #EFEFF1;\r\n\r\n\t$tfoot_border_top: 1px solid #EFEFF1;\r\n\t$tfoot_td_border_left: none;\r\n\r\n\t//etc: bd_top\r\n\t$tr_bd_strong: 2px solid #EFEFF1; //구분 강조 라인\r\n\r\n\t//bg\r\n\t$thead_th_bg: #F9F9FB;\r\n\t$tbody_th_bg: #F9F9FB;\r\n\t$tbody_td_bg: #fff;\r\n\t$tfoot_th_bg: #f9f9f9;\r\n\t$tfoot_td_bg: #f9f9f9;\r\n\r\n\t// style: bd, bg\r\n\t//----------\r\n\t& { \r\n\t\tposition: relative; border-collapse: collapse;\r\n\t\tborder-top: $table_border_top; border-bottom: $table_border_bottom;\r\n\t\tborder-left:$table_border_side; border-right:$table_border_side;\r\n\r\n\t\tth {border-left: $thead_th_border_left; border-bottom: $tbody_th_border_bottom; }\r\n\t\ttd {border-left: $tbody_td_border_left; border-bottom: $tbody_td_border_bottom; }\r\n\t\ttr > *:first-child {border-left: none;}// Table left-border:none\r\n\t\ttr:last-child{\r\n\t\t\ttd, th {border-bottom: none;}\r\n\t\t}\r\n\t\tthead{\r\n\t\t\tth {border-left: $thead_th_border_left; border-bottom: $thead_th_border_bottom; background: $thead_th_bg;}\r\n\t\t\ttr:last-child th{border-bottom: $thead_last_border_bottom;}\r\n\t\t\tth[rowspan]{border-bottom: $thead_last_border_bottom;}\r\n\t\t}\r\n\t\ttbody{\r\n\t\t\tth {border-left: $tbody_th_border_left;border-bottom: $tbody_th_border_bottom;background: $tbody_th_bg;}\r\n\t\t\ttd {border-left: $tbody_th_border_left;border-bottom: $tbody_th_border_bottom;background: $tbody_td_bg;}\r\n\t\t\tth + td{border-left: $tbody_th_border_left;}\r\n\t\t}\r\n\t\ttfoot{\r\n\t\t\tth{background: $tfoot_th_bg;}\r\n\t\t\ttd{border-left: $tfoot_td_border_left;background: $tfoot_td_bg;}\r\n\t\t\ttr{\r\n\t\t\t\t&:first-child{\r\n\t\t\t\t\tth, td{border-top: $tfoot_border_top;}\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t}\r\n\r\n\t\t//Border option\r\n\t\ttr {\r\n\t\t\tth.bd_l{border-left: $tbody_th_border_left;}\r\n\t\t\ttd.bd_l{border-left: $tbody_td_border_left;}\r\n\t\t\tth.bd_r{border-right: $tbody_th_border_left;}\r\n\t\t\t&.bd_strong{\r\n\t\t\t\tth, td{border-top: $tr_bd_strong;}\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n}\r\n\r\n\r\n@mixin table_style(){\r\n\t& {color: #3F3F41;font-size: 14px; line-height:1.2; font-weight: 400; \r\n\t\tstrong {font-weight: 500; color: #1F1F21;}\r\n\r\n\t\tth,\r\n\t\ttd {height: 50px; padding: 14px;  text-align: left; vertical-align: middle;}\r\n\t\ttd{color: #555557;}\r\n\r\n\t\tthead{\r\n\t\t\tth{text-align: center;}\r\n\t\t}\r\n\t\ttbody{}\r\n\t\ttfoot{\r\n\t\t\tth,\r\n\t\t\ttd{font-weight:500;}\r\n\t\t\tth{color:#000; text-align: left;}\r\n\t\t\ttd{color:$point_color;text-align:right;}\r\n\t\t}\r\n\t}\r\n}\r\n\r\n\r\n\r\n\r\n\r\n"]}