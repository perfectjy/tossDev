/**
* Work
* ------------------
* hm.lee
*/
.comp.gap {
  margin-top: 40px !important;
}

.tab_content .comp.gap {
  margin-top: 32px !important;
}

.list.step {
  margin: 8 0 32px;
}
.list.step .list_item_case {
  width: 100%;
  margin: 0 0 24px;
}
.list.step .list_item_case .box.gray {
  padding: 20px;
}
.list.step .list_item_case .box.gray .btn.text.blue {
  margin-top: 6px;
  background-color: transparent;
}
.list.step .list_item_case .box.gray + .box.gray {
  margin-top: 10px;
  border-radius: 12px;
}
.list.step .list_item_case .box.gray em {
  font-weight: 500;
}
.list.step .list_item_case .box.gray p {
  color: #555557;
}
.list.step .list_item_case .box.gray em + p {
  margin-top: 4px;
}
.list.step .icon {
  content: "";
  position: absolute;
  width: 32px;
  height: 32px;
  background-position: 50% 50%;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  top: 50%;
  left: 0;
  transform: translate(0, -50%);
}

.list.doc {
  margin-top: 16px;
}
.list.doc .list_item {
  flex-wrap: wrap;
  padding: 24px 0;
  border-bottom: 1px solid #efeff1;
}
.list.doc .list_item p {
  width: 100%;
  color: #1F1F21;
  font-size: 15px;
  font-weight: 500;
}
.list.doc .list_item .btn_area {
  display: flex;
  align-items: center;
  margin-top: 16px;
}
.list.doc .list_item .btn_area .btn {
  width: 75px;
}
.list.doc .list_item:last-child {
  border-bottom: 0;
}

.list.link_type .list_wrap {
  padding: 0;
}
.list.link_type a.list_wrap .list_body {
  padding-right: 0;
}
.list.link_type a.list_wrap .list_body:after {
  top: 24px;
  width: 26px;
  height: 26px;
  margin-top: 0;
  background-image: url("data:image/svg+xml,%3Csvg width='26' height='26' viewBox='0 0 26 26' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Ccircle cx='13' cy='13' r='12.5' fill='white' stroke='%23EFEFF1'/%3E%3Cpath d='M12 9L15.5 12.6923L12 17' stroke='black' stroke-width='1.4' stroke-linecap='round' stroke-linejoin='round'/%3E%3C/svg%3E%0A");
}
.list.link_type .list_body {
  flex-wrap: wrap;
  padding: 24px 0 24px 0;
}
.list.link_type .title {
  display: flex;
  align-items: center;
  width: 100%;
}
.list.link_type .title span {
  margin-top: 0;
}
.list.link_type .title .badge + .name {
  margin-left: 8px;
}
.list.link_type .title .name {
  color: #1F1F21;
  font-size: 16px;
  font-weight: 500;
  letter-spacing: -0.04em;
}
.list.link_type .dl_list {
  width: 100%;
  margin-top: 20px;
}
.list.link_type + .btn_area {
  margin-bottom: 32px;
}

.card_list.list_type input + .card_wrap {
  padding: 0;
  border: 0;
  box-shadow: none;
}
.card_list.list_type input + .card_wrap:before {
  top: 0;
  right: 0;
}
.card_list.list_type input + .card_wrap .card_header {
  display: flex;
  align-items: center;
}
.card_list.list_type .card_item {
  border-bottom: 1px solid #efeff1;
}
.card_list.list_type .card_item .card_body {
  margin-top: 20px;
  padding-bottom: 24px;
}
.card_list.breakdown_list .card_item + .card_item {
  margin-top: 10px;
}
.card_list.breakdown_list .dl_list dt {
  width: 86px;
}
.card_list.breakdown_list .dl_list dd {
  position: relative;
  width: calc(100% - 86px);
  text-align: left;
  color: #767678;
}
.card_list.breakdown_list .dl_list dd:before {
  content: "";
  position: absolute;
  top: 50%;
  left: -8px;
  display: inline-block;
  width: 3px;
  height: 3px;
  background: #C8C8CA;
  border-radius: 50%;
  transform: translateY(-50%);
}
.card_list.insurance_payment {
  margin-top: 40px;
}
.card_list.insurance_payment .card_header {
  display: flex;
  align-items: center;
}

.search_area {
  padding-bottom: 32px;
  border-bottom: 12px solid #f9f9fb;
}
.search_area .btn_area {
  margin-top: 40px;
}
.search_area + .comp {
  margin-top: -30px;
  padding-top: 62px;
}
.search_area ~ .governing.nodata[data-icon=nodata2] {
  margin-top: 88px;
}

.title_area.tab_title_area {
  margin-bottom: 16px;
}

